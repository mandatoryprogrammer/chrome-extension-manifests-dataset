{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Draft",
  "version": "2.3",
  "description": "A Chrome App to bring Draft to your Desktop.",
  "minimum_chrome_version": "23",
  "permissions": [
    "webview"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  }
}
