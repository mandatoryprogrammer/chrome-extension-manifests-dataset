{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keyata Media Keys",
  "version": "1.0.8",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Control your favorite web-based music player with your keyboard's media keys",
  "homepage_url": "https://github.com/keyata/keyata",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "manifest_version": 2,
  "commands": {
    "prev": {
      "suggested_key": {
        "default": "MediaPrevTrack",
        "mac": "MediaPrevTrack"
      },
      "description": "Play Previous Track",
      "global": true
    },
    "play-pause": {
      "suggested_key": {
        "default": "MediaPlayPause",
        "mac": "MediaPlayPause"
      },
      "description": "Play/Pause Active Audio",
      "global": true
    },
    "next": {
      "suggested_key": {
        "default": "MediaNextTrack",
        "mac": "MediaNextTrack"
      },
      "description": "Play Next Track",
      "global": true
    },
    "stop": {
      "suggested_key": {
        "default": "MediaStop",
        "mac": "MediaStop"
      },
      "description": "Stop Audio",
      "global": true
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "Keyata Media Keys extension",
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    }
  },
  "content_scripts": [
