{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unbiased Twitter",
  "version": "0.1.0",
  "description": "Obfuscates names and profile pictures in your Twitter feed.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "lib/jquery-3.1.1.js",
        "lib/pnglib.js",
        "lib/identicon.js",
        "obfuscator.js"
      ]
    }
  ]
}
