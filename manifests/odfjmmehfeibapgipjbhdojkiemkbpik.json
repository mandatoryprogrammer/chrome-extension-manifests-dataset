{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto remove HTML5 form validation",
  "description": "This plugin disables HTML5 form validation automatically which is perfect for web developers",
  "version": "1.0",
  "homepage_url": "https://github.com/Nyholm/Chrome-HTML5-form-validation-remover",
  "author": "Tobias Nyholm",
  "short_name": "Remove HTML from errors",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ],
  "manifest_version": 2
}
