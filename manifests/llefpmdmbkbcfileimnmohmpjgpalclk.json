{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SurfTime",
  "description": "measure your online time",
  "homepage_url": "https://github.com/max-pub/SurfTime",
  "manifest_version": 2,
  "version": "1.1",
  "background": {
    "scripts": [
      "STORE.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "bin/clock.thick.64.png"
    },
    "default_title": "SurfTime"
  },
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "idle"
  ],
  "icons": {
    "256": "bin/clock.256.png"
  }
}
