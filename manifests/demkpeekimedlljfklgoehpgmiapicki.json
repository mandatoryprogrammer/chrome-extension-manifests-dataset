{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "The Donald Speaks",
  "description": "On Google News, gives President a chance to comment on every newsworthy event.",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.google.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
