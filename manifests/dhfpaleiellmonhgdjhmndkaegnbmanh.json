{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LNMTL Update Notifier",
  "description": "Generate a notification each time a new chapter is released.",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/book-icon-128.png",
    "default_popup": "window.html",
    "default_title": "LNMTL Update Notifier!"
  },
  "background": {
    "scripts": [
      "js/bg.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "notifications",
    "https://lnmtl.com/"
  ],
  "icons": {
    "128": "images/book-icon-128.png"
  }
}
