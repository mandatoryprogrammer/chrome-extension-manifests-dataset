{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Serendip",
  "version": "0.13",
  "manifest_version": 2,
  "description": "Save the biggest visible picture on your screen",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "downloads",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
