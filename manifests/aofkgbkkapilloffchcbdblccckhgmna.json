{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UOW Loggerrinerer",
  "version": "0.0.6",
  "description": "",
  "options_page": "options.htm",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.htm"
  },
  "permissions": [
    "*://*.uow.edu.au/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://vista.uow.edu.au/webct/logonDisplay.dowebct"
      ],
      "js": [
        "vista.js"
      ]
    },
    {
      "matches": [
        "https://vista.uow.edu.au/webct/entryPageIns.dowebct"
      ],
      "js": [
        "vista_entry.js"
      ]
    },
    {
      "matches": [
        "https://solss.uow.edu.au/sid/sols_logon.display_logon_bottom"
      ],
      "js": [
        "sols.js"
      ]
    },
    {
      "matches": [
        "https://solss.uow.edu.au/sid/sols_logon.display_sols_menu*"
      ],
      "js": [
        "solsmenu.js"
      ]
    },
    {
      "matches": [
        "https://moodle.uowplatform.edu.au/login/index.php"
      ],
      "js": [
        "moodle.js"
      ]
    }
  ]
}
