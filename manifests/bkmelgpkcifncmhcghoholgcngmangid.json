{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PR me",
  "description": "Find someone in your orgination to help review your pull request",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "PR me"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
