{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Duolingo Binge",
  "short_name": "Duo Binge",
  "description": "Automatically click 'Next' in Duolingo",
  "version": "2.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.duolingo.com/**"
      ],
      "js": [
        "arrive.min.js",
        "clicker.js"
      ],
      "run_at": "document_end"
    }
  ]
}
