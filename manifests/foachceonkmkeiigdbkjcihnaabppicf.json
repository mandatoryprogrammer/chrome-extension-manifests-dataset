{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Webmapper",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Constructs a map of the web using the user's browser history.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-19x19.png"
  },
  "permissions": [
    "tabs",
    "history",
    "chrome://favicon/",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  }
}
