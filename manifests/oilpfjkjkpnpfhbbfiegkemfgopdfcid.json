{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Charged",
  "description": "See how positively or negatively charged headlines on news pages are. Green is positive, Red is negative.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "38_icon.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
