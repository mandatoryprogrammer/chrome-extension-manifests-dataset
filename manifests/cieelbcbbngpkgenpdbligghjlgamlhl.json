{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump-Blocker",
  "description": "This extension replaces Trumps face with cats",
  "version": "2.0.0",
  "browser_action": {
    "default_icon": "info.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://google.com/*",
        "*://twitter.com/*",
        "*://facebook.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
