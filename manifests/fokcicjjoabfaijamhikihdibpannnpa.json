{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Heim New Tab",
  "version": "0.2.3",
  "description": "Minimalist new tab with quick access to bookmarks and most visited pages",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "history",
    "sessions",
    "tabs",
    "storage",
    "management"
  ],
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
