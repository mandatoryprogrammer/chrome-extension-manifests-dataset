{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Add to Google Bookmarks (context menu)",
  "description": "Adds page to Google bookmarks",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
