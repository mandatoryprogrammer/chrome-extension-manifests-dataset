{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alertover",
  "description": "Alertover extension in chrome",
  "version": "0.4.1",
  "background": {
    "scripts": [
      "/dist/js/background.js"
    ]
  },
  "icons": {
    "16": "/icon.png",
    "48": "/icon.png",
    "128": "/icon.png"
  },
  "browser_action": {
    "default_icon": "/icon.png",
    "default_popup": "/html/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://api.alertover.com/",
    "http://push.alertover.com/"
  ],
  "optional_permissions": [
    "notifications",
    "http://www.google.com/"
  ]
}
