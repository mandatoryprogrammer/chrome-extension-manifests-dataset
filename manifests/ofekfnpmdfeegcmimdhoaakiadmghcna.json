{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyTools",
  "version": "0.0.0.3",
  "description": "MY工具箱",
  "permissions": [
    "activeTab",
    "bookmarks",
    "declarativeContent",
    "storage",
    "https://www.woainankai.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/tools16.png",
      "32": "images/tools32.png",
      "48": "images/tools48.png",
      "128": "images/tools128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/tools16.png",
    "32": "images/tools32.png",
    "48": "images/tools48.png",
    "128": "images/tools128.png"
  },
  "manifest_version": 2
}
