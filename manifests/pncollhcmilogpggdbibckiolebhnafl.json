{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Speak-Out",
  "version": "2.0",
  "description": "Speak-Out is a small Text to Speech tool to bring you a better internet ğŸ„â€â™€ï¸ğŸ„ğŸ¼ experienceğŸ˜‰!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "font.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "font/futura_bold.otf"
  ],
  "browser_action": {
    "default_icon": "./image/icon48.png",
    "default_popup": "index.html",
    "default_title": "Speak-Out"
  },
  "icons": {
    "16": "./image/icon16.png",
    "48": "./image/icon48.png",
    "128": "./image/icon128.png"
  }
}
