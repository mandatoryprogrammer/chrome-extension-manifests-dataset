{
  "name": "Send Open URLs",
  "description": "This extension sends open URLs to GMail.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Send Open URLs",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://*.googleapis.com/*",
    "https://mail.google.com/*",
    "https://*.google.com/*"
  ]
}
