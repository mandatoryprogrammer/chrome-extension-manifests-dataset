{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Riddit",
  "version": "1.6",
  "description": "Hide posts and block subs on Reddit.com",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://old.reddit.com/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "riddit.js"
      ],
      "css": [
        "riddit.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Riddit",
    "default_icon": "icon.png"
  }
}
