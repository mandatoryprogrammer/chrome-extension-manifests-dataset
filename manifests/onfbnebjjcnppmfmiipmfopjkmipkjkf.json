{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kickass Torrents SearchFocus",
  "description": "Sets the focus to the search field on kickass torrents",
  "version": "1.3",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://kat.ph/*",
        "https://kat.ph/*",
        "http://kickass.to/*",
        "https://kickass.to/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
