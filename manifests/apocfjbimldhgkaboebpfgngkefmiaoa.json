{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stella",
  "icons": {
    "16": "fluidicon.png",
    "24": "fluidicon.png",
    "32": "fluidicon.png",
    "128": "fluidicon.png"
  },
  "version": "0.0.0.4",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "https://api.github.com/",
    "storage"
  ],
  "description": "Notifications of your new github stars!",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
