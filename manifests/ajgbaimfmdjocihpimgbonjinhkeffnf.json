{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OCSP Checker",
  "version": "0.2",
  "description": "Performs an OCSP revocation check of the current TLS/SSL Certificate",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://ocspchecker.com/api/"
  ],
  "web_accessible_resources": [
    "error_ssl_revoked.html",
    "error_ssl_revoked.js",
    "error_ssl_unknown.html",
    "error_ssl_unknown.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "manifest_version": 2
}
