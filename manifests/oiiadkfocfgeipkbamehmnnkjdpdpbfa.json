{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cosmos BAM Disabler",
  "description": "Disable cosmos Deploy to live buttons",
  "version": "0.4.1",
  "author": "Roberto Magale",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "page_action": {
    "name": "Click to change configuration",
    "default_icon": "images/icon.png",
    "default_popup": "src/popup/popup.html"
  },
  "options_page": "src/options2/options.html",
  "background": {
    "persistent": false,
    "scripts": [
      "src/background/background.js"
    ]
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon-128.png"
  }
}
