{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Sidebar Ratings",
  "version": "2.6",
  "manifest_version": 2,
  "description": "See ratings on YouTube's sidebar",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch?v=*"
      ],
      "js": [
        "jquery.js",
        "ratings.js"
      ],
      "run_at": "document_end"
    }
  ]
}
