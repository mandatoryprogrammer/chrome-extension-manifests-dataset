{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AGE (a9lh guide enhancement)",
  "description": "This extension adds extra features for easier tracking of Plailect's arm9loaderhax guide.",
  "version": "1.0.6",
  "author": "Rourke",
  "short_name": "AGE",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/Plailect/Guide/wiki/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon128.png"
  },
  "web_accessible_resources": [
    "options.html",
    "images/close.png",
    "images/check.png",
    "images/check-all.png",
    "images/uncheck-all.png"
  ]
}
