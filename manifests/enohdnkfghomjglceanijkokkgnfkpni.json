{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "battleeye.js"
      ],
      "matches": [
        "*://*.erepublik.com/*/military/battlefield/*"
      ]
    }
  ],
  "description": "Live battle statistics for eRepublik.",
  "manifest_version": 2,
  "name": "BattleEye for eRepublik",
  "short_name": "BE",
  "homepage_url": "https://battleeye.raideer.xyz",
  "version": "2.1.1",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_popup": "settings.html"
  }
}
