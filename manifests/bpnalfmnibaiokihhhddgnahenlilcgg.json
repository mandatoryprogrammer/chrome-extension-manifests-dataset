{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fullscreen search",
  "short_name": "Fs search",
  "description": "Your personal search console",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "history",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "eventPage.js"
    ]
  },
  "web_accessible_resources": [
    "img/svg-out/css/svg/sprite.css-ad864b8a.svg",
    "img/no_favicon.png",
    "img/google.png",
    "img/yandex.png",
    "img/baidu.png",
    "img/bing.png",
    "img/youtube.png",
    "img/duckduck.png",
    "img/wikipedia.png",
    "img/github.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "bundle/bundle.content.min.css"
      ],
      "js": [
        "bundle/bundle.content.min.js"
      ]
    }
  ]
}
