{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitcoin Price Widget",
  "version": "1.0.0",
  "description": "Simple Bitcoin price widget powered by the CoinDesk API",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bit.html"
  },
  "permissions": [
    "activeTab"
  ],
  "author": "Adam Simon"
}
