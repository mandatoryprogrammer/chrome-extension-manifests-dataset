{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Web Historian Edu - History Visualizations",
  "version": "0.9.4",
  "short_name": "Web Historian Edu",
  "description": "Web Historian Edu - Visualize your web use to understand your habits",
  "permissions": [
    "history",
    "tabs",
    "storage",
    "alarms"
  ],
  "browser_action": {
    "default_icon": "images/star48.png"
  },
  "icons": {
    "16": "images/star16.png",
    "48": "images/star48.png",
    "128": "images/star128.png"
  },
  "background": {
    "scripts": [
      "js/app/chrome.js"
    ]
  },
  "offline_enabled": true
}
