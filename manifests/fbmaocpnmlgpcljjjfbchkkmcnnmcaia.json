{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Impersonatr",
  "description": "Session Manager",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "popup/icon.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs"
  ]
}
