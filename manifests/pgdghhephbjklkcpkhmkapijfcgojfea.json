{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "shrinktome",
  "description": "Shrink facebook by 5% every 10th second",
  "version": "1.0.2",
  "author": "Terkel Gjervig",
  "homepage_url": "https://github.com/terkelg/shrinktome",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*",
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "js": [
        "bg.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
