{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lightning To Reckless",
  "version": "1.0",
  "description": "Replaces the text ‘Lightning Network’ with ‘Reckless Network’.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
