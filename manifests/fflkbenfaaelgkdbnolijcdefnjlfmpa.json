{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stash extension",
  "description": "Stash notification extension to show how many pull requests you have",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "permissions": [
      "http://192.168.3.53:7990/*"
    ]
  },
  "permissions": [
    "<all_urls>",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon_48.png"
  ]
}
