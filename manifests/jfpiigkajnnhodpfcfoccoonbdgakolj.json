{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unmost Visited",
  "version": "0.1",
  "description": "Hide the 'Most Visited' section of Chrome's New Tab page",
  "background": {
    "scripts": [
      "startup.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "https://www.google.co.uk/_/chrome/newtab"
  ]
}
