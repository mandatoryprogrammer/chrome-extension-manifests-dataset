{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background_page": "bg.html",
  "name": "Favorite Image",
  "version": "2.1.2",
  "description": "Help you save your favorite images.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "default title"
  },
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "cookies",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "bg.js",
      "data_persistent.js",
      "js/av-min.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "popup.js",
        "data_persistent.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
