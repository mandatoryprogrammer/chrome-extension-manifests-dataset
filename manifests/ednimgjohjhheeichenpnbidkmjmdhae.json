{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Transform Qrcode",
  "version": "1.0.0.5",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "description": "Convert URI to qrcode",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'none'",
  "browser_action": {
    "default_popup": "popup-page/index.html",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_ui": {
    "page": "options-page/index.html",
    "chrome_style": false
  }
}
