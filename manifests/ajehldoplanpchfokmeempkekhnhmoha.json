{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Add StackEdit to any web site that supports Markdown",
  "version": "1.0.4",
  "name": "StackEdit Markdown Extension",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png",
    "512": "icon-512.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "64": "icon-64.png",
      "128": "icon-128.png",
      "256": "icon-256.png",
      "512": "icon-512.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icon.svg",
    "frame.html"
  ],
  "permissions": [
    "storage"
  ]
}
