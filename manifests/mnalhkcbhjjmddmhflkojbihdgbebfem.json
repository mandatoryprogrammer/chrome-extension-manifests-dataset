{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "General Data Monitor",
  "version": "0.2",
  "description": "A complete monitoring tool for your browser activity",
  "author": "General Data",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "view.html"
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2
}
