{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitch UNDELETE",
  "description": "Replaces <deleted message> with its original content",
  "version": "1.0000000",
  "content_scripts": [
    {
      "matches": [
        "https://twitch.tv/*",
        "https://www.twitch.tv/*"
      ],
      "js": [
        "twitch-undelete.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "permissions": [
    "https://twitch.tv/*",
    "https://www.twitch.tv/*"
  ]
}
