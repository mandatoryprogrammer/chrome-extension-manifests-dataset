{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Memorex",
  "description": "Memorize and practice on the go",
  "version": "1",
  "author": "Arsalan Yarveisi",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Memorex"
