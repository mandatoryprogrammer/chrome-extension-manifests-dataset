{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hypertext Magnifies Language",
  "version": "1.0.2",
  "description": "Highlights the H, T, M, and Ls on a webpage.",
  "homepage_url": "http://arts445.courses.bengrosser.com/",
  "manifest_version": 2,
  "browser_action": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "64": "logo-64.png",
    "128": "logo-128.png",
    "256": "logo-256.png",
    "512": "logo-512.png"
  },
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "64": "logo-64.png",
    "128": "logo-128.png",
    "256": "logo-256.png",
    "512": "logo-512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "exclude_globs": [
        "*://*/somewhere/*"
      ],
      "exclude_matches": [
        "*://*/somewhere/*"
      ],
      "run_at": "document_start",
      "css": [
        "hypertext.css"
      ],
      "js": [
        "jquery.min.js",
        "ready.js",
        "hypertext.js"
      ]
    }
  ]
}
