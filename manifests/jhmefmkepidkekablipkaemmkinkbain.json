{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Super Trello",
  "description": "Adds new features to trello",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/b/*/*",
        "https://trello.com/c/*/*"
      ],
      "js": [
        "dist/super-trello.js"
      ]
    }
  ]
}
