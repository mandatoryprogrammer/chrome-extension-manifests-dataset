{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "cow tab",
  "manifest_version": 2,
  "description": "A simple, fast, new tab page, with a retro feel and good advice.",
  "version": "1.3",
  "default_locale": "en",
  "permissions": [
    "management",
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  },
  "background": {
    "scripts": [
      "libraries/private.js",
      "background.js"
    ],
    "persistent": false
  }
}
