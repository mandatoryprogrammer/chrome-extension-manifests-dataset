{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Language Learner",
  "description": "Check Quizlet to highlight requested words",
  "version": "1.2",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.jpg"
  },
  "manifest_version": 2
}
