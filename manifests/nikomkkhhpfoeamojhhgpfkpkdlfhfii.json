{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabsPlus",
  "version": "8.8.5",
  "manifest_version": 2,
  "description": "Manage your tabs",
  "icons": {
    "128": "stack.png",
    "16": "stack.png",
    "48": "stack.png"
  },
  "page_action": {
    "default_icon": "unpin.png",
    "default_title": "Pin tab\n\n[ DON'T USE  it for Last Used Order ]"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
