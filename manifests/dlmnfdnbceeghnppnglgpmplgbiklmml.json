{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JSON Watcher",
  "version": "1.0",
  "description": "convert JSON format from string",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon_16.PNG",
      "32": "images/icon_32.PNG",
      "48": "images/icon_48.PNG",
      "128": "images/icon_128.PNG"
    }
  },
  "icons": {
    "16": "images/icon_16.PNG",
    "32": "images/icon_32.PNG",
    "48": "images/icon_48.PNG",
    "128": "images/icon_128.PNG"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest",
    "<all_urls>",
    "declarativeContent",
    "storage"
  ]
}
