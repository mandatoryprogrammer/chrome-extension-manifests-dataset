{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RT Message Quick Jump",
  "version": "0.4",
  "description": "Provides the ability to quickly navigate between email messages in Request Tracker",
  "content_scripts": [
    {
      "matches": [
        "http://*/*Ticket/Display*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "mousetrap-1.5.3.min.js",
        "content.js"
      ]
    }
  ]
}
