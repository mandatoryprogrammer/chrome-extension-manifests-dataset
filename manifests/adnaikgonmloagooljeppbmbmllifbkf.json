{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NOTE RINSVENT",
  "background": {
    "scripts": [
      "assets/js/jquery.js",
      "assets/js/app.js",
      "assets/js/background.js",
      "assets/js/oauth/chrome_ex_oauthsimple.js",
      "assets/js/oauth/chrome_ex_oauth.js",
      "assets/js/oauth/init.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    },
    "default_popup": "popup.html",
    "default_title": "NOTE RINSVENT"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Alt+R",
        "linux": "Alt+R",
        "mac": "Alt+R",
        "windows": "Alt+R"
      }
    },
    "_execute_page_action": {
      "suggested_key": {
        "default": "Alt+R",
        "mac": "Alt+R",
        "windows": "Alt+R"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "assets/js/jquery.js",
        "assets/js/app.js",
        "assets/js/script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "ru",
  "description": "note.rinsvent.ru request plugin",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "180": "icons/180.png",
    "192": "icons/192.png",
    "512": "icons/512.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>"
  ],
  "short_name": "Клиент сайта note.rinsvent.ru для простого поиска/добавления информации",
  "version": "1.0.0",
  "web_accessible_resources": [
    "chrome_ex_oauth.html"
  ]
}
