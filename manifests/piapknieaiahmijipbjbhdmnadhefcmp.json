{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lepro Upgrade",
  "description": "Улучшения для лепры",
  "version": "1.4.1",
  "icons": {
    "48": "lu48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.leprosorium.ru/*"
      ],
      "js": [
        "jquery.js",
        "lu.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Lepro Upgrade",
    "default_icon": "lu48.png",
    "default_popup": "popup.html"
  }
}
