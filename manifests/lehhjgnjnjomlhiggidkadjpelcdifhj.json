{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Codealike In-The-Zone Dashboard",
  "manifest_version": 2,
  "version": "0.7.6",
  "minimum_chrome_version": "39.0.2140.0",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "hid",
    "alarms",
    "<all_urls>",
    "storage",
    {
      "usbDevices": [
        {
          "vendorId": 10168,
          "productId": 493
        }
      ]
    }
  ]
}
