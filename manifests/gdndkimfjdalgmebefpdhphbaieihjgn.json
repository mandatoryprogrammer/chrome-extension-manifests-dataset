{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kifier",
  "version": "0.3.2",
  "description": "The missing tool to turn your Web 2.0 services into Wiki",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "img/edit-copy.png",
    "default_title": "Copy current page's URL with title",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "lib/jquery-1.7.1.min.js"
    ]
  }
}
