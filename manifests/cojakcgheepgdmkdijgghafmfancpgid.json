{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Let Me Pass",
  "short_name": "letmepass",
  "description": "__MSG_description__",
  "default_locale": "en",
  "author": "Adam Pohorecki",
  "version": "0.3.0",
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "password_helper.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "password_helper.js",
        "content.js"
      ],
      "css": [
        "password_helper.css",
        "uikit.min.css"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "password_helper.css",
    "uikit.min.css",
    "simulate_input.js",
    "OpenSans-Bold.ttf",
    "Roboto-Regular.ttf",
    "logo.png"
  ],
  "manifest_version": 2
}
