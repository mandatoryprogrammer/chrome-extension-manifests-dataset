{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inbox Zero",
  "description": "Reward yourself for reaching inbox zero in gmail",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "inbox-zero.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inbox-zero.jpg"
  ]
}
