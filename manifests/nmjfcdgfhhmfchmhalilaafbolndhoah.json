{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Estimator",
  "description": "Blog Reading Time Estimator",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "Reading Time Estimator",
    "default_popup": "popup.html"
  }
}
