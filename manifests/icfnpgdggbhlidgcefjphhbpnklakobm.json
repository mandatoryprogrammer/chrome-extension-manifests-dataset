{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trello List Resizer",
  "description": "Simple extension for wider Trello lists.",
  "author": "Benjamin Smith",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.trello.com/*"
      ],
      "css": [
        "estiloNuevo.css"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*.trello.com/*"
  ]
}
