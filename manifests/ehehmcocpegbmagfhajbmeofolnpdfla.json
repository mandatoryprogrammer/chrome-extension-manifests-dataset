{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/page.js"
      ],
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Screenshot button for YouTube.",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png"
  },
  "author": "Tuhnik",
  "manifest_version": 2,
  "name": "YouTube Screenshot Button",
  "permissions": [
    "https://*.youtube.com/*",
    "webNavigation"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.4"
}
