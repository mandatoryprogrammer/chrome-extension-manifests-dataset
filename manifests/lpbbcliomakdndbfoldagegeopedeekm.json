{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AngryGM Anti-Anti-Swear Filter",
  "short_name": "AngryGM Anti-Censor",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Replaces swear filter on angrygm.com with corresponding swearwords",
  "homepage_url": "https://theangrygm.com",
  "content_scripts": [
    {
      "matches": [
        "https://theangrygm.com/*",
        "https://*.theangrygm.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "https://theangrygm.com",
    "https://*.theangrygm.com"
  ],
  "key": ""
}
