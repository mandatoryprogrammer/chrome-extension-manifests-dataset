{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter GIF",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Converts MP4s on Twitter into downloadable GIF",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
