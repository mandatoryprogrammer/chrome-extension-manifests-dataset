{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Like KIt",
  "description": "Like Kit Â­ service for the increasing popularity of accounts /Public Vkontakte.",
  "version": "0.1",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}
