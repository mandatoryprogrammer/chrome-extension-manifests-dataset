{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Work Logger",
  "description": "Helps you log your work hours and makes sure that you do. This extension doesn't care how you log your hours, just that you do.",
  "version": "1.2",
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "icons": {
    "32": "overtime-32.png",
    "64": "overtime-64.png",
    "128": "overtime-128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Work Logger",
    "default_icon": "overtime-32.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
