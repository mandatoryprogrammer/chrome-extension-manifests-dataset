{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Proxer Modern Grey",
  "version": "0.3.5",
  "description": "Change the CSS file on the website Proxer.me",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "grey.css"
      ],
      "js": [
        "content.js"
      ],
      "default_icon": [
        "favicon.png"
      ],
      "matches": [
        "https://proxer.me/*"
      ]
    }
  ]
}
