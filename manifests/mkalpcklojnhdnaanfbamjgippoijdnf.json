{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Michael Abon",
  "description": "Media controls when watching Formula 1 on F1 TV",
  "manifest_version": 2,
  "name": "F1 TV Enhanced",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "https://f1tv.formula1.com/*"
      ],
      "js": [
        "src/media-player.js",
        "src/sync-current-time.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "src/background_script/background.js"
    ]
  }
}
