{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search for Selection",
  "version": "1.1",
  "description": "Search many popular websites for your highlighted words.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/search16.png",
    "48": "icons/search48.png",
    "128": "icons/search128.png"
  },
  "permissions": [
    "contextMenus"
  ]
}
