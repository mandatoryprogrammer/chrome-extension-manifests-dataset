{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabs Cache and Management",
  "version": "1.0",
  "description": "Store and manage your tabs!",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent"
  ]
}
