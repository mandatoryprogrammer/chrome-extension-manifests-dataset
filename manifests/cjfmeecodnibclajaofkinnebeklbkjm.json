  "content_scripts": [
    {
      "matches": [
        "*://www.nicovideo.jp/my/top",
        "*://www.nicovideo.jp/my/top/*",
        "*://www.nicovideo.jp/user/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
