{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fangraphs Search",
  "version": "1.1",
  "author": "Zachary Kellicutt",
  "homepage_url": "https://github.com/zkello/fangraphs-search",
  "description": "Search Fangraphs from the address bar or by right clicking",
  "omnibox": {
    "keyword": "fg"
  },
  "web_accessible_resources": [
    "settings.html"
  ],
  "permissions": [
    "contextMenus",
    "http://www.fangraphs.com/*",
    "https://www.fangraphs.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
