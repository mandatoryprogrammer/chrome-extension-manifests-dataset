{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Members Only Posts - Medium",
  "version": "0.2.0",
  "description": "An extension for removing medium only posts",
  "content_scripts": [
    {
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ],
      "matches": [
        "https://medium.com/*"
      ]
    }
  ]
}
