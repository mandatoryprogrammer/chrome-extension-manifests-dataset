{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Image Erosion",
  "version": "1.0",
  "browser_action": {
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "p5.js",
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "webNavigation"
  ]
}
