{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Frame",
  "version": "0.3.2",
  "manifest_version": 2,
  "description": "Frame",
  "homepage_url": "https://github.com/floating/frame",
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "browser_action": {
    "default_title": "Frame",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  }
}
