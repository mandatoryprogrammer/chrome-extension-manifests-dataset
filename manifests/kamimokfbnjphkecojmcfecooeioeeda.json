{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "T411 Downloader",
  "version": "0.43",
  "description": "Eases downloads from the t411 sharing site.",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Click to start downloading all open tabs",
    "default_icon": "icon_19.png"
  },
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "72": "icon_72.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "downloads",
    "tabs",
    "https://*.t411.al/*"
  ]
}
