{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiki-It",
  "description": "Edit and Export Wiki pages as PDFs with just a few clicks.",
  "version": "2.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "optional_permissions": [
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_title": "Wiki-It"
  },
  "icons": {
    "19": "icon-19.png",
    "128": "icon-128.png"
  },
  "short_name": "Wiki-It"
}
