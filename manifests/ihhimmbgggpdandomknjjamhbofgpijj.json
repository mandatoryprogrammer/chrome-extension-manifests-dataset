{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "deface",
  "version": "0.6",
  "description": "Minimal Facebook chat without any distractions",
  "icons": {
    "16": "img/16.png",
    "48": "img/64.png",
    "128": "img/128.png"
  },
  "author": "basiclaser",
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "css": [
        "deface.css"
      ],
      "js": [
        "deface.js"
      ]
    }
  ]
}
