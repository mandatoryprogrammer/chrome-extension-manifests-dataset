{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TranslateIt",
  "version": "1.0",
  "description": "TranslateIt gives the hindi meaning of the selected text on the screen",
  "icons": {
    "128": "icons/translateit48.png",
    "48": "icons/translateit48.png",
    "16": "icons/translateit16.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/",
    "https://*/"
  ]
}
