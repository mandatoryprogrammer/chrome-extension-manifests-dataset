{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Social Snap",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Be visually creative with websites screen and share via social networks",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
