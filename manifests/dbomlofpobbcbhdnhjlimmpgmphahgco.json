{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DevSprout Affiliate Redirect",
  "description": "DevSprout Udemy Affiliate Add-on, plus fix for Udemy Q&A link bug.",
  "version": "2.1",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://www.udemy.com/*/",
        "https://www.udemy.com/course/*/learn/lecture/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
