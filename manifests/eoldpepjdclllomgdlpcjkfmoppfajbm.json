{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo16.png",
    "default_title": "JSON-handle2"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content_scripts.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file://*",
        "file:///*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "description": "It's a browser and editor for JSON document.You can get a beautiful view",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "JSON-handle2",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "json2"
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.3",
  "web_accessible_resources": [
    "JSON-handle2/JSON-handle2.html"
  ]
}
