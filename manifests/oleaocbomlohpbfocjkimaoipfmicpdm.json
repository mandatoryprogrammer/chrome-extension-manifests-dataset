{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OSBot Auther",
  "version": "0.5",
  "content_scripts": [
    {
      "matches": [
        "https://osbot.org/forum/topic/*",
        "https://osbot.org/forum/profile/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "auther.js"
      ]
    }
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
