{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NewTabMe",
  "version": "0.1.29",
  "manifest_version": 2,
  "description": "Open every link in new tab. Use 'X + click' to open the default way.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
