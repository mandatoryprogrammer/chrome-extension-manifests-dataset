{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unseen",
  "short_name": "Unseen",
  "version": "1.0",
  "description": "Facebook without setting off seen notifications",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.facebook.com/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 2
}
