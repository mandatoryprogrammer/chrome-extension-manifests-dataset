{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Prioritizer",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Pivotal Tracker backlog prioritization optimizer using Fisher-Yates machine-learning algorithm",
  "homepage_url": "https://github.com/mkenyon/backlog-prioritizer",
  "icons": {
    "128": "icons/package_games_board.png"
  },
  "default_locale": "en",
  "permissions": [
    "contentSettings",
    "*://*.pivotaltracker.com/*",
    "*://pivotaltracker.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.pivotaltracker.com/*",
        "*://pivotaltracker.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "*://*.pivotaltracker.com/*",
        "*://pivotaltracker.com/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
