{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Langyfier",
  "version": "1.7",
  "description": "Stop automatic translation from the Microsoft Docs to an unwanted language.",
  "manifest_version": 2,
  "permissions": [
    "webNavigation",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "app/background.js"
    ]
  },
  "options_page": "app/settings.html",
  "page_action": {
    "default_popup": "app/popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
