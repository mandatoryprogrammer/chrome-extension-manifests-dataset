{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "OG Reddit",
  "version": "1.2.5",
  "description": "Publish and aggregate select reddit activity on your Facebook Timeline via an Open Graph app.",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.reddit.com/*",
        "https://*.reddit.com/*"
      ],
      "css": [
        "background.css"
      ],
      "js": [
        "lib/fb-all.js",
        "lib/jquery.min.js",
        "lib/jquery-ui.min.js",
        "utils.js",
        "og.js",
        "settings.js",
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "settings.html",
    "images/close.png",
    "images/facebook.png",
    "images/fbwarn.png",
    "images/texture.png",
    "images/texture-light.png"
  ],
  "icons": {
    "16": "16.jpg",
    "48": "48.jpg",
    "128": "128.jpg"
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
