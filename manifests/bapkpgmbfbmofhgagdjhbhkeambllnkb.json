{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Удаление размытия на TJournal",
  "version": "1.0",
  "homepage_url": "https://github.com/x-ider/tjournal-blur-remover",
  "description": "Убирает размытие на сайте при использовании расширения для блокировки рекламы",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.tjournal.ru/*"
      ],
      "css": [
        "tjournal.css"
      ],
      "js": [
        "tjournal.js"
      ]
    }
  ]
}
