{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pinboard enhancer",
  "version": "0.0.5",
  "description": "Pinboard enhancer",
  "content_scripts": [
    {
      "matches": [
        "http://pinboard.in/*",
        "https://pinboard.in/*",
        "https://api.pinboard.in/*"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "quick_copy.js",
        "keybind.js"
      ],
      "run_at": "document_start"
    }
  ]
}
