{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alt-Right to Neo-Nazi",
  "description": "This script goes through every web page you visit and changes the phrase \"Alt-Right\" to \"Neo-Nazi\".",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "200": "200px-Antifascist_Circle_BW.svg.png"
  }
}
