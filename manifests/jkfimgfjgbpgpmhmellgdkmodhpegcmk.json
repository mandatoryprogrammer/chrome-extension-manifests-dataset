{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabSearch",
  "description": "Hopping between tabs",
  "version": "1.0.4",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "32": "icon/icon_32.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon/icon_48.png",
    "default_popup": "popup/searchbar.html"
  }
}
