{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Skip Intro Platzi",
  "description": "Skip Platzi intro on current video",
  "version": "2.0",
  "permissions": [
    "tabs",
    "*://*.platzi.com/clases/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.platzi.com/clases/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Skip Platzi intro"
  },
  "manifest_version": 2
}
