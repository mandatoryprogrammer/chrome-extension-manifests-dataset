{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_title": "Share on Twitter"
  },
  "description": "Share currrent page on Twitter",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Share on Twitter",
  "permissions": [
    "contextMenus",
    "activeTab",
    "cookies",
    "*://twitter.com/*"
  ],
  "short_name": "Share on Twitter",
  "version": "0.4"
}
