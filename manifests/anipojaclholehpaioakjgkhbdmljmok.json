{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OKCash Price Ticker",
  "description": "This extension displays the updated current OK Price every minute (Price is Bittrex latest, updated every minute)",
  "version": "0.3.1",
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "storage",
    "https://bittrex.com/",
    "http://ads.a-ads.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
