{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Change Encoded URL",
  "version": "1.1",
  "description": "Performs URL Decode for the current URL. Allows to modify the decoded URL and navigate to it in one click.",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent"
  ],
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
