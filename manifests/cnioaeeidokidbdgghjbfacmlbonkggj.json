{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PyCon Funnel Filter",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Adds a filter dropdown to the PyCon India funnel page",
  "permissions": [
    "*://in.pycon.org/funnel/2014/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://in.pycon.org/funnel/2014/"
      ],
      "js": [
        "jquery.min.js",
        "pycon_funnel_filter.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
