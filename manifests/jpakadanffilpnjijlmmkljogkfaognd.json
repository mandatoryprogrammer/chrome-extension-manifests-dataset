{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Blocker",
  "short_name": "URL Blocker",
  "description": "Block URLs by blacklist",
  "version": "0.900",
  "browser_action": {},
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage"
  ]
}
