{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flying Cursors",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adds a bunch of cursors",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "velocity.min.js",
        "content.js"
      ]
    }
  ]
}
