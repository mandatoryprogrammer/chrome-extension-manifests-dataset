{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Metro Speed Dial - Modern New Tab page",
  "description": "A modern and simple speed dial meant to make your life easier.",
  "version": "1.0",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/favicon.png"
  },
  "permissions": [
    "tabs"
  ]
}
