{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "description": "Customize tab opening position and closing behavior.",
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Tab Position Customizer 2",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "version": "1.0"
}
