{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ADF Refresh",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Refresh any ADF page removing all its parameters.",
  "homepage_url": "http://pratikabu.users.sourceforge.net/oracletools",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/adfrefresh-16.png",
    "48": "icons/adfrefresh-48.png",
    "128": "icons/adfrefresh-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/adfrefresh-24.png"
    },
    "default_title": "Refresh ADF pages with ease :)"
  },
  "permissions": [
    "tabs"
  ]
}
