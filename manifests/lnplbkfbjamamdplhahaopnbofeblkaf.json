{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "USC WebReg Clippy",
  "version": "0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "https://camel2.usc.edu/webreg/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "icons": {
    "128": "registration-helper-128.png",
    "16": "registration-helper-16.png"
  }
}
