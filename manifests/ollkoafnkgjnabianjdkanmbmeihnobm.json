{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fast Notes",
  "version": "1.0.3",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_title": "Fast Notes",
    "default_icon": "icons/48x48.png",
    "default_popup": "screen.html"
  },
  "permissions": [
    "storage"
  ]
}
