{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "secure-feed",
  "version": "1.0.1",
  "description": "Removes non-secure (HTTPS) articles from your Facebook news feed",
  "browser_action": {
    "default_title": "Make this page red"
  },
  "author": "https://github.com/sgnl/secure-feed",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {},
  "permissions": [
    "activeTab"
  ],
  "minimum_chrome_version": "56"
}
