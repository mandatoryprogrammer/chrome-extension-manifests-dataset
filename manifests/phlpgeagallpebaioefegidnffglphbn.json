{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "wedit",
  "description": "Preview copy edits to live webpages.",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/logo.png"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
