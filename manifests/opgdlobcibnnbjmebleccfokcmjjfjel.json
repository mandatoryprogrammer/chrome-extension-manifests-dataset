{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ErrorsSpy",
  "description": "Track console & network issues and download their report files.",
  "version": "0.3",
  "permissions": [
    "debugger"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.green.png",
    "default_title": "ErrorsSpy"
  },
  "manifest_version": 2
}
