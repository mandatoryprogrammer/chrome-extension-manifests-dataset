{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Remy Sharp",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://*.twitter.com/*",
        "https://twitter.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Automatically redirects Twitter to the mobile version.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "m-tweet",
  "permissions": [
    "https://twitter.com/*",
    "https://*.twitter.com/*"
  ],
  "version": "1.1"
}
