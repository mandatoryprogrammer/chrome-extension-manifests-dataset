{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NikoNiko",
  "description": "www.nikoniko.co chrome extension",
  "version": "2.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "nikoniko.html"
  },
  "permissions": [
    "http://nikoniko.co/",
    "http://www.nikoniko.co/",
    "http://nikoniko-stage.herokuapp.com/",
    "http://0.0.0.0:3000/",
    "storage",
    "notifications",
    "contentSettings",
    "background",
    "tabs"
  ],
  "background": {
    "page": "nikoniko.background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
