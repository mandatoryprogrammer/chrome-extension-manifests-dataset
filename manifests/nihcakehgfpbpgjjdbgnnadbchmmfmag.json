{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sorter Bookmarks",
  "description": "Bookmarks ordener depending the usage rate",
  "version": "1.0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ]
}
