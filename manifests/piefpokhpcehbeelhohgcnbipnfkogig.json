{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quickmarks",
  "version": "0.92",
  "manifest_version": 2,
  "description": "Simply type 'b {keyword}' in omnibox to open bookmark in current tab.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "omnibox": {
    "keyword": "b"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "<all_urls>"
  ]
}
