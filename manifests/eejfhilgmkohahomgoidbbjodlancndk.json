{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Search Open in New Tab",
  "description": "Google Search Open in New Tab",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "img/app-icon-48X48.png",
    "96": "img/app-icon-96X96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
