{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickReview",
  "version": "1.0",
  "icons": {
    "16": "i/icon16.png",
    "48": "i/icon48.png",
    "128": "i/icon128.png"
  },
  "description": "Adds a status bar shortcut to quickly +1 pull requests",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.1.0.min.js",
        "js/background.js"
      ],
      "css": [
        "css/quickReview.css"
      ],
      "matches": [
        "https://github.com/*/pull/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "html/button.html"
  ]
}
