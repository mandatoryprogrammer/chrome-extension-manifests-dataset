{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SelfishTwit",
  "description": "A Twitter userstyle for antisocial news junkies.",
  "version": "1.2",
  "homepage_url": "http://jrubenoff.com/selfishtwit",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [
        "selfishtwit.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "Merriweather.ttf"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
