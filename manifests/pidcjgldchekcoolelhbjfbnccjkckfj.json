{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "BackStop",
  "description": "Stop Backspace returning to previous page.",
  "version": "2.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "all_frames": true
    }
  ]
}
