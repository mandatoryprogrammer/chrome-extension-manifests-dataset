{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Photos For Your Folks",
  "description": "Displays a random photo from a Flickr album in your new tab.",
  "version": "0.3",
  "incognito": "split",
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
