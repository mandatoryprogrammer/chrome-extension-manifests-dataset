{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Veggie Goggles",
  "description": "Makes vegetarian options more appealing",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Goggles",
    "default_icon": "icon.png"
  }
}
