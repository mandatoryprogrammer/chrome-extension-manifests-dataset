{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Old Header",
  "short_name": "github-old-header",
  "version": "2.4.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Brings the old header links back.",
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "extension/index.js"
      ]
    }
  ]
}
