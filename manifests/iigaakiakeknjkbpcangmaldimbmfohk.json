{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "jDashboard",
  "description": "This is a Google Chrome desktop extension that provides users with a more dynamic and functional home page.",
  "version": "1.3",
  "browser_action": {
    "default_icon": "JRP-logo.png",
    "default_popup": "popup.html",
    "default_title": "launch"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "geolocation",
    "http://*.google.com/"
  ]
}
