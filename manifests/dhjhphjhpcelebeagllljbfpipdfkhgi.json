{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Markingbook bookmarks",
  "version": "0.2.03",
  "description": "__MSG_description__",
  "default_locale": "es",
  "browser_action": {
    "background_page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Alex Zamora Regidor",
  "short_name": "Search ::",
  "background": {
    "scripts": [
      "background.js",
      "omnibox_search.js",
      "omnibox_background.js"
    ]
  },
  "omnibox": {
    "keyword": ">"
  },
  "externally_connectable": {
    "matches": [
      "*://markingbook.com/*"
    ]
  },
  "permissions": [
    "bookmarks",
    "background",
    "notifications",
    "storage",
    "unlimitedStorage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "optional_permissions": [
    "topSites",
    "management",
    "activeTab"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ]
}
