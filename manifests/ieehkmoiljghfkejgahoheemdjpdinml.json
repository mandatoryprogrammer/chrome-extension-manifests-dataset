{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.7",
  "short_name": "undo",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Undo Closed Tabs Button",
  "permissions": [
    "tabs",
    "storage",
    "sessions"
  ],
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "https://mybrowseraddon.com/undo.html",
  "description": "Easily undo closed tabs via your browser's toolbar popup!",
  "options_ui": {
    "open_in_tab": true,
    "chrome_style": true,
    "page": "data/options/options.html"
  },
  "browser_action": {
    "default_popup": "data/popup/popup.html",
    "default_title": "Undo Closed Tabs Button",
    "default_icon": {
      "16": "data/icons/toolbar/16.png",
      "32": "data/icons/toolbar/32.png",
      "48": "data/icons/toolbar/48.png",
      "64": "data/icons/toolbar/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
