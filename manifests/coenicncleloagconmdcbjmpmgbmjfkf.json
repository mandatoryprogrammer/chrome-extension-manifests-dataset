{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quip Conversation Killer",
  "short_name": "QCK",
  "version": "1.2.1",
  "description": "An extension to automatically close that annoying conversation window in Quip.",
  "permissions": [
    "declarativeContent",
    "https://*.quip.com/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon_16.png",
      "48": "assets/icon_48.png",
      "128": "assets/icon_128.png"
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "manifest_version": 2
}
