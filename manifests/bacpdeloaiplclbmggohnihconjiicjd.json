{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gaana.com extention, to make your life simple",
  "version": "2.4",
  "permissions": [
    "tabs",
    "<all_urls>",
    "notifications",
    "storage"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.js",
        "js/config.js",
        "js/local_storage.js",
        "js/event.js"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/config.js",
      "js/notifications.js",
      "js/local_storage.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Gaana Extention",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "extplayPause": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      },
      "description": "Toggle Play Pause on gaana.com"
    },
    "extNext": {
      "suggested_key": {
        "default": "Alt+Shift+N"
      },
      "description": "To play Next song on gaana.com"
    },
    "extPrev": {
      "suggested_key": {
        "default": "Alt+Shift+B"
      },
      "description": "Play Previous song on gaana.com"
    },
    "extShuffle": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "Toggle Shuffle gaana.com"
    }
  }
}
