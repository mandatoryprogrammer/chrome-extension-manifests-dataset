{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SidebarOverflow",
  "description": "Remove distracting sidebars from StackOverflow.",
  "version": "0.1.3",
  "homepage_url": "http://shivankaul.com",
  "icons": {
    "64": "icons/64.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.stackoverflow.com/*",
        "*://*.stackexchange.com/*",
        "*://*.serverfault.com/*",
        "*://*.superuser.com/*",
        "*://*.askubuntu.com/*",
        "*://*.stackapps.com/*",
        "*://*.mathoverflow.net/*"
      ],
      "js": [
        "src/remove.js"
      ],
      "run_at": "document_end"
    }
  ]
}
