{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tree History",
  "short_name": "Tree History",
  "description": "This extension demonstrates history as tree",
  "version": "0.0.0.2",
  "icons": {
    "16": "16p.png",
    "32": "32p.png",
    "48": "48p.png",
    "128": "128p.png"
  },
  "permissions": [
    "tabs",
    "history"
  ],
  "background": {
    "scripts": [
      "navigationsListener.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides": {
    "history": "main.html"
  }
}
