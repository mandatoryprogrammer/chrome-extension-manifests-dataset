{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Blognone/vim-mode",
  "description": "Vim support for Blognone",
  "version": "0.5",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.blognone.com/",
        "https://www.blognone.com/node",
        "https://www.blognone.com/node/*",
        "https://www.blognone.com/node?page=*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "smoothscroll.js",
        "jquery-2.2.3.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
