{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Recuse You",
  "version": "1.0.1",
  "description": "Replaces the word recuse with something more profane.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "middle-finger-16.png",
    "48": "middle-finger-32.png",
    "128": "middle-finger-128.png"
  }
}
