{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "kittyads",
  "version": "1.1",
  "description": "Replace ads on reddit with pictures of kittens!",
  "browser_action": {
    "name": "Kitty Ads",
    "icons": {
      "48": "icon48.png",
      "128": "icon.png"
    },
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
