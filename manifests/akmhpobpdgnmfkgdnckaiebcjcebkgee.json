{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Курс USD",
  "version": "0.6.13",
  "description": "Курс USD к UAH из разных источников.",
  "permissions": [
    "https://old.kurs.com.ua/*",
    "https://minfin.com.ua/*",
    "https://kharkov.obmenka.ua/*",
    "https://obmenka.kharkov.ua/*",
    "https://finance.ua/ru/*",
    "https://api.privatbank.ua/p24api/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "short_name": "Курс USD",
  "browser_action": {
    "default_title": "CurrencyRate",
    "default_icon": "img/icon48.png",
    "default_popup": "app/popup.html"
  }
}
