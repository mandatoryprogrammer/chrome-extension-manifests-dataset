{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "直播盒子",
  "description": "提供全网平台的直播订阅，卡片-小窗口观看等功能",
  "version": "2.3",
  "icons": {
    "128": "img/logo/128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/logo/128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "lib/jquery.js",
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options_page.html",
  "homepage_url": "https://237368756.github.io/live_box/index.html",
  "background": {
    "scripts": [
      "lib/jquery.js",
      "lib/event.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "alarms",
    "cookies",
    "background",
    "storage",
    "tabs",
    "webRequest",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}
