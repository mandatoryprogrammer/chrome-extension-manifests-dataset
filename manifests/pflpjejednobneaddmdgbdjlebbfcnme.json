{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CommenTab",
  "version": "0.1.1",
  "description": "Add ephemral sticky notes to your tabs.",
  "icons": {
    "16": "commentab-16.png",
    "48": "commentab-48.png",
    "128": "commentab-128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "index.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
