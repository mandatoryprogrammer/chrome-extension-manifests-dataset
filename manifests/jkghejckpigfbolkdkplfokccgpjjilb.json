{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Omnitweety",
  "short_name": "Omnitweety",
  "manifest_version": 2,
  "version": "0.8.1",
  "description": "Tweet your status & share what you are browsing now, from Omnibox (a.k.a. URL or Address bar)!",
  "omnibox": {
    "keyword": "tw"
  },
  "icons": {
    "128": "assets/icon_128.png",
    "48": "assets/icon_48.png",
    "16": "assets/icon_16.png"
  },
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "oauth.html"
  ],
  "permissions": [
    "notifications",
    "tabs",
    "https://api.twitter.com/",
    "https://abs.twimg.com/",
    "https://pbs.twimg.com/",
    "http://fonts.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' https://abs.twimg.com/ https://pbs.twimg.com/"
}
