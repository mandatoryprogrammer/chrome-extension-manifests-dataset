{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "default-src 'self'; script-src 'self' https://ajax.googleapis.com https://www.google.com https://ssl.google-analytics.com 'unsafe-eval'; style-src * 'unsafe-inline'; img-src 'self' http://www.google-analytics.com;",
  "background": {
    "scripts": [
      "js/jsapi.js",
      "js/storage.js",
      "js/announcer.js",
      "src/bg/event_page.js"
    ],
    "persistent": false
  },
  "options_page": "src/option/option.html",
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "__MSG_extActionTitle__",
    "default_popup": "src/popup/popup.html"
  },
  "commands": {
    "reload_extension": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "windows": "Ctrl+Shift+Q",
        "mac": "MacCtrl+Shift+Q",
        "chromeos": "Ctrl+Shift+Q",
        "linux": "Ctrl+Shift+Q"
      },
      "description": "shortcut for reloading extensions"
    }
  },
  "permissions": [
    "tts",
    "notifications",
    "storage",
    "unlimitedStorage",
    "alarms"
  ]
}
