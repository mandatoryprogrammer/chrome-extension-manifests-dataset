{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cluster",
  "version": "1.2.0",
  "description": "An easy way to group related pages and open them simultaneously!",
  "author": "Jeremy T.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Add page to Cluster",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "manifest_version": 2
}
