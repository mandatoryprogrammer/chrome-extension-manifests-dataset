{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "1.0.2",
  "default_locale": "en",
  "permissions": [
    "idle",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "app": {
    "background": {
      "scripts": [
        "main.js",
        "index.js"
      ]
    }
  },
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}
