{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Modern Index Pages",
  "description": "Nicer-looking directory index pages",
  "version": "1.1.1",
  "permissions": [
    "webNavigation",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "listener.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon128.png"
  }
}
