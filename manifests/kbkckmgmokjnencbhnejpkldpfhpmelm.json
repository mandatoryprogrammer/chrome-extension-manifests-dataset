{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Minimis",
  "version": "1.1.2",
  "description": "A minimal digital clock on your new tab page. Shows 24 hour format.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "clock.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
