{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UR Wallet",
  "description": "A tool that lets University of Rochester students see their Declining and URos balances!",
  "version": "2.2.2",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "*://my.rochester.edu/*",
    "*://ecard.sequoiars.com/*",
    "storage",
    "management",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon.png"
  }
}
