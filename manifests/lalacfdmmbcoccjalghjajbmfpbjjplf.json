{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "wikiScrape",
  "description": "Scrape wikipedia articles for PDFs.",
  "version": "2.6",
  "permissions": [
    "https://google.com/*",
    "https://www.googleapis.com/*",
    "https://*.wikipedia.org/*",
    "http://www.ncbi.nlm.nih.gov/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.wikipedia.org/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "data.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "arrow.png"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
