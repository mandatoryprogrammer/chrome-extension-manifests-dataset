{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "report2jira",
  "description": "Extract webdriver reports errors into Jira",
  "version": "0.2.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://*/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*/screenShot/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
