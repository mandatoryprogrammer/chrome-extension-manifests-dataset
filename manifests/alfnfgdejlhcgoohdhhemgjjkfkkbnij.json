{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitHubCleanup",
  "version": "1.7",
  "author": "Andrew LeTourneau, centerorbit@gmail.com",
  "description": "Removes unnecessary GitHub Issue comments (+1, thanks, and even üëç). OSS https://github.com/centerorbit/GitHubCleanup",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "*://github.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "cleanup.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
