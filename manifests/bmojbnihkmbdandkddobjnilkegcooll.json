{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fantasy Basketball WZRD",
  "version": "4.3.4",
  "description": "Fantasy Basketball WZRD will help ESPN & Yahoo Fantasy Basketball win their league.",
  "browser_action": {
    "default_icon": "frontend/icon.png",
    "default_popup": "frontend/popup.html"
  },
  "icons": {
    "128": "frontend/icon.png",
    "48": "frontend/icon48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://basketball.fantasysports.yahoo.com/nba/*"
      ],
      "js": [
        "js/yahoo/teamPage.js",
        "js/yahoo/playerNote.js"
      ]
    },
    {
      "matches": [
        "*://fantasy.espn.com/basketball/*"
      ],
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/espn/espn.js"
      ]
    },
    {
      "matches": [
        "https://basketball.fantasysports.yahoo.com/nba/*/research*"
      ],
      "js": [
        "js/yahoo/researchPage.js"
      ]
    },
    {
      "matches": [
        "https://basketball.fantasysports.yahoo.com/nba/*/buzzindex*"
      ],
      "js": [
        "js/yahoo/transactionTrendsPage.js"
      ]
    },
    {
      "matches": [
        "https://basketball.fantasysports.yahoo.com/nba/*/matchup*"
      ],
      "js": [
        "js/yahoo/matchupsPage.js",
        "js/yahoo/matchupsGamesToday.js"
      ]
    },
    {
      "matches": [
        "https://basketball.fantasysports.yahoo.com/nba/*/players*"
      ],
      "js": [
        "js/yahoo/playersPage.js"
      ]
    },
    {
      "matches": [
        "https://fantasy.premierleague.com/*"
      ],
      "js": [
        "js/premierLeague/myTeam.js"
      ]
    }
  ],
  "permissions": [
    "*://www.sportswzrd.com/*"
  ]
}
