{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "large remover",
  "manifest_version": 2,
  "version": "0.22",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "https://tweetdeck.twitter.com/",
    "https://twitter.com/*"
  ],
  "background": {
    "scripts": [
      "download-hook.js"
    ],
    "persistent": true
  }
}
