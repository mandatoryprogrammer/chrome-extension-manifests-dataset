{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QiitaSort",
  "version": "0.0.1",
  "description": "Add sort button to qiita search",
  "content_scripts": [
    {
      "matches": [
        "http://qiita.com/search*"
      ],
      "js": [
        "qs.js"
      ],
      "css": [
        "qs.css"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "img/qs128.png"
  }
}
