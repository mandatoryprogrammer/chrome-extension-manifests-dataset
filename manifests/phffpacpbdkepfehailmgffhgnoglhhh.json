{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IDO Conversation Enhancer",
  "version": "0.3",
  "description": "Hides posts from selected users on in-depthoutdoors forums",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "matches": [
        "https://www.in-depthoutdoors.com/community/forums/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "64": "icon-64.png"
  }
}
