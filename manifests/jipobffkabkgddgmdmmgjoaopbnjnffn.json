{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Udemy Video Playback Speed",
  "version": "1.2.6",
  "description": "Adds additional playback speed options for videos on Udemy.",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.udemy.com/*"
      ],
      "js": [
        "playbackRate.js"
      ],
      "css": [
        "menuList.css"
      ]
    }
  ]
}
