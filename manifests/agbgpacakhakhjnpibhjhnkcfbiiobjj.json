{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Explain xkcd",
  "short_name": "Explain xkcd",
  "description": "It's 'cause you're dumb",
  "version": "0.1.2",
  "author": "Martin Frodl",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.2.1.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
