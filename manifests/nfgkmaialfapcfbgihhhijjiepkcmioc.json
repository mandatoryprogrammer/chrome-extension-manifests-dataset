{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Walt",
  "description": "Your own IA personal assistant",
  "version": "2",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "128": "img/icon128.png"
    },
    "default_title": "Walt",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/notify.js",
      "js/menu.js"
    ]
  }
}
