{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "lazyreader",
  "version": "1.1",
  "description": "read with your face, not your hand",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/lazyreader_16.png",
      "48": "images/lazyreader_48.png",
      "128": "images/lazyreader_128.png"
    }
  },
  "icons": {
    "16": "images/lazyreader_16.png",
    "48": "images/lazyreader_48.png",
    "128": "images/lazyreader_128.png"
  },
  "web_accessible_resources": [
    "webcam_injector.js"
  ],
  "manifest_version": 2
}
