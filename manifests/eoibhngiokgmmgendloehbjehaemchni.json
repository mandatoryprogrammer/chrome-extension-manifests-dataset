{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Homestuck Fixer",
  "version": "0.2",
  "description": "Applies small fixes to Homestuck's text",
  "content_scripts": [
    {
      "matches": [
        "*://*.homestuck.com/*",
        "*://*.readmspa.org/*"
      ],
      "js": [
        "lib/findAndReplaceDOMText.js",
        "replacements.js",
        "fixer.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
