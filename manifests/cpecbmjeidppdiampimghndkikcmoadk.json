{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Type-ahead-find",
  "description": "Find text or links as you type",
  "version": "0.4.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "type-ahead.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48": "icon-48.png"
  }
}
