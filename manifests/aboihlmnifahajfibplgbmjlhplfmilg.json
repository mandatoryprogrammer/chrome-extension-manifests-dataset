{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lifetrack companion extension",
  "short_name": "LMS Companion",
  "version": "0.4",
  "description": "Provides an interface for RadiAnt and several utility functions",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "downloads",
    "downloads.shelf",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "ClearDownload.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16x12.png",
      "32": "32x24.png",
      "48": "48x36.png",
      "128": "128x97.png"
    }
  },
  "icons": {
    "16": "16x12.png",
    "32": "32x24.png",
    "48": "48x36.png",
    "128": "128x97.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.lifetrackmed.com/*",
        "*://*.devfakedomain.com/*",
        "*://*.dev.com/*",
        "*://*.lifetrackmedicalsystems.com/*"
      ],
      "js": [
        "ContentScript.js"
      ]
    }
  ],
  "manifest_version": 2
}
