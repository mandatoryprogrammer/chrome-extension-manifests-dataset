{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nightman",
  "version": "0.2",
  "icons": {
    "128": "icon128x128.png"
  },
  "description": "Unlocks Twitter's night mode on desktop",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
