{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Taskii - Time Tracker",
  "description": "Offline Project & Tasks Manager allow freelancers to :Track their time , Organize tasks in a kanban board,Create invoices",
  "version": "0.8",
  "browser_action": {
    "name": "Click to open the timer",
    "default_title": "Open Timer",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
