{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenTok LMS demo Screen Sharing",
  "author": "Kaustav Das Modak",
  "version": "0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Screensharing extension for OpenTok proctoring demos.",
  "homepage_url": "https://github.com/kaustavdm/opentok-lms-demos/tree/proctoring",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.tokbox.com/*",
      "https://*.kaustavdm.in/*",
      "https://opentok-proctoring-demo.herokuapp.com/*",
      "https://opentok-tutoring-demo.herokuapp.com/*"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon128.png"
  ]
}
