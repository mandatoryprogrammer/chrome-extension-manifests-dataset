{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Miner Away",
  "description": "Stop coin miners on the web!",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "notifications"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "img/logo.png",
    "64": "img/icon.png"
  },
  "browser_action": {
    "default_title": "Miner Away - Stop coin miners on the web.",
    "default_icon": "img/logo.png",
    "default_popup": "html/popup.html"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "55",
  "author": "Arthur Geron"
}
