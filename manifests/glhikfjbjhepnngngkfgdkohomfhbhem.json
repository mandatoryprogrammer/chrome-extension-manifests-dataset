{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GDAX Account Helper",
  "description": "Show balance in USD in your GDAX account.",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/neoshell/GDAXChromeExtension",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.gdax.com/accounts/*",
    "tabs",
    "webRequest"
  ]
}
