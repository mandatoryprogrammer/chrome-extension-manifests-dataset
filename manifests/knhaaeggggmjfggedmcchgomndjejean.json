{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Striker Manager Time Fixer",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Adds seconds to the clock in Striker Manager",
  "content_scripts": [
    {
      "matches": [
        "http://*.strikermanager.com/inicio.php*"
      ],
      "include_globs": [
        "http://en*.strikermanager.com*"
      ],
      "js": [
        "code.js"
      ]
    }
  ],
  "permissions": [
    "http://en3.strikermanager.com/",
    "http://en2.strikermanager.com/",
    "http://en1.strikermanager.com/",
    "http://en.strikermanager.com/"
  ]
}
