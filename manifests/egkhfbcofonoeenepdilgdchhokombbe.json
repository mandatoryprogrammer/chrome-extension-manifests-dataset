{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BrowserTools",
  "description": "BrowserTools",
  "version": "0.0.5.0",
  "permissions": [
    "tabs",
    "activeTab",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "cookies",
    "history",
    "privacy",
    "contentSettings",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/bt19.png",
    "default_popup": "popup.html",
    "default_title": "BrowserTools"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/watch*"
      ],
      "js": [
        "util/jquery.min.js",
        "js/youtube.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://www.hulu.com/watch*"
      ],
      "js": [
        "util/jquery.min.js",
        "js/hulu.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "options.html",
  "icons": {
    "128": "icons/bt128.png",
    "16": "icons/bt16.png",
    "19": "icons/bt19.png",
    "32": "icons/bt32.png",
    "48": "icons/bt48.png"
  }
}
