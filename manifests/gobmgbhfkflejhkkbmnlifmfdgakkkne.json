{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spotify Omnibox",
  "author": "Nate Lubeck",
  "version": "1.1.5",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "spot"
  },
  "icons": {
    "16": "icon-16.png",
    "128": "128x128.png"
  },
  "homepage_url": "http://www.natelubeck.com",
  "permissions": [
    "http://ws.spotify.com/",
    "tabs"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ws.spotify.com; object-src 'self' https://ws.spotify.com; connect-src 'self' http://ws.spotify.com"
}
