{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitHub Image Preview",
  "author": "Sam Kessaram",
  "version": "0.1.6",
  "description": "Preview issue images on GitHub. https://github.com/samkessaram/git-preview",
  "icons": {
    "128": "camera.png"
  },
  "web_accessible_resources": [
    "camera.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery.min.js",
        "scripts.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ]
}
