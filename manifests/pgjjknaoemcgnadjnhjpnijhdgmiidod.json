{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hypothetical Wikipedia",
  "description": "What if things in Wikipedia were hypothetical?",
  "version": "0.1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*.wikipedia.org/*",
        "https://*.wikipedia.org/*"
      ],
      "js": [
        "verbs.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
