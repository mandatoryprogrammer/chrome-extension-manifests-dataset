{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stannum",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Extension to experiment with resource blocking and other security measures in Chrome.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-256.png"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "tabs",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "contentSettings",
    "privacy"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "insecure-detector.css"
      ],
      "js": [
        "insecure-detector.js"
      ],
      "all_frames": true
    }
  ]
}
