{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "content.js"
  ],
  "manifest_version": 2,
  "name": "Shopify Developer Tools",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "short_name": "Shopify Developer Tools",
  "version": "0.23"
}
