{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Select n Search",
  "description": "Right click and search for selected text",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "search.png",
    "default_popup": "popup.html",
    "default_title": "Change right click  order preference"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "search.png",
    "48": "search.png",
    "128": "search.png"
  },
  "manifest_version": 2
}
