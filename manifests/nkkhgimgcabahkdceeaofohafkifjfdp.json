{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wolfware Downloader",
  "description": "This Extension helps user automatically organise their download item from Wolfware",
  "version": "1.0.3",
  "version_name": "1.1 beta",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "downloads",
    "*://*/*",
    "tabs",
    "https://ajax.googleapis.com/",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon.png"
  }
}
