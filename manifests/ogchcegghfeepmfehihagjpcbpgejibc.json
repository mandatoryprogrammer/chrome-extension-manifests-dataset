{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Instant Dictionary",
  "description": "A simple and fast dictionary that allows you to get results while typing. Powered by Youdao/Bing/Baidu",
  "version": "0.21",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
