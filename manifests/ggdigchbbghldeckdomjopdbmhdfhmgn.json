{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cookie Cutter",
  "description": "This extension allows you to copy and paste cookies between Chrome tabs.",
  "version": "1.0.1",
  "homepage_url": "https://whattheforbus.com",
  "icons": {
    "16": "cookie-16.png",
    "24": "cookie-24.png",
    "32": "cookie-32.png",
    "64": "cookie-64.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "cookies",
    "background",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "cookie-32.png",
    "default_title": "Cookie Cutter",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "cookie-cutter.js"
    ]
  }
}
