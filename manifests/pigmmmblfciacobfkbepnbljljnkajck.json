{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "favorite site",
  "version": "1.0",
  "description": "escキー二回押しで登録したURLへ遷移する",
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "manifest_version": 2
}
