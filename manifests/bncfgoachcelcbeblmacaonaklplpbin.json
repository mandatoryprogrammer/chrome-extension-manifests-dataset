{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "nthu_decaptcha",
  "version": "1.1",
  "description": "Automatically fills the captcha field in ccxp.nthu.edu.tw/ccxp/INQUIRE",
  "content_scripts": [
    {
      "matches": [
        "*://www.ccxp.nthu.edu.tw/ccxp/*"
      ],
      "js": [
        "decaptcha.js"
      ]
    }
  ]
}
