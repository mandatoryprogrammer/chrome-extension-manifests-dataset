{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Playlist Abandoner",
  "description": "Adds a button to YouTube playlists to go to the video outside the playlist.",
  "version": "1.1.1",
  "author": "Josh Asch",
  "homepage_url": "https://github.com/jobbogamer/PlaylistAbandoner",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/playlist*"
      ],
      "js": [
        "playlistAbandoner.js"
      ],
      "css": [
        "playlistAbandoner.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "*://*.youtube.com/*"
  ],
  "web_accessible_resources": [
    "buttonIcon.png",
    "shareIcon.svg"
  ]
}
