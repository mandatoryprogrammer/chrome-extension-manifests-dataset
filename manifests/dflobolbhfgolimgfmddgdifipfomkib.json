{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tweeter: App.net client inside Twitter.com",
  "manifest_version": 2,
  "version": "2.0.0",
  "description": "I can't believe it's not Twitter!",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ]
}
