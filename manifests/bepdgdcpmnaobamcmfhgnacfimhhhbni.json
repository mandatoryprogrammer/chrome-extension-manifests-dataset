{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Code Tweets",
  "description": "Render code in tweets on twitter.com",
  "version": "1.0",
  "homepage_url": "https://github.com/vutran/code-tweets",
  "permissions": [
    "https://twitter.com/"
  ],
  "icons": {
    "128": "128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "index.css"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
