{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jira Canned Responses",
  "description": "Choose from a set of canned responses for your jira comments and descriptions ",
  "version": "0.1.3",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [
        "editor.js",
        "contentScript.js"
      ],
      "css": [
        "options.css"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "include_globs": [
    "*jira*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "minimum_chrome_version": "51"
}
