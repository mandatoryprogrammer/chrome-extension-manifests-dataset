{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kyberia v3.1 features",
  "version": "1.65",
  "description": "Adds features that make kyberia.sk more fun.",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "icon.png",
    "options.html",
    "jquery-1.10.2.min.map",
    "jquery-2.0.3.min.map"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery2.js",
        "analytics.js",
        "kyberia-utils.js",
        "kyberia-v31.js",
        "features/AutoUpdater.js",
        "features/ExtensionOptions.js",
        "features/ChatBox.js",
        "features/Jargoniser.js",
        "features/FeatureMix.js",
        "features/AjaxButtons.js",
        "features/ShowKGivers.js",
        "features/FlashUnloader.js",
        "features/HideAvatars.js",
        "features/HideMoods.js",
        "features/QuickReply.js",
        "features/LimitNodeHeight.js",
        "features/LimitNodeWidth.js",
        "features/Desocializer.js",
        "features/StopAvatars.js",
        "features/DeleteButton.js",
        "features/TagUsers.js",
        "features/MailUpgrade.js",
        "features/InplaceEditing.js",
        "features/Cyrilliser.js",
        "features/Hiraganiser.js",
        "features/CompactMode.js"
      ],
      "matches": [
        "http://kyberia.sk/*",
        "https://kyberia.sk/*"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
