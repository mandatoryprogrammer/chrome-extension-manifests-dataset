{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kwdizr (Générateur de mots clés)",
  "description": "Générateur de mots clés à partir d'une de la racine d'un mot",
  "version": "1.1",
  "homepage_url": "http://www.serialcoder.net",
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Générateur de mots clés",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
