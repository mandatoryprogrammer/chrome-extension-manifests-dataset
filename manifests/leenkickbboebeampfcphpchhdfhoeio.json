{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redirector",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Catch and redirect url's based on RegEx identifier",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "myScript.js"
    ]
  },
  "options_page": "options.html"
}
