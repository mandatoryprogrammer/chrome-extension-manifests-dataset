{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mark as read Confluence extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds \"Mark as read\" capability to Confluence navigation",
  "homepage_url": "https://github.com/marcin-mazurek/mark-as-read-confluence-extension",
  "content_scripts": [
    {
      "js": [
        "js/functions.js",
        "js/main.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "https://*/*"
  ]
}
