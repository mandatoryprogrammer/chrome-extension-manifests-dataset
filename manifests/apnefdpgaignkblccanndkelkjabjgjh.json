{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabasco",
  "description": "Spicy Tab Managment - Search, sort and manage your tabs.",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.7.0",
  "offline_enabled": true,
  "homepage_url": "https://github.com/donatj/tabasco",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Tabasco: Spicy Tab Managment",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}
