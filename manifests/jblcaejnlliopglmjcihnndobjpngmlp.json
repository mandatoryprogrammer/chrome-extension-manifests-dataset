{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDb Rightclick",
  "version": "1.0.0",
  "description": "Opens the selected text in imdb.com",
  "author": "Johnie Hjelm",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "images/favicon.png",
    "128": "images/favicon.png"
  }
}
