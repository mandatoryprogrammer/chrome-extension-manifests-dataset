{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Christmas Puzzle Game - Christmas Games",
  "short_name": "Christmas Game",
  "version": "0.1.6",
  "description": "Easy to play Christmas Puzzle game with beautiful graphics, the kids will love it!",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Christmas Puzzle Game - Christmas Games",
    "default_icon": "icon_128.png"
  },
  "offline_enabled": false,
  "permissions": [
    "tabs"
  ]
}
