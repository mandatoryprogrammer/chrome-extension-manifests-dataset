{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "github-brief-description",
  "version": "1",
  "manifest_version": 2,
  "description": "A description of the star has been the github repository by other users, when move the cursor on github repository link.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
