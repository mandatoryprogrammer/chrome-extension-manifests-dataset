{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gitlab commit creator",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Automatically fills commit message and description when then are empty",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://*/*"
  ]
}
