{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ClockTab - Новая вкладка",
  "short_name": "ClockTab",
  "version": "1.6.28",
  "homepage_url": "http://danilkinkin.com/projects/clockTab/",
  "minimum_chrome_version": "48",
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Простая и красивая новая вкладка для chrome",
  "icons": {
    "16": "image/16x16.png",
    "32": "image/32x32.png",
    "64": "image/64x64.png",
    "128": "image/128x128.png"
  },
  "browser_action": {
    "default_title": "Добавить закладку в ClockTab",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "*://*/*",
    "http://danilkinkin.com/"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "background": {
    "page": "background.html"
  }
}
