{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Easy Login for Interactive Brokers",
  "description": "Base Level Extension",
  "manifest_version": 2,
  "version": "2.1",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "IB.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.interactivebrokers.com/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
