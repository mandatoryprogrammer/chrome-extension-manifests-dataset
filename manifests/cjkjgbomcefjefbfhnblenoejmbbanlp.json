{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AWSDocReader",
  "description": "Read management of AWS documents",
  "version": "1.0.2",
  "homepage_url": "https://github.com/enomotodev/AWSDocReader",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.aws.amazon.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ]
}
