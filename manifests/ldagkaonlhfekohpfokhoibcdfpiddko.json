{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EventBrite",
  "description": "Save EventBrite events to your database",
  "icons": {
    "16": "16.png",
    "96": "96.png",
    "128": "128.png"
  },
  "version": "3.1.5",
  "author": "Roman Danilov",
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "EventBrites",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "background"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.3.1.min.js"
      ],
      "run_at": "document_end"
    }
  ]
}
