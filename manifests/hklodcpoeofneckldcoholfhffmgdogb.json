{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UniposClapStorm",
  "description": "Uniposの拍手を指定数連打するアプリ。拍手ボタンを右クリックで拍手回数を指定できるようになります。",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://unipos.me/*"
      ],
      "js": [
        "js/content_script.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
