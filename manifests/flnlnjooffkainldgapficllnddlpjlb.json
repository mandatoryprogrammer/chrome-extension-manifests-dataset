{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BookSearch",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Webページ上の文字列を選択してからボタンを押すと、Amazonとカーリルへの検索結果へ遷移するリンクを作ります",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
