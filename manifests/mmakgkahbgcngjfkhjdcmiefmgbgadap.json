{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tieba Extension",
  "version": "0.2.2",
  "description": "Remove Live Video from Tieba",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://tieba.baidu.com/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://tieba.baidu.com/*"
      ],
      "js": [
        "myscripts.js"
      ],
      "run_at": "document_start"
    }
  ]
}
