{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Firespotting! Interesting Ideas, Every Day!",
  "version": "0.3.2",
  "description": "Displays recent ideas from Firespotting",
  "icons": {
    "48": "bulb48.png",
    "128": "bulb128.png"
  },
  "browser_action": {
    "default_title": "Firespotting",
    "default_icon": "bulb32.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://firespotting.com/",
    "http://firespotting.com/",
    "notifications"
  ],
  "web_accessible_resources": [
    "bulb48.png"
  ]
}
