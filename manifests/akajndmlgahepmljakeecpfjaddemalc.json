{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dusic",
  "short_name": "Dusic",
  "description": "Chrome extension for download music from dancering.cn",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": {
      "16": "img/logo.png",
      "48": "img/logo.png",
      "128": "img/logo.png"
    }
  },
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "offline_enabled": true,
  "background": {
    "scripts": [
      "js/base64.js",
      "js/bg.js"
    ]
  },
  "author": "PoppinL",
  "omnibox": {
    "keyword": "dusic"
  },
  "permissions": [
    "downloads",
    "contextMenus",
    "http://www.dancering.cn/"
  ]
}
