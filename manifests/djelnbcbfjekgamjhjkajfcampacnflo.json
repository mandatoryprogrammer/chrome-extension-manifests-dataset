{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Grzegorz's Site Blocker",
  "description": "This extension blocks unwanted sites.",
  "author": "Grzegorz Milka",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "128": "g_128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html"
}
