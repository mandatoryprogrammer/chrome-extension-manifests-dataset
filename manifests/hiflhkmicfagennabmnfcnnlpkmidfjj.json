{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mark As Read",
  "version": "0.0.0.7",
  "description": "Mark a page as read by clicking on the extension icon. ",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "downloads",
    "http://*/",
    "https://*/",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "changeLinkColor.js"
      ]
    }
  ],
  "options_page": "options.html"
}
