{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "histor.y",
  "description": "This extension sorts your bookmarks by most recently used.",
  "icons": {
    "128": "icon/Histor.y_Logo_128.png"
  },
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
