{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OGS Antisocial Mode",
  "short_name": "OGS Antisocial",
  "version": "0.0.3",
  "description": "Hides in-game chat on online-go.com.",
  "content_scripts": [
    {
      "matches": [
        "*://online-go.com/*"
      ],
      "js": [
        "ogs-antisocial.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "600": "icons/600.png"
  },
  "manifest_version": 2
}
