{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Issue Template Generator",
  "description": "Chrome Extension: Github Issue Template Generator",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Issue Template Generator"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
