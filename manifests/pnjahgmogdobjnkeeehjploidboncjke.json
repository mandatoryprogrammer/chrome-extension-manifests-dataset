{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ethereum Price Monitor",
  "version": "0.5",
  "description": "Simple Ethereum price monitor (GDAX/Coinbase)",
  "icons": {
    "48": "assets/img/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "assets/img/icon.png"
    }
  },
  "background": {
    "scripts": [
      "app/stats.js",
      "app/ticker.js",
      "app/dataModel.js",
      "app/background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
