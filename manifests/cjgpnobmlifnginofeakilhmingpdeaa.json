{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No Nextdoor Postcards",
  "short_name": "No Postcards",
  "description": "This extension suppresses the Postcards notification on Nextdoor",
  "version": "1.0.1",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://nextdoor.com/*"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}
