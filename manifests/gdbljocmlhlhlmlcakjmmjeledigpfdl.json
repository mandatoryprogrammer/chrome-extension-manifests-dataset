{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bypass Censorship",
  "description": "Find access to blocked websites",
  "version": "1.0.2",
  "icons": {
    "128": "rsc/icon-128.png"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": "rsc/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "libs/jquery.js",
      "backgroundPage.js"
    ]
  },
  "default_locale": "en"
}
