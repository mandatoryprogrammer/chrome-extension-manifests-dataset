{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screen Share",
  "author": "Altanai",
  "version": "2.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Screen Capturing and Sharing Chrome Extension",
  "homepage_url": "https://altanaitelecom.wordpress.com",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "*://localhost/*",
        "*://www.villageexperts.com/*",
        "*://ec2-52-66-78-44.ap-south-1.compute.amazonaws.com/*",
        "*://ec2-54-174-186-98.compute-1.amazonaws.com/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://www.villageexperts.com/*",
      "*://ec2-52-66-78-44.ap-south-1.compute.amazonaws.com/*",
      "*://ec2-54-174-186-98.compute-1.amazonaws.com/*"
    ]
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "desktopCapture"
  ]
}
