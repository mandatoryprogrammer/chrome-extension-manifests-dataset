{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "bilibili平板端优化",
  "description": "bilibili平板端优化，用于安卓平板Yandex浏览器，对Bilibili主要页面强制启用PC版，并进行横屏触屏优化",
  "permissions": [
    "*://*.bilibili.com/*",
    "https://*.acgvideo.com/*",
    "https://static.hdslb.com/*",
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/toolbar.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.bilibili.com/*"
      ],
      "exclude_matches": [
        "*://passport.bilibili.com/*"
      ],
      "js": [
        "js/useragent.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://www.bilibili.com/",
        "*://www.bilibili.com/?*"
      ],
      "js": [
        "js/home_preinject.js"
      ],
      "run_at": "document_start",
      "css": [
        "css/common.css",
        "css/home.css"
      ]
    },
    {
      "matches": [
        "*://www.bilibili.com/",
        "*://www.bilibili.com/?*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/common.js",
        "js/home.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://www.bilibili.com/video/*"
      ],
      "js": [
        "js/video_preinject.js"
      ],
      "css": [
        "css/common.css",
        "css/video.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://www.bilibili.com/video/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/common.js",
        "js/video.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://www.bilibili.com/bangumi/*"
      ],
      "css": [
        "css/common.css",
        "css/bangumi.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://www.bilibili.com/bangumi/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/common.js",
        "js/bangumi.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://t.bilibili.com/*"
      ],
      "css": [
        "css/common.css",
        "css/updates.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
