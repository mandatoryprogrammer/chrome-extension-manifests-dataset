{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dyslexia Unscrambled",
  "short_name": "Unscrambled",
  "version": "0.6.4",
  "manifest_version": 2,
  "author": "Avishaan @ Codehatcher",
  "description": "Make it easy for dyslexic people to read any website.",
  "homepage_url": "http://www.letsunscramble.com",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persisent": true
  },
  "web_accessible_resources": [
    "style.css",
    "fonts/*.*"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "./libs/jquery-2.1.1.min.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "http://*/",
        "https://*/",
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "https://*/*",
    "http://*/*",
    "contextMenus"
  ]
}
