{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "歐噴豆知識",
  "description": "歐噴豆知識 - Pixnet Hackathon 2015",
  "version": "0.2",
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "24": "icons/icon-24.png",
    "16": "icons/icon-16.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "http://test.wjhuang.net/"
  ],
  "background": {
    "scripts": [
      "scripts/jquery-2.0.0.min.js",
      "scripts/jquery.easing.1.3.js",
      "scripts/jquery.easing.compatibility.js",
      "scripts/flowtype.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "scripts/jquery-2.0.0.min.js",
        "scripts/questionbank.js",
        "scripts/contentscript.js",
        "scripts/jquery.easing.1.3.js",
        "scripts/jquery.easing.compatibility.js",
        "scripts/jquery.flip.min.js",
        "scripts/flowtype.js",
        "scripts/typed.js"
      ],
      "css": [
        "styles/yhack.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/icon-128.png",
    "icons/icon-48.png",
    "icons/icon-24.png",
    "images/background.jpg"
  ],
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_popup": "options.html"
  }
}
