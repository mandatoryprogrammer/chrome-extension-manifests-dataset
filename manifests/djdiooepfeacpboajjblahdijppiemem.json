{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Image Sherlock",
  "version": "1.0",
  "description": "Do a Google search for photos in a page",
  "icons": {
    "128": "poser.png",
    "16": "poser-16.png",
    "48": "poser-48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "poser.png",
    "default_title": "Find if an image shows up on other pages"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
