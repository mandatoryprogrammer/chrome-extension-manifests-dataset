{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Giant Bomb HTML5 Video Commander",
  "description": "Control Giant Bomb videos with your keyboard.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.giantbomb.com/*"
      ],
      "js": [
        "injector.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "vidcontrol.js"
  ]
}
