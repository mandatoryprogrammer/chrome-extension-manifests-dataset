{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "event-page.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "injected.js"
      ]
    }
  ],
  "description": "DevTools for Redux Ship.",
  "devtools_page": "devtools.html",
  "icons": {
    "48": "favicon.png"
  },
  "manifest_version": 2,
  "name": "Redux Ship DevTools",
  "version": "1.3"
}
