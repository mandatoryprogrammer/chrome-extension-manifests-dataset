{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mangadex Tama",
  "short_name": "Tama",
  "description": "Tama is your companion extension for mangadex.org",
  "version": "0.2.0",
  "author": "Kevin Xiao",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mangadex.org/**"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
        "bundle.css"
      ]
    }
  ],
  "permissions": [
    "https://mangadex.org/**"
  ]
}
