{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CommentOut",
  "description": "Blocks ads in Facebook comments.",
  "version": "1.70",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "settings.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "css": [],
      "js": [
        "jquery-1.12.4.min.js",
        "commentout.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
