{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Vaporwave NewTab",
  "version": "1.0.2",
  "description": "Vaporwave themed newtab page",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "fontSettings",
    "history"
  ],
  "web_accessible_resources": [
    "assets/font/*.*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src * chrome://favicon/ data:;",
  "icons": {
    "48": "./assets/img/icon48.png",
    "128": "./assets/img/icon128.png"
  }
}
