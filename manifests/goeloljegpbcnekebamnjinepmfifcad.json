{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabNabit",
  "short_name": "minimalist tab manager",
  "manifest_version": 2,
  "version": "1.3",
  "description": "View and navigate all open tabs.",
  "author": "Jake Wood",
  "browser_action": {
    "default_popup": "tabs.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}
