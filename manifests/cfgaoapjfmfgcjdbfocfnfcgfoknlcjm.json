{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "name": "Tab Shepherd",
  "options_page": "popup.html",
  "version": "1.2.1",
  "description": "Automatically closes inactive tabs and makes it easy to get them back",
  "minimum_chrome_version": "20",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  },
  "browser_action": {
    "default_title": "Tab Shepherd",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/icon48.png"
  ],
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "contextMenus"
  ],
  "commands": {
    "reopen-corral-tab": {
      "suggested_key": {
        "default": "Alt+Shift+T"
      },
      "description": "Opens the most recently saved tab from the corral."
    }
  }
}
