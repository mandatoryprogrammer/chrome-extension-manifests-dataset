{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ceceree",
  "description": "Add apps to Ceceree right from Chrome",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "toolbar-chrome.png",
      "38": "toolbar-chrome@2x.png"
    },
    "default_title": "Add to Ceceree"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "author": "Appiculous",
  "homepage_url": "http://ceceree.com"
}
