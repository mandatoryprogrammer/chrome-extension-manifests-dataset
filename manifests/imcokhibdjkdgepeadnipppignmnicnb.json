{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter UnHooked",
  "version": "0.5",
  "description": "Hides parts of Twitter that are unneeded/addictive: trends, follower recommendations.",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "manifest_version": 2
}
