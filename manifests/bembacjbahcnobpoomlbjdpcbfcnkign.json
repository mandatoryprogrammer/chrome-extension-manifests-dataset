{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CamCatch",
  "description": "Catch the contents of the course message",
  "version": "1.0.0",
  "icons": {
    "16": "CamCatch-16.png",
    "48": "CamCatch-48.png",
    "128": "CamCatch.png"
  },
  "browser_action": {
    "default_icon": "CamCatch.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.cambly.com/*"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}
