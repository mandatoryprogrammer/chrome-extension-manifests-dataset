{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Poor Man Feel It",
  "description": "Chrome extension for browsing clarin.com, ole.com.ar and lanacion.com without loggin in",
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
