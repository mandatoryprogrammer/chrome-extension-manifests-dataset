{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bye World Cup.",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This will remove all tweets containing the words \"World Cup\" from your Twitter feed.",
  "homepage_url": "http://willhuxtable.com",
  "default_locale": "en",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "background": {
    "scripts": [
      "ByeWorldCup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "ByeWorldCup.js"
  ]
}
