{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker News Color",
  "short_name": "Hacker News Color",
  "description": "Colors the scores of Hacker News pages according to score.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.news.ycombinator.com/*",
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "hackernewscolor.js"
      ]
    }
  ]
}
