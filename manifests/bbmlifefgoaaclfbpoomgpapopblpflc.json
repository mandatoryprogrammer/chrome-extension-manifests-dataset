{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "0.5",
  "author": "Vladimir Vakhromeev",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Link buncher"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
