{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Close Other Tabs",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extension used to close all other tabs than the active or pinned ones.",
  "icons": {
    "16": "close16.png",
    "48": "close48.png",
    "128": "close128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "close19.png",
    "default_title": "Close other tabs"
  },
  "permissions": [
    "tabs"
  ]
}
