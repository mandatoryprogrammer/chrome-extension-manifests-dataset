{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "寶寶不浪費時間Lite",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "blocking time-wasting websites to make you focus on working",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "寶寶不浪費時間Lite",
    "default_icon": "icon128.png"
  }
}
