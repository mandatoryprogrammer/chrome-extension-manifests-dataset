{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bGone",
  "short_name": "bGone",
  "version": "1.2",
  "description": "Click elements to delete them from the page",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "16": "images/Icon-16.png",
    "32": "images/Icon-32.png",
    "48": "images/Icon-48.png",
    "128": "images/Icon-128.png"
  },
  "manifest_version": 2
}
