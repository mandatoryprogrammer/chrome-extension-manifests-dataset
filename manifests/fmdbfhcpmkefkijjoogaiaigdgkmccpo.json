{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "0.0.5",
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "img/rappa_128.png"
  },
  "browser_action": {
    "default_icon": "img/axe.png",
    "default_title": ""
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "content/css/styles.css"
      ],
      "js": [
        "common/lodash.custom.min.js",
        "common/jquery-1.8.2.min.js",
        "content/lib/jquery-observe.js",
        "content/lib/handlebars.runtime-1.0.rc.1.min.js",
        "content/lib/templates.js",
        "content/js/repo_tree.js",
        "content/js/main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "content/img/collapse.png",
    "content/img/expand.png"
  ],
  "options_page": "options/index.html",
  "background": {
    "scripts": [
      "common/lodash.custom.min.js",
      "background/lib/sha256.js",
      "background/lib/base64.js",
      "background/lib/github.js",
      "background/lib/purl.js",
      "background/js/storage.js",
      "background/js/model.js",
      "background/js/main.js",
      "background/js/events/analytics.js",
      "background/js/events/massacre.js",
      "background/js/events/tree.js"
    ]
  },
  "permissions": [
    "https://github.com/*",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
