{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fixipedia",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "A simple Chrome extension that adds a SeeClickFix issue map to any 'place' article on Wikipedia.",
  "permissions": [
    "http://en.wikipedia.org/wiki/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://en.wikipedia.org/wiki/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "fixipedia.js"
      ]
    }
  ]
}
