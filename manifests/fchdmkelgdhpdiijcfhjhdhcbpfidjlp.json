{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "English Tab",
  "description": "",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "icons/newtab_search16.png",
      "32": "icons/newtab_search32.png"
    }
  },
  "icons": {
    "16": "icons/newtab_search16.png",
    "32": "icons/newtab_search32.png",
    "48": "icons/newtab_search48.png",
    "128": "icons/newtab_search128.png"
  }
}
