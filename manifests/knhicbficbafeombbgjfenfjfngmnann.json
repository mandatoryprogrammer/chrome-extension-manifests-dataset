{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Steam Booster packs Creator Helper",
  "version": "2.1.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Boostercreator"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://steamcommunity.com/tradingcards/boostercreator/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/injected.js",
    "/style.css"
  ]
}
