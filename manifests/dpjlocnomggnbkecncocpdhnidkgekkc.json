{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "reddit new tab",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Extension that makes reddit links open in new tab by default.",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/",
        "http://www.reddit.com/*"
      ],
      "js": [
        "newtabber.js"
      ]
    }
  ]
}
