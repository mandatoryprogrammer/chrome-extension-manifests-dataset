{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48": "cc98-48.png"
  },
  "name": "CC98工具箱",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "cc98-48.png",
    "default_title": "CC98工具箱 by pascale@cc98",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/reply.js"
      ],
      "matches": [
        "http://www.cc98.org/dispbbs.asp?Boardid=*&ID=*",
        "http://www.cc98.org/dispbbs.asp?boardID=*&ID=*",
        "http://www.cc98.org/dispbbs.asp?BoardID=*&id=*",
        "http://www.cc98.org/dispbbs.asp?boardid=*&id=*",
        "http://www.cc98.org/reannounce.asp?BoardID=*&id=*",
        "http://www.cc98.org/dispbbs.asp?BoardID=*&ID=*",
        "http://*.cc98.lifetoy.org/dispbbs.asp?BoardID=*&ID=*",
        "http://*.cc98.lifetoy.org/dispbbs.asp?Boardid=*&ID=*",
        "http://*.cc98.lifetoy.org/dispbbs.asp?boardID=*&ID=*",
        "http://*.cc98.lifetoy.org/dispbbs.asp?BoardID=*&id=*",
        "http://*.cc98.lifetoy.org/dispbbs.asp?boardid=*&id=*",
        "http://*.cc98.lifetoy.org/reannounce.asp?BoardID=*&id=*"
      ]
    },
    {
      "css": [
        "css/show.css"
      ],
      "js": [
        "js/jquery.js",
        "js/md5.js",
        "js/show.js"
      ],
      "matches": [
        "http://www.cc98.org/*",
        "http://*.cc98.lifetoy.org/*"
      ],
      "exclude_matches": [
        "http://www.cc98.org/login.asp",
        "http://*.cc98.lifetoy.org/login.asp"
      ]
    },
    {
      "js": [
        "js/jquery.js",
        "js/md5.js",
        "js/login.js"
      ],
      "matches": [
        "http://www.cc98.org/login.jsp",
        "http://*.cc98.lifetoy.org/login.asp"
      ]
    },
    {
      "js": [
        "js/jquery.js",
        "js/all.js"
      ],
      "matches": [
        "http://www.cc98.org/*",
        "http://*.cc98.lifetoy.org/*"
      ]
    }
  ],
  "options_page": "option.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*.cc98.org/",
    "http://*.cc98.lifetoy.org/",
    "contextMenus",
    "tabs"
  ],
  "version": "2.0.5"
