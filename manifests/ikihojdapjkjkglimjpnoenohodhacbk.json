{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Short URL and QR code generator",
  "version": "1.4",
  "icons": {
    "16": "i/16x16.png",
    "32": "i/32x32.png",
    "48": "i/48x48.png",
    "128": "i/128x128.png"
  },
  "permissions": [
    "tabs",
    "https://tinyurl.com/"
  ],
  "browser_action": {
    "default_title": "Generate short URL",
    "default_icon": "i/button.png",
    "default_popup": "popup.html"
  }
}
