{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gladdress.com",
  "version": "1.2.2",
  "description": "Auto fill in address fields from Gladdress.com",
  "browser_action": {
    "default_icon": {
      "19": "icons/icon_016.png",
      "38": "icons/icon_038.png"
    },
    "default_title": "Gladdress",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "ext/jquery-2.0.3.min.js",
        "content.js"
      ]
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "icons/icon_016.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_128.png"
  },
  "homepage_url": "http://www.gladdress.com"
}
