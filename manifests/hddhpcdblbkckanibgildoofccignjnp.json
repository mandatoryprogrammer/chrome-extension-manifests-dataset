{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HTTPClacks",
  "description": "Clacks adaptor for HTTP requests",
  "version": "0.8",
  "icons": {
    "128": "icon_on.gif"
  },
  "browser_action": {
    "default_title": "Clacks Viewer",
    "default_icon": "icon_off.gif",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
