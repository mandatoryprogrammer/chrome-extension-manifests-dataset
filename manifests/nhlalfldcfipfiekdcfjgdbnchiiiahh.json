{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dzr Slim",
  "description": "Minimal Deezer interface",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "author": "VK",
  "permissions": [
    "http://www.deezer.com/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Google Mail"
  },
  "web_accessible_resources": [
    "script.js",
    "fav_play.ico",
    "fav_pause.ico"
  ]
}
