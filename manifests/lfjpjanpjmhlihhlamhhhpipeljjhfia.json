{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BeaverEXT",
  "description": "An extension for Beaver students",
  "version": "0.1.3",
  "browser_action": {
    "default_icon": "icon48tr.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "alarms",
    "notifications"
  ],
  "icons": {
    "128": "icon128tr.png",
    "1024": "icon1024tr.png",
    "16": "icon16tr.png"
  }
}
