{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.4.1.08709",
  "name": "axe-coconut - Web Accessibility Testing",
  "manifest_version": 2,
  "icons": {
    "16": "assets/images/coconut16.png",
    "48": "assets/images/coconut48.png",
    "128": "assets/images/coconut128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "vendor.bundle.js",
      "background.bundle.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/images/coconut16.png",
      "48": "assets/images/coconut48.png",
      "128": "assets/images/coconut128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Axe Accessibility",
    "browser_style": true
  },
  "web_accessible_resources": [
    "*.woff2"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "browser-polyfill.js",
        "vendor.bundle.js",
        "adapter.bundle.js"
      ],
      "css": [
        "adapter.bundle.css"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "match_about_blank": true
    }
  ],
  "permissions": [
    "tabs",
    "debugger",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "description": "Accessibility Checker for Developers in Chrome",
  "version_name": "4.4.1.08709 build 64ce1e36b6",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com ; object-src 'self'"
}
