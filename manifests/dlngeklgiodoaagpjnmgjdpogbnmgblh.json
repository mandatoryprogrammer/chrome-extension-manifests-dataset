{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Support Save Image",
  "version": "0.0.2.2",
  "manifest_version": 2,
  "description": "手動で画像を保存するとき、簡単にフォルダ分けを出来るようにするアプリ",
  "browser_action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "downloads",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "vendor/jquery-3.2.1.min.js",
      "src/storage.js",
      "src/tools.js",
      "src/background.js"
    ]
  }
}
