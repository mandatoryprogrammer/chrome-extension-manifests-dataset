{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Image Transparency",
  "manifest_version": 2,
  "version": "4.0",
  "author": "Tomek Fijo≈Ç",
  "description": "Shows image transparency",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    },
    "default_title": "Image Transparency Options"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "minimum_chrome_version": "56",
  "homepage_url": "http://www.tomekf.pl"
}
