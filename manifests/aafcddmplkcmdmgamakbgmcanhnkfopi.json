{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "小羊搜索",
  "version": "1.0",
  "description": "chrome浏览器快速对选中文本，通过第三方网站进行搜索，并打开tab",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
