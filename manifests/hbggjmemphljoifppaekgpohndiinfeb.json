{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Joe Looney",
  "description": "Collect, Trade and Create Cryptogoods",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon-48.png"
  },
  "web_accessible_resources": [
    "js/*.*",
    "images/*.*",
    "css/*.*",
    "html/*.*"
  ],
  "permissions": [
    "storage",
    "https://*.digirare.com/",
    "https://*.xchain.io/"
  ],
  "manifest_version": 2,
  "name": "Freeport",
  "short_name": "freeport",
  "version": "0.3.0"
}
