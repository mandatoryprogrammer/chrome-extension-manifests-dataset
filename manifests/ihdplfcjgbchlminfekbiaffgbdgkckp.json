{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "www_brute",
  "description": "WWW bruteforce login forms",
  "version": "1.0.2",
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "page": "bg.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "www_brute.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "www_brute",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
