{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "24",
  "name": "Music Player Client",
  "version": "3.2.0",
  "description": "Control a Music Player Daemon (MPD)",
  "icons": {
    "128": "images/icon-128x128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "launcher.js"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    {
      "socket": [
        "tcp-connect"
      ]
    }
  ]
}
