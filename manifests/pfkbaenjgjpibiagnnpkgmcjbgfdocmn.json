{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0",
  "short_name": "Crazy Roll 3D",
  "name": "__MSG_appName__",
  "author": "Unblocked games",
  "version_name": "1",
  "description": "__MSG_appDesc__",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.crazygames.com/gameframe/crazy-roll-3d*"
      ],
      "js": [
        "js/roll.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "https://www.bing.com/*",
    "https://search.yahoo.com/*",
    "https://us.search.yahoo.com/*",
    "https://de.search.yahoo.com/*",
    "https://gb.search.yahoo.com/*",
    "https://fr.search.yahoo.com/*",
    "https://ca.search.yahoo.com/*",
    "https://nl.search.yahoo.com/*",
    "https://es.search.yahoo.com/*",
    "https://it.search.yahoo.com/*",
    "https://be.search.yahoo.com/*",
    "https://se.search.yahoo.com/*",
    "https://in.search.yahoo.com/*",
    "https://at.search.yahoo.com/*",
    "https://br.search.yahoo.com/*",
    "https://dk.search.yahoo.com/*",
    "https://no.search.yahoo.com/*",
    "https://fi.search.yahoo.com/*",
    "https://www.google.com/*",
    "https://www.google.com.au/*",
    "https://www.google.co.uk/*",
    "https://www.google.com.au/*",
    "https://www.google.co.jp/*",
    "https://www.google.co.nz/*",
    "https://www.google.de/*",
    "https://www.google.fr/*",
    "https://www.google.no/*",
    "https://www.google.dk/*",
    "https://www.google.nl/*",
    "https://www.google.ca/*",
    "https://www.google.us/*"
  ],
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "128.png"
  }
}
