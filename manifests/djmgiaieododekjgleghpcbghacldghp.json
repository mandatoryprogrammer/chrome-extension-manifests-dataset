{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "sockets": {
    "tcp": {
      "connect": "*"
    },
    "tcpServer": {
      "listen": [
        "127.0.0.1",
        "127.0.0.1"
      ]
    }
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "fileSystem"
  ],
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "description": "A server listening on localhost:[port] and transfering TCP to a [fqdn]:[port] through a SOCKS5 proxy.",
  "name": "Localhostify",
  "version": "0.0.1"
}
