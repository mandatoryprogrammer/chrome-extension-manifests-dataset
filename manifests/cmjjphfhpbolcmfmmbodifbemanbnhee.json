{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Repopulate History Extension",
  "version": "0.6",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Repopulates web history with random sites. For your use, modification, and distribution. Built by Dehyrf, GNU GPL v3.",
  "offline_enabled": true,
  "permissions": [
    "history"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Repopulate"
  }
}
