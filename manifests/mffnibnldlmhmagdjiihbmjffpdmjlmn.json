{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazon WishList Point",
  "version": "1.3",
  "description": "Add a function to display points on the Amazon wish list",
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*/wishlist/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "icons": {
    "128": "ap128.png"
  }
}
