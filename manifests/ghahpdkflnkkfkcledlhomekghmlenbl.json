{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ekşi Sözlük'te Ara",
  "description": "Seçilen metni Ekşi Sözlük'te arar.",
  "author": "Sercan Solmaz",
  "version": "1.0",
  "icons": {
    "128": "eksi-sozluk-logo-128.png",
    "48": "eksi-sozluk-logo-48.png",
    "16": "eksi-sozluk-logo-16.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "eksi-sozluk-logo-128.png"
  }
}
