{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Contact-Hunter",
  "description": "This app will find and show all phone number with country detail and emails",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Contact-Hunter",
    "default_icon": "./img/logo.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
