{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hotcold Typing!",
  "description": "Smart Touch Typing Learning",
  "version": "2.2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "app": {
    "background": {
      "scripts": [
        "init_crx.js"
      ]
    }
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "offline_enabled": true
}
