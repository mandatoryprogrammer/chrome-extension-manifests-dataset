{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Provides shortcuts for navigating github pull requests comments",
  "manifest_version": 2,
  "name": "GitHub Helper Plugin",
  "version": "1.1.0",
  "icons": {
    "48": "icons/hide-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "git-hub-helper-plugin.js"
      ],
      "css": [
        "git-hub-helper-style.css"
      ]
    }
  ]
}
