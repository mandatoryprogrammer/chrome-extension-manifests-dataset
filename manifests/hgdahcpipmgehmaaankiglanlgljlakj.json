{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Go back in time and see how a specific page used to look using a variety of page caching/archiving services",
  "icons": {
    "128": "icon-128.png",
    "16": "menu-icon.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2,
  "name": "Go Back in Time",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1"
}
