{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Import Cost",
  "version": "0.1",
  "description": "An extension to display import cost of javascript file",
  "manifest_version": 2,
  "permissions": [
    "*://*/*.js"
  ],
  "browser_action": {
    "default_icon": "import-cost.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*.js"
      ],
      "css": [
        "content-styles.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
