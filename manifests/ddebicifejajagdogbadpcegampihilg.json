{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "cUrly links",
  "version": "1.1",
  "description": "Gets a curl command with cookies form a link or page",
  "short_name": "curly",
  "offline_enabled": true,
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_title": "Get cUrly",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "clipboard.html"
  }
}
