{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Link Access",
  "short_name": "Save and Copy Your Favorites",
  "description": "This extension allows user to save oft-used links to use in messages and documents",
  "version": "0.0.0.2",
  "author": "Kim Allen",
  "icons": {
    "16": "imgs/icon16.png",
    "32": "imgs/icon32.png",
    "64": "imgs/icon64.png",
    "128": "imgs/icon128.png"
  },
  "browser_action": {
    "default_icon": "imgs/icon128.png",
    "default_popup": "popup.html",
    "default_title": "Easy access to common links"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
