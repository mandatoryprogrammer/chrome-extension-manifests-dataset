{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "夜就輕輕地來了, 開始阿明謀",
  "description": "This extension supports for playing King",
  "version": "1.9.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "*://*.icantw.com/*",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "tabs",
    "storage"
  ]
}
