{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pointbreak",
  "description": "Create, reorder and scale at your custom breakpoints.",
  "version": "0.12",
  "app": {
    "background": {
      "scripts": [
        "app.js"
      ]
    }
  },
  "icons": {
    "48": "icon/pointbreak-48.png",
    "128": "icon/pointbreak-128.png"
  },
  "permissions": [
    "webview",
    "storage"
  ]
}
