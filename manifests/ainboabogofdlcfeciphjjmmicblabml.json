{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "common.js",
      "author.js",
      "codesearch.js",
      "crbug.js",
      "crrev.js",
      "background.js"
    ],
    "persistent": false
  },
  "description": "Chromium search tools in the Omnibox",
  "icons": {
    "16": "assets/chromium16.png",
    "48": "assets/chromium48.png",
    "128": "assets/chromium128.png"
  },
  "manifest_version": 2,
  "name": "Omnibox for Chromium Developers",
  "omnibox": {
    "keyword": "cr"
  },
  "permissions": [
    "storage",
    "https://bugs.chromium.org/p/chromium/feeds/*",
    "https://cs.chromium.org/codesearch/json/*"
  ],
  "version": "15"
}
