{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Feedbin.me AutoRefresh",
  "description": "This Extension will auto refresh Feedbin.me on a configurable interval",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "permissions": [
    "https://feedbin.me/",
    "tabs"
  ]
}
