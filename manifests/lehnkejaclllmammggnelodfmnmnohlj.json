{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quotter - The Twitter Quotter Extension",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Allows Twitter quoting.",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-1.7.1.js",
        "quotter_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
