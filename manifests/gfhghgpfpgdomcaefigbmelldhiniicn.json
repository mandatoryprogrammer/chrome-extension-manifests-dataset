{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "chrome_inject.js"
      ],
      "matches": [
        "*://www.twitch.tv/twitchplayspokemon",
        "*://www.twitch.tv/twitchplayspokemon/"
      ],
      "run_at": "document_end"
    }
