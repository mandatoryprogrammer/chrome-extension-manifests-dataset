{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MTGO DeckFinder",
  "version": "3.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "http://mtgo-stats.com/*",
    "http://www.mtgo-stats.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_title": "Compare against my cards...",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
