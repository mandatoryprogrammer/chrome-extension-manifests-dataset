{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "closeTabByJS",
  "description": "javascript can close chrome tab through chrome extension",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*",
        "*://127.0.0.1/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
