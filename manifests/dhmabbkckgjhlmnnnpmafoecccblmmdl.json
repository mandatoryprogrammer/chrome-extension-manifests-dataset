{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SAD reacts only",
  "version": "1.6",
  "description": "",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": [
        "content.js"
      ],
      "css": [
        "sad.css"
      ],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ],
  "icons": {
    "32": "sad-icons/sad-32.png",
    "64": "sad-icons/sad-64.png",
    "128": "sad-icons/sad-128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
