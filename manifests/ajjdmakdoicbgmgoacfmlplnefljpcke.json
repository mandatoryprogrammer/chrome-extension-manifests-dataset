{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Heads Up",
  "short_name": "Heads Up",
  "version": "1.0.0",
  "description": "View your <head> data for easier developing.",
  "author": "De Voorhoede",
  "minimum_chrome_version": "69.0",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "36": "devtools/assets/icons/icon-36.png",
    "96": "devtools/assets/icons/icon-96.png"
  },
  "background": {
    "scripts": [
      "devtools/background.js"
    ]
  },
  "devtools_page": "devtools/devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "devtools/content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' http://localhost:8098; object-src 'self'",
  "manifest_version": 2
}
