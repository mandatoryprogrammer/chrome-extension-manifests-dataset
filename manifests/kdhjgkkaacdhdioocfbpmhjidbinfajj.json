{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Frame",
  "manifest_version": 2,
  "description": "Adds context menu items to open a frame (or iframe) in the current tab, a new tab, new window, or incognito window.",
  "version": "7",
  "permissions": [
    "clipboardWrite",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
