{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Octotab",
  "version": "1.0.2",
  "description": "Extension to make github news feed organized!",
  "minimum_chrome_version": "26",
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "page_action": {
    "default_title": "Disable Octotab"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/"
      ],
      "js": [
        "src/content_script.js"
      ],
      "css": [
        "src/custom.css"
      ],
      "run_at": "document_end"
    }
  ]
}
