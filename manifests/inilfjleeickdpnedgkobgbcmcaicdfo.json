{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Newskitten",
  "description": "Guaranteed to improve any news website.",
  "version": "0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "cat-128.png",
    "default_popup": "window.html"
  },
  "web_accessible_resources": [
    "calm.gif"
  ],
  "icons": {
    "16": "cat-16.png",
    "128": "cat-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "newskitten-inject.js"
      ]
    }
  ]
}
