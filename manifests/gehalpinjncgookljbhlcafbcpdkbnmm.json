{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EkşiR",
  "description": "Ekşisözlüğü canlı takip edin.",
  "version": "1.19",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "css/images/favicon.png",
    "default_title": "EkşiR - Modern ekşisözlük"
  },
  "icons": {
    "16": "css/images/icon16.png",
    "48": "css/images/icon48.png",
    "128": "css/images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://eksisozluk.com/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "homepage_url": "https://chrome.google.com/webstore/detail/ek%C5%9Fir/gehalpinjncgookljbhlcafbcpdkbnmm"
}
