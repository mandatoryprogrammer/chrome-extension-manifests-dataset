{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yale NetID Linker",
  "version": "1.3.2",
  "manifest_version": 2,
  "description": "Turns each netid into a link to the person's Yale Phonebook entry",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "jquery-1.7.2.min.js",
        "core.js"
      ],
      "all_frames": true
    }
  ]
}
