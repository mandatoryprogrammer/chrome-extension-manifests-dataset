{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "neat extension",
  "version": "0.0.4",
  "description": "Neat is a community for gathering information on the internet",
  "browser_action": {
    "default_title": "Save to neat",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://neat.to"
  ]
}
