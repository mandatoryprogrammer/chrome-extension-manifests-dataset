{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MM Tools",
  "version": "0.0.15",
  "manifest_version": 2,
  "description": "Maxymiser tools for developers",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "mmtools",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "scripts/script.js",
    "images/icon-128.png"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
