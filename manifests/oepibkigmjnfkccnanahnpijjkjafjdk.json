{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EtymOwl",
  "description": "This extension searches etymonline.com for a selected term",
  "background": {
    "page": "background.html"
  },
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "assets/EtymOwl_Icon@0,125x.png",
    "48": "assets/EtymOwl_Icon@0,375x.png",
    "128": "assets/EtymOwl_Icon.png"
  }
}
