{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gif One-click Download",
  "description": "Download On Click",
  "version": "1.03.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "file:///*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "main.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "ui.html",
    "default_icon": {
      "16": "images/sceptic16x16.png",
      "32": "images/sceptic32x32.png"
    }
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "downloads",
    "cookies",
    "background",
    "storage",
    "tabs",
    "declarativeContent"
  ],
  "icons": {
    "128": "images/sceptic128x128.png"
  }
}
