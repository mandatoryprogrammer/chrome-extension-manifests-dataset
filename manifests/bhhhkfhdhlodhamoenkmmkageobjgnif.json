{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "我的工具包",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.2.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://*/*",
    "http://*/*",
    "notifications",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "javascript/fdev-min.js",
      "javascript/bg.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "file:///C:/Users/liqi.zhulq/Desktop/chorme.html",
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "javascript/fdev-min.js",
        "javascript/contentscript.js"
      ],
      "css": [
        "contentscript.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_css": "fdev-op.css"
  }
}
