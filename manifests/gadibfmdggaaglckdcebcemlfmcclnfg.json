{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HttpNoInput",
  "version": "1.1.0",
  "description": "Disables input fields and JavaScript on insecure websites.",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contentSettings",
    "contextMenus",
    "declarativeContent",
    "tabs",
    "http://*/*",
    "ftp://*/*"
  ],
  "page_action": {
    "default_icon": {},
    "default_title": "https upgrade"
  }
}
