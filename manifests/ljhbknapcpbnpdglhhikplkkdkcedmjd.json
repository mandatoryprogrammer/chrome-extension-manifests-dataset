{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "pagemove",
  "version": "2.3",
  "manifest_version": 2,
  "description": "Move pages between chrome and iPhone and iPad",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "jquery.min.js"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "http://.pagemove.se/saveUrl"
  ]
}
