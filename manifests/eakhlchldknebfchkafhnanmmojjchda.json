{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pair Me Up^",
  "description": "Help Coders to Better Pair Program Skills",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js",
      "jquery-2.1.3.min.js"
    ]
  },
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost:9393/session",
    "http://*/*",
    "https://*/*"
  ]
}
