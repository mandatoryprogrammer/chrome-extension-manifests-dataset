{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hungry Panda: Clipboard & Notepad",
  "description": "Clever Clipboard & Notepad in one extension. Save text snippets as one single file - just feed up your Hungry Panda!",
  "version": "0.0.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "short_name": "Hungry Panda",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
