{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Canada Goose",
  "description": "A game about the majestic canada goose!",
  "version": "1.2",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "system.cpu",
    "system.memory",
    "system.storage",
    "system.display",
    "storage"
  ]
}
