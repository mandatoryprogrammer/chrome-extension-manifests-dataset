{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BandcampVolume",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "bandcamp.com add volume bar and repeat button",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.bandcamp.com/*"
      ],
      "js": [
        "scripts/jquery-2.2.2.min.js",
        "scripts/volume.js"
      ]
    }
  ]
}
