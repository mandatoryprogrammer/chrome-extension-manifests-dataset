{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spectro",
  "description": "Remote Testing of Chemicals Sensors",
  "version": "0.9",
  "manifest_version": 2,
  "permissions": [
    "serial",
    "<all_urls>",
    "*://*/*",
    "fullscreen"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "/images/icon-16.png",
    "128": "/images/icon-128.png"
  }
}
