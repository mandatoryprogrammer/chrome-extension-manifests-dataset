{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BackOnTrack",
  "version": "0.0.0.1",
  "description": "Blocks websites!",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "idle",
    "alarms",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon16.png",
      "48": "images/icon16.png",
      "128": "images/icon16.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon16.png",
    "48": "images/icon16.png",
    "128": "images/icon16.png"
  },
  "manifest_version": 2
}
