{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eScanner",
  "version": "2.8",
  "description": "Verifica a acessibilidade de uma página WEB segundo as recomendações do Modelo de Acessibilidade de Governo Eletrônico - e-MAG (3.0)",
  "icons": {
    "16": "images/icon.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs"
  ]
}
