{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Accessibility Ruleset Runner",
  "short_name": "arr",
  "description": "A Web Browser Extension to run accessibility rulesets.",
  "version": "1.1.3",
  "minimum_chrome_version": "54",
  "permissions": [
    "contextMenus",
    "tabs",
    "https://raw.githubusercontent.com/*",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "content/rightClickMenu.js"
    ]
  },
  "icons": {
    "48": "content/arr.png"
  }
}
