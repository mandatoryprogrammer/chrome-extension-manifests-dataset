{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gmail Refresh Button Fixer",
  "description": "Keeps the refresh button around when mail is selected.",
  "version": "0.4",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "64": "refresh64.png",
    "128": "refresh128.png"
  }
}
