{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AddLinkToTwitter for pplog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add link to twitter.",
  "author": "Toshinari Suzuki",
  "homepage_url": "http://suzuki.toshinari.jp",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://pplog.net/*"
      ],
      "run_at": "document_end",
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ]
}
