{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DOT Search",
  "description": "Use short keywords to access all of your company's universe in a second.⚡️",
  "version": "0.0.0.3",
  "author": "Théo Blochet",
  "browser_action": {
    "default_title": "Update your shortcuts! ⚡️"
  },
  "options_ui": {
    "page": "setup/setup.html",
    "open_in_tab": false
  },
  "omnibox": {
    "keyword": "."
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
