{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Baconizer",
  "description": "Turns every image on the page into bacon with a single click!  Because bacon!",
  "version": "1.2.1",
  "background": {
    "scripts": [
      "jquery-1.9.1.min.js",
      "baconizer.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "images/bacon1.jpg",
    "images/bacon2.jpg",
    "images/bacon3.jpg",
    "images/bacon4.jpg",
    "images/bacon5.jpg",
    "images/bacon6.jpg",
    "images/bacon7.jpg",
    "images/bacon8.jpg",
    "images/bacon9.jpg",
    "images/bacon10.jpg",
    "images/bacon11.jpg",
    "images/bacon12.jpg",
    "images/bacon13.jpg",
    "images/bacon14.jpg",
    "images/bacon15.jpg"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.9.1.min.js"
      ]
    }
  ]
}
