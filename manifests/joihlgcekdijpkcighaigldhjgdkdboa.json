{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extTitle__",
  "version": "0.2",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "homepage_url": "http://yanke.ru",
  "default_locale": "en",
  "options_page": "options/index.html",
  "permissions": [
    "contextMenus",
    "storage",
    "https://*/*",
    "http://*/* "
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/ImageSkipper.js"
      ],
      "css": [
        "css/inject.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/*.png"
  ]
}
