{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Canvas to Moodle",
  "description": "Simplifying your life, with just a simple install.",
  "version": "2.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "canvas-128.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.instructure.com/",
        "https://lms.neumont.edu/"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "underscore-min.js",
        "toastr.min.js",
        "moment.min.js",
        "clndr-1.0.7.min.js",
        "contentscript.js",
        "jquery.tinysort.min.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "https://*.instructure.com/api/*",
    "https://lms.neumont.edu/api/*",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "arrow-24.png",
    "gear-24.png",
    "contentscriptv2.coffee",
    "jquery-1.10.2.min.map",
    "underscore-min.map",
    "toastr.min.js.map",
    "contentscript.js.map"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
