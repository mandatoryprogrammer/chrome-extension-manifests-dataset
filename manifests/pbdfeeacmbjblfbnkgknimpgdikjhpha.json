{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Web Nanny",
  "description": "Filter profanity in webpages",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Clean this page!",
    "default_popup": "nannyGUI.html",
    "default_icon": "webNannyLogoSolid.png"
  },
  "manifest_version": 2
}
