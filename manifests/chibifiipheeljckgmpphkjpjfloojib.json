{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitDownload",
  "description": "this is github download folder plugin",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "http://*.github.com/*",
    "https://*.github.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.github.com/*",
        "https://*.github.com/*"
      ],
      "css": [
        "app.css"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "jquery-ajax-native.js",
        "filesaver.min.js",
        "jszip.js",
        "app.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
