{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quarry",
  "description": "Simple web scraping service and Chrome extension.",
  "version": "0.1.0",
  "author": "James O'Shea",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "icons": {
    "16": "images/16q.png",
    "48": "images/48q.png",
    "128": "images/128q.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
