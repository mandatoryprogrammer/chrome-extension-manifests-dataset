{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Unwanted",
  "short_name": "HideUnwanted",
  "description": "Prevents user from getting distracted while using internet.",
  "version": "1.1",
  "author": "Manav Shrivastava",
  "homepage_url": "https://github.com/manavshrivastavagit/hideunwanted",
  "icons": {
    "32": "icon.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "permissions": [
    "activeTab"
  ],
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.4.0.min.js",
        "hide-unwanted.js"
      ],
      "run_at": "document_end"
    }
  ]
}
