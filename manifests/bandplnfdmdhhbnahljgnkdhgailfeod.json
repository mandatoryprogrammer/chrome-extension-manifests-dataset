{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KaffeeShare",
  "description": "This extension adds a share button to the toolbar which allows you to share a page via KaffeeShare.",
  "version": "0.7.3",
  "icons": {
    "128": "comic_128x128.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true,
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "page_action": {
    "default_title": "kshare this page",
    "browser_style": true
  }
}
