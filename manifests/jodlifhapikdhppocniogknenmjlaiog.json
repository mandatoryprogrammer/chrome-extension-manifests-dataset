{
  "manifest_version": 2,
  "name": "Livly",
  "version": "0.2.4",
  "description": "A non-distracting, minimalist New Tab page.",
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Livly"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "short_name": "Livly",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "minimum_chrome_version": "29.0.1547"
}
