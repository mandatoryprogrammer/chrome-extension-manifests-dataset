{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RedForester Keeper Extension",
  "short_name": "RedForester Keeper",
  "description": "RedForester extension to save web page quickly",
  "version": "0.0.6",
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "{1088b865-56bf-4d40-a665-b2c501a29bfd}",
      "strict_min_version": "53.0"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "https://app.redforester.com/*",
    "https://beta.app.redforester.com/*"
  ],
  "browser_action": {
    "default_popup": "/src/popup.html"
  }
}
