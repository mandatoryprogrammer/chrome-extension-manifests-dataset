{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AutoKnight",
  "description": "An automated assistant useful to any Knight of the Button",
  "version": "1.2.1",
  "permissions": [
    "http://*.reddit.com/",
    "https://*.reddit.com/",
    "storage"
  ],
  "icons": {
    "128": "AutoKnight-128.png",
    "48": "AutoKnight-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "foreground.js"
      ],
      "matches": [
        "http://*.reddit.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "foreground.js"
      ],
      "matches": [
        "https://*.reddit.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "pagescript.js"
  ],
  "manifest_version": 2
}
