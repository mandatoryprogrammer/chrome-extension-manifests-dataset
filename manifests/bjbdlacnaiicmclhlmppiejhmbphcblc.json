{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoProxy",
  "version": "0.0.1",
  "options_page": "options.html",
  "permissions": [
    "proxy",
    "webNavigation",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/proxy.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_title": "AutoProxy"
  },
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png"
  }
}
