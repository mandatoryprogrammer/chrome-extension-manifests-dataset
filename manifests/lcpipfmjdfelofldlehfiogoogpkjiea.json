{
  "browser_action": {
    "default_icon": "/src/assets/images/cat.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "src/js/jquery.min.js",
        "src/js/app.js",
        "src/js/googAnalytics.js",
        "src/js/menu.js",
        "src/js/googleTagManager.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://ajax.googleapis.com https://fonts.googleapis.com https://s3.amazonaws.com/meowmeoww00f https://d9m01xi7ip4je.cloudfront.net https://www.google-analytics.com https://www.googletagmanager.com; object-src 'self'",
  "permissions": [
    "activeTab"
  ],
  "description": "Replace your new tab page with a cat videos. No ads!",
  "icons": {
    "16": "/src/assets/images/16cat.png",
    "48": "/src/assets/images/48cat.png",
    "128": "/src/assets/images/128cat.png"
  },
  "incognito": "split",
  "key": "",
  "manifest_version": 2,
  "name": "New Tab - Cat Video Extension",
  "short_name": "meow meow woof",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.0.1"
}
