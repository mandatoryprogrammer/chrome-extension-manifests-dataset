{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Moments To Limericks",
  "version": "2.0.1",
  "description": "Removes Moments link from the Twitter menu and replaces with a link to cool bot generated Limericks.",
  "browser_action": {
    "default_icon": "nm.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "libs/jquery.min.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
