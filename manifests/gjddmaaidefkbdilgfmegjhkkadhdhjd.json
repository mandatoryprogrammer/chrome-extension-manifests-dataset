{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cur8",
  "description": "Save & search only your favourite sites.",
  "version": "1.1.5",
  "manifest_version": 2,
  "homepage_url": "https://cur8.io",
  "background": {
    "scripts": [
      "lib/jquery-2.1.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://cur8.io/*",
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "logo.png",
    "16": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Cur8",
    "default_popup": "popup.html"
  }
}
