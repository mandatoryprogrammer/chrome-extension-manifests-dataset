{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ancap News",
  "version": "1.0",
  "description": "bring out Hacker News' true colors",
  "author": "Chas Emerick",
  "homepage_url": "http://github.com/cemerick/ancap-news",
  "content_scripts": [
    {
      "matches": [
        "*://news.ycombinator.com/*"
      ],
      "js": [
        "ancapnews.user.js"
      ],
      "run_at": "document_end"
    }
  ]
}
