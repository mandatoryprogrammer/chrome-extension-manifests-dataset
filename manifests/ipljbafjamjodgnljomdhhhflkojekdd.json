{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "2ooU! Link Share",
  "version": "0.1.2",
  "description": "Shorten and share current web page URL",
  "browser_action": {
    "default_popup": "./build/popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "128": "icon.png"
  }
}
