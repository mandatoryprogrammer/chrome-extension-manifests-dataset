{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ITVSlayer",
  "version": "1.1",
  "description": "Bypass ITV Hub ads",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.itv.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
