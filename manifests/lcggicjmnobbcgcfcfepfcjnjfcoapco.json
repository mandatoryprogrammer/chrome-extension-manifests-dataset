{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kobiton Get metric's devices",
  "description": "Kobiton Get metric's devices - Developed by Khanh Do",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "options_page": "popup.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_title": "Kobiton Websocket",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
