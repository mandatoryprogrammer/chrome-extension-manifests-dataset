{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome header toggler",
  "description": "Chrome header toggler",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "background": {
    "page": "html/background.html"
  },
  "options_page": "html/options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "background",
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "img/build_72.png"
  },
  "page_action": {
    "default_icon": {
      "36": "img/build_38.png"
    },
    "default_title": "Chrome Header Toggler - (No headers configured for this host)"
  }
}
