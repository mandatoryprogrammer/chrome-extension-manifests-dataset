{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scrollf*cker",
  "author": "Dan Mattern",
  "description": "Tries to add the scrollbar back to sites that remove it with overlays. Inspired by F*ck Overlays",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "naw.js"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "scrollfuck.js"
    ]
  }
}
