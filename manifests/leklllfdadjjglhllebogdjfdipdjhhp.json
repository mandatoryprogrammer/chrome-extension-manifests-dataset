{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Command-Click Fix",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Makes Command-Click open a new tab on pages that don't do it properly.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.4.2.min.js",
        "commandclickfix.js"
      ],
      "run_at": "document_start"
    }
  ]
}
