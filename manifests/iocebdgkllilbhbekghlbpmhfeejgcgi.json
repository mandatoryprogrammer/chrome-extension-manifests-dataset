{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Numbered Tabs",
  "version": "0.2.2",
  "description": "Add a numeric prefix before the tab's title.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
