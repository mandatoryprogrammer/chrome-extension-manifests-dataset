{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub-Color",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "GitHubのヘッダの色を変更できる拡張です。",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "lib/farbtastic-1.3u/farbtastic.min.js",
        "lib/rgbcolor.min.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css",
        "lib/farbtastic-1.3u/farbtastic.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "lib/farbtastic-1.3u/*.png"
  ]
}
