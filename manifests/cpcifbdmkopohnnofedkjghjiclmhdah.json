{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "ct.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png"
    }
  },
  "description": "Sends Download Links to Neat Download Manager",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "homepage_url": "https://www.neatdownloadmanager.com/",
  "manifest_version": 2,
  "offline_enabled": false,
  "name": "NeatDownloadManager Extension",
  "web_accessible_resources": [
    "img/icon16.png",
    "img/icon16_2x.png",
    "img/close16.png",
    "img/close16_2x.png"
  ],
  "permissions": [
    "<all_urls>",
    "cookies",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "nativeMessaging",
    "storage"
  ],
  "author": "J.Motallebi",
  "version": "1.5.0"
}
