{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flash Note",
  "description": "This extension allows you to write small notes in your browser.",
  "version": "0.0.0.4",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ]
}
