{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vídeos dos Mamilos Molengas",
  "description": "A extensão avisa quando um vídeo novo é postado no canal e permite você assistir os vídeos sem sair da página atual.",
  "version": "1.0.1",
  "icons": {
    "16": "icones/16.png",
    "48": "icones/48.png",
    "128": "icones/128.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*.youtube.com/"
  ],
  "browser_action": {
    "default_icon": "icones/19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
