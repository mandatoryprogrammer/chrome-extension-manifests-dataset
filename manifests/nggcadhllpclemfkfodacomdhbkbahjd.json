{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "timeTrackr",
  "description": "This extension will let you know how much time you spend on individual websites.",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "jquery-2.1.4.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "storage"
  ]
}
