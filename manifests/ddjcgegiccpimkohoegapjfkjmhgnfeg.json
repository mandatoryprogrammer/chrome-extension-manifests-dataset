{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NZIFF Calendar export tool",
  "description": "This extension adds a button to download your wishlist as a calendar file on the wishlist page.",
  "version": "2.1",
  "permissions": [
    "*://www.nziff.co.nz/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.nziff.co.nz/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js",
    "ics.js",
    "moment.js",
    "moment-timezone.js"
  ]
}
