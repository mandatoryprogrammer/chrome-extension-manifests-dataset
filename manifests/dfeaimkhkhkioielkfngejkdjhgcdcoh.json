{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AudioBud",
  "author": "Test Group DI",
  "version": "1.1",
  "description": "Web Audio Visualizer",
  "permissions": [
    "tabCapture",
    "activeTab",
    "declarativeContent",
    "storage",
    "contextMenus",
    "tabs",
    "notifications"
  ],
  "icons": {
    "16": "images/AudioBud16.png",
    "32": "images/AudioBud32.png",
    "48": "images/AudioBud48.png",
    "128": "images/AudioBud128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/AudioBud16.png",
      "32": "images/AudioBud32.png",
      "48": "images/AudioBud48.png",
      "128": "images/AudioBud128.png"
    },
    "default_title": "AudioBud"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  }
}
