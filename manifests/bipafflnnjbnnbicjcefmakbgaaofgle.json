{
  "name": "Extensions HotKey",
  "description": "Ctrl + Shift + E to open extensions page",
  "version": "0.6.1",
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "hotkey.js"
      ]
    }
  ]
}
