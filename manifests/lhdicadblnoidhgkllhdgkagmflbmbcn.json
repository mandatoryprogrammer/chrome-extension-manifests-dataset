{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "kwitty",
  "description": "A chrome application of twitter",
  "version": "1.5.9",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "tabs",
    "*://*.twitter.com/*",
    "*://*.appspot.com/*",
    "http://api.longurl.org/",
    "http://api.embed.ly/"
  ],
  "web_accessible_resources": [
    "main.html"
  ]
}
