{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SimpleClear",
  "version": "1.1.2.7",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_manifest_description__",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19-0.png",
      "38": "icon-38-0.png"
    },
    "default_title": "__MSG_manifest_toolbarbtn_tooltip__",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "common.js",
      "bg.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "browsingData"
  ]
}
