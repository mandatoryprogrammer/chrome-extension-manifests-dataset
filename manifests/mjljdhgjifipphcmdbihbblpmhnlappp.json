{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Snapblock",
  "version": "0.3.1",
  "description": "Hides Snap.com popups",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": [
        "snapblock.css"
      ]
    }
  ],
  "manifest_version": 2
}
