{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ethereum Tab - Streaming price & market info.",
  "short_name": "Ethereum Tab",
  "version": "1.0.1",
  "description": "Replace the New Tab with streaming price, news, community updates, and more.",
  "default_locale": "en",
  "icons": {
    "16": "img/img_16.png",
    "32": "img/img_32.png",
    "48": "img/img_48.png",
    "128": "img/img_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "browser_action": {
    "default_title": "Ethereum Tab"
  },
  "permissions": [
    "activeTab",
    "https://blockmodo.com/*"
  ]
}
