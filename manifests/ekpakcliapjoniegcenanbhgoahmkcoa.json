{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trellify Tabs",
  "description": "Chrome extension to save and restore tabs using Trello",
  "version": "1.0",
  "background": {
    "scripts": [
      "js/trellifyBackground.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "options_page": "html/page_options.html",
  "browser_action": {
    "default_popup": "html/page_popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "html/page_options.html"
  ]
}
