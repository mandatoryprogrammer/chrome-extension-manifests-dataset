{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FlipTab",
  "version": "1.0.1",
  "description": "The simplest way to remember your goals",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "flipband.html"
  },
  "permissions": [
    "storage"
  ]
}
