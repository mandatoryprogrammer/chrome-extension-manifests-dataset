{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MMRA",
  "version": "0.0.1",
  "description": "A plugin to make Medium readable again!",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "index.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "styles.css"
  ]
}
