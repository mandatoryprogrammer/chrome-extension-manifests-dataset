{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gkey",
  "short_name": "Gkey",
  "description": "Keyboard shortcut [CTRL + L] to quickly sign out of Gmail",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "cat_icon.png"
  },
  "manifest_version": 2
}
