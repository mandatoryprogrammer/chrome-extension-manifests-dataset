{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Stealth",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "images/show.png",
    "default_title": "Toggle Stealth"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "tabs",
    "https://*/*"
  ],
  "options_page": "options.html"
}
