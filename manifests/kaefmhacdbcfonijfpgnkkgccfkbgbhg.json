{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "github-freshness",
  "version": "2.0.0.1",
  "manifest_version": 2,
  "description": "Alert that looking at the old Github repository.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery.min.js",
        "moment.min.js",
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
