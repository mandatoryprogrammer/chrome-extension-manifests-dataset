{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My New Tab",
  "version": "2.1",
  "description": "Replace newtab with any url & keep the addres bar clean!",
  "manifest_version": 2,
  "icons": {
    "128": "img/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "homepage_url": "https://github.com/tborychowski/mynewtab",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "settings.html",
    "chrome_style": true
  }
}
