{
  "name": "Tg Miner Block",
  "description": "Blocks mining sites",
  "version": "0.1.0",
  "author": "Oleg E. Ivanov",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "resources/libs/core.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "resources/icons/16-disabled.png",
      "32": "resources/icons/32-disabled.png",
      "48": "resources/icons/48-disabled.png",
      "128": "resources/icons/128-disabled.png"
    }
  },
  "icons": {
    "16": "resources/icons/16-enabled-blocked.png",
    "32": "resources/icons/32-enabled-blocked.png",
    "48": "resources/icons/48-enabled-blocked.png",
    "128": "resources/icons/128-enabled-blocked.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/*"
  ]
}
