{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rundstedt e-Transfer Booster",
  "description": "Yooo",
  "version": "1.2.4",
  "icons": {
    "128": "vR.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-1.11.3.min.js",
      "underscore.min.js",
      "date.js",
      "global.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://service.rundstedt-transfer.de/firma_dtl_tab.php*",
        "http://service.rundstedt-transfer.de/job_edit.php",
        "http://service.rundstedt-transfer.de/knd2firma_list.php*",
        "http://service.rundstedt-transfer.de/sub_people_finder.php"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "extension.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ]
}
