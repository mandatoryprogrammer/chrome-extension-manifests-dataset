{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Yellowkies Login",
  "version": "0.92",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Yellowkies Login",
    "default_popup": "options.html"
  }
}
