{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Era Youtube",
  "version": "1.9",
  "description": "High Speed Youtube Proxy. Youtube reimagined!",
  "devtools_page": "devpage.html",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon_128.png"
    }
  },
  "icons": {
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "archivesavecheck.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "content.js",
    "New-Era-Youtube/jquery-2.1.0.min.js"
  ],
  "manifest_version": 2
}
