{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paridae",
  "short_name": "Paridae",
  "version": "0.8.7",
  "manifest_version": 2,
  "description": "Make twitter.com more responsive (previously named Responsive Twitter)",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "omnibox": {
    "keyword": "@"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  }
}
