{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Site Manager",
  "description": "History, Bookmarks and Cookies management for a current web page",
  "version": "1.1.2",
  "minimum_chrome_version": "57",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "cookies",
    "contextMenus",
    "<all_urls>"
  ]
}
