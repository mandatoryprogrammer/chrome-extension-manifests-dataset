{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "js/lib/http.js",
        "js/lib/utils.js",
        "js/github.js"
      ],
      "matches": [
        "*://github.build.ge.com/*"
      ],
      "all_frames": true
    }
  ],
  "description": "Why do we need to know others SSO, lets see the name.",
  "manifest_version": 2,
  "name": "GE github SSO Resolver",
  "permissions": [
    "tabs",
    "storage",
    "*://github.build.ge.com/*",
    "webNavigation"
  ],
  "icons": {
    "128": "img/github-ge.png"
  },
  "version": "1.0.2.4"
}
