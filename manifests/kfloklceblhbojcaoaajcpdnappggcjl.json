{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "Take a screenshot"
  },
  "commands": {
    "save": {
      "description": "Take a screenshot",
      "suggested_key": {
        "default": "Alt+X"
      }
    }
  },
  "description": "Download the page screen with just one click",
  "icons": {
    "128": "icons/128x128.png",
    "16": "icons/16x16.png",
    "48": "icons/48x48.png"
  },
  "manifest_version": 2,
  "name": "Easy Screenshot",
  "permissions": [
    "tabs",
    "<all_urls>",
    "downloads"
  ],
  "short_name": "Easy Screenshot",
  "version": "1.1"
}
