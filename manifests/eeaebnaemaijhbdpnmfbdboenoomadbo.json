{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Chromi",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Facilitate command-line and scripted access to chrome's extension API.",
  "permissions": [
    "tabs",
    "bookmarks",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "extension/chromi.js"
    ]
  }
}
