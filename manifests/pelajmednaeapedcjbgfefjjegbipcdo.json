{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "version": "2013.6.8",
  "name": "Top Sites Widget [ANTP]",
  "description": "Displays your top sites in a widget.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "widget.html"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "topSites",
    "tabs"
  ]
}
