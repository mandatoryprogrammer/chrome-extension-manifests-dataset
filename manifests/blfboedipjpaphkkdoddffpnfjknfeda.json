{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "The Ungender",
  "version": "0.1",
  "description": "Replace gender-specific with gender-neutral pronouns",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
