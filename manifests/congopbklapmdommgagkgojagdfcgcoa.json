{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Plain",
  "version": "2.2.0",
  "description": "simplify twitter extension",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "matches": [
        "*://twitter.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  }
}
