{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Toggle extension"
    }
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    }
  },
  "description": "Determine the most used font-size and zoom the page so the x-height is the specified size",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "SK Zoomer",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "short_name": "SK Zoomer",
  "version": "1.1.1"
}
