{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Popup",
  "description": "This simple chrome extension lets you open Reddit links in a popup.",
  "version": "1.5.4",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*",
        "*://reddit.com/*"
      ],
      "js": [
        "inject.js"
      ],
      "css": [
        "inject.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "loading.html"
  ]
}
