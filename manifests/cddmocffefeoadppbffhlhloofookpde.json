{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quote Player",
  "description": "Get some inspiration throughout your day: Play and manage your favourite Quotes, while working ony your Computer",
  "version": "1.0",
  "icons": {
    "16": "iconk.png",
    "32": "iconm.png",
    "64": "iconb.png"
  },
  "browser_action": {
    "default_icon": "iconk.png",
    "default_popup": "index.html#/popup"
  },
  "permissions": [
    "notifications",
    "tabs",
    "unlimitedStorage",
    "http://www.hannaseithe.de/",
    "identity",
    "identity.email"
  ],
  "background": {
    "page": "app3/index.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*"
  ]
}
