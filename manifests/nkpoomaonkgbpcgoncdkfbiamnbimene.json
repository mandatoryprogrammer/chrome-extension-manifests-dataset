{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Onur Suyalcinkaya",
  "name": "Notiful",
  "short_name": "Notiful",
  "description": "Open a new tab and note your best thoughts.",
  "browser_action": {},
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "version": "1.1.0"
}
