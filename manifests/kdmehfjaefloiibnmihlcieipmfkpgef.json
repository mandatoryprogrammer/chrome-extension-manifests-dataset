{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "comm.prod",
  "version": "0.0.6",
  "description": "Get quick access to current comm.prods.",
  "browser_action": {
    "default_icon": "img/bomb.png",
    "default_popup": "popup.html",
    "default_title": "comm.prod"
  },
  "permissions": [
    "http://burtonthird.com/.*",
    "http://localhost:8000/.*",
    "cookies",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48": "img/btb-logo-48.png",
    "128": "img/btb-logo-128.png"
  }
}
