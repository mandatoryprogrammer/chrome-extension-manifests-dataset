{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Demandware Logs Notifier",
  "description": "Recieve a notification everytime there is an update to your Demandware logs.",
  "version": "1.0.3",
  "permissions": [
    "https://*.demandware.net/",
    "notifications",
    "alarms",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "settings_list.js",
      "functions.js",
      "jquery-2.1.0.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
