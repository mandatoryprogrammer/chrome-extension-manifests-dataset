{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Ad Skipper",
  "description": "Skip youtube advertise",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "/assets/js/jQuery.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Youtube Ad Skipper",
    "default_icon": "/assets/logo/log.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "/assets/js/jQuery.js"
      ]
    }
  ],
  "manifest_version": 2
}
