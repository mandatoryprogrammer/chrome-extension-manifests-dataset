{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Making Progress",
  "version": "1.0.1",
  "description": "Minimal progress bars for your hour, day, week, month, and year in your new tab.",
  "author": "Ryder Damen",
  "icons": {
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
