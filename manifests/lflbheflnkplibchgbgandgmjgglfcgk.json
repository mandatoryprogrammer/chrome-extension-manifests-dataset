{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "page_action": {
    "default_icon": "yamcheck_19.png",
    "default_title": "This content has been mentioned in Yammer."
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "name": "Storyful Yamcheck",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "Detects if a YouTube, Instagram, Vimeo, VK, or Facebook video or Tweet or Storyful story has been mentioned in Yammer.",
  "content_security_policy": "script-src 'self' https://www.yammer.com/; object-src 'self'",
  "permissions": [
    "https://www.yammer.com/",
    "tabs"
  ],
  "icons": {
    "16": "yamcheck_16.png",
    "48": "yamcheck_48.png",
    "128": "yamcheck_128.png"
  }
}
