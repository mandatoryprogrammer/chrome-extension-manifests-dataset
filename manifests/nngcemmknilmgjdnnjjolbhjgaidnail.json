{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Binkey: Bing Keyboard Shortcuts",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Gives you the power of keyboard shortcuts for Bing. Search Bing and press 'h' to begin!",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.bing.com/search*"
      ],
      "css": [],
      "js": [
        "shortcuts.js"
      ]
    }
  ]
}
