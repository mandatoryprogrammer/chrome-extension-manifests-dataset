{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_title__",
  "short_name": "LFO",
  "description": "__MSG_description__",
  "version": "1.3.2",
  "default_locale": "en",
  "icons": {
    "16": "icon016.png",
    "48": "icon048.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ]
}
