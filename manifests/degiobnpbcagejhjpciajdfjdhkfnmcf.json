{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Form Freedom",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Never have to scroll to find the USA again",
  "browser_action": {
    "name": "Form Freedom",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
