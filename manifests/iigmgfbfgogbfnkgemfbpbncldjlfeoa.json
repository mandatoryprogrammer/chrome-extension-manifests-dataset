{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "description": "Configure Chrome to treat .dev URLs (Pow top-level domains) as valid domains",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Pow domains",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
