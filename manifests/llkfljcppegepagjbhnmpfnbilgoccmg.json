{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Was It Discussed on HackerNews?",
  "version": "0.2.2",
  "description": "Check if current page was discussed on Hacker News.",
  "homepage_url": "https://github.com/vanyakosmos/discussed-on-hn",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/mid-128.png",
    "default_title": "Click to check."
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  }
}
