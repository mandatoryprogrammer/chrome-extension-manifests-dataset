{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook \"Sponsored\" Ads Blocker",
  "description": "Removes all the \"sponsored\" ads/posts that you might find in your Facebook news feed.",
  "version": "2.1.3",
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon_small.png",
    "192": "icon.png"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  },
  "permissions": [
    "*://www.facebook.com/*"
  ],
  "manifest_version": 2
}
