{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "URL Page Turner",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Allows users to use right/left arrows to turn pages.",
  "browser_action": {
    "default_icon": {
      "19": "page-19.png"
    },
    "default_title": "URL Page Turner",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "page-48.png",
    "128": "page-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "keyaction.js"
      ]
    }
  ]
}
