{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yucata iMproved",
  "version": "1.0",
  "description": "Improvements for the yucata.de board game website.",
  "permissions": [
    "http://yucata.de/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.yucata.de/*/Game/*"
      ],
      "run_at": "document_end",
      "js": [
        "jquery-1.11.3.min.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
