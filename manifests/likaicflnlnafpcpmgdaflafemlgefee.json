{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Checklist Everywhere",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Create checklist on any website you browse!",
  "icons": {
    "50": "images/default_50.png",
    "100": "images/default_100.png",
    "128": "images/default_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "50": "images/uncheck_50.png",
      "100": "images/uncheck_100.png",
      "128": "images/uncheck_128.png"
    }
  }
}
