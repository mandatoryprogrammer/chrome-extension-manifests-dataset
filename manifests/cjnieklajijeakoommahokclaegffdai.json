{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "yyyymmddesa",
  "version": "0.2.4",
  "manifest_version": 2,
  "description": "Tiny tools for daily report in esa.io",
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.esa.io/posts/*"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
