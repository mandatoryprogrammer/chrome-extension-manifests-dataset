{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YOLO",
  "description": "This extension turns Submit buttons into YOLO buttons.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "yolo.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
