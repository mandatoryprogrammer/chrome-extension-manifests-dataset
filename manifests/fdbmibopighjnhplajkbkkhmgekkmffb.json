{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extension_name__",
  "version": "2",
  "minimum_chrome_version": "29.0.1521.3",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "find_regexp.js",
        "tabsee_content.js"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extension_description__",
  "icons": {
    "48": "tabsee48x48.png",
    "128": "tabsee128x128.png"
  },
  "omnibox": {
    "keyword": "ts"
  },
  "optional_permissions": [
    "tabs"
  ],
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "tabsee_omnibox.js",
      "tabsee_menu.js"
    ],
    "persistent": false
  }
}
