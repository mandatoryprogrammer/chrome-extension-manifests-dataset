{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facesnoop",
  "description": "Read your Facebook messages without notifying your friends",
  "version": "7.5",
  "author": "Aaron",
  "icons": {
    "16": "res/icon16.png",
    "32": "res/icon32.png",
    "48": "res/icon48.png",
    "128": "res/icon128.png",
    "256": "res/icon256.png"
  },
  "background": {
    "scripts": [
      "facesnoop.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "20": "res/icon16.png",
      "32": "res/icon32.png",
      "48": "res/icon48.png",
      "128": "res/icon128.png",
      "256": "res/icon256.png"
    },
    "default_popup": "interface.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*"
  ],
  "manifest_version": 2
}
