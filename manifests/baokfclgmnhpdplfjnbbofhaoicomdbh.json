{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Account Switcher Into Same Window",
  "version": "1.0.2",
  "description": "Changes Google Account Switcher behavior from opening new window/tab into staying in the same window.",
  "icons": {
    "48": "icon/48.png",
    "72": "icon/72.png",
    "96": "icon/96.png",
    "144": "icon/144.png",
    "192": "icon/192.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://*.google.com/*"
      ]
    }
  ]
}
