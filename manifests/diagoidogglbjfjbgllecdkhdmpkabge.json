{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IMDb to Movieo",
  "description": "Adds a button on IMDb to open the movie in Movieo.",
  "manifest_version": 2,
  "version": "1.2",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.imdb.com/title/tt*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png"
  ]
}
