{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Color Diff for Gmailâ„¢",
  "version": "0.1.6",
  "manifest_version": 2,
  "description": "This extension colors source code's diff inline of an email.",
  "permissions": [
    "https://mail.google.com/*"
  ],
  "icons": {
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "gcolordiff.js"
      ],
      "css": [
        "gcolordiff.css"
      ],
      "run-at": "document_end"
    }
  ]
}
