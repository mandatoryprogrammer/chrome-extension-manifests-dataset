{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "交通大學選課系統Plus",
  "version": "1.2.5",
  "description": "大大美化選課系統，提供自動填寫問卷功能",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "page_action": {
    "default_icon": "icon-fill.png",
    "default_title": "填寫"
  },
  "content_scripts": [
    {
      "matches": [
        "*://course.nctu.edu.tw/TeachPoll/question.asp"
      ],
      "js": [
        "callfill.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/*"
      ],
      "css": [
        "lib/jqueryui/css/sunny/jquery-ui-1.9.2.custom.css",
        "styles.css"
      ],
      "js": [
        "jquery.js",
        "preload.js",
        "lib/jqueryui/js/jquery-ui-1.9.2.custom.min.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/"
      ],
      "js": [
        "toindex.js"
      ]
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/getSafeCode.asp"
      ],
      "js": [
        "lib/fancybtn.js",
        "pagejs/getSafeCode.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/index.asp"
      ],
      "js": [
        "skip.js",
        "lib/cookie.js",
        "pagejs/index.js",
        "lib/fancybtn.js"
      ],
      "css": [
        "pagecss/index.css"
      ]
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/inHelp.asp"
      ],
      "js": [
        "pagejs/inHelp.js",
        "pagejs/inHelp/cufon-yui.js",
        "pagejs/inHelp/arial.js",
        "pagejs/inHelp/cuf_run.js"
      ],
      "css": [
        "pagecss/inHelp.css"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/inMenu.asp"
      ],
      "js": [
        "pagejs/inMenu.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/inTitle.asp"
      ],
      "css": [
        "pagecss/inTitle.css"
      ],
      "js": [
        "pagejs/inTitle.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/Course/History/adList.ASP/DISABLED"
      ],
      "css": [
        "jquery/tipTip.css",
        "pagecss/adList.css"
      ],
      "js": [
        "jquery/jquery.tipTip.js",
        "pagejs/adList.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/Course/History/",
        "*://course.nctu.edu.tw/Course/History/index.asp"
      ],
      "js": [
        "pagejs/History/index.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://course.nctu.edu.tw/adNow.asp"
      ],
      "js": [
        "lib/cookie.js",
        "pagejs/adNow.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "menu.gif",
    "submenu.gif",
    "pagecss/inHelp/gallery_1.gif",
    "pagecss/inHelp/gallery_2.gif",
    "pagecss/inHelp/gallery_3.gif",
