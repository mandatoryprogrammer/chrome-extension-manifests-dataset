    "pagecss/inHelp/search.gif",
    "pagecss/inHelp/search_bg.gif",
    "lib/fancybtn/button_bg.png",
    "lib/fancybtn/buttons.css",
    "lib/jqueryui/css/sunny/images/ui-bg_diagonals-medium_20_d34d17_40x40.png",
