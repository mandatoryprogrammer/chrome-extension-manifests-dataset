{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stack Overflow Padding",
  "version": "1.0",
  "description": "Fix the bottom scrollbar hiding pre formated text/code on stack overfow",
  "manifest_version": 2,
  "author": "Daniel Kipkemei",
  "homepage_url": "https://github.com/dann254/stack-overflow-padding",
  "icons": {
    "429": "icon.png"
  },
  "permissions": [
    "https://stackoverflow.com/*"
  ],
  "content_scripts": [
    {
      "css": [
        "paddings.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://stackoverflow.com/*"
      ]
    }
  ]
}
