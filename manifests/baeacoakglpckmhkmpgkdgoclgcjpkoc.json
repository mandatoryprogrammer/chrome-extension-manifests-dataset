{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "nicopip",
  "version": "1.1.0",
  "description": "【非公式】ニコニコ動画・ニコニコ生放送をPinPで再生します",
  "icons": {
    "16": "img/16.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png",
    "256": "img/256.png",
    "512": "img/512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.nicovideo.jp/watch/*",
        "https://live.nicovideo.jp/watch/*",
        "https://live2.nicovideo.jp/watch/*"
      ],
      "js": [
        "js/contentScript.js"
      ]
    }
  ]
}
