{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IdeaScale Exporter",
  "description": "Export the ideas of an IdeaScale consultation.",
  "version": "0.1.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "http://ideas.ideascale.com/*",
    "storage"
  ],
  "icons": {
    "32": "ideascale_32.png",
    "128": "ideascale_128.png"
  }
}
