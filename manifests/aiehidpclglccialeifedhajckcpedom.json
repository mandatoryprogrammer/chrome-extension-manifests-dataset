{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "disabled.png",
    "default_popup": "popup.html"
  },
  "description": "Access virtual hosts of a server without the need of host redirection or DNS.",
  "manifest_version": 2,
  "name": "Virtual Hosts",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
