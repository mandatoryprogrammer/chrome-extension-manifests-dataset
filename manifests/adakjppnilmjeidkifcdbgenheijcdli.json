{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "dodo article chat",
  "version": "1.1",
  "description": "chat something",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/chat-speech-bubbles_16.png",
      "32": "images/chat-speech-bubbles_32.png",
      "128": "images/chat-speech-bubbles_128.png"
    }
  },
  "icons": {
    "16": "images/chat-speech-bubbles_16.png",
    "32": "images/chat-speech-bubbles_32.png",
    "128": "images/chat-speech-bubbles_128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src https://cdn.firebase.com https://*.firebaseio.com https://*.gstatic.com; object-src 'self'"
}
