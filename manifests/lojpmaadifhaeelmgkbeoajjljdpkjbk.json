{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoCongrats",
  "description": "ウェブ上の祝いなんてもんはただの文字列だ",
  "version": "1.0",
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/background.js"
      ],
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}
