{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "jandan段子助手",
  "version": "1.0.1",
  "description": "煎蛋段子/评论筛选高亮",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "matches": [
        "http://*.jandan.net/*",
        "https://*.jandan.net/*"
      ]
    }
  ],
  "manifest_version": 2
}
