{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "脳死感謝〜オートマチックサンクス〜",
  "description": "脳死感謝〜オートマチックサンクス〜",
  "version": "1.1",
  "permissions": [
    "https://www.chatwork.com/",
    "https://kcw.kddi.ne.jp/",
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.chatwork.com/*",
        "https://kcw.kddi.ne.jp/*"
      ],
      "js": [
        "js/jquery-2.1.4.min.js",
        "js/myscript.js"
      ]
    }
  ],
  "options_page": "options.html"
}
