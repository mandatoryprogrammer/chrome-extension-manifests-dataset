{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IceHrm-Hours",
  "description": "This extension fill hours in ice hrm website.",
  "version": "1.1.1",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "page_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "IceHrm - Hours"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://ice.devalore.com/app/?g=modules&n=time_sheets&m=module_Time_Management"
      ],
      "js": [
        "content.js",
        "jquery-3.1.0.min.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://developer.chrome.com/*"
  ]
}
