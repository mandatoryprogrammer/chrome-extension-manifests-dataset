{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BrassReader",
  "description": "RSS Reader.",
  "version": "0.1.3",
  "icons": {
    "16": "img/star-16.png",
    "48": "img/star-48.png",
    "128": "img/star.png"
  },
  "browser_action": {
    "default_icon": "img/star-16.png",
    "default_title": "Brass Reader"
  },
  "background": {
    "scripts": [
      "js/launch.js"
    ]
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "offline_enabled": true
}
