{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Proxy Auto Connection",
  "short_name": "PRAC",
  "version": "0.3",
  "description": "Digite seu login e senha apenas uma vez para autenticar no proxy de sua rede corporativa",
  "author": "Pedro Muniz <pedro.muniz@geeklab.com.br>",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "icon/icone_nopopup.png"
  },
  "browser_action": {
    "default_icon": "icon/icone_nopopup.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "browsingData"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "22.0.0"
}
