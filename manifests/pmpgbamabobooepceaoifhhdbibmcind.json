{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GithubToDos",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Github ToDos",
  "short_name": "GithubToDos",
  "homepage_url": "https://github.com/t0do",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "vendor/react.js",
        "vendor/react-dom.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
