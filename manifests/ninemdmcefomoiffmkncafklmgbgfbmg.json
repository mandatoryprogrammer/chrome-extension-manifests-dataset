{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stackview",
  "version": "1.0",
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persist": true
  },
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/iconblue48.png",
    "128": "./images/iconblue128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./jquery-3.2.1.min.js",
        "./content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Stackview"
  }
}
