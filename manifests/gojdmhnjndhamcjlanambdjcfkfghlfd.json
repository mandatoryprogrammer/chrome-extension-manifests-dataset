{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cryllic Converter",
  "description": "This extension will convert Latin letters to Cryllic letters as it sounds",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "input",
    "storage"
  ]
}
