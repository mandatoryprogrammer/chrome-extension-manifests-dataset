{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "snique",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Snique message reader",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "findresources.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ],
  "background": {
    "scripts": [
      "aes.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "settings.html"
}
