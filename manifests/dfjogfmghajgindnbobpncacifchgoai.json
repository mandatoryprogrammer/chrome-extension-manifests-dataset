{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cachify This Site",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Get Google's cached version of the current page.",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
