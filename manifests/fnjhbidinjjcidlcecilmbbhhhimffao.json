{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "hoax.ly",
  "description": "The hoax.ly browser extension will warn you when you browse questionable or maybe misleading content.",
  "version": "0.0.3",
  "icons": {
    "16": "icon16.png",
    "128": "icon180.png"
  },
  "browser_action": {
    "default_icon": "icon180.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "webNavigation",
    "<all_urls>"
  ]
}
