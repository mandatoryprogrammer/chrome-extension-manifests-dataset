{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Copy",
  "description": "Copy things when you select them",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "copy.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
