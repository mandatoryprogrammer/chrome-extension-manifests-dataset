{
  "manifest_version": 2,
  "name": "Spaulding Computer Web Defender",
  "description": "Ensure your online safety by automatically blocking known malware websites. Updated daily with new defintions!",
  "author": "Safety Redirector, LLC.",
  "homepage_url": "http://spauldingcomputers.com/",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "6.0.2",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "page": "pages/background.html"
  },
  "browser_action": {
    "default_icon": "icons/refresh.png",
    "default_title": "Update rules"
  },
  "icons": {
    "16": "icons/button.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "/icons/icon128.png"
  ],
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": true
  },
  "options_page": "pages/options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
