{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "miniGestures",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "mouseTrack.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "version": "1.5",
  "options_page": "options.html",
  "description": "a very simple mouse gestures program"
}
