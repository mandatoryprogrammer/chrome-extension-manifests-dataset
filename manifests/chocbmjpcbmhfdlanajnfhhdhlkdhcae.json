{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kick me off",
  "version": "0.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "alarms",
    "storage"
  ],
  "icons": {
    "48": "icon.png"
  }
}
