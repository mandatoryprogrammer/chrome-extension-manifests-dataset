{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Icon Come Back",
  "version": "1.2.3",
  "description": "Twitterの丸くなったアイコンを元に戻します",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://tweetdeck.twitter.com/"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_start"
    }
  ]
}
