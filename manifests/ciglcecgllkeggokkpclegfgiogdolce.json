{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inbox Pop",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "scripts/*"
  ],
  "browser_action": {
    "default_title": "Inbox Pop",
    "default_icon": {
      "48": "images/48.png"
    }
  },
  "icons": {
    "48": "images/48.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "identity",
    "storage",
    "http://inboxpop.com/*",
    "http://www.inboxpop.com/*",
    "https://www.inboxpop.com/*",
    "https://inboxpop.com/*",
    "http://localhost:8080/*",
    "https://fonts.googleapis.com/*"
  ]
}
