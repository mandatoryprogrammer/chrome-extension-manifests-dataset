{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Add feed to Slack",
  "short_name": "Feed2Slack",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "55",
  "description": "Add feed to Slack's RSS integration by Page Action.",
  "author": "Syoichi Tsuyuhara",
  "homepage_url": "https://github.com/syoichi/add-feed-to-slack",
  "icons": {
    "16": "img/icon/16.png",
    "48": "img/icon/48.png",
    "128": "img/icon/128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "img/icon/16.png",
      "24": "img/icon/24.png",
      "32": "img/icon/32.png"
    },
    "default_title": "Add feed to Slack"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
