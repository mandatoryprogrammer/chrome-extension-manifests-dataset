{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TV Vorhersage",
  "description": "Finde heraus, ob und wann bestimmte Episoden im TV gesendet werden",
  "version": "4.0.0",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "http://www.fernsehserien.de/*",
    "https://www.fernsehserien.de/*"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "TV Vorhersage",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
