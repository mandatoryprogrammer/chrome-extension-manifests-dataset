{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoTube",
  "description": "ほら、横に積んでいる仕事をしよう",
  "version": "1.0",
  "background": {
    "scripts": [
      "static/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "static/images/icon.png",
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
