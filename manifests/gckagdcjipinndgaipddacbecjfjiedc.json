{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Socialcast high five!",
  "version": "0.0.0.7",
  "manifest_version": 2,
  "description": "Socialcast high five!",
  "permissions": [
    "https://*.socialcast.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.socialcast.com/*"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "highFive.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "replace.js"
  ]
}
