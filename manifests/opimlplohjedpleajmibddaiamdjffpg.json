{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fullscreen Homestar Runner",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "A Chrome extension to automatically maximize the main content of homestarrunner.com cartoons including Strong Bad emails.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "incognito": "spanning",
  "content_scripts": [
    {
      "matches": [
        "http://homestarrunner.com/*",
        "http://*.homestarrunner.com/*"
      ],
      "js": [
        "fullscreen-homestar-runner.js"
      ]
    }
  ]
}
