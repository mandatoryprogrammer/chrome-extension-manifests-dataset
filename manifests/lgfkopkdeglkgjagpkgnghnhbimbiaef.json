{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "include_globs": [
        "*www.reddit.com/r/thebutton/*"
      ],
      "js": [
        "jquery.js",
        "button.js"
      ],
      "matches": [
        "http://www.reddit.com/r/thebutton/*",
        "https://www.reddit.com/r/thebutton/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Teh Button"
  },
  "icons": {
    "16": "images/icon16.png"
  },
  "converted_from_user_script": true,
  "description": "Reddit button highlighter and monitor",
  "name": "Teh Button",
  "version": "0.92",
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "contextMenus",
    "clipboardWrite"
  ]
}
