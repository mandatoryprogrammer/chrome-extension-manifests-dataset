{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "History Domain Scrubber",
  "description": "Automatically and continually clears Chrome history for any domain you enter",
  "version": "1.4",
  "homepage_url": "http://alexcrooks.me",
  "permissions": [
    "history",
    "alarms",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Manage scrubbed domains"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
