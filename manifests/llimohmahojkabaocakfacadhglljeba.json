{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "hanhai.user.js"
      ],
      "matches": [
        "http://bbs.ustc.edu.cn/*",
        "http://bbs2.ustc.edu.cn/*",
        "http://bbs3.ustc.edu.cn/*",
        "http://bbs6.ustc.edu.cn/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Add Keyboard Shortcuts for bbs*.ustc.edu.cn",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "name": "HanHai(瀚海星云)",
  "version": "0.9995"
}
