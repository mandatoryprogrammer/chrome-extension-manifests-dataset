{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabby Katz",
  "version": "1.1.1",
  "description": "catching cats and loving cats",
  "web_accessible_resources": [
    "images/*",
    "sounds/*"
  ],
  "chrome_url_overrides": {
    "newtab": "manager.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "images/cat.png",
    "48": "images/cat.png",
    "128": "images/cat.png"
  },
  "browser_action": {
    "default_icon": "images/cat.png",
    "default_popup": "popup.htm"
  },
  "background": {
    "scripts": [
      "background.js",
      "manager.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_globs": [
        "*/manager.html"
      ],
      "css": [
        "fix.css"
      ],
      "js": [
        "script.js",
        "jquery-3.2.1.js"
      ]
    }
  ]
}
