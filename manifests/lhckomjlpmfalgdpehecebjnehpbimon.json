{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BG and SB Popular Verses",
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "BG and SB verses most qouted by S.P.",
  "version": "2.0",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "images/vb-16.png",
    "32": "images/vb-32.png",
    "48": "images/vb-48.png",
    "128": "images/vb-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  }
}
