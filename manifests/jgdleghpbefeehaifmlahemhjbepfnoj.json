{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Widny-Chat",
  "author": "Widny St. Louis",
  "description": "Real-time screen share",
  "version": "0.0.0.4",
  "manifest_version": 2,
  "homepage_url": "http://tokbox.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
