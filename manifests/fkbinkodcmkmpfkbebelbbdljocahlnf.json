{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GeeksQuiz",
  "description": "The aim of the app is to generate random quiz from the site www.geeksquiz.com from the topics you choose.",
  "version": "1.1",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    },
    "permissions": [
      "<all_urls>"
    ]
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "icons": {
    "16": "images/logo.png",
    "128": "images/logo128.png"
  }
}
