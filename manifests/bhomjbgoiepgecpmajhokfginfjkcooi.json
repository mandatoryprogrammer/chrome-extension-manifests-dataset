{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Blu's Meme Organizer",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Organize all your memes and automate image storage",
  "homepage_url": "https://github.com/alimoabd2127/BlusMemeOrganizer",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/popup.html"
  },
  "permissions": [
    "contextMenus",
    "downloads",
    "storage"
  ]
}
