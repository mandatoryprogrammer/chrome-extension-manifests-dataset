{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bittrex night",
  "description": "This extension apply a dark theme to bittrex.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "https://bittrex.com/*"
      ]
    }
  ],
  "icons": {
    "128": "assets/icon128.png",
    "48": "assets/icon48.png",
    "16": "assets/icon16.png"
  }
}
