{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.2",
  "default_locale": "en",
  "omnibox": {
    "keyword": "tab"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  }
}
