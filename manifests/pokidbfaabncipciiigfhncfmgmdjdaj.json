{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lounge Companion (Dota 2 & CS:GO)",
  "short_name": "Lounge Companion",
  "description": "The best community addon for csgolounge and dota2lounge",
  "version": "3.5.5",
  "icons": {
    "128": "img/128x128.png"
  },
  "options_page": "settings.html",
  "permissions": [
    "storage",
    "identity",
    "cookies",
    "notifications",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "http://steamcommunity.com/"
  ],
  "oauth2": {
    "client_id": "920642326718-67ol845j1otp18blo54rb5m2ff5s15lc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "web_accessible_resources": [
    "img/aspin.gif",
    "img/astattrak-icon.png",
    "img/astattrak-icon-big.png",
    "img/asouvenir-icon.png",
    "img/asouvenir-icon-big.png",
    "js/lib/jquery-2.1.0.min.js",
    "js/lib/jquery.tinysort.min.js",
    "js/lib/jquery.tablesorter.min.js",
    "js/lib/jquery.cookie.js",
    "js/lib/signin.js",
    "js/main.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icons/19.png",
      "38": "img/icons/38.png"
    },
    "default_title": "CSGO Lounge Settings",
    "default_popup": "settings.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://csgolounge.com/*",
        "http://dota2lounge.com/*"
      ],
      "css": [
        "css/lib/font-awesome.min.css",
        "css/main.css"
      ],
      "js": [
        "js/lib/jquery-2.1.0.min.js",
        "js/lib/jquery.tinysort.min.js",
        "js/lib/jquery.cookie.js",
        "js/lib/signin.js",
        "js/main.js"
      ]
    }
  ]
}
