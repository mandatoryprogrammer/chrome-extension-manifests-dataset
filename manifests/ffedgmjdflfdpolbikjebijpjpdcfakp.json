{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Motivational New Tab",
  "description": "A Chrome extension to give you a bit of motivation, with quotes displayed on your new tab.",
  "version": "1.0.0",
  "author": "Sunday Code",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ],
  "homepage_url": "http://sundaycode.xyz"
}
