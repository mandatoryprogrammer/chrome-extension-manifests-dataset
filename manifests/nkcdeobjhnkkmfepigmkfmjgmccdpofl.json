{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CRX Helper",
  "version": "2.4",
  "manifest_version": 2,
  "description": "List CRX nodes.",
  "icons": {
    "19": "icon19.png",
    "128": "icon128.png"
  },
  "page_action": {
    "name": "CRX node explorer",
    "default_popup": "popup.html",
    "default_icon": "icon19.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}
