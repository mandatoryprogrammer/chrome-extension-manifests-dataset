{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text Translate",
  "version": "1.0",
  "browser_action": {
    "default_icon": "48-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "16-icon.png",
    "24": "24-icon.png",
    "32": "32-icon.png",
    "48": "48-icon.png",
    "64": "64-icon.png",
    "72": "72-icon.png",
    "96": "96-icon.png",
    "128": "128-icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
