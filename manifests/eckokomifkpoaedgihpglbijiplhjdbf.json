{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Without Numbers",
  "version": "1.1.0",
  "description": "remove numbers from Twitter.com",
  "author": "Julian Gruber, K.E. Mitchell",
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs"
  ]
}
