{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrueRL",
  "description": "Modifies deceptive links to point to where they say they do.",
  "version": "1.6",
  "icons": {
    "16": "icons/truerl_16.png",
    "48": "icons/truerl_48.png",
    "128": "icons/truerl_128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "truerl.js"
      ],
      "all_frames": true
    }
  ]
}
