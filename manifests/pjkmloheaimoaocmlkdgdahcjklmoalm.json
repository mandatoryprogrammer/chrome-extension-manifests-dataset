{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Pic",
  "description": "Показывает изображения из ссылок в Twitter-ленте.",
  "version": "1.0.4",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "assets/icon-128.png",
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "libs/jquery.min.js",
        "js/content.js"
      ]
    }
  ]
}
