{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "linkInjector.js"
      ],
      "matches": [
        "http://xkcd.com/*/"
      ]
    }
  ],
  "description": "Create a link on every XKCD comic to its corresponding Explain XKCD page.",
  "name": "Explain XKCD Link",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
