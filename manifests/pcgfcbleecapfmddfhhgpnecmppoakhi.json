{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sub-folders",
  "version": "1.0.0",
  "description": "Organize your youtube subs into folders",
  "author": "shesmu",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "jquery.min.js",
        "jquery.ui.js",
        "index.js"
      ],
      "css": [
        "index.css"
      ]
    }
  ],
  "icons": {
    "16": "1-01.png",
    "48": "1-01.png",
    "128": "1-01.png"
  }
}
