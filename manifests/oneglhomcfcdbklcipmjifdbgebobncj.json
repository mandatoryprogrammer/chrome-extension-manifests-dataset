{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CodeBindle",
  "description": "Highlight code snippets using the keyboard ('Ctrl-]' and 'Ctrl-[')",
  "author": "Pavel Henkin",
  "version": "0.2",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "libs/jquery-2.1.4.min.js",
        "libs/jquery.hotkeys.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
