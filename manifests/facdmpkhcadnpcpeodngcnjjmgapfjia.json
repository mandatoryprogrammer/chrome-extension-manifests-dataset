{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Div Highlighter",
  "version": "1.0",
  "description": "tool to highlight divs!",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2
}
