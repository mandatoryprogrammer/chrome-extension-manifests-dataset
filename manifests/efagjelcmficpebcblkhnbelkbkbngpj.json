{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kitty Replace",
  "description": "This extension will replace animated gifs on a webpage with images of kittens.",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Replace GIFs",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}
