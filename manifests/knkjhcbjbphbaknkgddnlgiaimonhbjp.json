{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Saso Highlighted Search",
  "description": "Allows for a quick search of highlighted text with search engines.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery-3.1.0.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "options.html"
  ]
}
