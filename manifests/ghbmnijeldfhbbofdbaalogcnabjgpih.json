{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Current Year to 2016",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "description": "Replaces the text 'current year' with '2016'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
