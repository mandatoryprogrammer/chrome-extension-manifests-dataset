{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The chosen tab",
  "description": "Chose your own tab size!",
  "version": "0.1.1",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "tab-sizer.js"
      ]
    }
  ]
}
