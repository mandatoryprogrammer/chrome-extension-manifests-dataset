{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Showkey转屏助手",
  "version": "1.0.1",
  "description": "“showkey转屏助手”插件可以让你通过“傻瓜式”操作帮您获取正在访问页面的视频种子地址，一键转屏到电视大屏幕上。小屏转大屏，推你想推！",
  "permissions": [
    "cookies",
    "tabs",
    "http://tt.showkey.tv/*",
    "https://*/*",
    "http://*/*",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "showkey assistant.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/contextMenu.js"
    ]
  },
  "manifest_version": 2
}
