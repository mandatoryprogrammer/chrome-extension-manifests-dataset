{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Theor",
  "description": "Enhance theoldreader.com keyboard shortcuts (and more)",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://theoldreader.com/*"
      ],
      "js": [
        "theor.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
