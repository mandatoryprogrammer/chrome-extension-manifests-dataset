{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html",
    "default_title": "百度百科"
  },
  "content_scripts": [
    {
      "css": [
        "content_scripts/baike.css"
      ],
      "js": [
        "content_scripts/baike.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' ; object-src 'self' ; img-src 'self' chrome-extension://'",
  "description": "百科一下，你就知道，来自百度百科官方，让人类更好地认知世界！",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "百度百科",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.8",
  "web_accessible_resources": [
    "icon.png"
  ]
}
