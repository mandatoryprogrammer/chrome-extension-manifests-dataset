{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Birds Aren't Real",
  "version": "1.2",
  "description": "Corrects the word 'bird' to 'government drone'.",
  "author": "Anna Sobolewska",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "bird16.png",
    "24": "bird24.png",
    "32": "bird32.png",
    "128": "bird128.png"
  },
  "manifest_version": 2
}
