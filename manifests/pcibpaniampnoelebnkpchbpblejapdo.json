{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "https://portal.nctu.edu.tw/portal/login.php"
      ],
      "js": [
        "tesseract.js",
        "app.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.rawgit.com/; object-src 'self'",
  "description": "自動辨識交大 portal 登入頁面的驗證碼。",
  "manifest_version": 2,
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "name": "NCTU portal",
  "permissions": [
    "https://portal.nctu.edu.tw/*"
  ],
  "version": "0.1.0"
}
