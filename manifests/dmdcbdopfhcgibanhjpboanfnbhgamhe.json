{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chipest",
  "description": "Best deal searcher on Aliexpress, Ebay in Israel",
  "version": "1.0",
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "128": "./images/128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "<all_urls>",
    "tabs"
  ]
}
