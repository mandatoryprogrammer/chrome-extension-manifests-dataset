{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "timer.html"
  },
  "icons": {
    "48": "icon.png",
    "128": "img1.png"
  },
  "description": "Stops you from getting distracted when you are studying! This extension automatically deletes a tab if it has a distracting website",
  "manifest_version": 2,
  "name": "Studdy Buddy Deluxe",
  "options_page": "options.html",
  "offline_enabled": true,
  "permissions": [
    "notifications",
    "tabs",
    "background",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
