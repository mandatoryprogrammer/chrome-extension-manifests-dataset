{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Concentrate",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Prevents users from entering distracting websites.",
  "icons": {
    "48": "concentrate_logo.png",
    "128": "concentrate_ON.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "storage.js",
      "list.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Concentrate",
    "default_icon": "concentrate_logo.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "webRequestBlocking"
  ]
}
