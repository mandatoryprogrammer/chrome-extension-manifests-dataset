{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK Plus-markör",
  "version": "1.3",
  "description": "Markerar VK:s Plus-artiklar med bakgrundsfärg och ikon.",
  "content_scripts": [
    {
      "matches": [
        "*://*.vk.se/*"
      ],
      "js": [
        "nonplus.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "plus.svg"
  ]
}
