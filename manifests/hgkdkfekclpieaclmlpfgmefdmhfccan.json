{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Speak Thai",
  "description": "Reads out selected thai text",
  "version": "0.1",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "speak-thai.js"
    ]
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
