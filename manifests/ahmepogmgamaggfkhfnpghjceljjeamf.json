{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Github TextWrapper",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Wrap github textareas for proper mail columns",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery.js",
        "jquery.textWrapper.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://github.com/"
  ]
}
