{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DaltonTab",
  "version": "0.8.0",
  "icons": {
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "storage",
    "*://*.dalton.org/",
    "*://*.myhomework.space/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
