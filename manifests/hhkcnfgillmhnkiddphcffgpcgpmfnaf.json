{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blur Images",
  "version": "1.0",
  "description": "Blur images and show up on mouve over",
  "icons": {},
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "css": [
        "blur-images.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/blur-icon.jpeg"
    }
  }
}
