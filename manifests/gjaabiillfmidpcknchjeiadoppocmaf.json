{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Cursor",
  "description": "A productivity tool to automatically select the first visible text input.",
  "version": "1.0",
  "icons": {
    "128": "auto-cursor-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "auto-cursor.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
