{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "Creates a brand new TinyURL for the current tab that you are on!",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "TinyURL Maker",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "version": "2.0.1"
}
