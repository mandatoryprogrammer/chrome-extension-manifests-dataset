{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1",
  "name": "wallpool - Wallpaper for ChromeOS",
  "description": "Wallpaper extension for chromeOS",
  "permissions": [
    "wallpaper"
  ],
  "background": {
    "scripts": [
      "backend/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Change your Wallpapaer",
    "default_popup": "interface/popup.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
