{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DePlaylist",
  "version": "1.0",
  "description": "Allows you to quickly jump out of a YouTube playlist to the current standalone video",
  "icons": {
    "128": "icon-pa.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-pa.png",
    "default_title": "DePlaylist video"
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
