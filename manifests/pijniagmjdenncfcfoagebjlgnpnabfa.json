{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "E-Okul Ortalama Hesaplayıcı",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Notlarınızı düzenleyerek dönem sonu ortalamanızı ve alacağınız belgeyi görebilirsiniz.",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "options_ui": {
    "page": "ayarlar.html"
  },
  "content_scripts": [
