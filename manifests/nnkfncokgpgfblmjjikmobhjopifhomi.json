{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Draw Your Professor Upload Tool",
  "description": "Upload pictures to drawyourprofessor.com",
  "version": "1.1",
  "manifest_version": 2,
  "short_name": "dyp-upload",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "js/main.js"
      ],
      "matches": [
        "*://www.drawyourprofessor.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
