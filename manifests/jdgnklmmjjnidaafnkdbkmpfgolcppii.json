{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Real Florida Man",
  "short_name": "Real Florida Man",
  "version": "1.1.3",
  "description": "Switches 'Donald Trump' and 'Florida Man' in headlines/Twitter/other things? Life's an adventure. Based on idea by @DAZZLESTORM",
  "browser_action": {
    "default_title": "Real Florida Man",
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "manifest_version": 2
}
