{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tumblr Batch Block",
  "manifest_version": 2,
  "version": "1.8.4",
  "description": "Block a comma separated list of bloggers",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://www.tumblr.com/settings/blog/*",
    "https://www.tumblr.com/settings/blog/*"
  ]
}
