{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Genconnator",
  "version": "1.02",
  "description": "Click on the button to see the event on genconplanner.com.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "https://www.gencon.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
