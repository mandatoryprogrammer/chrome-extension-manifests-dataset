{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "FollowUpChrome",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Sends the current URL to FollowUpThen",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "FollowUp on the current URL",
    "default_popup": "popup.html",
    "default_icon": "icons/icon19.png"
  },
  "permissions": [
    "tabs"
  ]
}
