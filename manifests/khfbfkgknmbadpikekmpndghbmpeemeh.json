{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "I Heart What I See",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Every heart is a heart and when you see a heart you heart the heart.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ]
}
