{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quicken",
  "description": "Surf faster than your mouse can handle!",
  "version": "2.4",
  "short_name": "Accelerate Keyboard Browsing",
  "permissions": [
    "tabs",
    "*://*/*",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "Open Next Page",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "humane.min.js",
        "mousetrap.min.js",
        "element_searcher.js"
      ],
      "css": [
        "libnotify.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "manifest_version": 2
}
