{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.2.0",
  "name": "Callback hell",
  "description": "lists all callbacks",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "512": "./callback_hell.png"
  },
  "permissions": [
    "<all_urls>",
    "history",
    "webRequest",
    "webNavigation"
  ]
}
