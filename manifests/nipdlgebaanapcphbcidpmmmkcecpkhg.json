{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PrettyPrint",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "version": "0.0.25",
  "description": "JavaScript and CSS formatter/syntax highlighter",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "bootstrap.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "file:///*"
      ],
      "js": [
        "content.js",
        "bootstrap.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/",
    "ftp://*/"
  ],
  "manifest_version": 2
}
