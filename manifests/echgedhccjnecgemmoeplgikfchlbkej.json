{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iMusic.am Downloder",
  "description": "Downloads current track from iMusic.am",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "https://imusic.am/*",
    "https://stream.imusic.am/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Click to download the current track."
  },
  "manifest_version": 2
}
