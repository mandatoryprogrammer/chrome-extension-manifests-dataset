{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "name": "Notifycon",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "0.9.0",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "background": {
    "scripts": [
      "js/Rain1/Overlay.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Notifycon",
    "default_icon": "img/16.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": []
}
