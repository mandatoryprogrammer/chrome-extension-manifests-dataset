{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Speculative Fictions",
  "short_name": "Speculative Fictions",
  "description": "Replaces fossil-fuel-related text with absurdity and commentary from speculative fiction.",
  "author": "Nick Lyell, Simone Doing, Max Puchalsky",
  "version": "1.0.2",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
