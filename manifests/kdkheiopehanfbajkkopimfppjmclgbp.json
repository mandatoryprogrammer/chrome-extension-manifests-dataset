{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simpl.js",
  "short_name": "Simpl.js",
  "description": "Simpl.js lets you write and run full-stack javascript apps in your browser",
  "offline_enabled": true,
  "icons": {
    "128": "icon.png"
  },
  "version": "0.76",
  "minimum_chrome_version": "33",
  "permissions": [
    "unlimitedStorage",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage",
    "browser",
    "http://*/",
    "https://*/",
    "ftp://*/"
  ],
  "app": {
    "background": {
      "scripts": [
        "simpl.js",
        "loader.js",
        "modules/crypto.js",
        "modules/database.js",
        "modules/html.js",
        "modules/http.js",
        "modules/socket.js",
        "modules/string.js",
        "modules/system.js",
        "modules/websocket.js",
        "background.js"
      ]
    }
  },
  "sockets": {
    "tcpServer": {
      "listen": "*"
    },
    "tcp": {
      "connect": "*"
    }
  },
  "url_handlers": {
    "launch": {
      "matches": [
        "https://simpljs.com/launch*"
      ],
      "title": "Launch Simpl.js Console"
    }
  },
  "externally_connectable": {
    "matches": [
      "https://simpljs.com/*"
    ]
  },
  "key": ""
}
