{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spoiled",
  "description": "This extension removes spoiler css from r/wow.",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/r/wow*"
      ],
      "js": [
        "jquery.js",
        "remove_spoilers.js"
      ]
    }
  ]
}
