{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Treat Yo'self",
  "description": "Swaps amazon's various versions of \"Get Yourself a Little Something\" text for something with a little more pizzazz.",
  "version": "720.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.amazon.com/*"
      ],
      "js": [
        "treat.js"
      ]
    }
  ],
  "icons": {
    "128": "crown.png"
  },
  "manifest_version": 2
}
