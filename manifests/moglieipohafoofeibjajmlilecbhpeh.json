{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "make-links.js"
      ],
      "matches": [
        "http://trello.com/*",
        "https://trello.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "External #id links for Trello",
  "options_page": "options.html",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.4"
}
