{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pro Life To Anti Choice",
  "version": "1.0",
  "description": "Replaces the text 'pro life' with 'anti choice'.",
  "icons": {
    "128": "antichoice.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
