{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reverse Geocoding",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Easily reverse geocode an address into a latitude and longitude",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "scripts/main.js"
      ]
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "<all_urls>"
  ]
}
