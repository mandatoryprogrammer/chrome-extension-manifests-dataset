{
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/ICON19.png",
      "38": "icon/ICON38.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "injected/dom.js",
        "js/common/commonext.js",
        "injected/sea-debug.js",
        "injected/seajs_boot.js",
        "injected/ready.js",
        "js/common/theme.js",
        "injected/nodeSelector.js",
        "injected/msgbox.js",
        "injected/autoload.js",
        "injected/plugin/loader.js"
      ],
      "css": [
        "css/jquery-ui.structure.css",
        "css/theme/jqueryui/jquery-ui.theme-light.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "lib/*",
    "injected/*",
    "js/*",
    "popup.html"
  ],
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "description": "Add your own JavaScript (powered by JQuery) and/or CSS to any page to trick it!",
  "icons": {
    "128": "icon/icon128.png",
    "16": "icon/icon16.png",
    "24": "icon/icon24.png",
    "48": "icon/icon48.png"
  },
  "manifest_version": 2,
  "name": "JavaScript Tricks",
  "options_page": "options.html",
  "permissions": [
    "declarativeContent",
    "webNavigation",
    "contextMenus",
    "activeTab",
    "tabs",
    "cookies",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.4"
}
