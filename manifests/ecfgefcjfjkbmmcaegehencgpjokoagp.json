{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "itlyst",
  "description": "Remember places you want to visit",
  "version": "0.0.0.4",
  "manifest_version": 2,
  "minimum_chrome_version": "6",
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery-3.0.0.min.js"
    ]
  },
  "icons": {
    "48": "img/itlyst-fork-logo-128x128-black.png",
    "128": "img/itlyst-fork-logo-128x128-white.png"
  },
  "web_accessible_resources": [
    "img/itlyst-fork-logo-128x128-black.png"
  ],
  "browser_action": {
    "default_icon": "img/itlyst-fork-logo-128x128-white.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-3.0.0.min.js",
        "js/sendnote.js",
        "js/lyster.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "activeTab",
    "cookies",
    "background"
  ]
}
