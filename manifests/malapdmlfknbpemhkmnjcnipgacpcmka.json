{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NW MLS Matrix Sanity Extension",
  "version": "0.3",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "description": "Content scripts to bring sanity into the NW MLS Matrix UI. Crappy extension to make the webapp a little less crappy.",
  "content_scripts": [
    {
      "matches": [
        "http://www.matrix.nwmls.com/Matrix/Public/Portal.aspx*"
      ],
      "css": [
        "mystyles.css"
      ],
      "all_frames": false,
      "js": [
        "add_frames.js",
        "intercept_controls.js"
      ]
    }
  ]
}
