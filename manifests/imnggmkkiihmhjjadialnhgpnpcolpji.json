{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Original Size Image Downloader",
  "description": "트위터,티스토리,인스타그램,다음카페에서 원본 이미지를 쉽게 다운받아주는 프로그램",
  "version": "0.30",
  "permissions": [
    "contextMenus",
    "downloads",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "operation.js"
    ]
  },
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
