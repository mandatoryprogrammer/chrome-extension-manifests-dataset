{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "16": "img/icon.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/bootstrap.css",
        "css/popup.css",
        "css/thankyou.css"
      ],
      "js": [],
      "matches": [
        "*://*.instagram.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "description": "Deixar de seguir os que n√£o te seguem",
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "name": "NoFollowMe",
  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>",
    "cookies",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "0.1.0",
  "web_accessible_resources": [
    "noFollow.html",
    "css/*",
    "img/*"
  ]
}
