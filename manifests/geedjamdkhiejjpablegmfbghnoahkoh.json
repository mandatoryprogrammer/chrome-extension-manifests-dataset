{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Little Laracasts",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Add a button to laracasts to let you use the entirety of your window efficiently.",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://laracasts.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
