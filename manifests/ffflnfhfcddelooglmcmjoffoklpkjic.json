{
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "pullrequest.js"
      ],
      "matches": [
        "https://github.com/*/*/compare/*"
      ]
    },
    {
      "js": [
        "jquery.min.js",
        "pt_project.js"
      ],
      "matches": [
        "https://www.pivotaltracker.com/n/projects/*"
      ]
    }
  ],
  "description": "Do alot about Github and PT process in EA",
  "key": "",
  "manifest_version": 2,
  "name": "EA Github and PT",
  "permissions": [
    "https://github.com/*",
    "https://www.pivotaltracker.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0"
}
