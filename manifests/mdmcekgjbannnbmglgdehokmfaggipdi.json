{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabStopper",
  "version": "0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "page_action": {
    "default_title": "This page is open in multiple tabs",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
