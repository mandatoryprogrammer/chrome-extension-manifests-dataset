{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nice Backgrounds",
  "description": "Makes white backgrounds easy to read",
  "version": "2.0",
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://stackoverflow.com/*",
        "*://*.stackoverflow.com/*",
        "*://youtube.com/*",
        "*://*.youtube.com/*",
        "*://reddit.com/*",
        "*://*.reddit.com/*",
        "*://xkcd.com/*",
        "*://*.xkcd.com/*"
      ],
      "js": [
        "changeColour.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "changeColour.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Beautify it"
  },
  "manifest_version": 2
}
