{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mirmindr",
  "version": "1.0",
  "description": "Mirmindr extension",
  "browser_action": {
    "default_title": "Mirmindr",
    "default_popup": "mirmindr.html",
    "default_icon": "mirmindr-icon.png"
  },
  "content_security_policy": "script-src 'self' https://mirmindr.firebaseio.com https://*.firebaseio.com; object-src 'self'",
  "permissions": [
    "identity",
    "identity.email",
    "storage"
  ]
}
