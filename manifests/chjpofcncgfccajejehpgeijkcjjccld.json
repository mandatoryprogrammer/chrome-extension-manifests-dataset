{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTitle",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Youtubeでタイトルを通知する拡張機能",
  "permissions": [
    "http://i.ytimg.com/vi/*",
    "tabs",
    "https://www.youtube.com/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "song.png",
    "default_title": "YouTitle\n\nYoutubeで再生する曲を通知します。"
  },
  "background": {
    "scripts": [
      "myscript.js"
    ]
  }
}
