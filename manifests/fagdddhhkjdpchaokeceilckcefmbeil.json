{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My はてブ Search",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "アイコンをクリックすることで現在のページから移動することなく、自分のはてなブックマークを参照できるようにする Extension です。",
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_icon": "icon/icon-19.png",
    "default_title": "My はてブ Search",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "unlimited_storage"
  ],
  "icons": {
    "128": "icon/icon-128.png"
  }
}
