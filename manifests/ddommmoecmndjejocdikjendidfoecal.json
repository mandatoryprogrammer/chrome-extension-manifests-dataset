{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "togist",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "github-little.png",
    "128": "github-big.png"
  },
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "https://api.github.com/gists"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "gist.js"
      ]
    }
  ]
}
