{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gerador de CPF e CNPJ",
  "description": "Gera CPF e CNPJ atrav√©s do clique direto do mouse.",
  "version": "1.2",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
