{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Infinite Viral",
  "description": "Viral news all the time.",
  "version": "1.3.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "infiniteViral.js"
      ],
      "run_at": "document_end"
    }
  ]
}
