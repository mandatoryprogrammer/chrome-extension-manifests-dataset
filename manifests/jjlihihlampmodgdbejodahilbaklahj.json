{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One-Click Cache Cleaner",
  "description": "Silently clean cache with just one click",
  "manifest_version": 2,
  "version": "1.0.0.8",
  "permissions": [
    "browsingData",
    "storage",
    "notifications",
    "cookies",
    "history",
    "downloads",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "One-Click Cache Cleaner"
  },
  "options_page": "options.html"
}
