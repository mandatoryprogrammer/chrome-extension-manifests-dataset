{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OmniRT - アドレスバーからリアルタイム検索",
  "version": "1.1",
  "manifest_version": 2,
  "description": "アドレスバーからヤフーのリアルタイム検索を直接実行",
  "icons": {
    "64": "icon_64.png",
    "48": "icon_64.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "rt"
  },
  "permissions": [
    "tabs"
  ]
}
