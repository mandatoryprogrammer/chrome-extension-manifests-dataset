{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ちょっとお気に入り",
  "version": "1.0",
  "description": "期間限定のお気に入りを登録",
  "icons": {
    "48": "icon/fav48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon/fav19.png",
    "default_popup": "popup.html",
    "default_title": "ちょっとお気に入り"
  }
}
