{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Obliviate",
  "version": "1.2",
  "description": "Automatically clears the cache on each page load, with ability to toggle on and off.",
  "permissions": [
    "storage",
    "browsingData",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "obliviate.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
