{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "explain xkcd",
  "version": "1.1",
  "description": "It's 'cause you're dumb.",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
