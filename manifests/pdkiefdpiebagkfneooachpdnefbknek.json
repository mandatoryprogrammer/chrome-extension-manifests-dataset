{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inker",
  "description": "Vector graphics editor",
  "version": "0.10.25",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "file_handlers": {
    "drawing": {
      "extensions": [
        "ink"
      ]
    }
  },
  "icons": {
    "16": "logo/inker-icon-16.png",
    "48": "logo/inker-icon-48.png",
    "128": "logo/inker-icon-128.png"
  },
  "permissions": [
    "fileSystem",
    "fileSystem.retainEntries",
    "fileSystem.write",
    "storage",
    "system.display"
  ]
}
