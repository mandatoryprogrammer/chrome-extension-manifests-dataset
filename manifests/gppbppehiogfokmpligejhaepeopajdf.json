{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "mailto.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "description": "__MSG_description__",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "name": "Mailto:",
  "version": "1.28.2",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "minimum_chrome_version": "26",
  "content_security_policy": "default-src 'self' ",
  "options_page": "options.html",
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "storage",
    "alarms"
  ]
}
