{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_app_title__",
  "version": "0.4.1",
  "manifest_version": 2,
  "description": "__MSG_app_description__",
  "homepage_url": "https://ully.in",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/icon48.png"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/underscore/underscore.min.js",
      "js/socket.io/socket.io.js",
      "src/bg/config.js",
      "src/bg/background.js",
      "src/bg/context_menu.js"
    ],
    "persistent": false
  },
  "options_page": "options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Ully",
    "default_popup": "src/browser_action/popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "contextMenus",
    "notifications",
    "https://ully.in/api/*"
  ],
  "omnibox": {
    "keyword": "ully"
  }
}
