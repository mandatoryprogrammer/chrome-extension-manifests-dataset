{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pobieracz Audycji Polskiego Radia",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Pobieraj w łatwy sposób audycje radiowe Polskiego Radia do odsłuchania offline.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  }
}
