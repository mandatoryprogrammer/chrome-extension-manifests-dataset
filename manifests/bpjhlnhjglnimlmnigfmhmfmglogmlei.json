{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UnTrump: Replace 'Trump' with Trump insults.",
  "short_name": "UnTrump",
  "description": "Replaces the text of Trump's Name with an accurate insult.",
  "author": "Len Smith",
  "version": "2.3",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
