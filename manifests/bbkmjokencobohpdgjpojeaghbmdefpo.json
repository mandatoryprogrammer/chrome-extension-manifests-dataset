{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Extend TweetDeck",
  "version": "2.2.0",
  "description": "Improve your TweetDeck experience with new features and tweaks (including Keyboard shortcuts).",
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "permissions": [
    "https://web.tweetdeck.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://web.tweetdeck.com/"
      ],
      "js": [
        "js/jquery.js",
        "js/contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/better-tweetdeck.js",
    "css/custom.css",
    "templates/help.html",
    "templates/mustaches.js"
  ],
  "background": {
    "page": "analytics/ga.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
