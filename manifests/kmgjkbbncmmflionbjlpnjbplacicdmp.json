{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cloud To Arse Plus",
  "version": "0.0.0.1",
  "description": "Replaces the text 'the cloud' with 'my arse', as well as 'cloud' with 'arse' in certain contexts.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "cta-16.png",
    "128": "cta-128.png"
  }
}
