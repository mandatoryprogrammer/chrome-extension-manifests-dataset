{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vegetarian Times Printing",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "A better print stylesheet for Vegetarian Times",
  "homepage_url": "http://github.com/jmhobbs/vegetarian-times-print",
  "permissions": [
    "http://www.vegetariantimes.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.vegetariantimes.com/recipe/*"
      ],
      "css": [
        "print.css"
      ]
    }
  ]
}
