{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eight / 8card エクスポート",
  "version": "1.2",
  "author": "J. Maurice <j@wiz.biz>",
  "description": "名刺管理サービス「Eight」(8card.net) から連絡先情報をエクスポートする",
  "icons": {
    "16": "doge16.png",
    "48": "doge48.png",
    "128": "doge128.png"
  },
  "permissions": [
    "*://*.8card.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://8card.net/myhome/people"
      ],
      "js": [
        "jquery-1.10.2.js",
        "export.js"
      ]
    }
  ]
}
