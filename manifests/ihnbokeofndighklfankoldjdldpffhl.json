{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDB Friends Link",
  "description": "A simple link/anchor to your IMDB friends",
  "version": "1.1",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.imdb.com/*"
      ],
      "css": [],
      "js": [
        "jquery-2.1.3.min.js",
        "imdb-friends-link.js"
      ]
    }
  ]
}
