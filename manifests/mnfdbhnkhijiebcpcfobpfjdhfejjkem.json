{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "2Day's New Tab",
  "version": "0.1.16",
  "description": "A functional new tab page that displays Google Calendar events, and custom bookmarks.",
  "permissions": [
    "https://ajax.googleapis.com/*",
    "identity",
    "bookmarks",
    "storage",
    "tabs",
    "unlimitedStorage",
    "https://accounts.google.com/*",
    "identity.email"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://query.yahooapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "559223177845-t78ldg5pg7t7nqlskkuksqa6r3sl6l2e.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  }
}
