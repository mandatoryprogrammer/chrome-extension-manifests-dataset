{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bing Knowledge",
  "version": "0.1",
  "description": "Find info on entities in the page with the Bing Knowledge Widget",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png"
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
