{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bootstrings",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Load Bootstrap webpages with custom options access via querystrings",
  "icons": {
    "16": "icons/icon16.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/jquery-latest.js",
        "src/bootstrap.min.js",
        "src/bootstrings.js"
      ],
      "run_at": "document_end"
    }
  ]
}
