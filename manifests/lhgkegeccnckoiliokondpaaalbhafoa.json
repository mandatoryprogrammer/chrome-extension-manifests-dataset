{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "webdevmedia",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "default_locale": "en",
  "version": "4.0.6.0",
  "manifest_version": 2,
  "icons": {
    "30": "icon30.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://*.dnslytics.net/*"
  ],
  "content_security_policy": "default-src 'self'; connect-src https://*.dnslytics.net; img-src https://*.dnslytics.net;"
}
