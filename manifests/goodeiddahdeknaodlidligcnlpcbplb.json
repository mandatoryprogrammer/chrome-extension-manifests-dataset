{
  "manifest_version": 2,
  "name": "Amino Topliste",
  "description": "Få et hurtigt overblik over de mest aktive emner på amino.dk",
  "version": "1.0.1",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Amino Topliste",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.amino.dk/"
  ],
  "content_security_policy": "default-src 'none'; connect-src https://www.amino.dk/",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
