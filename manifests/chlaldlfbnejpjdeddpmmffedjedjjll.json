{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabba",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "storage",
    "commands"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "MacCtrl+Period"
      }
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "A chrome extension to help you navigate your tabs.",
  "version": "0.0.2"
}
