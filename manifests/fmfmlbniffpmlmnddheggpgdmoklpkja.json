{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Definitorium",
  "short_name": "Definy",
  "version": "0.7",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/128.png",
    "default_title": "Definitorium",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "assets/128.png",
    "64": "assets/64.png"
  }
}
