{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "In-page Highlighter",
  "version": "0.7",
  "description": "__MSG_description__",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "j.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "get_options.js"
    ],
    "persistent": true
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon@2x.png"
    }
  },
  "options_page": "options.html"
}
