{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Calendar Color Monday",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Google Calendar の土日に色をつけます。月曜始まりに対応しています。",
  "icons": {
    "16": "icon_016.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/*",
        "https://calendar.google.com/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "https://calendar.google.com/*",
    "https://calendar.google.com/*"
  ]
}
