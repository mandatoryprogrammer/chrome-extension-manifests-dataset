{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "./content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "Block annoying web notification popups",
  "manifest_version": 2,
  "name": "Web Notifications Disabler",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "unlimitedStorage",
    "contextMenus"
  ],
  "version": "0.0.3"
}
