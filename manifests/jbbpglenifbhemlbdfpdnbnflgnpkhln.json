{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Just Breathe",
  "version": "1.1",
  "description": "Simple breathing guide to calm anxiety.",
  "browser_action": {
    "default_icon": "ball.png"
  },
  "icons": {
    "16": "ball16.png",
    "48": "ball48.png",
    "128": "ball128.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
