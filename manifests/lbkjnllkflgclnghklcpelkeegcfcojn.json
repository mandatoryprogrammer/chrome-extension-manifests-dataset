{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Browser Command",
  "short_name": "browser-cmd",
  "version": "1.0.2",
  "description": "Control the browser from nodejs or the terminal",
  "icons": {
    "256": "icon256.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "main.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "browser-polyfill.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "optional_permissions": [
    "alarms",
    "bookmarks",
    "browsingData",
    "contentSettings",
    "contextMenus",
    "cookies",
    "declarativeContent",
    "downloads",
    "history",
    "identity",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "power",
    "printerProvider",
    "privacy",
    "sessions",
    "system.cpu",
    "system.memory",
    "system.storage",
    "tabCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "http://*/",
    "https://*/"
  ],
  "permissions": [
    "storage"
  ]
}
