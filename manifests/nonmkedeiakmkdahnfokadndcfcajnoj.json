{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Last Modified",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Shows the last modified date of a webpage.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
