{
  "manifest_version": 2,
  "name": "Ali infinite scroll",
  "description": "Ali infinite scroll",
  "version": "0.0.2",
  "author": "Artur Oleynik",
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png",
      "48": "icons/icon48.png"
    },
    "default_title": "Enjoy"
  },
  "content_scripts": [
    {
      "css": [
        "inject.css"
      ],
      "exclude_matches": [],
      "js": [
        "inject.js"
      ],
      "matches": [
        "http://*.aliexpress.com/*",
        "https://*.aliexpress.com/*"
      ]
    }
  ],
  "homepage_url": "https://github.com/artu-ole/aliinfinite",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "short_name": "aliinfinite",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "inject.css",
    "shadow.css"
  ]
}
