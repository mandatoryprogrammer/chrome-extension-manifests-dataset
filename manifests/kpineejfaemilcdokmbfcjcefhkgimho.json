{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tube Keys",
  "version": "0.61",
  "description": "The missing keyboard shortcuts for YouTube",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "jquery.min.js",
        "tubeKeys.js"
      ]
    }
  ],
  "background": "background.html",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*.youtube.com/*",
    "https://*.youtube.com/*"
  ],
  "manifest_version": 2
}
