{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Basic To-Do List",
  "short_name": "Basic To-Do List",
  "description": "Just quickly add your lists and notes.",
  "version": "1.2.7",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "Basic To-Do List"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "popup.html"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
