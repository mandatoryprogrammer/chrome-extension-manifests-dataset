{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart Zoom",
  "description": "You can use dobule click to smart zoom like mac magic mouse.",
  "version": "1.0.2",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "zoom.js",
        "smartZoomApp.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
