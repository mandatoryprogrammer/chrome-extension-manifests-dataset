{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Caranchoa The Game",
  "version": "1.3",
  "description": "Caranchoa The Game! Kick the screen! Triple Click #CARANCHOA",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "jquery.min.js",
    "middleware.js",
    "caranchoa.js",
    "caranchoa1.png",
    "caranchoa2.png",
    "caranchoa.wav"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "middleware.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
