{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Close Your Tabs",
  "description": "Manages and displays all tabs in browser and color-codes them depending on the duration of time since active use.",
  "version": "0.0.9",
  "browser_action": {
    "default_icon": "images/app-logo-favicon-32.png",
    "default_popup": "popup.html",
    "default_title": "Close Your Tabs"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "*://*/*",
    "https://www.closeyourtabs.com/dashboard*",
    "cookies",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "js/main.js"
    ]
  }
}
