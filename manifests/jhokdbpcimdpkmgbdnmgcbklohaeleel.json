{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Switcher",
  "version": "0.1.0",
  "description": "Navigate URL Hierarchy",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "url.js"
    ],
    "persistent": false
  }
}
