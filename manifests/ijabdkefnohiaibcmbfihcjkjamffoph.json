{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rocket Launch Schedule",
  "description": "List of all upcoming Launches",
  "version": "20.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "libs/onesignal/OneSignal.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
