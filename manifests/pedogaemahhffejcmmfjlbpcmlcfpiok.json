{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dhampir",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension allows for pop-up, customizable searches of Monster's resume database.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
