{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "homepage_url": "https://github.com/pd4d10/qrcode-helper",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  },
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "storage"
  ],
  "options_ui": {
    "page": "dist/options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "__MSG_generateCurrent__"
  }
}
