{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Popout for YouTubeâ„¢",
  "description": "Allows you to open videos in popups.",
  "short_name": "popout",
  "version": "0.0.2",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "tabs",
    "*://www.youtube.com/*"
  ],
  "web_accessible_resources": [
    "img/*.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*"
      ],
      "css": [
        "css/popout.css"
      ],
      "js": [
        "js/popout.js"
      ]
    }
  ]
}
