{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JIRA Ticket Browser",
  "short_name": "Softcrylic Labs",
  "manifest_version": 2,
  "version": "1.2",
  "description": "Typing the entire url to visit a ticket in JIRA?No more!! Developed by Krishnanand for Softcrylic and Waggle.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "browser_action": {
    "default_icon": "logo-waggle.png",
    "default_popup": "popup.html",
    "default_title": "♥ Waggle Ticket Browser ♥"
  }
}
