{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Accept-Timezone",
  "version": "0.0.1",
  "description": "Adds the Accept-Timezone header to all requests",
  "author": "Eric Saupe",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "accept-timezone.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "Accept-Timezone"
}
