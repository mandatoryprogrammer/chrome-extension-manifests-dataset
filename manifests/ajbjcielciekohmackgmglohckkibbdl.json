{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A-V2EX",
  "description": "Anti V2EX, block it when you spend to much time on it.",
  "version": "1.2.4",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*.v2ex.com/",
    "*://v2ex.com/"
  ]
}
