{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "movie_iIIustration",
  "description": "为电影天堂|orzx.im列表页增加相应图片",
  "version": "0.9",
  "background": {
    "scripts": [
      "jquery-2.0.0.min.js",
      "spider.js"
    ]
  },
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "movie_iIIustration"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [],
      "js": [
        "jquery-2.0.0.min.js",
        "spider.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
