{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "homepage_url": "https://github.com/daviddahl/nuke-dialog",
  "name": "Nuke Dialog",
  "description": "This extension attempts to discover and hide all cookie / GDPR dialogs found on web pages",
  "version": "0.0.2",
  "author": "David Dahl",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "nuke.js",
        "mutation.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "css-fetch.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "32": "images/icon.png"
    },
    "default_title": "Nuke Dialog: Auto-dismiss cookie & GDPR dialogs",
    "default_popup": "ui/ui.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon.png",
    "128": "images/icon128.png"
  }
}
