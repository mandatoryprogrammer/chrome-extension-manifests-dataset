{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vertcoin Price Tracker",
  "description": "View Vertcoin price in USD, EUR, BTC, ETH, LTC, ZEC, and Satoshi.",
  "version": "1.3.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Vertcoin Price Tracker"
  },
  "background": {
    "scripts": [
      "event_page.dart.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://coincap.io/",
    "alarms"
  ]
}
