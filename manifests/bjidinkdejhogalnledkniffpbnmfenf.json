{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Time Wasted",
  "description": "This extension tracks how much time has been wasted with facebook and twitter open.",
  "version": "1.2",
  "permissions": [
    "tabs",
    "idle"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "timer.js"
    ]
  }
}
