{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Privacyst",
  "version": "1.0.0",
  "description": "A Google Chrome Privacy Extension",
  "manifest_version": 2,
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Privacyst",
    "default_popup": "popup.html",
    "default_icon": "images/icons/icon16.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}
