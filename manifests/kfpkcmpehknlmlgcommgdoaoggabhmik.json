{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Foobar",
  "description": "Making developers life easy.",
  "version": "2.0.0.0",
  "short_name": "Foobar",
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "index.html",
    "default_title": "Inject Script!"
  },
  "background": {},
  "icons": {
    "128": "images/logo.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
