{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aliaser",
  "description": "Allows users to set aliases",
  "version": "1.3.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-1.12.0.min.js",
        "js/script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
