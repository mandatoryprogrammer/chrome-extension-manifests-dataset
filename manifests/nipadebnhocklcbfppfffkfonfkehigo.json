{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "TUID Helper"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "description": "A helper extension to generate and dissect TUID.",
  "name": "TUID Helper",
  "version": "1.3",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ]
}
