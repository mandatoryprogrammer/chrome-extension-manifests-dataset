{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photoapps",
  "description": "Расширение функционала сайта mywed.ru",
  "version": "0.0.2",
  "permissions": [
    "cookies",
    "http://*.mywed.ru/",
    "http://*.photoapps.ru/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.mywed.ru/photographer/view/profile/*"
      ],
      "js": [
        "js/jquery-1.9.1.min.js",
        "js/mywed-profile.js"
      ]
    },
    {
      "matches": [
        "http://*.mywed.ru/photo/",
        "http://*.mywed.ru/photo/page/*",
        "http://*.mywed.ru/photo/popular/*",
        "http://*.mywed.ru/photo/discussed/*",
        "http://*.mywed.ru/photo/discovery/*",
        "http://*.mywed.ru/photo/edchoice/*",
        "http://*.mywed.ru/photo/favorites/*"
      ],
      "js": [
        "js/jquery-1.9.1.min.js",
        "js/mywed-photos.js"
      ]
    }
  ]
}
