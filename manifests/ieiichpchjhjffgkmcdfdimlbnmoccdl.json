{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bias Evaluation",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Displays the political bias of a webpage",
  "web_accessible_resources": [
    "arrow2.png",
    "dot.png",
    "css.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css.css"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Check Bias!"
  },
  "permissions": [
    "activeTab"
  ]
}
