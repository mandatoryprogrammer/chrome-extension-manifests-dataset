{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "description": "Push your update!.",
  "icons": {
    "128": "img/feedpusher.png",
    "16": "img/feedpusher.png"
  },
  "manifest_version": 2,
  "name": "Feed Pusher",
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
    "storage",
    "identity",
    "*://*/*",
    "webview"
  ],
  "version": "0.2.5"
}
