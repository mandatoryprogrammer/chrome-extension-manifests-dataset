{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StackOverflow & Github Responsive",
  "description": "A tool to make StackOverflow and Github resizable",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "mygithubstyle.css"
      ]
    },
    {
      "matches": [
        "http://stackoverflow.com/*"
      ],
      "css": [
        "mystackstyle.css"
      ]
    }
  ]
}
