{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Report Bad Ad",
  "description": "Report Bad Ad",
  "version": "0.1",
  "author": "M Wasfi",
  "icons": {
    "16": "images/btn.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/btn.png",
    "default_title": "Report Bad Ad",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
