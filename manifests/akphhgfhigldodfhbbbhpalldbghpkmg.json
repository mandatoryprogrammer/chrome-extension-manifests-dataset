{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "no_frills.png",
    "default_popup": "popup.html"
  },
  "name": "No Frills",
  "description": "End the atrocity that is called UI/UX",
  "short_name": "No Frills",
  "version": "0.9.0",
  "icons": {
    "16": "no_frills16.png",
    "48": "no_frills48.png",
    "128": "no_frills.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "javascripts/content.js"
      ]
    }
  ],
  "permissions": [
    "chrome://favicon/",
    "<all_urls>"
  ]
}
