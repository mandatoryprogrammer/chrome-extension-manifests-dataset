{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Konamikitten",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Make all the images kittens!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "kittens.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
