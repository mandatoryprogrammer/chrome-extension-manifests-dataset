{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SSL",
  "version": "1.0",
  "description": "SSL 4 Chrome",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "homepage_url": "https://github.com/osikov/SSL4Chrome/wiki/About",
  "options_page": "ssl_dialog.html",
  "browser_action": {
    "default_popup": "ssl_dialog.html",
    "default_icon": {
      "16": "images/ssl16.png",
      "32": "images/ssl32.png",
      "48": "images/ssl48.png",
      "128": "images/ssl128.png"
    }
  },
  "icons": {
    "16": "images/ssl16.png",
    "32": "images/ssl32.png",
    "48": "images/ssl48.png",
    "128": "images/ssl128.png"
  }
}
