{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jovem Nerd Modo Noturno",
  "version": "1.0",
  "description": "Extensão que mantém o modo noturno do site Jovem Nerd sempre ativo.",
  "permissions": [
    "webNavigation",
    "*://jovemnerd.com.br/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
