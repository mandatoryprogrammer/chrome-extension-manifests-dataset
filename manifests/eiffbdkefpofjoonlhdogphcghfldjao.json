{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Launch: Cleaner New Tab",
  "version": "0.1.0",
  "version_name": "0.1.0 Pre-release",
  "author": "Mingjie Jiang",
  "icons": {
    "16": "shared/resources/rocket-ship.png",
    "48": "shared/resources/rocket-ship.png",
    "128": "shared/resources/rocket-ship.png"
  },
  "description": "A minimalist new tab page with express urls.",
  "chrome_url_overrides": {
    "newtab": "public/index.html"
  },
  "background": {
    "scripts": [
      "helper/background.js"
    ]
  },
  "options_ui": {
    "page": "private/settings.html"
  },
  "permissions": [
    "storage"
  ]
}
