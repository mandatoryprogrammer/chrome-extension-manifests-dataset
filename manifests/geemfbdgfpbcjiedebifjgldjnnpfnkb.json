{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "foxCaves",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "foxCaves screenshot and file sharing right in your browser",
  "homepage_url": "https://foxcav.es",
  "browser_action": {
    "default_icon": "foxcaves-19.png",
    "default_title": "foxCaves for Chrome",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "https://foxcav.es/",
    "https://fox.re/",
    "<all_urls>",
    "tabs",
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "pageCapture"
  ],
  "icons": {
    "16": "foxcaves-16.png",
    "32": "foxcaves-32.png",
    "48": "foxcaves-48.png",
    "128": "foxcaves-128.png"
  },
  "web_accessible_resources": [
    "foxcaves-16.png",
    "foxcaves-19.png",
    "foxcaves-32.png",
    "foxcaves-48.png",
    "foxcaves-128.png"
  ]
}
