{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Conditions of Privacy",
  "description": "This narrows down the conditions of online services.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "The Conditions of Privacy.",
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_badge": "icon.png",
    "run_at": "document_end"
  },
  "manifest_version": 2
}
