{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bingo Helper",
  "description": "Help with Bingo",
  "version": "0.8",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://speedrunslive.com/tools/oot-bingo/*",
        "http://www.speedrunslive.com/tools/oot-bingo/*"
      ],
      "js": [
        "jquery.min.js",
        "seedrandom-min.js",
        "goal-list.js",
        "generator.js",
        "bingohelper.js"
      ]
    }
  ],
  "manifest_version": 2
}
