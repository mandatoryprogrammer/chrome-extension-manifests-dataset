{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sparknotes Cleaner",
  "version": "1.0",
  "description": "Removes distractions from Sparknotes",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://www.sparknotes.com/*",
        "https://www.sparknotes.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
