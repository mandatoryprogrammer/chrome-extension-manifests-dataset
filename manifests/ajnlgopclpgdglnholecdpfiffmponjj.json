{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Datadog Cycle",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "An extension to cycle through datadog graphs on a dashboard",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://app.datadoghq.com/dash/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://*/",
    "https://*/"
  ]
}
