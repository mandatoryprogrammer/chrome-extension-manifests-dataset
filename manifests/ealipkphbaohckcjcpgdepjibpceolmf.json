{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save RedGIFs, gfycat, Imgur GIFs as mp4",
  "description": "download all redgifs, gfycat & imgur gifs open across all tabs as mp4",
  "version": "1.9",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "name": "Click to get URL",
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "tabs",
    "downloads"
  ]
}
