{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuizCards: Spanish",
  "version": "0.99",
  "manifest_version": 2,
  "description": "Use these interactive browser-based flash cards to easily learn Spanish vocabulary.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Show the next flash card!",
    "default_icon": "browser_icon.png",
    "default_popup": "popup.html"
  }
}
