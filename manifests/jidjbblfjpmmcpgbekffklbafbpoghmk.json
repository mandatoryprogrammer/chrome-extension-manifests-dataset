{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "ru",
  "version": "0.6",
  "incognito": "split",
  "background": {
    "persistent": true,
    "scripts": [
      "core/bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "templates/tab.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
