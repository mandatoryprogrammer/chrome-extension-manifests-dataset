{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The practice session",
  "description": "Enhances thesession.org with tools to organise the tunes you're learning",
  "version": "0.0.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://thesession.org/members/*/sets"
      ],
      "run_at": "document_end",
      "css": [
        "practice-session.css"
      ],
      "js": [
        "content-scripts/shared.js",
        "content-scripts/set-list.js"
      ]
    },
    {
      "matches": [
        "https://thesession.org/members/*/sets/*"
      ],
      "exclude_matches": [
        "https://thesession.org/members/*/sets/add*"
      ],
      "run_at": "document_end",
      "css": [
        "practice-session.css"
      ],
      "js": [
        "content-scripts/shared.js",
        "content-scripts/set.js"
      ]
    },
    {
      "matches": [
        "https://thesession.org/*"
      ],
      "run_at": "document_end",
      "css": [
        "practice-session.css"
      ],
      "js": [
        "content-scripts/shared.js"
      ]
    }
  ]
}
