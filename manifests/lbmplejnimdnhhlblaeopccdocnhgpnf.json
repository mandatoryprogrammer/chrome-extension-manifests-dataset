{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DOBBS",
  "version": "1.0",
  "description": "DOBBS",
  "permissions": [
    "tabs",
    "idle",
    "contextMenus",
    "webNavigation",
    "history",
    "storage",
    "background",
    "*://*/*"
  ],
  "icons": {
    "16": "deri-16.png",
    "48": "deri-48.png",
    "128": "deri-128.png"
  },
  "background": {
    "page": "background.html",
    "run_at": "document_end"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-1.7.2.js",
        "dobbs_content.js"
      ]
    }
  ]
}
