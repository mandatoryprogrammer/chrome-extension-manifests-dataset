{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Colorized Tabs for Ultimate Guitar",
  "version": "0.0.11",
  "icons": {
    "128": "img/logo.png"
  },
  "description": "Removes all the clutter from Ultimate Guitar tabs and uses color to enhance your pattern recognition.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "init.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
