{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MySQL Console",
  "description": "This application provides you 'MySQL Console' shell windows.",
  "version": "2.2.10",
  "author": "Yoichiro Tanaka",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "offline_enabled": true,
  "sockets": {
    "tcp": {
      "connect": "*"
    }
  }
}
