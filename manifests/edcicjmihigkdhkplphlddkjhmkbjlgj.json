{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Статьи из черновиков habr.com",
  "description": "Возможность смотреть статьи, спрятанные в черновики",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": [
        "https://habr.com/*/post/*"
      ],
      "js": [
        "assets/javascripts/habra_chrome.js"
      ],
      "run_at": "document_end"
    }
  ]
}
