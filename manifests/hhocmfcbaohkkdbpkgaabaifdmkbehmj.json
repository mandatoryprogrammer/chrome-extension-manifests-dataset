{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Censual",
  "version": "5.0",
  "manifest_version": 2,
  "icons": {
    "128": "imgs/censualnowhite.png"
  },
  "description": "Útil extensión para copiar laboratorio de la plataforma Labsis del INN.",
  "author": "Luis Ruelas",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_title": "Copia tus laboratorios!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "tabs"
  ]
}
