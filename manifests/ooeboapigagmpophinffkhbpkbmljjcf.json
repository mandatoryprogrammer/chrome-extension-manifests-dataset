{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Last Video I Swear",
  "version": "1.1.0",
  "description": "Automatically close the current YouTube tab when your video ends.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/lvis_16.png",
      "32": "images/lvis_32.png",
      "48": "images/lvis_48.png",
      "128": "images/lvis_128.png"
    }
  },
  "icons": {
    "16": "images/lvis_16.png",
    "32": "images/lvis_32.png",
    "48": "images/lvis_48.png",
    "128": "images/lvis_128.png"
  },
  "manifest_version": 2
}
