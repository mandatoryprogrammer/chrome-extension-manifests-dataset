{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ungender Deutsch",
  "version": "1.1",
  "description": "Vereinfache gegenderte deutsche Sätze, entscheide dich für ein Geschlecht",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "manifest_version": 2
}
