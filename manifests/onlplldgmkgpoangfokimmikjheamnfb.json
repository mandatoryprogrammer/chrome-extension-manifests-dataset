{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Docomplete",
  "description": "This extension re-enables autocomplete for password fields on websites that intentionally disable it (autocomplete=off).",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "docomplete.js"
      ],
      "all_frames": true
    }
  ]
}
