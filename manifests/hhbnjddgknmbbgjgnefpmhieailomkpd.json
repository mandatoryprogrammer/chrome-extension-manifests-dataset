{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "User agent randomizer",
  "version": "1.0",
  "description": "Randomizes the user agent.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "user_agents.js"
    ]
  },
  "manifest_version": 2
}
