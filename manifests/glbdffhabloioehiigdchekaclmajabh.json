{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dragon Go Server notifier",
  "version": "0.4.1",
  "description": "Notifier for Dragon Go server",
  "background": {
    "scripts": [
      "find_tab.js",
      "state.js",
      "checker.js",
      "background_task.js"
    ]
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "games_list.html"
  },
  "permissions": [
    "http://www.dragongoserver.net/",
    "tabs"
  ],
  "icons": {
    "128": "128.png"
  }
}
