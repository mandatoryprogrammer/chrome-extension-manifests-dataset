{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Дополнение для Народной карты",
  "version": "1.3.9",
  "manifest_version": 2,
  "description": "Дополнение для удаления ненужных нотификаций.",
  "icons": {
    "16": "nmaps16.png",
    "48": "nmaps48.png",
    "128": "nmaps128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "nmaps.ico"
  },
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "tabs",
    "webNavigation",
    "storage",
    "alarms",
    "idle",
    "notifications",
    "contextMenus",
    "*://*.yandex.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://n.maps.yandex.ru/users/*",
        "*://n.maps.yandex.ru/notifications/*",
        "*://n.maps.yandex.ru/modconsole/events-region/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}
