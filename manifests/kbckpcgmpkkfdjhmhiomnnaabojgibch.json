{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Spectacles",
  "description": "Remove Quora's answer obfuscation",
  "version": "0.1.2",
  "minimum_chrome_version": "20.0.0",
  "icons": {
    "48": "img/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.quora.com/*",
        "https://*.quora.com/*"
      ],
      "js": [
        "lib/jquery.js",
        "spectacles.js"
      ]
    }
  ],
  "manifest_version": 2
}
