{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "偷偷下",
  "version": "1.0.4",
  "description": "支持正版，偷偷下载",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content_style.css"
      ],
      "js": [
        "jquery.min.js",
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "music.png",
    "music.png",
    "music.png",
    "music.png",
    "music.png",
    "music.png"
  ],
  "icons": {
    "16": "music.png",
    "48": "music.png",
    "128": "music.png"
  }
}
