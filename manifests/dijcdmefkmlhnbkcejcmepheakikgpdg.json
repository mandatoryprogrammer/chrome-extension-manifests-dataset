{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "short_name": "__MSG_name__",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "24": "icons/icon_24.png",
      "32": "icons/icon_32.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_searchName__",
      "keyword": "__MSG_searchKeyword__",
      "search_url": "https://sf-addon.com/wondertab/search/__MSG_searchGeo__/{searchTerms}",
      "favicon_url": "https://sf-addon.com/wondertab/img/__MSG_searchFaviconFile__",
      "suggest_url": "https://sf-addon.com/wondertab/suggest/__MSG_searchGeo__/{searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "manifest_version": 2,
  "minimum_chrome_version": "48",
  "permissions": [
    "tabs",
    "history",
    "unlimitedStorage",
    "chrome://favicon/",
    "contextMenus",
    "storage",
    "topSites",
    "activeTab",
    "management",
    "<all_urls>"
  ],
  "version": "1.3.0"
}
