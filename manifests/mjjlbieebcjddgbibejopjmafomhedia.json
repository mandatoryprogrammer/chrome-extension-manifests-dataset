{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JumpToTheMiddle",
  "description": "Jump to around the middle tab in a chrome window",
  "version": "0.1",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "shortcut": {
      "suggested_key": {
        "default": "Ctrl+8"
      },
      "description": "jump to the middle"
    }
  }
}
