{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Doggieradicator",
  "description": "Substitua os comentários do G1 em fotos aleatórias de cachorros",
  "version": "1.1.3",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*.g1.globo.com/*"
  ],
  "browser_action": {
    "default_icon": "32.png"
  },
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "64": "64.png"
  }
}
