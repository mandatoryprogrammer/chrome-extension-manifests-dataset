{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redirectr",
  "description": "Easily set up site redirects so you can see the sites you want to visit!",
  "version": "1.0.2",
  "author": "Matt Walters",
  "icons": {
    "16": "images/redirectr_logo16.png",
    "48": "images/redirectr_logo48.png",
    "128": "images/redirectr_logo128.png"
  },
  "background": {
    "scripts": [
      "js/redirect.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/redirectr_logo128.png",
    "default_popup": "popup.html",
    "default_title": "Redirectr"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://*.bootstrapcdn.com https://*.googleapis.com ; object-src 'self'"
}
