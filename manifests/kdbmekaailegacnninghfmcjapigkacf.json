{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Minimum Score",
  "version": "0.64",
  "content_scripts": [
    {
      "matches": [
        "https://*.reddit.com/r/*",
        "https://*.reddit.com/me/m/*"
      ],
      "exclude_matches": [
        "https://*.reddit.com/r/*/comments/*"
      ],
      "js": [
        "minimumscore.js"
      ]
    }
  ]
}
