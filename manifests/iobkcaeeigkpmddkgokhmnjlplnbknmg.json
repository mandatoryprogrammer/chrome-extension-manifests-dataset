{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": "FDR Amazon Donator"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://www.amazon.com/*",
        "https://www.amazon.com/*",
        "http://www.amazon.ca/*",
        "https://www.amazon.ca/*",
        "http://www.amazon.co.uk/*",
        "https://www.amazon.co.uk/*",
        "http://www.amazon.de/*",
        "https://www.amazon.de/*",
        "http://www.amazon.es/*",
        "https://www.amazon.es/*",
        "http://www.amazon.fr/*",
        "https://www.amazon.fr/*",
        "http://www.amazon.it/*",
        "https://www.amazon.it/*",
        "http://www.amazon.co.jp/*",
        "https://www.amazon.co.jp/*",
        "http://www.amazon.cn/*",
        "https://www.amazon.cn/*"
      ]
    }
  ],
  "description": "Automatically applies FreedomainRadio's Amazon Affiliate Tracking Id to Amazon webpages you visit.",
  "homepage_url": "http://www.freedomainradio.com",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "FDR Amazon Donator",
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://www.amazon.com/*",
    "https://www.amazon.com/*",
    "http://www.amazon.ca/*",
    "https://www.amazon.ca/*",
    "http://www.amazon.co.uk/*",
    "https://www.amazon.co.uk/*",
    "http://www.amazon.de/*",
    "https://www.amazon.de/*",
    "http://www.amazon.es/*",
    "https://www.amazon.es/*",
    "http://www.amazon.fr/*",
    "https://www.amazon.fr/*",
    "http://www.amazon.it/*",
    "https://www.amazon.it/*",
    "http://www.amazon.co.jp/*",
    "https://www.amazon.co.jp/*",
    "http://www.amazon.cn/*",
    "https://www.amazon.cn/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1"
}
