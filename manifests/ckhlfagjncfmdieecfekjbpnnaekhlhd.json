{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No MouseWheel Zoom",
  "description": "This extension disables the annoying zoom shortcut Ctrl + Mousewheel on Chrome.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "NoMouseWheelZoom.min.js"
  ]
}
