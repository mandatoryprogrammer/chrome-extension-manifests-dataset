{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Daylight Clock",
  "description": "A location based daylight clock.",
  "version": "1.8",
  "chrome_url_overrides": {
    "newtab": "daylight.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "clock.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "geolocation",
    "contextMenus",
    "storage"
  ]
}
