{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Moonlet Wallet Legacy",
  "version": "0.38.0",
  "description": "Moonlet Wallet Legacy Extension",
  "permissions": [
    "storage",
    "identity",
    "https://api.moonlet.xyz/*"
  ],
  "optional_permissions": [
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "bundle.background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://cryptolandtech.github.io/dapp-wallet-util/*"
      ],
      "js": [
        "bundle.cs.permission-manager.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "bundle.inject.dapp.js"
  ],
  "icons": {
    "16": "assets/icon-16px.png",
    "48": "assets/icon-48px.png",
    "128": "assets/icon-128px.png"
  },
  "browser_action": {
    "default_popup": "index.html?popup=1"
  },
  "manifest_version": 2,
  "key": ""
}
