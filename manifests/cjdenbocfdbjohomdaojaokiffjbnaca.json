{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks Organizer",
  "version": "2.0.9",
  "manifest_version": 2,
  "author": "kadaj",
  "description": "Organize Bookmarks",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "js/bookmark.js",
      "js/events.js",
      "js/sortManager.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Bookmarks Organizer",
    "default_icon": "res/icons/bo-16.png",
    "default_popup": "bookmarks-organizer.html"
  },
  "icons": {
    "16": "res/icons/bo-16.png",
    "48": "res/icons/bo-48.png",
    "128": "res/icons/bo-128.png"
  },
  "offline_enabled": true
}
