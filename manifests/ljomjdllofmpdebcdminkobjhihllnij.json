{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Anonymizr",
  "description": "Remove personal information from social networks and other websites before sharing a screenshot.",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "images/guy-fawkes.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
