{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Julius Layer",
  "description": "Julius Layer is an app for chrome that puts one image layer on top the of your HTML at its actual size.",
  "version": "0.3",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Put URL image",
    "default_icon": "julius-16.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/bg-popup.png"
  ],
  "icons": {
    "16": "julius-16.png",
    "128": "julius-128.png"
  }
}
