{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Habrahabr flame hider",
  "version": "0.0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://habrahabr.ru/*",
        "https://habrahabr.ru/*",
        "http://geektimes.ru/*",
        "https://geektimes.ru/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://habrahabr.ru/",
    "https://habrahabr.ru/",
    "http://geektimes.ru/",
    "https://geektimes.ru/"
  ]
}
