{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "English Learner's Dictionary",
  "author": "Saeed Moghadamzadeh",
  "version": "1.7",
  "description": "English Learner's Dictionary helps you to lookup the word definitions, synonyms, examples, pronunciation and etc.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "data/script.js"
      ],
      "css": [
        "style/styles.css",
        "style/themes.css",
        "style/fontello.css"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "*://*.smoqadam.me/",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "icons": {
    "16": "icons/dictionary-16.png",
    "32": "icons/dictionary-32.png"
  },
  "web_accessible_resources": [
    "icons/*",
    "style/*",
    "template.html",
    "settings.html"
  ],
  "options_ui": {
    "page": "settings.html"
  }
}
