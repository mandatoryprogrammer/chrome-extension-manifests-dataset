{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NBI ATS",
  "version": "0.23",
  "description": "Finds link and email address in the web page",
  "icons": {
    "128": "jecho.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "*://*.liepin.com/*",
    "*://*.maimai.cn/*",
    "*://*.linkedin.com/*",
    "*://*.google.com/*",
    "*://*.gmail.com/*",
    "*://*.twitter.com/*",
    "*://*.github.com/*",
    "*://*.behance.net/*",
    "*://*.cakeresume.com/*"
  ],
  "page_action": {
    "default_name": "NBI ATS",
    "default_icon": "jecho.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "jecho.png"
  ],
  "manifest_version": 2
}
