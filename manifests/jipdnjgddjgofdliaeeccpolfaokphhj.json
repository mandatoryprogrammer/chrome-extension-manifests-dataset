{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "fightcade-links",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Converts fightcade challenge IDs to clickable links",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_globs": [
        "*://fightcade.com/*",
        "*://*.fightcade.com/*"
      ],
      "js": [
        "jquery.min.js",
        "jquery.ba-replacetext.js",
        "replace.js"
      ]
    }
  ]
}
