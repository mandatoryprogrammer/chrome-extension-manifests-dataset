{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SaveForLater",
  "version": "1.0.0",
  "description": "Save your webpages to read later",
  "permissions": [
    "declarativeContent",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "src/popup.html"
  },
  "icons": {
    "16": "x16.png",
    "48": "x48.png",
    "128": "x128.png"
  },
  "manifest_version": 2
}
