{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "To Caret, From Github",
  "short_name": "GH2Caret",
  "description": "A demo plugin that sends Github source to a new Caret tab.",
  "version": "1.0.3",
  "permissions": [
    "https://*.github.com/",
    "tabs"
  ],
  "page_action": {
    "default_icon": "icon-128.png",
    "default_title": "Send file to Caret"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
