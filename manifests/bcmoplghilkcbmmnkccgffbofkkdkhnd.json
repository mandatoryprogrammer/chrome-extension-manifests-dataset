{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit saved posts organizer",
  "version": "1.1.17",
  "manifest_version": 2,
  "minimum_chrome_version": "46",
  "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='",
  "description": "Lets you display and organize saved posts from your reddit account.",
  "icons": {
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_title": "Reddit saved posts organizer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.reddit.com/*",
    "storage"
  ],
  "background": {
    "page": "background.html"
  }
}
