{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wachete - Monitor website content changes",
  "short_name": "Wachete",
  "description": "This extension will monitor web or part of web for changes",
  "version": "1.3",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "wachete.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
