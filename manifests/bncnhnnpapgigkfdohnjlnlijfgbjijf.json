{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Quick Marker",
  "version": "1.1",
  "description": "Tune YouTube recommendations easily! Right click thumbnails to pull up the feedback dialog; use 1-2 on your keyboard to select & go!",
  "manifest_version": 2,
  "permissions": [
    "https://www.youtube.com/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "vendor/jquery-3.min.js",
        "index.js"
      ],
      "css": []
    }
  ]
}
