{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shortcuts For Googling",
  "description": "Do googling like a boss",
  "version": "1.0.4",
  "author": "makerj (ohenwkgdj@gmail.com)",
  "permissions": [
    "*://www.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://www.google.co.kr/*"
      ],
      "js": [
        "js.js"
      ],
      "css": [
        "css.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
