{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jira Estimate Sum",
  "version": "1.1.1",
  "description": "Displays estimate sum for total sprint and each column.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.atlassian.net/secure/RapidBoard.jspa*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "sumEstimates.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "https://*.atlassian.net/secure/RapidBoard.jspa*"
  ]
}
