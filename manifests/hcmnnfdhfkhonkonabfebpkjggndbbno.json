{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Middlebury Menu",
  "version": "0.1.3",
  "description": "Makes the Middlebury Menu easier to use.",
  "author": "Dana Silver",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://menus.middlebury.edu/*"
      ],
      "js": [
        "vendor/jquery-2.0.3.min.js",
        "vendor/moment.min.js",
        "content_script.js"
      ]
    }
  ]
}
