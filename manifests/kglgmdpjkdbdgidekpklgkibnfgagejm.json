{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "Todoist key nav",
  "version": "0.0.4",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icon_128.png",
    "256": "icon_256.png",
    "48": "icon_48.png"
  },
  "author": "uechoco",
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "https://todoist.com/app*",
        "https://*.todoist.com/app*"
      ],
      "run_at": "document_start"
    }
  ]
}
