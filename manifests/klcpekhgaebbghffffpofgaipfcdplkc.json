{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CC Generator",
  "description": "Generates valid credit card numbers for development use.",
  "version": "1.3.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "CC Generator"
  },
  "background": {
    "scripts": [
      "eventPage.js",
      "ccFunctions.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardWrite"
  ]
}
