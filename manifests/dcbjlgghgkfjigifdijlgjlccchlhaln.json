{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Website Highlighter",
  "short_name": "WebHighlight",
  "version": "1.0.0",
  "description": "Highlights different phrases on different websites",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "jquery.highlight-3.js",
        "selection.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
