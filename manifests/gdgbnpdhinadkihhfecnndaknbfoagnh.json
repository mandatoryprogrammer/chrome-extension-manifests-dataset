{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Greed",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Display a grid on any web page. You can configure grid size, shifting and opacity.",
  "icons": {
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Greed",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
