{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "书签导出",
  "description": "支持书签自定义导出",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "书签管理!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "bookmarks",
    "chrome://favicon/"
  ]
}
