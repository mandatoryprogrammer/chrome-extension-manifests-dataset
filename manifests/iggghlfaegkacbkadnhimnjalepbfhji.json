{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GO for Chrome",
  "description": "To use, type 'go' into the omnibox. To add a short link, click the browerAction button",
  "manifest_version": 2,
  "version": "0.9.1",
  "omnibox": {
    "keyword": "go"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default title": "GO"
  },
  "permissions": [
    "storage"
  ]
}
