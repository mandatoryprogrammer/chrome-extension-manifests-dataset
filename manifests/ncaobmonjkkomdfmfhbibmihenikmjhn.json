{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search and Buttonfy Blogs UNLEASH",
  "version": "1.1",
  "description": "Use SaBB unleash. Buttofyier is now free and an un-restricted",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "chstl.js",
        "buttonsurffjs2.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/surf16.png",
      "24": "images/surf24.png",
      "32": "images/surf32.png",
      "48": "images/surf48.png"
    },
    "default_title": "Search and Buttonfy Blogs",
    "default_popup": "pop.html"
  },
  "icons": {
    "16": "images/surf16.png",
    "24": "images/surf24.png",
    "32": "images/surf32.png",
    "48": "images/surf48.png",
    "128": "images/surf128.png"
  }
}
