{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "MarqueeMe.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "onPage.js"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "MarqueeMe",
  "description": "Why did you install this",
  "version": "2.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "real-world.css"
  ],
  "browser_action": {
    "default_title": "Why did you install this"
  },
  "options_page": "options.html"
}
