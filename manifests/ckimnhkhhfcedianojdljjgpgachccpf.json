{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Domainr",
  "version": "4.0.1",
  "manifest_version": 2,
  "description": "Worldwide domain search.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "domainr.html",
    "default_title": "Domainr"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://domainr.com/*",
    "https://domainr.com/*",
    "tabs"
  ]
}
