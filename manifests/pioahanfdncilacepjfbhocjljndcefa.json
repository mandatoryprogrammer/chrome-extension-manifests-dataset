{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Url Replacer",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Replace the URL to another depending on the  configurations",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html",
    "default_title": "URL replacer configuration"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "webRequest",
    "storage",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html"
}
