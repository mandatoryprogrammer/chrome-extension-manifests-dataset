{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Share Quickly",
  "short_name": "Share Quickly",
  "minimum_chrome_version": "35",
  "version": "1.16",
  "description": "Quickly take screenshots of the active tab and publish it online.",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Take a screen shot!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
