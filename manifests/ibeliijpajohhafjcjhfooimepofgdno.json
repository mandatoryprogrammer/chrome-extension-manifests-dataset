{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Muter",
  "version": "1.0.1",
  "description": "Mute Your Tabs",
  "default_locale": "en_US",
  "icons": {
    "16": "speaker-16.png",
    "48": "no-audio-48.png",
    "128": "no-audio-128.png"
  },
  "browser_action": {
    "default_title": "Click to mute tabs"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
