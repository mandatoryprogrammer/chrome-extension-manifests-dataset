{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "This is Not (a) Normal Todo",
  "version": "1.2",
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "images/raised-fist.png",
      "48": "images/raised-fist.png",
      "96": "images/raised-fist.png",
      "128": "images/raised-fist-128.png"
    },
    "default_title": "Not a Normal Todo",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "alarms",
    "http://*/*",
    "https://*/*"
  ]
}
