{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RedditMinimal",
  "description": "Cleans up and minimizes Reddit.com.",
  "version": "1.3.1",
  "web_accessible_resources": [
    "img/36px-blended.png",
    "img/down-arrow.png",
    "img/overlay-button.png",
    "img/reddit-logo.gif",
    "img/search.png",
    "script.js",
    "style.css"
  ],
  "icons": {
    "128": "Icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "exclude_matches": [
        "*://code.reddit.com/*",
        "*://blog.reddit.com/*",
        "*://www.reddit.com/r/apple*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://*.reddit.com/*"
  ]
}
