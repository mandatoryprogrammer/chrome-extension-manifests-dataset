{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Talker Styles",
  "version": "0.1.1",
  "description": "Simplify (even more) the Talker rooms styles",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.talkerapp.com/rooms/*"
      ],
      "css": [
        "css/talker-styles.css"
      ],
      "js": [
        "js/body-style.js"
      ]
    }
  ],
  "permissions": [
    "*://*.talkerapp.com/rooms/*"
  ]
}
