{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitHub Issue Filter Saver",
  "description": "Automatically preserves filters when navigating GitHub issues.",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.github.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
