{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Force To Horse",
  "description": "Based on XKCD comic 1418, this extension changes all instances of the word 'Force' to 'Horse'.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "horse.js"
      ],
      "run_at": "document_end"
    }
  ]
}
