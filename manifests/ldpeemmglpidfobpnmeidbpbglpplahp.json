{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "github-light-header",
  "description": "Make GitHub header light again",
  "version": "0.3",
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://www.github.com/*",
    "http://github.com/*",
    "https://www.github.com/*",
    "https://github.com/*",
    "http://gist.github.com/*",
    "https://gist.github.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://github.com/*",
        "https://github.com/*",
        "http://www.github.com/*",
        "https://www.github.com/*",
        "http://gist.github.com/*",
        "https://gist.github.com/*"
      ],
      "css": [
        "light.css"
      ],
      "run_at": "document_start"
    }
  ]
}
