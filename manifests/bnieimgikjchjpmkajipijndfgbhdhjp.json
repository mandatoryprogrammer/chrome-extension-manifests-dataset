{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome YouTube Watched Videos Remover",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Removes watched videos from the YouTube subscriptions page",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://www.youtube.com/feed/subscriptions"
  ]
}
