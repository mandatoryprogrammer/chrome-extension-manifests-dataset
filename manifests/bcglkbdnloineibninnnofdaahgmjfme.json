{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "緊急地震速報 by Extension β",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "設定",
    "default_icon": "icon16.png",
    "default_popup": "options.html"
  },
  "description": "twitterを利用した緊急地震速報です。",
  "icons": {
    "128": "icon.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "notifications",
    "geolocation"
  ],
  "version": "0.7.15"
}
