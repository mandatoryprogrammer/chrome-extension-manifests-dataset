{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Focus",
  "version": "1.2.2",
  "description": "Get your work done by blocking any distracting websites until you've completed your Focus list! Referenced IBM Gawkblocker.",
  "short_name": "ng-Focus",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "permissions": false
  },
  "browser_action": {
    "default_icon": "images/Focus.png",
    "default_title": "Focus",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/Focus16.png",
    "48": "images/Focus48.png",
    "128": "images/Focus128.png"
  },
  "permissions": [
    "tabs",
    "debugger",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
