{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark++",
  "description": "Makes bookmark searching, replacing, and deleting easier for the user.",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "background"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bookmark++.html"
  }
}
