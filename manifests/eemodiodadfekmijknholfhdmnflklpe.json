{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "synaesthesia",
  "description": "A chrome extension that changes the default colours of the GAE admin console and the google cloud console based on app ID.",
  "version": "1.0.0",
  "options_page": "./html/options.html",
  "background": {
    "scripts": [
      "./js/synaesthete.js"
    ],
    "run_at": "document_start"
  },
  "page_action": {
    "default_icon": {
      "19": "./images/page_icon.png"
    }
  },
  "permissions": [
    "background",
    "tabs",
    "https://appengine.google.com/*",
    "https://console.developers.google.com/project/repcore-dev"
  ]
}
