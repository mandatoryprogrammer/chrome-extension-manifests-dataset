{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "keyboarClicker",
  "version": "1.1",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/script.js"
      ]
    }
  ],
  "offline_enabled": true,
  "sandbox": {
    "pages": [
      "index.html"
    ]
  },
  "icons": {
    "16": "icon32.png",
    "48": "icon100.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "description": ""
  },
  "manifest_version": 2
}
