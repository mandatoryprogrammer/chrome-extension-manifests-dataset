{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Prosta",
  "version": "0.2",
  "description": "toolbox for page content editions by Oren Aksakal (orenaksakal@gmail.com).",
  "browser_action": {
    "default_title": "Prosta",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
