{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "smarTab Homepage",
  "short_name": "smarTab",
  "version": "0.0.0.7",
  "description": "home page extension",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_title": "smarTab"
  },
  "chrome_url_overrides": {
    "newtab": "html/home.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/*",
    "*://*.bing.com/*",
    "*://*.zimoapps.com/*",
    "*://www.google.com/s2/*"
  ]
}
