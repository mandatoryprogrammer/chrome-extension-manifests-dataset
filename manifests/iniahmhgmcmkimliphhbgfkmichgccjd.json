{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unstyler",
  "version": "1.0.1",
  "description": "Toggle website styling.",
  "homepage_url": "https://miguelpiedrafita.com",
  "author": "Miguel Piedrafita",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Unstyler"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "assets/logo.png"
  }
}
