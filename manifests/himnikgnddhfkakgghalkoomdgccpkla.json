{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Key Hole",
  "description": "This extension reinforces your password security by helping you remember it really, really well.",
  "version": "1.0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "main.js"
      ]
    }
  ]
}
