{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PokeTranslate",
  "description": "Translates Pokemon-related Japanese words to English",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "PokeTranslate"
  },
  "icons": {
    "16": "img/pokeball.png"
  },
  "manifest_version": 2
}
