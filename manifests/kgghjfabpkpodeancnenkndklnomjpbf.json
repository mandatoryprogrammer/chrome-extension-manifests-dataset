{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disable links.",
  "description": "Hold Ctrl+Alt to disable the links temporarily.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "keyboardhook.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
