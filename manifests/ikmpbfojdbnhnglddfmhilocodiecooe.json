{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Ecourse Fix",
  "version": "2.0",
  "manifest_version": 2,
  "description": "修正中正大學課程教學系統在Google Chorme上的相容性問題",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://ecourse.elearning.ccu.edu.tw/php/bar.php",
        "http://ecourse.elearning.ccu.edu.tw/php/textbook/course_menu.php*",
        "http://ecourse.elearning.ccu.edu.tw/php/login_s.php*",
        "http://ecourse.webedu.ccu.edu.tw/php/bar.php",
        "http://ecourse.webedu.ccu.edu.tw/php/textbook/course_menu.php*",
        "http://ecourse.webedu.ccu.edu.tw/php/login_s.php*",
        "http://cyberccu.elearning.ccu.edu.tw/php/bar.php",
        "http://cyberccu.elearning.ccu.edu.tw/php/textbook/course_menu.php*",
        "http://cyberccu.elearning.ccu.edu.tw/php/login_s.php*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
