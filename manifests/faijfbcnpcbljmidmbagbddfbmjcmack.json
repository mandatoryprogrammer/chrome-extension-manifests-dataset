{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Two-dimensional code",
  "version": "1.0",
  "description": "Generate a two-dimensional code, to facilitate the transfer of information to mobile devices",
  "icons": {
    "16": "images/icon-16.png",
    "64": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-128.png",
    "default_popup": "htm/popup.htm"
  },
  "web_accessible_resources": [
    "js/popup.js",
    "js/index.js",
    "images/icon-16.png",
    "images/icon-48.png",
    "images/icon-128.png",
    "style/index.css",
    "style/popup.css"
  ],
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ]
}
