{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fantia Auto Renamer",
  "version": "0.0.3",
  "description": "Fantia Auto Renamer",
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "webRequest",
    "*://fantia.jp/*",
    "*://fantia.jp/posts/*/download/*",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://fantia.jp/*"
      ],
      "js": [
        "content_scripts/down.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/options.html"
  }
}
