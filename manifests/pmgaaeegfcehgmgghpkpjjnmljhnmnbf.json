{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "no tabloids",
  "description": "remove facebook's 'trending news' section",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://www.facebook.com/*"
      ]
    }
  ],
  "manifest_version": 2
}
