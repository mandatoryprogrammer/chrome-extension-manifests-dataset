{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Card this to Trello",
  "description": "A Chrome extension wrapper for Trello-Bookmarklet",
  "version": "0.2.6",
  "author": "@hewigovens",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "background": {
    "page": "bg.html"
  },
  "web_accessible_resources": [
    "img/trello-48.png"
  ],
  "icons": {
    "48": "img/trello-48.png",
    "128": "img/trello.png"
  },
  "content_security_policy": "script-src 'self' https://trello.com; object-src 'self'"
}
