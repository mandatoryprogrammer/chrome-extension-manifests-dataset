{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "XKCD Explainer",
  "version": "1.0",
  "description": "Adds a button to every XKCD webcomic that links to the relevant page in explainxkcd.",
  "icons": {
    "48": "icons/xkcd.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.xkcd.com/*"
      ],
      "js": [
        "explain_xkcd.js"
      ]
    }
  ]
}
