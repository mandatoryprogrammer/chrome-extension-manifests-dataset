{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Analyzer ",
  "version": "2.0",
  "description": "Play active tab video & pause background tab videos",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "./source/jquery.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "./source/jquery.min.js",
        "front_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Stop Youtube Video 2",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
