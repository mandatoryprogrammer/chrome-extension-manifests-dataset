{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "movie_reserve_plugin",
  "description": "if opened web page is about a movie, this extension directly open reservation page of that movie!",
  "version": "1.3.0",
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/vue 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "popcorn.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
