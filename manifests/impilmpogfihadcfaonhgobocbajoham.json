{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BoroBlock",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Removes Borowitz Articles from The New Yorker Most Popular",
  "content_scripts": [
    {
      "js": [
        "jquery-3.1.1.min.js",
        "hide.js"
      ],
      "matches": [
        "http://*.newyorker.com/*",
        "https://*.newyorker.com/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
