{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Right Click to TTS",
  "description": "Speak highlighted text. Text to speach",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "TextToSpeach"
  },
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}
