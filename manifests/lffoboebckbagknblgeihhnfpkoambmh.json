{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Controle de Senhas",
  "description": "Desenvolvido para uso interno do OGMO Santos por Welison da Silva - Container Solution",
  "version": "1.2.1",
  "author": "Welison da Silva",
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "Controle de Senhas",
    "default_popup": "main.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "sessions",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://www.ogmo-santos.com.br/*",
    "https://ajax.googleapis.com/"
  ],
  "icons": {
    "16": "16.png"
  }
}
