{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Download Shelf Disabler for Mac",
  "description": "Turns off Chrome's downloads shelf on Mac computers.",
  "version": "1.2",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "downloads",
    "downloads.shelf"
  ]
}
