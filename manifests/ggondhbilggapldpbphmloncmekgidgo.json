{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "InstaDic",
  "version": "1.0",
  "description": "Speak out and Search the meaning of the selected word",
  "icons": {
    "128": "icon128.jpg",
    "48": "icon48.jpg",
    "16": "icon16.jpg"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
