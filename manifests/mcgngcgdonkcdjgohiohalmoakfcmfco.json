{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GoalDash",
  "description": "Set your Daily, Weekly, and Yearly Goals",
  "version": "1.0.4",
  "author": "GoalDashApp",
  "short_name": "GoalDash - A dashboard for goals",
  "browser_action": {
    "default_icon": "gdicon-32.png",
    "default_title": "Crush your Goals"
  },
  "permissions": [
    "activeTab",
    "storage",
    "geolocation",
    "tabs"
  ],
  "icons": {
    "16": "gdicon-16.png",
    "32": "gdicon-32.png",
    "48": "gdicon-48.png",
    "128": "gdicon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
