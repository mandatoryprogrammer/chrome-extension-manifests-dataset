{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CourseOff Social",
  "description": "To view friends who are taking/have taken the same class",
  "version": "1.0.7",
  "author": "Jialiang (Jason) Zhang, Yiqi (Victor) Chen, Haoli (Henry) Du",
  "short_name": "CS",
  "content_scripts": [
    {
      "matches": [
        "https://*.courseoff.com/workspace*"
      ],
      "css": [
        "app.css"
      ],
      "js": [
        "jquery-1.11.2.min.js",
        "dataparser.js",
        "friendservice.js",
        "app.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "img/cs38.png"
  },
  "permissions": [
    "tabs",
    "https://*.courseoff.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
