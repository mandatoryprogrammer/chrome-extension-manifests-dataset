{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nashey Cougar",
  "version": "1.3",
  "description": "Replaces the text 'Nicolas Cage' with 'Nashey Cougar'.",
  "browser_action": {
    "default_icon": "nc.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
