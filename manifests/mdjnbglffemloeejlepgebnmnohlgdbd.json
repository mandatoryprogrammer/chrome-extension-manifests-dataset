{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CheckVist Encryption",
  "version": "1.7",
  "manifest_version": 2,
  "icons": {
    "16": "/icon16.png",
    "48": "/icon48.png",
    "128": "/icon128.png"
  },
  "description": "Store encrypted todo list items at checkvist.com.",
  "content_scripts": [
    {
      "matches": [
        "https://checkvist.com/checklists/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "sjcl.js",
        "main.js"
      ]
    }
  ]
}
