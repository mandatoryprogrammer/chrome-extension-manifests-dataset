{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dyn Admin Utils",
  "description": "This utility is designed in order to improve a dyn admin usability.",
  "version": "1.2.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*/dyn/admin/*"
      ],
      "js": [
        "jquery-1.12.0.min.js",
        "utils.js",
        "common.js",
        "dynadmin_console.js"
      ]
    },
    {
      "matches": [
        "*://*/dyn/admin*/PipelineManager/"
      ],
      "js": [
        "jquery-1.12.0.min.js",
        "utils.js",
        "common.js",
        "pipeline_manager_addition.js"
      ]
    },
    {
      "matches": [
        "*://*/dyn/admin*/PipelineManager/?propertyName=definitionFile"
      ],
      "js": [
        "jquery-1.12.0.min.js",
        "utils.js",
        "common.js",
        "pretty_pipeline.js"
      ]
    }
  ]
}
