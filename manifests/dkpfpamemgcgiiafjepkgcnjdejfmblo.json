{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LATOCY",
  "version": "2.1",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": [
    "http://%2A/",
    "https://*/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {},
    "default_title": "LATOCY",
    "js": "popup/popup.js",
    "default_popup": "popup/popup.html",
    "content_security_policy": "script-src 'self' https://apis.google.com%3B/ object-src 'self'"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "windows": "Ctrl+Q",
        "mac": "Command+Shift+E",
        "linux": "Ctrl+Q"
      },
      "description": "Open Extensions Tab"
    }
  }
}
