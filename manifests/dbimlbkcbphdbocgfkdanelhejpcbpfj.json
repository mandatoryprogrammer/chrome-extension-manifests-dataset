{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Auto Scroll",
  "description": "Automatically scroll a web page",
  "version": "1.0",
  "author": "Michael Standen",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "64": "img/icon64.png",
      "128": "img/icon128.png"
    },
    "default_title": "Auto Scroll (OFF)"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_page": "options.html"
}
