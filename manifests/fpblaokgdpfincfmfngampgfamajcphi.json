{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Last Tweet Date",
  "description": "This extension shows last tweet dates at following list",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-2.1.0.js",
        "lasttweet.js"
      ]
    }
  ]
}
