{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JD_ShareOrder_Hulk!",
  "description": "One-click get 10 page about current browsing goods base on jd.com.",
  "version": "1.7",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "popup.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "http://kejt.cn/",
    "tabs"
  ]
}
