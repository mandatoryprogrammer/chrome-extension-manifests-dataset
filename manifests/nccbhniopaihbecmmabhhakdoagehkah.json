{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook Unsee",
  "version": "0.3",
  "description": "Stops people you talk to from knowing that you've seen their messages on Facebook chat.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
