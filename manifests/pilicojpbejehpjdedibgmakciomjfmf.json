{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Permafrost",
  "version": "2016.12.1",
  "description": "A local ajax.googleapis.com mirror",
  "author": "Harmen",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "https://github.com/alicebob/permafrost",
  "minimum_chrome_version": "22.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://ajax.googleapis.com/ajax/libs/*",
    "https://ajax.googleapis.com/ajax/libs/*"
  ],
  "web_accessible_resources": [
    "pool/*"
  ]
}
