{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ConnectorDB Chrome Logger",
  "short_name": "connectordb",
  "description": "Logs the websites you visit to ConnectorDB",
  "version": "0.0.6",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "connectordb.min.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "img/128.png"
  },
  "browser_action": {
    "default_title": "ConnectorDB",
    "default_icon": "img/128.png"
  },
  "offline_enabled": true,
  "options_page": "options.html"
}
