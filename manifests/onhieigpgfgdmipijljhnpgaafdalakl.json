{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Password Generator",
  "description": "Generate a password in one click using Javascript.",
  "version": "1.1",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "pwg_small.png",
    "default_popup": "popup.html",
    "default_title": "Generate a password."
  },
  "icons": {
    "16": "pwg_small.png",
    "48": "pwg_medium.png",
    "128": "pwg_large.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "clipboardWrite"
  ]
}
