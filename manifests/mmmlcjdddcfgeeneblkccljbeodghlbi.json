{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pokus",
  "description": "Website blocking extension",
  "version": "2.1.4",
  "author": "Sinokaba",
  "icons": {
    "16": "img/pokusIcon16.png",
    "48": "img/pokusIcon48.png",
    "128": "img/pokusIcon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Let's Pokus"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "redirect.html"
  ],
  "permissions": [
    "activeTab",
    "contextMenus",
    "alarms",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>",
    "notifications"
  ]
}
