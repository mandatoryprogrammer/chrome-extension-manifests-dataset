{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Foundit",
  "version": "0.4.6",
  "manifest_version": 2,
  "description": "Bookmark management made easy!",
  "omnibox": {
    "keyword": "foundit"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "stemmer.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/"
  ]
}
