{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Snapshot/Screenshot Capture",
  "version": "0.4.21",
  "manifest_version": 2,
  "description": "Capture current frame image of video elements.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "crxcs.js",
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}
