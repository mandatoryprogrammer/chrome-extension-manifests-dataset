{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "chatNGEN",
  "description": "Chat it up.",
  "version": "1.5.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "chatNGEN",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "qrcode.min.js"
      ]
    }
  ],
  "manifest_version": 2
}
