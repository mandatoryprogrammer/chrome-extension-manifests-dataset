{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FUT Pack Price Checker",
  "description": "This extension will show the prices of items when opening packs.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*.ea.com/*",
    "*://*.easports.com/*"
  ],
  "web_accessible_resources": [
    "img/loading.gif"
  ]
}
