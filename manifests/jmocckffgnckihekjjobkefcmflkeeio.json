{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search Social Media",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "128": "./icons/icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "description": "Search selected text in Youtube, Facebook, Twitter and Instagram",
  "author": "Bhuvana Krishna",
  "short_name": "Search Social Media"
}
