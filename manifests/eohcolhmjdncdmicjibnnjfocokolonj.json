{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Grade Calculator for Blackboard Learn",
  "version": "4.0.0",
  "manifest_version": 2,
  "description": "Find out what your grade would be with certain assignments added.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
