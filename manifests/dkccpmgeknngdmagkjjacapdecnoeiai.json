{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Visual History",
  "short_name": "HistoryChart",
  "description": "Visualize and break down your Chrome browsing history by site.",
  "version": "0.0.5",
  "permissions": [
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "History Analysis"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
