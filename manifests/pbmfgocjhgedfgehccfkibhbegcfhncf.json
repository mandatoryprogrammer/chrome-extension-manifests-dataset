{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Star Gazer",
  "description": "Shows which users have starred each chat message in chat.stackoverflow",
  "version": "1.11",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://chat.stackoverflow.com/rooms/*"
      ],
      "js": [
        "data/jquery.js",
        "data/polyfill.js",
        "data/background.js"
      ],
      "css": [
        "data/style.css"
      ]
    }
  ],
  "manifest_version": 2
}
