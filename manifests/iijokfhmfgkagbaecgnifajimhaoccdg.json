{
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "QR",
    "default_popup": "qr.html"
  },
  "icons": {
    "128": "icon-128.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  },
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.1",
  "offline_enabled": true,
  "incognito": "split",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
