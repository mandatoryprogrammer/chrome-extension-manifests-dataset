{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SearchIT",
  "description": "Searches your query where you'd like.",
  "version": "1.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Select a website.",
    "default_icon": {
      "19": "icon2.png",
      "38": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
