{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CrunchHunt Chrome Extension",
  "description": "CrunchHunt right in your toolbar.",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "http://www.crunchhunt.com/"
  ],
  "icons": {
    "16": "icons/tcc16.png",
    "48": "icons/tcc48.png",
    "128": "icons/tcc128.png"
  },
  "browser_action": {
    "default_title": "CrunchHunt",
    "default_icon": {
      "19": "icons/tcc19.png",
      "38": "icons/tcc38.png"
    },
    "default_popup": "popup.html"
  }
}
