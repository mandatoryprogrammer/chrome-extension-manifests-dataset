{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Goodbye Quora Embed Button",
  "version": "1.47",
  "manifest_version": 2,
  "description": "Stop the Embed Quote button on Quora from popping up when you select/highlight text.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.quora.com/*"
      ],
      "css": [
        "goodbye.css"
      ],
      "all_frames": false
    }
  ]
}
