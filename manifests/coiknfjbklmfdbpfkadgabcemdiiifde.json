{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AutoFocus for YouTube",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Focuses the Flash video player when loading a YouTube video page",
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/slikts/youtube-autofocus"
}
