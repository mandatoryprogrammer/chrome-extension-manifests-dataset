{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Tracker",
  "description": "This extension simply adds a number to your tab in front of the title.",
  "version": "1.0",
  "background": {
    "scripts": [
      "tabTracker.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
