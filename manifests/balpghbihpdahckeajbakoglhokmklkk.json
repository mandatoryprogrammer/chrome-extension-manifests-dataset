{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/enums.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/toastr.css",
        "css/inject.css"
      ],
      "js": [
        "js/jquery-2.0.0.min.js",
        "js/toastr.js",
        "js/enums.js",
        "js/inject.js"
      ],
      "matches": [
        "http://tv.lrytas.lt/*",
        "https://tv.lrytas.lt/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Send lrytas.lt videos to your XBMC media center",
  "icons": {
    "128": "assets/icon_128.png",
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png"
  },
  "manifest_version": 2,
  "name": "Lrytas to XBMC",
  "options_page": "settings.html",
  "permissions": [
    "http://*/*",
    "storage"
  ],
  "web_accessible_resources": [
    "assets/play.png",
    "assets/plus.png"
  ],
  "version": "0.2"
}
