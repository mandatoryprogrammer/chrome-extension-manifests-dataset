{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LoudCloud Enhanced",
  "version": "1.1.1",
  "description": "Extension for BNED LoudCloud version 7.1 which adds useful features as well as dark mode",
  "manifest_version": 2,
  "incognito": "not_allowed",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://lms-trad.gcu.edu/*",
        "https://lms-testdrive.gcu.edu/*"
      ],
      "js": [
        "loudCloudEnhanced.js"
      ]
    },
    {
      "matches": [
        "https://lms-trad.gcu.edu/learningPlatform/user/GCU/*",
        "https://lms-trad.gcu.edu/learningPlatform/user/login.lc*"
      ],
      "js": [
        "loudCloudEnhancedLogin.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "LoudCloud Enhanced",
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "popup/options/options.html",
    "open_in_tab": false
  },
  "web_accessible_resources": [
    "themes/*",
    "*.png",
    "*.html",
    "popup/resetWidgets2.js",
    "lmsjs/users.js"
  ]
}
