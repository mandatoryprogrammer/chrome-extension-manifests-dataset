{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Demotivate",
  "short_name": "demotivate",
  "version": "0.1.2",
  "description": "Show a random demotivational picture in new tab",
  "icons": {
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://www.reddit.com/"
  ],
  "manifest_version": 2
}
