{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BackBack",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "Backspace for back/forward",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "js": [
        "backspace.js"
      ]
    }
  ]
}
