{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Surf No More",
  "short_name": "Surf No More",
  "version": "0.3",
  "description": "Stop mindlessly surfing the web through realtime feedback regarding your browsing habits!",
  "icons": {
    "16": "img/Icon.png",
    "48": "img/Icon.png",
    "128": "img/Logo.jpg"
  },
  "browser_action": {
    "default_title": "Check my browsing!",
    "default_popup": "src/popup/popup.html"
  },
  "background": {
    "page": "background.html"
  }
}
