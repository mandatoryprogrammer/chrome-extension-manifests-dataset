{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github commit marker",
  "description": "This extension adds a checkbox to commits in github to mark them as read.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/commits/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://github.com/*"
  ]
}
