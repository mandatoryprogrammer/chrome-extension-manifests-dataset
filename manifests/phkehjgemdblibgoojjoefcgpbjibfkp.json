{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simila.rs",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "48": "icon48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
