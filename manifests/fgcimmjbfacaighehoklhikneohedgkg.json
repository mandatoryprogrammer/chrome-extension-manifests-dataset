{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cookie ACL Manager",
  "version": "0.2",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {
    "scripts": [
      "cc_bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_0.png",
    "default_popup": "cc_popup.html"
  },
  "permissions": [
    "contentSettings",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
