{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Justweet",
  "description": "Retweet cleaner.",
  "version": "0.2.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/content/index.css"
      ]
    }
  ]
}
