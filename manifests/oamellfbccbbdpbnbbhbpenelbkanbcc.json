{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WaniKani Lite",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Ashley Pressley",
  "description": "At-a-glance WaniKani Status",
  "icons": {
    "16": "images/wanikani-logo.png",
    "32": "images/wanikani-logo.png",
    "48": "images/wanikani-logo.png",
    "128": "images/wanikani-logo.png"
  },
  "browser_action": {
    "name": "Update lesson count",
    "default_icon": "images/wanikani-logo.png",
    "default_title": "WaniKani Lite",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
