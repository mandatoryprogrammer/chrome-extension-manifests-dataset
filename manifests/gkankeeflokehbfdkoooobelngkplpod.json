{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "RapidReddit",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A rapid way to reddit.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "RapidReddit"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "homepage_url": "https://github.com/mdorsey0/RapidReddit",
  "options_page": "options.html",
  "permissions": [
    "http://*.reddit.com/",
    "history",
    "storage"
  ]
}
