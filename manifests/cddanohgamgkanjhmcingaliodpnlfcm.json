{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WaniKani New Reviews Notifier",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Displays how many minutes you have until your next reviews in WaniKani.",
  "author": "Konrad Listwan <konrad.listwan@gmail.com>",
  "icons": {
    "128": "icon.png",
    "256": "icon-retina.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "storage",
    "https://www.wanikani.com/"
  ],
  "browser_action": {
    "default_title": "WaniKani New Reviews Notifier",
    "default_icon": "wanikani-api-key.png"
  }
}
