{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SL Extender",
  "version": "0.0.5",
  "description": "Adds a toolbar icon that shows you the surf conditions at your favorite surf spots.",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Is it firing?",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://www.surfline.com/*",
    "http://www.surfline.com/*",
    "activeTab",
    "storage"
  ]
}
