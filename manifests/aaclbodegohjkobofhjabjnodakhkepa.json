{
  "background": {
    "persistent": false,
    "scripts": [
      "lib/qrscanner.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/content.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "lib/jquery.qrcode.min.js",
        "lib/toutf8.js",
        "js/cs.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "default_locale": "en",
  "description": "generate QRcode and read QRcode via context menu locally",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "name": "uniqrcode",
  "offline_enabled": true,
  "page_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_popup": "pageaction.html",
    "default_title": "uniqrcode"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "unlimitedStorage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1",
  "web_accessible_resources": [
    "res/*"
  ]
}
