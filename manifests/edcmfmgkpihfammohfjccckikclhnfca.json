{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "How Did I Get Here?",
  "version": "1.1",
  "description": "Find out how you got to a certain page when opening links in new tab",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "How Did I Get Here?",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png"
  }
}
