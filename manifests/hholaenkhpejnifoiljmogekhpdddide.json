{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stop Rus Music VK",
  "description": "В случае обнаружения проигрывания русской музыки, происходит переключение на следующую",
  "author": "Larkin Maksim",
  "version": "1.0",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}
