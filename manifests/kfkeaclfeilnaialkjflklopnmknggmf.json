{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flickr Secret Faves",
  "description": "Secretly fave photos on Flickr",
  "version": "1.1",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.flickr.com/*"
      ],
      "css": [
        "css/secret-faves.css"
      ],
      "js": [
        "js/jquery-2.0.3.min.js",
        "js/secret-faves.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    "img/secret.png",
    "img/secret-off.png"
  ]
}
