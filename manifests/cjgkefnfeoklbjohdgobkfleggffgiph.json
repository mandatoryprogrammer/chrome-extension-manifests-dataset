{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bronson's Pixel Painter Chrome Add On",
  "description": "Draw anything you want in a portable paint application with a stunning easter egg.",
  "version": "1",
  "author": "Chaz",
  "content_scripts": [
    {
      "css": [
        "main.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "icons": {
    "16": "images/bpp16.png",
    "48": "images/bpp48.png",
    "128": "images/bpp128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/bpp2.png"
  },
  "permissions": [
    "activeTab"
  ]
}
