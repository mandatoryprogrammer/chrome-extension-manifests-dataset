{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Incognito Killer",
  "version": "1.0",
  "description": "Kills all Chrome Incognito tabs when the screen gets locked",
  "permissions": [
    "activeTab",
    "idle"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
