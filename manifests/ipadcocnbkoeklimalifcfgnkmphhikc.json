{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weekendpedia",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Event recommendation from Wikipedia browsing",
  "icons": {
    "128": "icons/Logo.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/Logo_greyscale.png",
    "default_title": "Weekendpedia",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ]
}
