{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Breaking Clicking Bad",
  "content_scripts": [
    {
      "matches": [
        "http://*.nullism.com/*"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ]
    }
