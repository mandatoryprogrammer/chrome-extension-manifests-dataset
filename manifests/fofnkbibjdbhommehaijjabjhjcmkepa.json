{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Status de la chaîne Twitch Hexa-Gaming",
  "description": "Ne rate plus jamais un seul live de notre WebTV grâce à cette extension !",
  "version": "1.4",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "assets/images/logo.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "background": {
    "scripts": [
      "assets/js/jquery.js",
      "assets/js/background.js"
    ]
  }
}
