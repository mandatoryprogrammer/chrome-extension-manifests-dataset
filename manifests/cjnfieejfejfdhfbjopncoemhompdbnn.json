{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL to QR",
  "version": "1.0",
  "description": "Simple tool for passing  a website URL from desktop to mobile device through QR-code",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "49": "icons/49x49.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Generate QR",
    "default_icon": "icons/49x49.png",
    "default_popup": "main.html"
  }
}
