{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "seventh-audio",
  "description": "Adds a direct link to download 7th Circuit argument audio",
  "version": "1.0.3",
  "manifest_version": 2,
  "permissions": [
    "http://media.ca7.uscourts.gov/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://advance.lexis.com/*"
      ],
      "js": [
        "third-party/jquery-3.1.1.min.js",
        "contentscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
