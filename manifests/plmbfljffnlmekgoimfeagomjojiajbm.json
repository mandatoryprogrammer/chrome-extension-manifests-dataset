{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "ja",
  "version": "1.2",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "shortcut": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "close and store a tab"
    }
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
