{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link saver for OS X",
  "description": "Saves current page as .webloc file",
  "icons": {
    "16": "images/icons/save/save_16.png",
    "48": "images/icons/save/save_48.png",
    "128": "images/icons/save/save_128.png",
    "500": "images/icons/save/save_500.png"
  },
  "version": "0.0.3",
  "minimum_chrome_version": "21",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icons/save/save_38.png",
    "default_title": "Save this page a .webloc file"
  },
  "permissions": [
    "tabs",
    "downloads"
  ]
}
