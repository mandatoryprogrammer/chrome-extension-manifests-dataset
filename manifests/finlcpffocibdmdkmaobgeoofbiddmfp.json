{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "31.0",
  "offline_enabled": true,
  "name": "Window Tiler",
  "author": "Nicholas Clawson",
  "description": "A chrome extension to quickly organize your windows.",
  "version": "1.2.0",
  "permissions": [
    "tabs",
    "system.display"
  ],
  "browser_action": {
    "default_icon": "image/icon533.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  }
}
