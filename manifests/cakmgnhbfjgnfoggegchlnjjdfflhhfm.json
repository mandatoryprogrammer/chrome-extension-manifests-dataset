{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Is It On",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A YouTube service that shows if a song is available for instant streaming on rdio or spotify.",
  "icons": {
    "48": "icon48.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "isiton.user.js"
      ]
    }
  ]
}
