{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Your Snippet library",
  "description": "Minimalist text/snippet storage.",
  "version": "1.0.3",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "app/background.js"
    ]
  }
}
