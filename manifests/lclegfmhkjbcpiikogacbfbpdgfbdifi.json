{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "short_name": "__MSG_extName__",
  "description": "__MSG_description__",
  "default_locale": "pt_BR",
  "author": "Webysther Nunes",
  "version": "1.7",
  "icons": {
    "16": "assets/clear-all-16.png",
    "32": "assets/clear-all-32.png",
    "64": "assets/clear-all-64.png",
    "128": "assets/clear-all-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "*://*.wikipedia.org/*",
    "*://*.wiktionary.org/*",
    "*://*.wikiquote.org/*",
    "*://*.wikibooks.org/*",
    "*://*.wikisource.org/*",
    "*://*.wikinews.org/*",
    "*://*.wikiversity.org/*",
    "*://*.wikimedia.org/*",
    "*://*.mediawiki.org/*",
    "*://*.wikidata.org/*",
    "*://*.wikivoyage.org/*",
    "*://*.wikimediafoundation.org/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_tooltip__",
    "default_icon": "assets/clear-all-128.png"
  }
}
