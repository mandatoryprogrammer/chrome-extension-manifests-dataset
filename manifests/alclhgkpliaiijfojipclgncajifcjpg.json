{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LinkLove",
  "description": "The LinkLove extension makes sharing links on LinkLove easier than ever.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://www.linklove.us/"
  ],
  "browser_action": {
    "default_icon": "heart.png",
    "default_popup": "popup.html"
  }
}
