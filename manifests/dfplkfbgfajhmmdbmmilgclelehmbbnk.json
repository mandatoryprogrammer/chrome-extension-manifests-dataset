{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Click Your Achievements",
  "version": "0.2.1",
  "description": "Keep track of your progress toward achieving your goals",
  "icons": {
    "16": "static/img/icon16.png",
    "128": "static/img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "static/img/icon19.png"
    },
    "default_title": "Click Your Achievements",
    "default_popup": "index.html"
  },
  "author": "andornaut",
  "homepage_url": "https://github.com/andornaut/cya",
  "minimum_chrome_version": "55",
  "offline_enabled": true,
  "permissions": [
    "storage"
  ],
  "short_name": "CYA"
}
