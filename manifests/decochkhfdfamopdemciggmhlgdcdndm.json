{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Link in Duplicate Tab",
  "description": "Opens a link in a new tab with history from current tab",
  "version": "0.0.2",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "icons": {
    "16": "Tab-16.png",
    "48": "Tab-48.png",
    "128": "Tab-128.png"
  },
  "manifest_version": 2
}
