{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Backbase Widget Factory",
  "description": "Have a Backbase Portal? Want to build Widgets faster? Try this new tool out!",
  "manifest_version": 2,
  "version": "8.0",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "sandbox": {
    "pages": [
      "widget.html",
      "http://www.thenerdnomad.com/portalserver/main.php"
    ]
  },
  "permissions": [
    "cookies",
    "*.//*",
    "appWindow",
    "webview",
    "http://localhost",
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "http://*/",
    "storage"
  ],
  "file_handlers": {
    "text": {
      "types": [
        "text/*"
      ],
      "title": "Text editor"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
