{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AthLib",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "Google Chrome Extension to streamline access to Athabasca University Online Library Resources",
  "icons": {
    "16": "book.png",
    "48": "book.png",
    "128": "book.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "book.png",
    "default_title": "AthLib",
    "default_popup": "popup.html"
  }
}
