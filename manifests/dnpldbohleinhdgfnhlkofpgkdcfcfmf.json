{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hackage-Fu",
  "version": "0.2.0.3",
  "description": "Productivity Tools for HackageDB",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/statusfailed/hackage-fu",
  "content_scripts": [
    {
      "css": [
        "main.css"
      ],
      "js": [
        "jquery.min.js",
        "hackage.js"
      ],
      "matches": [
        "*://hackage.haskell.org/package*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://hackage.haskell.org/package*"
  ],
  "manifest_version": 2
}
