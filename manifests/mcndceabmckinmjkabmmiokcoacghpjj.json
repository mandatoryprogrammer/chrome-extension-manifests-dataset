{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nextflix",
  "description": "Nextflix",
  "version": "1.3",
  "icons": {
    "128": "nextflix_transparent.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
