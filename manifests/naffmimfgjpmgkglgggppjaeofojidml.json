{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "Leonardo Ciaccio",
  "homepage_url": "https://github.com/LeonardoCiaccio/buffer-to-telegram",
  "version": "1.3.9",
  "name": "Buffer To Telegram",
  "default_locale": "en",
  "description": "__MSG_des__",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "img/icon.16.png",
    "24": "img/icon.24.png",
    "48": "img/icon.48.png",
    "128": "img/icon.128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://buffer.com/api/1/updates/create.json",
    "*://api.telegram.org/",
    "*://*.buffer.com/",
    "storage"
  ]
}
