{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "goodKCLS",
  "version": "1.4",
  "description": "Launch King County Library Catalog searches from Goodreads.com",
  "icons": {
    "128": "books-icon-128.png"
  },
  "page_action": {
    "default_name": "Search KCLS",
    "default_icon": "books-icon-32.png"
  },
  "background": {
    "scripts": [
      "gotback.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://kcls.bibliocommons.com/",
    "https://www.goodreads.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
