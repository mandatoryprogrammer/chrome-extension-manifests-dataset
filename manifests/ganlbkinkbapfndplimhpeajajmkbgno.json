{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photos Tagged",
  "short_name": "Photos Tagged",
  "description": "This extension will add a button on Facebookâ„¢ to show photos that the current profile was tagged in",
  "version": "1.1.2",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "addphotosofbutton.js"
      ],
      "run_at": "document_start"
    }
  ]
}
