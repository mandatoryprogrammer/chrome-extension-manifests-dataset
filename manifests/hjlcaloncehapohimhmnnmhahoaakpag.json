{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "♩♪ Remember Me ♫♩",
  "version": "1.0.1",
  "description": "Appropriately notifies you when a web page offers to remember you",
  "manifest_version": 2,
  "permissions": [
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
