{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NU-NET Auto Auth",
  "version": "0.0.5",
  "author": "Antony Harfield",
  "homepage_url": "http://www.mobcomlab.com",
  "description": "Stay online all the time at Naresuan University",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "alarms",
    "background",
    "tabs",
    "https://login3.nu.ac.th:1000/*",
    "http://mobcomlab.s3.amazonaws.com/pingtest.json"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "NU-NET"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
