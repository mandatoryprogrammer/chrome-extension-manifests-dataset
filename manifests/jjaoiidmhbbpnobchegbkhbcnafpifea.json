{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unical",
  "description": "Add your classes directly to calendar from Coursebook",
  "version": "1.1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://coursebook.utdallas.edu/*",
        "https://coursebook.utdallas.edu/*"
      ],
      "js": [
        "myScripts.js"
      ]
    }
  ]
}
