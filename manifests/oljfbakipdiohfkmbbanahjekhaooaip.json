{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "spf.io bulk translate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A Chrome extension to automate the translation of multiple languages in spf.io",
  "homepage_url": "http://github.com/camerondevine/bulk-translate",
  "content_scripts": [
    {
      "matches": [
        "https://*.spf.io/document/editor*"
      ],
      "js": [
        "lib/jquery-3.3.1.min.js",
        "bulkautotranslate.js"
      ]
    }
  ]
}
