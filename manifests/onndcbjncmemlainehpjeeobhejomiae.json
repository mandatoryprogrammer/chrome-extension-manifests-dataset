{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AliExpress Challenge",
  "description": "Guess the price of the weird AliExpress Item.",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "https://*.aliexpress.com/item/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "assets/icon128.png"
  }
}
