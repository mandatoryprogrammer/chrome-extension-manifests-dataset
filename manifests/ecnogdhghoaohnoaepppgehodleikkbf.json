{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "drippr",
  "short_name": "drippr",
  "version": "0.0.1",
  "description": "Share articles and entertainment with friends.",
  "browser_action": {
    "default_icon": "drop.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ]
}
