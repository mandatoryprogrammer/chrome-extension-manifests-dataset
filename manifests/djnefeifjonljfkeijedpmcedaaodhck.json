{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "chrome-scn-dev",
  "description": "Scn Dev Extension for Chrome",
  "version": "1.1.0",
  "icons": {
    "16": "assets/img/icon-16.png",
    "48": "assets/img/icon-48.png",
    "128": "assets/img/icon-128.png"
  },
  "options_page": "assets/html/options.html",
  "background": {
    "persistent": false,
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "assets/js/content.js"
      ],
      "all_frames": true
    }
  ],
  "commands": {
    "tabReorderLeft": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "MacCtrl+Shift+Left"
      },
      "description": "Move current Tab one position to the left."
    },
    "tabReorderRight": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "MacCtrl+Shift+Right"
      },
      "description": "Move current Tab one position to the right."
    }
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ]
}
