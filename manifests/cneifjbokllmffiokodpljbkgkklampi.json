{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SOLR SQL Console",
  "description": "Web SQL Console for Apache Solr.",
  "version": "1.0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "storage"
  ]
}
