{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "inyecta js a cualquier pagina",
  "homepage_url": "http://danielarriola.com",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "codeinjector.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icon16.png"
    },
    "default_title": "CODE injector"
  },
  "manifest_version": 2,
  "name": "CODE injector",
  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>",
    "storage"
  ],
  "version": "1.0"
}
