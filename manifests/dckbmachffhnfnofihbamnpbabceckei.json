{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cartociudad IGN",
  "version": "1.1",
  "description": "Permite utilizar Cartociudad del Instituo Geografico Nacional (IGN)",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Cartociudad IGN",
    "popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://www.cartociudad.es/",
    "http://labs.metacarta.com/",
    "http://www.idee.es/",
    "http://ovc.catastro.meh.es/"
  ]
}
