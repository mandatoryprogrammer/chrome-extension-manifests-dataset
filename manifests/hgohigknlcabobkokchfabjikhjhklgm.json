{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eureka for Aha!",
  "description": "This extension aims to fix styling and functional issues with Aha!",
  "version": "1.1.1",
  "icons": {
    "128": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.aha.io/*"
      ],
      "css": [
        "assets/eureka.css"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "assets/eureka.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://*.aha.io/*"
  ]
}
