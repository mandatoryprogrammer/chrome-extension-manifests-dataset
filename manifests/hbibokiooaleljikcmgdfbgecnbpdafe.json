{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reveal It",
  "version": "1.0",
  "description": "Highlight a startup/company to reveal what they do. By @saranyan",
  "manifest_version": 2,
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "hoverIntent.js",
        "CompanyName.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
