{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Print Image Menu",
  "description": "This extension adds 'print' item to context menu of images.",
  "version": "1.0.0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}
