{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Peaceful Mind",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Blocks the online ads",
  "browser_action": {
    "default_icon": "on.png"
  },
  "icons": {
    "16": "b.png",
    "48": "b.png",
    "128": "b.png"
  },
  "background": {
    "scripts": [
      "filters.js",
      "block.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "incognito": "spanning",
  "web_accessible_resources": [
    "empty.gif"
  ],
  "homepage_url": "http://thevillagesoftware.com/"
}
