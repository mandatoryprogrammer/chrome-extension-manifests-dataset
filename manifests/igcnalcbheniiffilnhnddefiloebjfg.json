{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Renew",
  "description": "Reload current page by closing it and opening a new tab",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Is Chrome glitching again?"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "script.js"
    ]
  }
}
