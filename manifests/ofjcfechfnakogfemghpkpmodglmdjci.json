{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PDF Sniffer",
  "description": "Detect embeded pdfs in major digital libraries.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://arxiv.org/abs/*"
      ],
      "js": [
        "sniff.js"
      ]
    },
    {
      "matches": [
        "*://www.sciencedirect.com.ezproxy.lib.purdue.edu/*"
      ],
      "js": [
        "sniff.js"
      ]
    },
    {
      "matches": [
        "*://ieeexplore.ieee.org.ezproxy.lib.purdue.edu/*"
      ],
      "js": [
        "sniff.js"
      ]
    },
    {
      "matches": [
        "*://dl.acm.org.ezproxy.lib.purdue.edu/*"
      ],
      "js": [
        "sniff.js"
      ]
    }
  ]
}
