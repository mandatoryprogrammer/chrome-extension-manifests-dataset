{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AudioBooks",
  "description": "Listen, collect and download free mp3 audiobooks.",
  "version": "0.1",
  "permissions": [
    "webview",
    "http://localhost/"
  ],
  "webview": {
    "partitions": [
      {
        "name": "rce",
        "accessible_resources": [
          "*.*"
        ]
      }
    ]
  },
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
