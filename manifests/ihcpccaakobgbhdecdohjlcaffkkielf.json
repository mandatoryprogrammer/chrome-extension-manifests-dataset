{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Opt-out: Donald Trump",
  "description": "This extension searches the current page and deletes elements which contain words or phrases to be filtered out",
  "version": "1.2",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "underscore.js",
        "underscore.string.min.js",
        "default.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab"
  ]
}
