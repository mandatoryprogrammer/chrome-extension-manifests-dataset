{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zhihu Topbar Remover",
  "content_scripts": [
    {
      "matches": [
        "http://*.zhihu.com/*",
        "https://*.zhihu.com/*",
        "http://zhihu.com/*",
        "https://zhihu.com/*"
      ],
      "css": [
        "css/remover.css"
      ],
      "js": [
        "js/jquery.js",
        "js/remover.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "0.8.0"
}
