{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet Page",
  "version": "0.0.3",
  "description": "Tweet share current page",
  "icons": {
    "16": "icons/icon@16px.png",
    "48": "icons/icon@48px.png",
    "128": "icons/icon@128px.png"
  },
  "browser_action": {
    "default_title": "tweet current page"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "options_page": "options.html"
}
