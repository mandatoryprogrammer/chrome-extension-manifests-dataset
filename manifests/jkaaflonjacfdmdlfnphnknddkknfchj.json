{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SimplePW",
  "version": "1.3",
  "manifest_version": 2,
  "description": "SimplePW",
  "browser_action": {
    "default_icon": "icon.png",
    "default_menu": "Menu",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "activeTab",
    "webRequest",
    "http://simplepw.herokuapp.com/dashboard"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./js/info.js",
        "./js/jquery.js",
        "./js/crypto.js",
        "./js/JsonFormatter.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
