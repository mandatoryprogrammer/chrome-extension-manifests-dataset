{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "focusNow",
  "name": "focusNow",
  "description": "Brings a focused to-do app to the newtab",
  "icons": {
    "16": "focusnow_extension_icon16.png",
    "32": "focusnow_extension_icon32.png",
    "48": "focusnow_extension_icon48.png",
    "128": "focusnow_extension_icon128.png"
  },
  "version": "1.4.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "app/background.js"
    ]
  },
  "browser_action": {
    "default_title": "focusNow",
    "default_icon": {
      "32": "focusnow_extension_icon32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.nicoraven.com/*"
      ],
      "css": [
        "/static/css/app.css"
      ],
      "js": [
        "/static/js/content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "/static/css/content.css"
  ],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
