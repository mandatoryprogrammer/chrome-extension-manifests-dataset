{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Socialimiter",
  "description": "Social Limiter, limit the social network. Get freedom",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "default_locale": "en",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "main.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  }
}
