{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShoutBox",
  "description": "Tweet faster with the power of Chrome.",
  "version": "2.0",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "@"
  },
  "icons": {
    "16": "ShoutBox128.png",
    "48": "ShoutBox128.png",
    "128": "ShoutBox128.png"
  },
  "permissions": [
    "notifications",
    "http://www.twitter.com/"
  ],
  "options_page": "popup.html",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "ShoutBox128.png",
    "default_popup": "realpopup.html"
  },
  "web_accessible_resources": [
    "ShoutBox128.png"
  ]
}
