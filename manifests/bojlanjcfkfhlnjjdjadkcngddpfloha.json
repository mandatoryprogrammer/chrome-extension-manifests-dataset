{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL Quick Append",
  "version": "1.4.5",
  "description": "Quickly Append ID to URLs.",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/urlqa16.png",
      "32": "images/urlqa32.png",
      "48": "images/urlqa48.png",
      "128": "images/urlqa128.png"
    }
  },
  "icons": {
    "16": "images/urlqa16.png",
    "32": "images/urlqa32.png",
    "48": "images/urlqa48.png",
    "128": "images/urlqa128.png"
  },
  "manifest_version": 2
}
