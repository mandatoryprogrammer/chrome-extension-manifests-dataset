{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ads-tester",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "Simple browser extension that helps with live debugging frame ads",
  "icons": {
    "16": "resources/icons/icon.png",
    "48": "resources/icons/icon.png"
  },
  "author": "Mateusz Bagi≈Ñski <cziken58@gmail.com>",
  "browser_action": {},
  "devtools_page": "panel.html",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "resources/exposed/*"
  ],
  "homepage_url": "https://github.com/Mati365/ad-tester",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "browsingData",
    "*://*/*"
  ]
}
