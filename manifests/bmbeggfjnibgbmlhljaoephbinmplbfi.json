{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoLitics",
  "description": "This extension replace all politic post by photos of cats",
  "version": "2.0",
  "content_scripts": [
    {
      "js": [
        "utils.js",
        "twitter.js"
      ],
      "matches": [
        "https://twitter.com/*",
        "https://twitter.com/*"
      ]
    },
    {
      "js": [
        "utils.js",
        "facebook.js"
      ],
      "matches": [
        "https://www.facebook.com/*",
        "https://facebook.com/*"
      ]
    }
  ]
}
