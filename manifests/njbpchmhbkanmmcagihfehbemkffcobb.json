{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Felt Temperature",
  "version": "1.1.9",
  "manifest_version": 2,
  "description": "Felt temperature gives you meaningful measure of temperature by considering humidity and wind.",
  "icons": {
    "16": "icons/thermo16.png",
    "19": "icons/thermo19.png",
    "48": "icons/thermo48.png",
    "128": "icons/thermo128.png"
  },
  "browser_action": {
    "default_icon": "icons/thermo19.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "geolocation",
    "http://api.wunderground.com/"
  ]
}
