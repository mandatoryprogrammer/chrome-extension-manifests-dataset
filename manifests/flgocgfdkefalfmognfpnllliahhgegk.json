{
  "background": {
    "scripts": [
      "js/porter.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/main.css"
      ],
      "js": [
        "js/porter.js",
        "js/content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Full text search across Chrome tabs.",
  "manifest_version": 2,
  "name": "Hello.",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3",
  "web_accessible_resources": [
    "img/*.png",
    "html/*.html",
    "js/searchBar.js"
  ]
}
