{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Runrun.it Task Manager",
  "short_name": "RunrunTM",
  "description": "A Runrun.it Task Manager!",
  "version": "1.3.0",
  "browser_action": {
    "default_icon": "images/icon_16.png",
    "default_title": "Runrun.it Task Manager",
    "default_popup": "popup.html"
  },
  "permissions": [
    "idle",
    "tabs",
    "notifications",
    "alarms",
    "storage",
    "https://secure.runrun.it/"
  ],
  "background": {
    "scripts": [
      "axios.min.js",
      "moment.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "options_page": "options.html"
}
