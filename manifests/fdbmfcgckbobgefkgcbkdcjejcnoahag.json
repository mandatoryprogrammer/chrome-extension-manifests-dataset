{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "hipMenu checklist",
  "description": "Turn a group order into an easy to manage checklist",
  "version": "0.9.6",
  "permissions": [
    "tabs",
    "http://www.hipmenu.ro/*",
    "https://www.hipmenu.ro/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "hipMenu.html",
    "default_title": "hipMenu checklist"
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
