{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GAMEPAD for Twitch Plays Pokemon",
  "description": "Play Pokemon at twitch.tv using your keyboard",
  "version": "0.2",
  "permissions": [
    "tabs",
    "*://www.twitch.tv/",
    "*://twitch.tv/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
