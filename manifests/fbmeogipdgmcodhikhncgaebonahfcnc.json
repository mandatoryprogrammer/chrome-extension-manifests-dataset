{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RARBG redirector",
  "description": "Adds an 'r' omnibox command and opens RARBG movie/tvshow pages by searching in IMDB",
  "version": "1.0.1",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "omnibox": {
    "keyword": "r"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
