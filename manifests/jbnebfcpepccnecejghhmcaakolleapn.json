{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Right Click to YouTube",
  "description": "Search YouTube for highlighted text via right-click menu.",
  "version": "1.2",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}
