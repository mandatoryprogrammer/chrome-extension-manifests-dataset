{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Downloader",
  "description": "Download single files from GitHub",
  "version": "2.2",
  "permissions": [
    "downloads",
    "https://github.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "scripts": [
      "extensionToMIME.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "author": "Turbatu Dumitru Titus <titus.tur.dum@gmail.com>"
}
