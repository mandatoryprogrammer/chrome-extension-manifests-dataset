{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ceser",
  "short_name": "Ceser",
  "version": "1.1.17.5",
  "description": "Credits Extension",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/Extension.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/Logo.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "js/cs.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/Api.js",
    "js/CreditsWork.js",
    "js/Extension.js",
    "img/Logo.png"
  ],
  "icons": {
    "16": "img/Logo.png",
    "32": "img/Logo.png",
    "48": "img/Logo.png",
    "128": "img/Logo.png"
  },
  "manifest_version": 2
}
