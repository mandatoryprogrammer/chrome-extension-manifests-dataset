{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Neil Supreme",
  "description": "Inspired by this this Chrome Extension: https://github.com/benhalpern/dave-coulier-chrome. But Neil instead.",
  "version": "1.0.1",
