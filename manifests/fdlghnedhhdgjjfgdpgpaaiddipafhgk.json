{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cookie Tab Viewer",
  "version": "2.0",
  "description": "Get cookies of the current tab.",
  "permissions": [
    "cookies",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "cookie-16.png",
    "48": "cookie-48.png",
    "128": "cookie-128.png"
  },
  "browser_action": {
    "default_icon": "cookie-128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
