{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quieter-Twitter-Facebook",
  "description": "Focus only on what you came for",
  "version": "0.1.0",
  "permissions": [
    "https://rawgit.com/"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com https://rawgit.com; https://www.gstatic.com; object-src 'self';",
  "browser_action": {
    "default_icon": "icons/main_icon16.png",
    "default_title": "Quieter-Twitter-Facebook"
  },
  "icons": {
    "16": "icons/main_icon16.png",
    "48": "icons/main_icon48.png",
    "128": "icons/main_icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*",
        "*://*.facebook.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
