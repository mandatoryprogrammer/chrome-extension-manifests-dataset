{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark Assistant",
  "version": "1.2.1",
  "description": "Having trouble managing hundreds of bookmarks? This chrome app is for you!",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "browser_action": {
    "default_title": "Bookmark Assistant",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
