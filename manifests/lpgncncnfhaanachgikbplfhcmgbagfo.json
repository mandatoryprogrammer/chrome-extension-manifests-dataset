{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.3.0",
  "name": "Anole",
  "description": "Transform the selected text.",
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "anole-icon-16.png",
    "48": "anole-icon-48.png",
    "128": "anole-icon-128.png"
  },
  "options_page": "options.html"
}
