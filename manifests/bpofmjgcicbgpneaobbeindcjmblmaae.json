{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lazy Helpers",
  "version": "1.9.0",
  "manifest_version": 2,
  "description": "Github, Stackoverflow, Google dark theme | Custom Github search helper | Click next image | Slow scroll",
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
        "rabbitHole.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "background",
    "clipboardRead",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "hot-reload.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    }
  }
}
