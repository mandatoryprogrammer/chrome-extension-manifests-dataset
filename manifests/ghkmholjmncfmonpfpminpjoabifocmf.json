{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "description": "Chrome extension for lobste.rs.",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "Lobster",
  "version": "2.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://lobste.rs/*",
        "http://lobste.rs/*"
      ],
      "js": [
        "ext.js"
      ],
      "run_at": "document_end"
    }
  ]
}
