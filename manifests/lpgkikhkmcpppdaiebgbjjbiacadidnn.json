{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JSON Diff Checker",
  "description": "JSON Diff checker shows you the difference between two JSONs on each side considering key & value.",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "JSON Diff Checker"
  }
}
