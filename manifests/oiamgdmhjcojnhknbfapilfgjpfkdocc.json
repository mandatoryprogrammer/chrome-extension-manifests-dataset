{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sen Việt Notifications",
  "description": "Extension giúp nhận thông báo từ Sen Việt.",
  "manifest_version": 2,
  "version": "1.0.1",
  "background": {
    "scripts": [
      "analytics.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "*://*/*",
    "management",
    "tabs",
    "webNavigation"
  ],
  "icons": {
    "128": "gcm_128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com/ga.js; object-src 'self'"
}
