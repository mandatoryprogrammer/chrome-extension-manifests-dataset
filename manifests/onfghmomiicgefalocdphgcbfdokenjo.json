{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkedIn All Star!",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.linkedin.com/*"
  ]
}
