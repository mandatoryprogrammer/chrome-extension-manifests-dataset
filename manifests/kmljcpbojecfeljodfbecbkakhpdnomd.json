{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fingertip QR Link",
  "description": "Convert link or current address to QR Code for mobile",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "icons": {
    "16": "icon_016.png",
    "48": "icon_048.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
