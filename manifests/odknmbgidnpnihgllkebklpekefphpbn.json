{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Foundation Overlay",
  "description": "This extension creates an overlay for the Foundation grid system",
  "version": "1.2.1",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon32_off.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
