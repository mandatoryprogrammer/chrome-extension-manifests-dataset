{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CallMiner v10 Categories Navigator",
  "version": "0.1.5",
  "manifest_version": 2,
  "description": "CallMiner category navigation and management tool",
  "homepage_url": "http://creativeIV.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "tabs",
    "storage",
    "identity",
    "https://www.googleapis.com/",
    "https://*/*",
    "http://*/*"
  ],
  "oauth2": {
    "client_id": "484434675258-37sai44a39pu3esn4reutrgsi9v3d0qc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "web_accessible_resources": [
    "Templates/*",
    "images/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "src/inject/inject.css",
        "src/inject/select2.css",
        "js/intro.js-2.4.0/introjs.css",
        "js/intro.js-2.4.0/themes/introjs-modern.css"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "js/node_modules/arrive/src/arrive.js",
        "js/intro.js-2.4.0/intro.js",
        "insert.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
