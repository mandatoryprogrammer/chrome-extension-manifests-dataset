{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IGN CN No Redirect",
  "version": "0.5",
  "description": "阻止ign自动跳转ign.中国",
  "permissions": [
    "cookies",
    "http://*.ign.com/*",
    "https://*.ign.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
