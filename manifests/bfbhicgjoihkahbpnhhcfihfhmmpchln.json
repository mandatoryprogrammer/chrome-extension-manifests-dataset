{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blanja Tools",
  "description": "Memudahkan pengguna untuk melakukan transaksi di Blanja",
  "version": "1.0",
  "permissions": [
    "tabs",
    "alarms",
    "cookies",
    "storage",
    "activeTab",
    "*://*.blanja.com/*",
    "https://trade.blanja.com/*",
    "https://blanja.com/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "img/icon128.png"
  ]
}
