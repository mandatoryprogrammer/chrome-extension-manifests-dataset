{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dislike Button for Facebook",
  "version": "3.0.3",
  "manifest_version": 2,
  "homepage_url": "http://www.dislike.cc/",
  "description": "Mit Hilfe dieser Extension kannst du Beitr√§ge bei Facebook auch disliken!",
  "background": {
    "scripts": [
      "jquery-1.8.1.min.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "64": "logo_64.png",
    "128": "logo_128.png"
  },
  "permissions": [
    "*://dislike.netnovate.com/*"
  ],
  "web_accessible_resources": [
    "images/dislike.png"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*.facebook.com/*"
      ],
      "exclude_matches": [
        "*://*.facebook.com/ai.php*",
        "*://*.facebook.com/ajax/*",
        "*://*.facebook.com/pull/*",
        "*://*.facebook.com/plugins/*",
        "*://*.facebook.com/connect/*",
        "*://*.facebook.com/dialog/*",
        "*://*.facebook.com/sharer/*",
        "*://*.facebook.com/attachments/*"
      ],
      "js": [
        "jquery-1.8.1.min.js",
        "dislike.js"
      ]
    }
  ]
}
