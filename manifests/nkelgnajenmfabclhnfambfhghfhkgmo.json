{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Select-n-date Kraut Edition",
  "description": "Create a new calendar event by selecting a date or a title from a website.",
  "version": "0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "app.js",
      "lib/date.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "images/icon38.png"
    }
  }
}
