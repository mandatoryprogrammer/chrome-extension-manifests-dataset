{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "github image link expander",
  "description": "Replaces links to images in Github issues/prs with linked img tags",
  "version": "1.1",
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
