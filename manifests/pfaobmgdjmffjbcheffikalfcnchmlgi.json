{
  "background": {
    "scripts": [
      "hook_vanguard.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Manage Vanguard Copy settings"
  },
  "content_scripts": [
    {
      "js": [
        "hook_vanguard.js"
      ],
      "matches": [
        "*://personal.vanguard.com/us/TPView"
      ]
    }
  ],
  "description": "This extension makes it easy to copy your investment information into a spreadsheet",
  "homepage_url": "http://github.com/jjshoe/VanguardCopy",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Vanguard Copy",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "short_name": "VC",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
