{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The200 - Handful of features",
  "version": "0.1",
  "description": "Add features to the100 website",
  "permissions": [
    "tabs",
    "declarativeContent",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.the100.io/*"
      ],
      "js": [
        "plugins/jquery.3.3.1.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "name": "Add players raid completions"
  },
  "icons": {
    "16": "images/destiny_logo16.png",
    "32": "images/destiny_logo32.png",
    "48": "images/destiny_logo48.png",
    "128": "images/destiny_logo128.png"
  },
  "manifest_version": 2
}
