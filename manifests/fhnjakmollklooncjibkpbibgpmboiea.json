{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noments",
  "description": "Come on, Twitter, Seriously. It *had* to go where notifications were?",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "noments.js"
      ]
    }
  ]
}
