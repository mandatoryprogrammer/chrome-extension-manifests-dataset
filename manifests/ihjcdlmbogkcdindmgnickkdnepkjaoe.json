{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Contributions Are Work",
  "description": "Makes Github contribution graph look more like work by making it red.",
  "version": "2.0",
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "make_contributions_red.css"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "change-text.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
