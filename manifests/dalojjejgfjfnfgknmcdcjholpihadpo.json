{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pobble 365",
  "description": "Get some instant inspiration, every time you open a new tab. One picture. One teaching resource. Every day.",
  "version": "0.0.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.pobble365.com/api/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}
