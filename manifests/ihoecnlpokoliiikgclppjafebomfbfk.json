{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ThatTab",
  "description": "Name all your windows and manage where your new tabs go by the names of the windows!",
  "version": "0.2",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Configure ThatTab"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "refresh_icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
