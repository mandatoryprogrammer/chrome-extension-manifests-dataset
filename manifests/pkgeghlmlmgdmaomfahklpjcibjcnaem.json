{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Deleted Video Finder for YouTubeâ„¢",
  "version": "0.1",
  "description": "Using Wayback Machine and Google, this will try to find deleted or private YouTube Videos",
  "icons": {
    "16": "DVFYT_icon_128.png",
    "48": "DVFYT_icon_128.png",
    "128": "DVFYT_icon_128.png"
  },
  "author": "Al Sharairi",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "jquery-3.3.1.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "DVFYT_icon_128.png",
    "default_title": "Deleted Youtube Video Finder"
  },
  "background": {
    "scripts": [
      "background.js",
      "contextMenu.js",
      "jquery-3.3.1.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "https://*.youtube.com/*",
    "https://*.archive.org/*",
    "tabs"
  ],
  "short_name": "DVFYT"
}
