{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swap Envi (Beta)",
  "description": "This extension allows you to switch between your Development, Test, QA, Production, or any other environments.",
  "version": "1.1",
  "author": "Mitch Cohen",
  "homepage_url": "http:/www.mitch-cohen.com",
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "background": {
    "scripts": [
      "js/lodash.js",
      "js/eventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus"
  ]
}
