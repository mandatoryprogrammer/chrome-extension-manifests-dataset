{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "_blank for GitHub",
  "short_name": "_blank",
  "description": "Links in readme files will always open in new tabs",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://github.com/*",
        "http://gist.github.com/*",
        "https://github.com/*",
        "https://gist.github.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
