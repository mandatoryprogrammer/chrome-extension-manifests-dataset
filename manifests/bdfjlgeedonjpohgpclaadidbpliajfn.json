{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Detour",
  "short_name": "Detour",
  "description": "Use Detour to improve your browsing habits.",
  "version": "0.0.2",
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "32": "32.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "jquery.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "name": "Detour",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
