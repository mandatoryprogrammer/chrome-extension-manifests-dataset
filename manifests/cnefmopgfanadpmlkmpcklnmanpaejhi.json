{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Force Render",
  "short_name": "GFR",
  "description": "Force render imgs in commit and PR pages",
  "author": "gecko655",
  "manifest_version": 2,
  "version": "1.0.0.3",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/commit/*",
        "https://github.com/*/*/pull/*",
        "https://github.com/*/*/compare/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
