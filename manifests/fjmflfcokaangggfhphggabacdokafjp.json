{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mock Speak",
  "description": "Convert text to mock speak",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
