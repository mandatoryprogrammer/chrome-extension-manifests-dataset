{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "lib/jq/jquery-2.1.1.min.js",
        "porgify/porgify.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": [
      "porgify/bg.js"
    ]
  },
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "__MSG_appName__",
  "version": "1.1.0",
  "default_locale": "en",
  "icons": {
    "128": "porgify/logo128.png",
    "16": "porgify/logo16.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "webRequest",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage"
  ],
  "options_page": "porgify/options/options.html",
  "options_ui": {
    "page": "porgify/options/options.html",
    "chrome_style": true
  }
}
