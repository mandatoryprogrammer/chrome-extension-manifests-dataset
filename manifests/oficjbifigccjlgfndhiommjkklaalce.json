{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ico16.png",
    "default_title": "download monster",
    "icons": [
      "ico16.png"
    ],
    "name": "网页下载",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "网页下载，包括html,css,image,video",
  "icons": {
    "16": "ico16.png",
    "48": "ico48.png",
    "128": "ico128.png"
  },
  "name": "网页下载",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "webRequest"
  ],
  "version": "1.0.1",
  "manifest_version": 2
}
