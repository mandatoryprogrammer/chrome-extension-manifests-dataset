{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "libdoge",
  "description": "such god doge",
  "version": "0.1.9",
  "icons": {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "scripts": [
      "run.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "doge go",
    "default_icon": "img/icon-19.png"
  }
}
