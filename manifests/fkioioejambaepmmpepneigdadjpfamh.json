{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Math Equations Anywhere",
  "author": "brendenA",
  "description": "This extension allows you to create math equation with latex, mathml, ascii text and convert them to Img.",
  "version": "4.2",
  "icons": {
    "16": "./Img/16x16.png",
    "19": "./Img/19x19.png",
    "20": "./Img/20x20.png",
    "24": "./Img/24x24.png",
    "25": "./Img/25x25.png",
    "30": "./Img/30x30.png",
    "32": "./Img/32x32.png",
    "35": "./Img/35x35.png",
    "38": "./Img/38x38.png",
    "40": "./Img/40x40.png",
    "44": "./Img/44x44.png",
    "48": "./Img/48x48.png",
    "50": "./Img/50x50.png",
    "64": "./Img/64x64.png",
    "96": "./Img/96x96.png",
    "128": "./Img/128x128.png",
    "150": "./Img/150x150.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "./Img/16x16.png",
      "19": "./Img/19x19.png",
      "20": "./Img/20x20.png",
      "24": "./Img/24x24.png",
      "25": "./Img/25x25.png",
      "30": "./Img/30x30.png",
      "32": "./Img/32x32.png",
      "35": "./Img/35x35.png",
      "38": "./Img/38x38.png",
      "40": "./Img/40x40.png",
      "48": "./Img/48x48.png",
      "64": "./Img/64x64.png",
      "96": "./Img/96x96.png",
      "128": "./Img/128x128.png"
    },
    "default_title": "Math Equations"
  },
  "developer": {
    "name": "Brenden A",
    "url": "https://github.com/brendena"
  },
  "homepage_url": "https://github.com/brendena/MathEquations-Extension",
  "web_accessible_resources": [
    "Img/*.png",
    "Img/*.svg"
  ],
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "commands": {
    "open-close-extension": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Opens the extension or closes it"
    }
  },
  "applications": {
    "gecko": {
      "id": "{f7d3c7b6-861a-49b6-85d7-04ae3f260ab9}",
      "strict_min_version": "55.0"
    }
  }
}
