{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "TuziMoe",
  "manifest_version": 2,
  "name": "StopMining",
  "version": "1.0.4",
  "description": "阻止网页挖矿脚本",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "http://*/*/",
        "https://*/*/"
      ],
      "js": [
        "js/stop.js"
      ]
    }
  ],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "scripts": [
      "js/stop.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "js/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_option": "white.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "19": "icon/icon19.png",
      "38": "icon/icon38.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "background",
    "management",
    "webNavigation"
  ]
}
