{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CouponRecorder",
  "version": "1.0.2",
  "description": "Record user view log for Coupon.",
  "icons": {
    "128": "images/icon_128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "bg.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "*://*.smzdm.com/*"
  ],
  "browser_action": {
    "default_title": "CouponRecorder",
    "default_icon": {
      "19": "images/icon_19.png"
    },
    "default_popup": "popup.html"
  }
}
