{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Star Daddy",
  "description": "A browser extension for rediscovering starred GitHub repos",
  "version": "1.0.1",
  "version_name": "v1.0.1-alpha",
  "manifest_version": 2,
  "icons": {
    "48": "icons/logo-48.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/logo-48.png",
    "default_title": "Star Daddy - Star managment for GitHub users"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
