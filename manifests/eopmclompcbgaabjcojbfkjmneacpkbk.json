{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GitHub Participation Graph",
  "version": "0.1",
  "description": "Github participation graph improvement",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "js": [
        "lib/jquery-1.8.1.min.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
