{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pull Favors",
  "short_name": "GitHub Pull-Request Beautifier",
  "description": "GitHub Pull-Request Beautifier",
  "version": "1.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "pr-favors.js"
      ],
      "run_at": "document_end"
    }
  ]
}
