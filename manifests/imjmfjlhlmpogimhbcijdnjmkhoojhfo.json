{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ViewState Detector",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "A viewstate detector, shows a badge when viewstate is detected.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "badge.png",
    "x.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
