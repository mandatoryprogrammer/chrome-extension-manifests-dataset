{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Paginator Down",
  "description": "Navigates to previous page by decrementing a number in the URL",
  "version": "1.0",
  "browser_action": {
    "default_icon": "down.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "down.js"
    ]
  },
  "icons": {
    "128": "logo-128.png"
  }
}
