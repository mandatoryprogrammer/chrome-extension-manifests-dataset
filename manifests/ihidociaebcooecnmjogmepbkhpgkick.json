{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Mini",
  "short_name": "Reddit Mini",
  "description": "Get a quick snapshot of your favorite subreddits",
  "version": "0.4.2.0",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "pop-up.html"
  },
  "icons": {
    "128": "img/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://www.reddit.com https://ssl.google-analytics.com; object-src 'self'"
}
