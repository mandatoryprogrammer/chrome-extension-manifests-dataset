{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Ruslan Osmanov",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "commands": {
    "bee-editor": {
      "description": "Open external editor",
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      }
    }
  },
  "description": "Allows to edit form text fields with an external editor",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "Browser's external editor",
  "options_ui": {
    "chrome_style": true,
    "page": "html/options.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "nativeMessaging",
    "<all_urls>"
  ],
  "short_name": "Bee",
  "version": "2.8",
  "web_accessible_resources": [
    "js/pattern.js",
    "js/storage.js"
  ]
}
