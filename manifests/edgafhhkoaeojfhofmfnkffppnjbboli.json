{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_AddonName__",
  "description": "__MSG_AddonDescription__",
  "author": "JƒÅnis Elmeris",
  "version": "2.1.0",
  "options_ui": {
    "page": "ui/settings.html"
  },
  "options_page": "ui/settings.html",
  "browser_action": {
    "default_icon": "ui/icon.png",
    "default_title": "__MSG_AddonName__",
    "default_popup": "ui/popup.html",
    "browser_style": true,
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*/*"
      ],
      "js": [
        "src/content.js",
        "src/jquery-3.4.1.min.js",
        "src/collect.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "ui/icon.png",
    "64": "ui/icon64.png"
  }
}
