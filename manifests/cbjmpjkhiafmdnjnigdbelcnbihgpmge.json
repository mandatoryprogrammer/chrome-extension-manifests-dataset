{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UnXSS",
  "version": "0.0.4",
  "short_name": "UnXSS",
  "description": "Intercept and modify or delete websites' security headers",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "32": "icons/32.png",
    "19": "icons/19.png"
  },
  "browser_action": {
    "default_title": "UnXSS Options",
    "default_icon": "icons/19.png",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
