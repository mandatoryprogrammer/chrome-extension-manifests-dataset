{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timegrip",
  "version": "0.7.9",
  "description": "Timegrip helper",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://dktimegrip/*",
        "https://timegrip.creuna.dk/*",
        "https://timegrip.creuna.no/*"
      ],
      "js": [
        "mappy_content_script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "script.js",
    "santa.png"
  ],
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ]
}
