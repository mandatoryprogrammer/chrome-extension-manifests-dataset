{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Tab Extension",
  "short_name": "One Tab",
  "version": "1.0.0.10",
  "description": "Reuses Chrome tab for existing Matecat, Smartcat, Lingotek or Crowdin job when called from ApSIC Xbench",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
