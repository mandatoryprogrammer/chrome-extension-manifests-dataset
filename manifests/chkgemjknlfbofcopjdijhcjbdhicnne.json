{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Quick Reminder",
  "version": "0.2",
  "manifest_version": 2,
  "description": "The One-Click Reminder with Notification Center support(Mac OS X 10.8+)",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "32.png",
    "default_title": "Quick Reminder",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "qr.js"
    ]
  },
  "permissions": [
    "notifications"
  ]
}
