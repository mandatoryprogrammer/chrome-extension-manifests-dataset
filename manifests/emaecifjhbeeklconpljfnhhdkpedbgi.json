{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabArtStation",
  "version": "0.1.3",
  "description": "Display image in daily rss for artstation.com",
  "permissions": [
    "activeTab",
    "https://www.artstation.com/",
    "downloads"
  ],
  "browser_action": {
    "default_title": "TabArtStation",
    "default_icon": "res/art_station_icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "res/art_station_icon_16.png",
    "32": "res/art_station_icon_32.png",
    "48": "res/art_station_icon_64.png",
    "128": "res/art_station_icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
