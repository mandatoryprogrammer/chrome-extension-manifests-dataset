{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "3.1.4",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon/icon_16.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "options_page": "option.html",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}
