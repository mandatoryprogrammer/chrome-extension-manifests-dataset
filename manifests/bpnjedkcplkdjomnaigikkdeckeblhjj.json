{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Engagement Ring Wish List",
  "version": "1.0.3",
  "description": "The ultimate Wish List for choosing your dream ring.",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/engagement-ring-wish-list-16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/engagement-ring-wish-list-16.png",
    "48": "icons/engagement-ring-wish-list-48.png",
    "128": "icons/engagement-ring-wish-list-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
