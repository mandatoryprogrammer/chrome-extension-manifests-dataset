{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lazy Fuzzer",
  "description": "Help with fuzzing various input fields",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "ic_launcher.png"
  },
  "browser_action": {
    "default_title": "Lazy Fuzzer",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
