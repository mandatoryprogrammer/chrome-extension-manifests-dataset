{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "cookie2file",
  "version": "0.0.1.2",
  "description": "cookie2file",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "16x16.jpg",
    "default_popup": "popup.html",
    "default_title": "Save Cookie to file"
  },
  "minimum_chrome_version": "21",
  "icons": {
    "16": "16x16.jpg",
    "48": "48x48.jpg"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "16x16.jpg"
  ]
}
