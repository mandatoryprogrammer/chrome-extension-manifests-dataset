{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NetRec",
  "version": "1.2",
  "permissions": [
    "webRequest",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "record.png"
    },
    "default_popup": "popup.html"
  }
}
