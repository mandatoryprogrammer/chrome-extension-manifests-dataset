{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ToDo",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "basehtml.html",
    "default_title": "MY_TODO"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
