{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Qaz2Lat",
  "description": "Qaz2Lat: converts Qazaq text from Cyrillic to Latyn alphabet.",
  "version": "0.2",
  "browser_action": {
    "default_icon": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "scripts/main.js"
    ]
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  }
}
