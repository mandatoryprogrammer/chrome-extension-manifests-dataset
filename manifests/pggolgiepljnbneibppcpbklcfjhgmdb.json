{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": ".gif to .gifv",
  "description": "Tired of viewing gifs on Imgur? Use gifv!",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://i.imgur.com/*.gif",
    "*://www.i.imgur.com/*.gif",
    "webRequestBlocking"
  ]
}
