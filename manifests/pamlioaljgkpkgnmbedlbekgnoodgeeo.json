{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JobTab",
  "description": "Get the latest offers for jobs in your area",
  "version": "1.0.3",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://use.fontawesome.com https://cdnjs.cloudflare.com https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "storage",
    "https://authenticjobs.com/api/",
    "http://quotesondesign.com/wp-json/posts"
  ],
  "icon": {
    "16": "./assets/16.png",
    "32": "./assets/32.png"
  }
}
