{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chapatiz Textarea",
  "description": "Cette extension permet de pr√©-remplir les messages sur les forums de Chapatiz (chapatiz.com/bbs).",
  "version": "3.3",
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "48": "48.png",
    "32": "32.png",
    "16": "16.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.chapatiz.com/bbs/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
