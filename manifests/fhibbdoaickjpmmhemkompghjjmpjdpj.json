{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wrona History Menu",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "author": "≈Åukasz A.J. Wrona (http://layv.net/)",
  "description": "__MSG_description__",
  "version": "2.0.2",
  "permissions": [
    "tabs",
    "history",
    "sessions",
    "chrome://favicon/",
    "storage"
  ],
  "icons": {
    "128": "icons/history-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "browser_action": {
    "deafult_icon": {
      "19": "icons/history-19-granite.png"
    },
    "default_title": "History Menu",
    "default_popup": "popup.html"
  }
}
