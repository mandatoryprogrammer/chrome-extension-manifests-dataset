{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "nSteveBallmer",
  "version": "1.0.0",
  "description": "A Chrome extension that replace all images with Steve Ballmer. DEVELOPERS, DEVELOPERS, DEVELOPERS, DEVELOPERS!",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "menu.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "nSteveBallmer.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*.jpg"
  ]
}
