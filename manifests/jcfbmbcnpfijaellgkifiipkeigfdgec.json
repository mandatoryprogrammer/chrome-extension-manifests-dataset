{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mURLin",
  "short_name": "mURLin",
  "description": "Mearl's URL Input",
  "version": "0.2.41",
  "author": "MEarl <michael@michaelearl.me>",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "html/newtab.html"
  },
  "web_accessible_resources": [
    "css/main.css",
    "js/main.js"
  ],
  "permissions": [
    "storage"
  ]
}
