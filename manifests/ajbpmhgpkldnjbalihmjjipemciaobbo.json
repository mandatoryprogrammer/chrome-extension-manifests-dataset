{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HN current page search",
  "description": "Fetches HN threads for the current visited page",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Search HN"
  },
  "permissions": [
    "activeTab"
  ]
}
