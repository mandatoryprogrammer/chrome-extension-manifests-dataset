{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TaxaFind",
  "short_name": "TaxaFind",
  "version": "0.2.0",
  "description": "Find taxa in biodiversity databases such as GBIF, OBIS, WoRMS and iNaturalist.",
  "author": "Pieter Provoost",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "web_accessible_resources": [
    "tab.js",
    "jquery-3.3.1.min.js",
    "bootstrap.min.css",
    "handlebars-v4.0.12.js"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
