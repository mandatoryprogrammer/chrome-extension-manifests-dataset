{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Maspik",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Informed browsing for peace in Palestine.",
  "icons": {
    "128": "images/peace.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/libs/jquery-2.1.1.min.js",
        "scripts/libs/jquery.fittext.js",
        "scripts/overlay.js",
        "scripts/contentscript.js",
        "scripts/filters.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "images/peace.svg",
    "images/test.svg",
    "styles/overlay.css",
    "images/mdn-logo-sm.png",
    "options.html",
    "scripts/filters.js",
    "scripts/overlay.js",
    "css/options.css",
    "scripts/libs/jquery.fittext.js",
    "scripts/libs/jquery-2.1.1.min.js",
    "scripts/libs/jquery.fittext.js"
  ]
}
