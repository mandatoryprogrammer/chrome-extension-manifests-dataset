{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Access",
  "short_name": "Better Access",
  "description": "An easier way to perform school related tasks!",
  "background": {
    "scripts": [
      "dawgHouse.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "author": "Eli Davis",
  "content_scripts": [
    {
      "matches": [
        "https://my.msstate.edu/",
        "https://mycourses.msstate.edu/webapps/portal/*"
      ],
      "css": [
        "material.css",
        "inject.css"
      ],
      "js": [
        "inject.js"
      ]
    },
    {
      "matches": [
        "https://my.msstate.edu/cp/home/displaylogin"
      ],
      "js": [
        "forwardToSite.js"
      ]
    },
    {
      "matches": [
        "https://my.msstate.edu/web/home-community"
      ],
      "js": [
        "wrong_page_notify.js"
      ]
    },
    {
      "matches": [
        "https://my.msstate.edu/web/home-community/classroom"
      ],
      "js": [
        "classroom_main.js"
      ],
      "css": [
        "bootstrap.min.css",
        "inject.css"
      ]
    }
  ],
  "icons": {
    "16": "BB_16.png",
    "48": "BB_48.png",
    "128": "BB_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "version_name": "1.1.5-QuickOriginFix",
  "version": "1.1.5"
}
