{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EyeRelax.ru",
  "version": "1.0",
  "default_locale": "en",
  "description": "__MSG_application_description__",
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "alarms"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "browser_action": {},
  "manifest_version": 2
}
