{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Select Hover Search",
  "version": "1.0.6",
  "description": "Select → Hover → Search",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "brain.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "structure.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
