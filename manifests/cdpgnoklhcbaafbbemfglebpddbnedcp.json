{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Azure Commit Copy",
  "version": "0.0.4",
  "description": "Copy a Git commit ID, build version, and their Azure URLs with a single click!",
  "icons": {
    "16": "icons/azure-copy-16.png",
    "32": "icons/azure-copy-32.png",
    "48": "icons/azure-copy-48.png",
    "128": "icons/azure-copy-128.png"
  },
  "page_action": {
    "default_title": "Azure Commit Copy",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
