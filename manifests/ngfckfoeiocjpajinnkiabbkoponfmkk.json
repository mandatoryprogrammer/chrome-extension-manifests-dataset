{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DoubleJump",
  "version": "1.1",
  "description": "Jump forwards and backwards in videos by double-tapping.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "reload.js"
    ]
  }
}
