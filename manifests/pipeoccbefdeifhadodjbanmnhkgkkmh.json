{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_application_title__",
  "description": "__MSG_application_description__",
  "manifest_version": 2,
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "contentSript.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "include_globs": [
        "*"
      ],
      "exclude_globs": [
        "http://g.mozest.com/*",
        "http://www.google.com/reader/*",
        "http://www.google.com/*",
        "http://mail.*",
        "https://mail.*"
      ],
      "js": [
        "j.js",
        "contentSript.js"
      ]
    }
  ],
  "icons": {
    "128": "logo128.png"
  },
  "default_locale": "zh_CN",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
