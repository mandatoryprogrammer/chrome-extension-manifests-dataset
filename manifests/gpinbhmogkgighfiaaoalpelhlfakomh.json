{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/config.js",
        "js/script.js"
      ],
      "matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.1688.com/*",
        "https://*.1688.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://*.tmall.hk/*",
        "https://*.tmall.hk/*"
      ]
    }
  ],
  "description": "Công cụ Đặt hàng của nhaphangexpress.com trên trình duyệt Chrome & Cờ rôm+.",
  "manifest_version": 2,
  "name": "Công Cụ Đặt Hàng nhaphangexpress.com",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "notifications"
  ],
  "version": "1.1",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "web_accessible_resources": [
    "template/index.html",
    "css/main.css"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
