{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Crypto-Games Currency Helper",
  "description": "UI Updates by bcwfs to Crypto-Games",
  "version": "0.4",
  "manifest_version": 2,
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://www.crypto-games.net/*"
      ],
      "js": [
        "jquery-2.1.0.min.js",
        "jquery-cookie.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://www.crypto-games.net/*"
      ],
      "js": [
        "pricer.js"
      ]
    }
  ],
  "icons": {
    "16": "calculator-16.png",
    "128": "calculator-128.png"
  }
}
