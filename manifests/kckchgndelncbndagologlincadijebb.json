{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reload All Tabs",
  "description": "Reload all tabas",
  "version": "1.0.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "page": "data/background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "Icon-19.png",
      "38": "Icon-38.png"
    }
  },
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  }
}
