{
  "background": {
    "page": "asset/html/background.html"
  },
  "browser_action": {
    "default_icon": "asset/image/direct.png",
    "default_title": "Click to change the proxy"
  },
  "description": "cycle through your proxy settings",
  "homepage_url": "https://github.com/dearsatan/tunnel-switch",
  "icons": {
    "128": "asset/image/tunnel_switch.png"
  },
  "manifest_version": 2,
  "name": "TunnelSwitch",
  "options_page": "asset/html/options.html",
  "permissions": [
    "proxy",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.6"
}
