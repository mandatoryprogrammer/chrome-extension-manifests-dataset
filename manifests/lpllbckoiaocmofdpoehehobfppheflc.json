{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Toggle extension"
    }
  },
  "web_accessible_resources": [
    "shadow-root.html"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    }
  },
  "description": "Select a part of the viewport and compute the maximum contrast ratio",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "SK Contrast",
  "permissions": [
    "activeTab"
  ],
  "short_name": "SK Contrast",
  "version": "1.0.6"
}
