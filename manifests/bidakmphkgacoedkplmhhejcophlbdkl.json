{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mjc-linkedin-extension-staging",
  "description": "My Job Company LinkedIn extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "mjc-chrome",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "http://mjc-ats.vm/",
    "https://www.linkedin.com/",
    "http://www.linkedin.com/"
  ]
}
