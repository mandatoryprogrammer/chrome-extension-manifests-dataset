{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "WikiLinks",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Adds a one-sentence hover description to Wikipedia links.",
  "permissions": [
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.wikipedia.org/wiki/*"
      ],
      "js": [
        "jquery.js",
        "wiki.js"
      ]
    }
  ]
}
