{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Žiūrėk, Mama, nėra komentarų!",
  "version": "1.1",
  "description": "Pašalina nuorodas į komentarus iš delfi ir 15min.",
  "icons": {
    "48": "icons/nocomments.png",
    "96": "icons/nocomments-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.delfi.lt/*"
      ],
      "js": [
        "nocomments_delfi.js"
      ]
    },
    {
      "matches": [
        "*://*.15min.lt/*"
      ],
      "js": [
        "nocomments_15min.js"
      ]
    }
  ]
}
