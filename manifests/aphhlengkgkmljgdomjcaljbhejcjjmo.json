{
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "name": "OPSkins Key Seller",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Efficient tool to quickly sell your keys on OPSkins.",
  "version": "1.6.3",
  "author": "Brian & Poheart edit by creazy.",
  "content_scripts": [
    {
      "matches": [
        "https://opskins.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "seller.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "https://opskins.com/"
  ]
}
