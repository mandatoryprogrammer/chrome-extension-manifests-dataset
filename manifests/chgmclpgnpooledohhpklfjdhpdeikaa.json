{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mappit",
  "description": "Adds a Google Maps popup window for quick location lookup.",
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://maps.googleapis.com/"
  ],
  "web_accessible_resources": [
    "new_content_script.js",
    "bundle.js",
    "bundle.css",
    "jquery.js",
    "initMap.js"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  }
}
