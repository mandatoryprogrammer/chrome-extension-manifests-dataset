{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebsiteSequence",
  "description": "Goes through a sequence of websites with the click of a button.  Especially useful for the webcomic addict.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "browser_action": {
    "default_icon": "img/icon256.png",
    "default_title": "WebsiteSequence"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
