{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Solshal",
  "description": "The easiest way to bookmark and organize links. Be productive.",
  "version": "3.0.0",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://solshal.com/",
    "http://localhost/*",
    "bookmarks"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
