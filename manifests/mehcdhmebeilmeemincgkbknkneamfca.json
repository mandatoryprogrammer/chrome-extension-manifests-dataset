{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dot",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "Click the Dot",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery-1.11.3.min.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "img/*.png"
  ],
  "browser_action": {
    "default_title": "Dot",
    "default_icon": "head-a3.png",
    "default_popup": "home.html"
  },
  "icons": {
    "48": "icon-a3-2.png"
  }
}
