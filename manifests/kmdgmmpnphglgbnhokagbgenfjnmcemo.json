{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clean Weibo (简洁微博)",
  "version": "0.1.2",
  "description": "新浪微博（weibo.com）非官方功能增强插件，可以无限制地屏蔽推广微博，还您一个清爽干净的微博！",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png",
    "32": "icon_32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://weibo.com/*",
        "https://weibo.com/*",
        "http://*.weibo.com/*",
        "https://*.weibo.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/clean.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "manifest_version": 2
}
