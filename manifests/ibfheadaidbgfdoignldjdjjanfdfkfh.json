{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Poncho",
  "description": "Poncho is the world's BEST personalized weather forecast - now in your browser!",
  "version": "1.1.2",
  "author": "Neal Shyam",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Poncho says...",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "op.html",
  "manifest_version": 2
}
