{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit visible arrows",
  "description": "Show reddit arrows regardless of subreddit style",
  "version": "0.3.0",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon.png",
    "128": "icon128.png"
  }
}
