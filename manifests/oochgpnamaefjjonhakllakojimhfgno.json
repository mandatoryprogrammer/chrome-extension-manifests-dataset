{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Marketing Dicks",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.0",
  "description": "Replaces the marketing term 'brand' with 'dick' in relevant instances.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
