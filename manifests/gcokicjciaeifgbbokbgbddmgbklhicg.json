{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TypoCountr Plugin",
  "description": "This extension will help readers count the number of typos in an article",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "typocountr.html",
    "default_title": "Find a typo? Tweet it"
  },
  "permissions": [
    "activeTab"
  ]
}
