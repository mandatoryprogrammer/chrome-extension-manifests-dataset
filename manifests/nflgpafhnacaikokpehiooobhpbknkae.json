{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "xkcdTopframeCorrection",
  "description": "for people checking xkcd.com with OCD",
  "version": "1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.xkcd.com/*",
        "https://www.xkcd.com/*"
      ],
      "js": [
        "XKCDCSSCorrection.js"
      ]
    }
  ]
}
