{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mutual Funds Tracker",
  "description": "A chrome extension to track Indian mutual funds.",
  "version": "4.4.1",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "resources/icon.png",
