{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "testing",
  "content_scripts": [
    {
      "css": [
        "add-toolbar.css"
      ],
      "js": [
        "add-toolbar.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "youtube.js"
      ]
    }
  ]
}
