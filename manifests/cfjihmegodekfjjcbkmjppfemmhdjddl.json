{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MLBC Lookup",
  "version": "2.3",
  "description": "Look up listings & sales history for a player in MLB Champions",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "https://mlbc.app/*",
    "https://cryptoslam.io/*",
    "https://api.cryptoslam.io/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://mlbc.app/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/jh-16.png",
      "32": "images/jh-32.png",
      "48": "images/jh-48.png",
      "128": "images/jh-128.png"
    }
  },
  "icons": {
    "16": "images/jh-16.png",
    "32": "images/jh-32.png",
    "48": "images/jh-48.png",
    "128": "images/jh-128.png"
  },
  "web_accessible_resources": [
    "iframe.html",
    "iframe.js",
    "iframe.css",
    "css/bootstrap.min.css",
    "js/bootstrap.min.js",
    "js/jquery-3.3.1.slim.min.js",
    "js/popper.min.js",
    "js/datatables.min.js",
    "css/datatables.min.css"
  ],
  "manifest_version": 2
}
