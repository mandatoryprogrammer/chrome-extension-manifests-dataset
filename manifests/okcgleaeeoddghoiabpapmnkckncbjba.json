{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linkle",
  "version": "0.1.9",
  "description": "Send link to aria2 RPC",
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "linkle.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "activeTab"
  ],
  "optional_permissions": [
    "cookies",
    "http://*/",
    "https://*/"
  ],
  "short_name": "Linkle",
  "homepage_url": "https://github.com/Jimmy-Z/Linkle"
}
