{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "防止误关douban.fm",
  "description": "关闭douban.fm的时候会弹对话框确认",
  "version": "0.1.2",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "防止误关douban.fm"
  },
  "content_scripts": [
    {
      "matches": [
        "*://douban.fm/",
        "*://douban.fm/?*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ]
}
