{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redmine Ticket # URL Shortcut",
  "description": "Type into URL Bar: # followed by a Space and the Redmine Ticket number to open.",
  "version": "0.0.0.1",
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "#"
  }
}
