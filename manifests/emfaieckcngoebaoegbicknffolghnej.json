{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scrolly",
  "description": "A beautiful custom scrollbar",
  "version": "0.2",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "scrolly.css"
      ]
    }
  ]
}
