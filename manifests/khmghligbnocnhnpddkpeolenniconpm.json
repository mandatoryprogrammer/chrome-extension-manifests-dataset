{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Openure",
  "description": "Expose your Backbone views in a single click.",
  "version": "0.6.1",
  "web_accessible_resources": [
    "openure.js"
  ],
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "openure-startup.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "openure.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "openure-plugin.js"
    ]
  }
}
