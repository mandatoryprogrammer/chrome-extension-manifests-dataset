{
  "name": "ClownTube",
  "version": "0.1.5",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "description": "Entérate de los últimos videos de ClownTube antes que nadie...",
  "icons": {
    "16": "tubi16.png",
    "48": "tubi48.png",
    "128": "tubi128.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "tubi32.png"
    },
    "default_title": "ClownTube",
    "default_popup": "index.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
