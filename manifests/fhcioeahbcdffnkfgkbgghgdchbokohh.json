{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.facebook.com/",
        "*://*.facebook.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Mute words on facebook newsfeed!",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Mute Words on Facebook",
  "permissions": [
    "storage",
    "tabs",
    "*://*.facebook.com/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
