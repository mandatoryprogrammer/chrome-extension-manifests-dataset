{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabIpsum",
  "short_name": "TabIpsum - Lorem Ipsum Generator",
  "version": "1.0.1",
  "description": "Just a Lorem Ipsum generator.",
  "author": "Edgard Kozlowski <hello@edkf.com.br> (http://edkf.com.br)",
  "commands": {
    "_execute_browser_action": {}
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "24": "icon-24.png",
      "32": "icon-32.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}
