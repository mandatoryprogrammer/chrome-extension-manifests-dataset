{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "#ДАНСwithme",
  "description": "Това разширение показва предупредителен екран, когато отваряте сайт от медийната група на Делян Пеевски. #ДАНСwithme",
  "version": "0.6.5",
  "homepage_url": "http://ignorepeevski.tumblr.com/",
  "icons": {
    "16": "icon_16px.png",
    "48": "icon_48px.png",
    "128": "icon_128px.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "ignore.jpg"
  ],
  "background": {
    "scripts": [
      "background.js",
      "blocked_domains.js"
    ]
  }
}
