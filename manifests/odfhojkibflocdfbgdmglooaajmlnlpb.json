{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Skyscraper",
  "description": "Add urls and page htmls for comparison in Skyscraper app",
  "version": "1.01",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html",
    "default_title": "Comparator"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "icon2.png",
    "48": "icon2.png",
    "128": "icon2.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
