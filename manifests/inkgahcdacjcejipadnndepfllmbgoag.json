{
  "background": {
    "scripts": [
      "lib/jquery.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "lib/jquery.js",
        "lib/core.js",
        "lib/domobserver.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Auto-refresh and automated pagination of your Google Data Studio reports, on a chosen interval.",
  "icons": {
    "128": "img/favicon_full.png",
    "16": "img/favicon_16.png",
    "32": "img/favicon_32.png",
    "64": "img/favicon_64.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "46",
  "name": "Data Studio Auto Refresh",
  "page_action": {
    "default_icon": "img/favicon_full.png",
    "default_popup": "popup.html",
    "default_title": "Data Studio Auto Refresh"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://datastudio.google.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.1",
  "web_accessible_resources": [
    "/*"
  ]
}
