{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "pile",
  "description": "pile",
  "version": "0.15.1",
  "icons": {
    "16": "/content/icons/ic_library_books_black_16dp.png",
    "48": "/content/icons/ic_library_books_black_48dp_1x.png",
    "128": "/content/icons/ic_library_books_black_48dp_2x.png"
  },
  "browser_action": {
    "default_icon": "content/icons/ic_library_books_black_48dp_2x.png",
    "default_title": "pile",
    "default_popup": "list.html"
  },
  "permissions": [
    "alarms",
    "https://getpocket.com/v3/*",
    "https://www.google.com/s2/favicons/*",
    "https://img.readitlater.com/i/*"
  ],
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "settings.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "oauth.html"
  ]
}
