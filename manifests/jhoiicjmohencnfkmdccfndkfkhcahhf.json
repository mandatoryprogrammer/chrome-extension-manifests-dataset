{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "How Many Pens?",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "findreplace.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "things.json"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "storage"
  ]
}
