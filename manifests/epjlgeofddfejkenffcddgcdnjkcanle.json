{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "21.0",
  "name": "Ski Goggles",
  "version": "1.9.1",
  "description": "Inspect Web Analytic requests including Snowplow, Adobe Analytics, and more",
  "icons": {
    "16": "images/ski-goggles-16.png",
    "48": "images/ski-goggles-48.png",
    "128": "images/ski-goggles-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "devtools_page": "devtools.html",
  "homepage_url": "https://github.com/ski-goggles/ski-goggles",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "*://*/*"
  ],
  "short_name": "Ski Goggles"
}
