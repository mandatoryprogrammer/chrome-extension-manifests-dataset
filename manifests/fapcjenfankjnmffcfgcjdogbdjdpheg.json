{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Productive Time",
  "version": "1.2.2",
  "description": "Procrastination is over once and for all!",
  "permissions": [
    "storage",
    "alarms",
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "128": "images/128.png"
  },
  "options_page": "options.html",
  "author": "Alexis Morel",
  "manifest_version": 2
}
