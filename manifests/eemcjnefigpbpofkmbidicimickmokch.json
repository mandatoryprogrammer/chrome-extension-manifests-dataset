{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spilled",
  "version": "0.1",
  "description": "Detects exploitable storage of private info in cookies",
  "homepage_url": "https://github.com/PrinsFrank/Spilled",
  "background": {
    "page": "html/background.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "html/popup.html",
    "default_title": "Spilled",
    "default_icon": {
      "16": "icons/favicon-16.png",
      "32": "icons/favicon-32.png"
    }
  }
}
