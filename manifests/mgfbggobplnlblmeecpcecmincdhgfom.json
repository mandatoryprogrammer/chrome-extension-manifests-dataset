{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Custom Feeds",
  "version": "1.0",
  "description": "Categorize your subscirbed subreddits into personalized feeds.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "48": "reddit_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ]
    }
  ]
}
