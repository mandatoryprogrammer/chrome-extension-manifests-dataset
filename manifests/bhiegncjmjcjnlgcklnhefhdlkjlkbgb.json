{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "github-lgtm.user.js"
      ],
      "matches": [
        "http://github.com/*",
        "https://github.com/*"
      ]
    }
  ],
  "description": "Display LGTM on github.com",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "64": "icon-64.png"
  },
  "manifest_version": 2,
  "name": "GitHub - LGTM",
  "version": "0.6.5"
}
