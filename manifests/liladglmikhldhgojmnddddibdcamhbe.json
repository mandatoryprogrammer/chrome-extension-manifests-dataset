{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "reddiText",
  "version": "0.0.1",
  "description": "Filter the images out of reddit to focus on stories.",
  "homepage_url": "https://github.com/zzmp/reddiText",
  "author": "Zach Pomerantz <zmp@umich.edu> (garabagne.io)",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
