{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StockQ Fund Parser",
  "description": "Parse StockQ Fund Data",
  "version": "1.6.2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "default_locale": "zh_TW",
  "permissions": [
    "*://*.stockq.org/",
    "activeTab",
    "tabs",
    "storage"
  ]
}
