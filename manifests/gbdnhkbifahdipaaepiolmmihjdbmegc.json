{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiki Age",
  "description": "Specifies what age the Wikipedia subject was during given year.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.wikipedia.org/wiki/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
