{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IDX Broker Developer Tools (by imFORZA)",
  "short_name": "idxbroker-devtools-by-imforza",
  "version": "2.1.1",
  "description": "A chrome extension to help developers code for and troubleshoot for IDX Broker.",
  "homepage_url": "https://www.imforza.com",
  "background": {
    "scripts": [
      "assets/js/background.min.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/images/idx-19.png",
      "38": "assets/images/idx-48.png"
    },
    "default_popup": "popup.html",
    "default_title": "IDX Broker DevTools"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "assets/images/idx-48.png",
    "128": "assets/images/idx-128.png"
  },
  "manifest_version": 2
}
