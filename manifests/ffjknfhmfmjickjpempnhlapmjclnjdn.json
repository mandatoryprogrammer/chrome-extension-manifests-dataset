{
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "github-mac-replacer",
  "description": "Replaces github-mac link to any other url provider",
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "github-mac_128px.png"
  },
  "version": "0.1.0"
}
