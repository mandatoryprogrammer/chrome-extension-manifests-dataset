{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vk video minimizer",
  "description": "This extension automatically minimizes videos from the Vk feed, that is out of the screen scope.",
  "version": "0.0.3",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "128": "/img/icon128.png"
  },
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "js": [
        "/js/main.js"
      ]
    }
  ]
}
