{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Prohodienator",
  "description": "This extension helps spend time to useful things",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "20": "img/icon20.png",
      "128": "img/icon128.png"
    },
    "default_popup": "html/popup.html"
  },
  "icons": {
    "48": "img/icon48.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "tabs",
    "webNavigation",
    "alarms",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.1.0.min.js",
      "js/url-tld.min.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "html/addTask.html",
    "html/blockPlug.html",
    "html/list.html"
  ]
}
