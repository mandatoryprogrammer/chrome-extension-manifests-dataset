{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MTS  play  youtube inside facebook",
  "description": "Enable you to play youtube links inside facebook",
  "version": "1.0.2",
  "author": "Santa Bogdan <bogdansanta26@gmail.com>",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "short_name": "MTS"
}
