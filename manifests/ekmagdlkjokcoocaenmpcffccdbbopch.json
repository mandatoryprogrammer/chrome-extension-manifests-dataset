{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Who Owns?",
  "description": "Chrome extension that displays which company owns the current website you're on.",
  "icons": {
    "48": "icon.png"
  },
  "version": "0.1",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
