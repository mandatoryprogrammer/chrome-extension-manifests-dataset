{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copy Window URL",
  "description": "An extension to copy a chrome 'popup' window URL",
  "version": "1.2.4",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
