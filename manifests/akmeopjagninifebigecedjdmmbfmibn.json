{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "localhost",
  "short_name": "localhost",
  "version": "1.0.7",
  "description": "Convert the port after localhost to numbers",
  "omnibox": {
    "keyword": "localhost/"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
