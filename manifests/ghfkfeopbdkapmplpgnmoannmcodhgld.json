{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lightweight Tab Search",
  "version": "1.1",
  "description": "Quick way to navigate through tabs!",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/lts16.png",
      "32": "images/lts32.png",
      "64": "images/lts64.png",
      "128": "images/lts128.png"
    }
  },
  "icons": {
    "16": "images/lts16.png",
    "32": "images/lts32.png",
    "64": "images/lts64.png",
    "128": "images/lts128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+I",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  },
  "manifest_version": 2
}
