{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PlainText Offenders",
  "version": "1.0.3",
  "description": "Alert when current website stores passwords insecurely!",
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/shield_broken-16.png",
    "32": "images/shield_broken-32.png",
    "48": "images/shield_broken-48.png",
    "128": "images/shield_broken-128.png"
  },
  "manifest_version": 2
}
