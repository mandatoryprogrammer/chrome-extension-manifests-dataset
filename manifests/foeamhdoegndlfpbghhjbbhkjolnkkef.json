{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Tweetbot This"
  },
  "name": "Tweetbot This",
  "description": "Tweet Title + URL on Tweetbot for Mac",
  "homepage_url": "http://5typos.net/tweetbot-this-chrome",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.3",
  "manifest_version": 2
}
