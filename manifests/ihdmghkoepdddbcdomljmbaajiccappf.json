{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Start in Non-Pinned Tab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Opens news windows to the first non-pinned tab (like the default Firefox behavior).",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "pin_plus16.png",
    "48": "pin_plus48.png",
    "128": "pin_plus128.png"
  },
  "permissions": [
    "tabs"
  ]
}
