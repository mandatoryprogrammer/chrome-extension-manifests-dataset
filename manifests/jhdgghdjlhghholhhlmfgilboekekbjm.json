{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Order Splitter",
  "description": "This extension processes orders from orderup",
  "version": "0.6.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://orderup.com/orders/*"
      ],
      "js": [
        "./contentScript.js"
      ]
    },
    {
      "matches": [
        "*://orderup.com/orders/*"
      ],
      "js": [
        "./order.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "./order.js"
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
