{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cryptex",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "A minimal cryptocurrency monitor.",
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "page": "cryptex.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "cryptex.js"
  ],
  "chrome_url_overrides": {
    "newtab": "cryptex.html"
  }
}
