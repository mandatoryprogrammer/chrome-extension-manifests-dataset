{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MarketHide",
  "version": "1.0",
  "description": "A simple extension to hide the Facebook Marketplace tiles in the right hand column of the home page.",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.facebook.com/*",
        "https://facebook.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
