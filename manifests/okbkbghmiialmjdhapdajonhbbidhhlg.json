{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Burning Man from Facebook",
  "description": "Remove Man from Facebook Homepage",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "facebook_filter.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  }
}
