{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.7.1",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "unlimitedStorage"
  ],
  "offline_enabled": true,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/selection.js"
      ]
    }
  ]
}
