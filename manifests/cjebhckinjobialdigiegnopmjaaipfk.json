{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gitter Notifier",
  "description": "Shows Gitter unread messages count badge",
  "version": "1.3",
  "background": {
    "persistent": true,
    "scripts": [
      "vendor/moment.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/favicon-read.ico"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "alarms",
    "storage",
    "contextMenus",
    "https://api.gitter.im/"
  ]
}
