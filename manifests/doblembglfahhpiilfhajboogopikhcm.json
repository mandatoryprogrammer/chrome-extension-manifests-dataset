{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PasswordMaker",
  "version": "0.2.3",
  "description": "Generates a unique password for every site from a single master password.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "PasswordMaker",
    "default_popup": "passwordmaker.html?options=false"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "commands",
    "<all_urls>"
  ],
  "options_page": "passwordmaker.html?options=true",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+Z"
      }
    }
  },
  "manifest_version": 2
}
