{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Phone Helper",
  "version": "1.1",
  "description": "Location, timezone, and click-to-dial",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "10",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "phoneHelper.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "icon48.png"
  ],
  "permissions": [
    "*://*/*"
  ]
}
