{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Medium Megaclap",
  "short_name": "Megaclap",
  "description": "Instantly give 50 claps to awesome Medium posts.",
  "version": "1.0.0",
  "author": "Zach Caceres (@zachcaceres | zachcaceres.com)",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "https://medium.com/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "medium.com",
    "cookies"
  ]
}
