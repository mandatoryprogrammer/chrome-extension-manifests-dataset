{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "xkcd1288",
  "description": "This extension replaces words in websites based on XKCD 1288.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "xkcd1288.js"
      ],
      "run_at": "document_end"
    }
  ]
}
