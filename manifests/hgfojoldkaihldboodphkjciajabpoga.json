{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Shipit Generator",
  "version": "0.0.15",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "http://github.com/*"
      ],
      "js": [
        "libs/jQuery.min.js",
        "content.js"
      ],
