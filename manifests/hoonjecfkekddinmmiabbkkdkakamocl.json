{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "s2i",
  "version": "1.0",
  "description": "This convert a string to images for Chrome",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "spinner.css"
      ],
      "js": [
        "s2i.js"
      ],
      "run_at": "document_end"
    }
  ],
  "homepage_url": "http://midnightsuyama.org"
}
