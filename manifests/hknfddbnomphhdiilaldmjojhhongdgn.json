{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "inject.js"
      ],
      "matches": [
        "https://*.slack.com/messages/*",
        "http://*.slack.com/messages/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "index.js"
  ],
  "converted_from_user_script": true,
  "description": "Enhance Slack Grid",
  "name": "Slack Grid Plus",
  "version": "2.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
