{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AmaJSON exporter",
  "version": "0.9.2",
  "manifest_version": 2,
  "description": "When visiting 'Your Notes and Highlights' on Amazon, context-click on a page and export your notes as JSON",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus",
    "background"
  ]
}
