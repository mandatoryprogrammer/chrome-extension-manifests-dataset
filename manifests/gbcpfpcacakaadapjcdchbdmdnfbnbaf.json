{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SendToKodi",
  "short_name": "SendToKodi",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "send almost every stream from chrome to kodi with SendToKodi",
  "version": "0.0.6"
}
