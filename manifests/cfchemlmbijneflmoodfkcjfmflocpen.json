{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Refined appear.in",
  "description": "Adds useful interface to appear.in",
  "version": "0.0.2",
  "homepage_url": "https://github.com/minodisk/refined-appear.in",
  "permissions": [
    "tabs",
    "https://appear.in/*"
  ],
  "icons": {
    "128": "icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://appear.in/*"
      ],
      "js": [
        "refined-appear.in.js"
      ]
    }
  ]
}
