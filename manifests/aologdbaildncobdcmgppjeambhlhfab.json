{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "v JEB!",
  "version": "1.2",
  "description": "make your browser v JEB! https://github.com/j3ychen/v-jeb",
  "icons": {
    "128": "media/logo128.png",
    "48": "media/logo48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/chill.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
