{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "reddit inbox hush",
  "version": "0.0.3",
  "description": "A very tiny extension that allows inbox counts to be hushed on reddit.",
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "contentscript.js"
      ],
      "css": [
        "hush.css"
      ]
    }
  ]
}
