{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescr__",
  "version": "1.0.0.2",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png"
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "incognito": "split",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/myscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ]
}
