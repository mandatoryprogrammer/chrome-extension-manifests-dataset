{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Give Back",
  "description": "Give back to community without effort",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "version": "1.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon/32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-1.9.1.min.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ]
}
