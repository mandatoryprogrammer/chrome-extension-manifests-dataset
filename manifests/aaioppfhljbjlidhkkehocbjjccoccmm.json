{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Voice Text",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This app will transcribe your speech",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "client/index.js"
      ]
    }
  ],
  "background": {
    "page": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
