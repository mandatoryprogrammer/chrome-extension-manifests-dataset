{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mirror for Twitter",
  "version": "0.2",
  "description": "Use your webcam to keep others in mind while you tweet.",
  "content_scripts": [
    {
      "matches": [
        "http://*.twitter.com/*",
        "https://*.twitter.com/*"
      ],
      "js": [
        "mirror.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
