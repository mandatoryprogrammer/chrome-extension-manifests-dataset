{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_title": "Pasty",
    "default_icon": "pasty48.png",
    "default_popup": "popup.html"
  },
  "description": "Create tabs from clipboard URL list",
  "icons": {
    "128": "pasty128.png",
    "48": "pasty48.png"
  },
  "manifest_version": 2,
  "name": "Pasty",
  "version": "1.5",
  "permissions": [
    "clipboardRead"
  ],
  "options_page": "options.html"
}
