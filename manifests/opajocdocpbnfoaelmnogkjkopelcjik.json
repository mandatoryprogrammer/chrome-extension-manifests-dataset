{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BulkSMS Remaining Credits",
  "version": "1.5.9",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.ico",
    "32": "icon32.ico",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "This shows the amount of credits left on a configurable BulkSMS account.",
  "browser_action": {
    "default_icon": "icon16.ico",
    "default_title": "BulkSMS Remaining Credits",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "https://bulksms.2way.co.za/*",
    "https://bulksms.vsms.net/*",
    "https://usa.bulksms.com/*",
    "https://www.bulksms.co.uk/*",
    "https://bulksms.de/*",
    "https://bulksms.com.es/*"
  ],
  "options_page": "options.html"
}
