{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FS.UA helper",
  "version": "0.2.5",
  "description": "Watch FS.UA without hassle.",
  "homepage_url": "https://chrome.google.com/webstore/detail/",
  "minimum_chrome_version": "22",
  "content_scripts": [
    {
      "matches": [
        "http://*.to/*/view/*"
      ],
      "js": [
        "shyplayer/shyplayer.js",
        "fsplayer.js",
        "contentscript.js"
      ],
      "css": [
        "shyplayer/shyplayer.css"
      ],
      "run_at": "document_end"
    }
  ]
}
