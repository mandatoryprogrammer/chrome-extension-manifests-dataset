{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extDesc__",
  "version": "0.2",
  "default_locale": "cs",
  "permissions": [
    "contextMenus",
    "http://www.slovnik.cz/"
  ],
  "icons": {
    "16": "slovnikcz-tiny.png",
    "48": "slovnikcz-small.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
