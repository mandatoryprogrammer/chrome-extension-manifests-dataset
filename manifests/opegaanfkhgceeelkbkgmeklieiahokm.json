{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "You're still here.",
  "version": "1.1.1",
  "description": "Just a reminder every once in a while that you're still here.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery.js",
        "js/jquery.notification.js",
        "js/yourestillhere.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
