{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "zh_CN",
  "name": "Side Tabs",
  "description": "鼠标移到网页最左侧显示，Github 有 BUG，与 Stylish 插件不兼容。",
  "version": "0.2.0",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/common.js",
        "lib/jquery-3.3.1.min.js",
        "lib/velocity-2.0.0.min.js",
        "js/content.js"
      ],
      "css": [
        "css/icon.css"
      ]
    }
  ]
}
