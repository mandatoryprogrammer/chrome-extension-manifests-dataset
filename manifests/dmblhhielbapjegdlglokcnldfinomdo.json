{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kadenze Forum Modernizer",
  "manifest_version": 2,
  "version": "1.3.0",
  "description": "Enhancements to the online classroom site Kadenze: makes forum posts open inline, so you can browse them faster.",
  "page_action": {
    "default_icon": "ic_forum_black_24dp/android/drawable-xxxhdpi/ic_forum_black_24dp.png"
  },
  "icons": {
    "16": "ic_forum_black_24dp/android/drawable-mdpi/ic_forum_black_24dp.png",
    "48": "ic_forum_black_24dp/android/drawable-xhdpi/ic_forum_black_24dp.png",
    "128": "ic_forum_black_24dp/android/drawable-xxxhdpi/ic_forum_black_24dp.png"
  },
  "permissions": [
    "http://*.kadenze.com/*",
    "https://*.kadenze.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.kadenze.com/*"
      ],
      "js": [
        "jquery.js",
        "facebook_event.js"
      ],
      "css": [
        "facebook_event.css"
      ]
    }
  ]
}
