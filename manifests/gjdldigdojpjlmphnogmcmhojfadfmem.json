{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disable Download Bar",
  "version": "1.5",
  "description": "Removes the download bar at the bottom of Chrome windows (does nothing else).",
  "permissions": [
    "downloads",
    "downloads.shelf"
  ],
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "offline_enabled": true
}
