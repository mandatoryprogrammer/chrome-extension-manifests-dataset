{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Markdown Deck",
  "short_name": "mdD",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "description": "Convert Markdown file into flashcards",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "template.html",
    "fonts/*.*",
    "css/*.css",
    "js/*.js",
    "icons/*.png",
    "readme.md"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon16.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
