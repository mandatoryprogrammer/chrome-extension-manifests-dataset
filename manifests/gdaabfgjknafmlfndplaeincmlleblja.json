{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sad News Headline",
  "description": "Adds a :/ face to news headlines on the interwebz",
  "version": "1.1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*.cnn.com/",
    "http://*.bbc.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.cnn.com/*",
        "*://*.bbc.com/*"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_end"
    }
  ]
}
