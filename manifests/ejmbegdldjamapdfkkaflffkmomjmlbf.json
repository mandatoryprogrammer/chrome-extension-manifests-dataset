{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copy As HTML Entity",
  "description": "Copies the HTML Entity version of the selected Unicode characters",
  "version": "0.2",
  "permissions": [
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "manifest_version": 2
}
