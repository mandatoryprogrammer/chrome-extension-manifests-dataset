{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stop Me!",
  "description": "This extension alarms you when you spend more than a customized daily limit on a site.",
  "version": "1.0",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Stop Me"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "javascript/background.js",
      "javascript/eventTracker.js",
      "javascript/helpers.js"
    ]
  }
}
