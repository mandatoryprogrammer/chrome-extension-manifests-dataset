{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "天气预报",
  "version": "1.0",
  "description": "查看未来两周的天气情况",
  "icons": {
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon128.png"
    },
    "default_title": "天气预报",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://api.openweathermap.org/data/2.5/forecast?q=*"
  ]
}
