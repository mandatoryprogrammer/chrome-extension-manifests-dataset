{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Thr0w URL",
  "description": "Allows one to remotely control browser's location using a Thr0w server.",
  "version": "0.2.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "socket.io.js",
      "bower_components/thr0w-client/dist/thr0w-base.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
