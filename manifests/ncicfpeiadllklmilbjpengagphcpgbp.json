{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Less Wrong Comment Highlight",
  "short_name": "LWComments",
  "version": "1.1",
  "description": "Allows you to choose the time after which comments on a thread on Less Wrong are highlighted.",
  "content_scripts": [
    {
      "matches": [
        "http://lesswrong.com/lw/*",
        "http://lesswrong.com/r/*/lw/*"
      ],
      "js": [
        "lw.user.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
