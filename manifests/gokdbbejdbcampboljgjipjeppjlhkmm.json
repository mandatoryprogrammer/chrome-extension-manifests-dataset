{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Doormat",
  "short_name": "Doormat",
  "author": "GB-Labs",
  "description": "User Interface for S3 file management",
  "version": "1.3.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "downloads",
    "https://*.amazonaws.com/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_popup": "popup/doormat_popup.html",
    "default_icon": "envelope_black.png"
  },
  "icons": {
    "128": "envelopex128.png",
    "256": "envelope_black.png"
  },
  "background": {
    "scripts": [
      "lib/aws-sdk-2.215.1.min.js",
      "poller.js"
    ],
    "persistent": false
  }
}
