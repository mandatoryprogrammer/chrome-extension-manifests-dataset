{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Verse",
  "description": "New tab chrome extensions for topical verses",
  "icons": {
    "48": "theverse48.png",
    "128": "theverse128.png"
  },
  "version": "0.0.0.2",
  "permissions": [
    "management"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-2.1.4.min.js"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
