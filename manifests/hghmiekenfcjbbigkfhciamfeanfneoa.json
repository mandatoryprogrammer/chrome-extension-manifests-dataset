{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Click to open",
  "description": "Adds “click to open” to elements you can click to open",
  "version": "48.2.3",
  "icons": {
    "16": "16-on.png",
    "48": "48-on.png",
    "128": "128-on.png"
  },
  "browser_action": {
    "default_icon": "48-off.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
