{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hands off my console!",
  "version": "1.0",
  "description": "Keep the likes of Netflix and Facebook from messing with your console",
  "icons": {
    "48": "handsoff.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "handsoff.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
