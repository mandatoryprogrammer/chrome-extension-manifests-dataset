{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "XC Ski Strava",
  "version": "0.1.6",
  "description": "XC Ski Strava extension",
  "content_scripts": [
    {
      "js": [
        "xc.js"
      ],
      "matches": [
        "https://www.strava.com/dashboard"
      ]
