{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "shackertags",
  "version": "1.1",
  "minimum_chrome_version": "18",
  "manifest_version": 2,
  "description": "Add tags to chatty users.",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.shacknews.com/chatty*"
      ],
      "css": [
        "css/shackertags.css"
      ],
      "js": [
        "js/jquery.js",
        "js/knockout.js",
        "js/underscore.js",
        "js/shackertags.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "img/tag_on.png",
    "img/tag_off.png"
  ]
}
