{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What's New on GitHub",
  "version": "2.0.1",
  "description": "See what's new and what's not in your GitHub home feed.",
  "icons": {
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "{a66968ab-490c-43b9-ba9e-f9a561f13a24}"
    }
  }
}
