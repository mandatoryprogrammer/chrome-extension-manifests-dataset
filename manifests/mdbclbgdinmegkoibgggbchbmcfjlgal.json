{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Plunker Hide Banners",
  "version": "0.1.0",
  "description": "Hides ad. banners in next.plnkr.co",
  "content_scripts": [
    {
      "matches": [
        "*://*.plnkr.co/*"
      ],
      "js": [
        "plunker-hide-banners.js"
      ],
      "css": [
        "plunker-hide-banners.css"
      ]
    }
  ]
}
