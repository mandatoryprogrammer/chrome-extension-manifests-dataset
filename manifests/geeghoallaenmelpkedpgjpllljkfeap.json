{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter Volume Control",
  "version": "1.3.1",
  "description": "Adds a volume slider to twitter videos - TWITTER HAS A NATIVE ONE NOW",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/i/videos/*"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
