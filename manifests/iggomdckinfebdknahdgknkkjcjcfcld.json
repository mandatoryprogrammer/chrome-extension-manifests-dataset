{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UBC-RMP",
  "version": "1.0.9",
  "description": "Shows UBC professor's Rate My Prof ratings on SSC",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://courses.students.ubc.ca/cs/*"
      ],
      "css": [
        "src/css/contentStyle.css"
      ],
      "js": [
        "src/js/vendor/tippy_all.min.js",
        "src/js/contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/js/*",
    "src/data/*"
  ],
  "icons": {
    "16": "images/16logo.png",
    "32": "images/32logo.png",
    "48": "images/48logo.png",
    "128": "images/128logo.png"
  },
  "manifest_version": 2
}
