{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NotesOnGit",
  "short_name": "NoteonGit",
  "description": "Use this extension to take notes and save your links to your GitHub repo.",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "scripts/popup.js"
    ]
  },
  "icons": {
    "16": "icons/standard-16.png",
    "48": "icons/standard-48.png",
    "64": "icons/standard-64.png",
    "128": "icons/standard-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Notes on GitHub",
    "default_icon": "icons/standard-16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "homepage_url": "https://github.com/bishoyh/notes-on-git",
  "options_page": "options.html"
}
