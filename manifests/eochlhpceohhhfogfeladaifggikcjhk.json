{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Scroll",
  "version": "0.2.3",
  "description": "Auto Scroll. Google Reader works!",
  "icons": {
    "48": "autoscroll.png",
    "128": "autoscroll.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "",
    "default_icon": "autoscroll.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jp_ddo_dekuyou_autoscroll.js"
      ]
    }
  ]
}
