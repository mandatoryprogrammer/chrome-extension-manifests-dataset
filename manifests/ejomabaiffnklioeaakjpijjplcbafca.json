{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ニコニコ音量保存くん",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.nicovideo.jp/watch/*"
      ],
      "js": [
        "js/jquery-2.1.4.min.js",
        "js/nicovs.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
