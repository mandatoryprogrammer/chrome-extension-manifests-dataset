{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Haxball Roomlist",
  "description": "Fetches the haxball roomlist",
  "version": "2.0.0",
  "browser_action": {
    "default_popup": "build/popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "geolocation",
    "http://www.haxball.com/"
  ]
}
