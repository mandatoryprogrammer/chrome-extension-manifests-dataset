{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "uncommentedHN",
  "version": "1.0.0",
  "description": "Removes comments from Hacker News",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://news.ycombinator.com/*"
      ],
      "css": [
        "hideComments.css"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
