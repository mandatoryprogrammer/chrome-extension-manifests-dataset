{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wonder to Wunder",
  "version": "1.0",
  "description": "Replaces 'wonder' with 'wunder' and 'ly' with 'lee'",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
