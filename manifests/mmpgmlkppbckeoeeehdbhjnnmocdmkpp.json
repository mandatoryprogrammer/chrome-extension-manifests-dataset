{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pragma Header",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Pragma Header",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "ligth.png",
    "default_title": "Pragma Header"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "light_128.png",
    "64": "light_64.png",
    "16": "ligth.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  }
}
