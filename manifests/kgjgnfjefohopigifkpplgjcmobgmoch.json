{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Handy Handbook Tool",
  "description": "Improves the user experience of the University of Melbourne handbook",
  "version": "1.2.2",
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://handbook.unimelb.edu.au/*",
        "https://handbook.unimelb.edu.au/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "loading-overlay.min.js",
        "helpers.js",
        "contentscript.js"
      ]
    }
  ]
}
