{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "imgur-folders",
  "description": "Folders for Imgur",
  "version": "0.0.2",
  "content_scripts": [
    {
      "matches": [
        "http://imgur.com/*",
        "https://imgur.com/*"
      ],
      "css": [
        "style/jquery-ui.min.css",
        "style/imgur-folders.css",
        "style/nanoscroller.css"
      ],
      "run_at": "document_end",
      "js": [
        "lib/jquery.min.js",
        "lib/jquery-ui.min.js",
        "lib/treo.min.js",
        "lib/jquery.nanoscroller.min.js",
        "src/Folder.js"
      ]
    }
  ]
}
