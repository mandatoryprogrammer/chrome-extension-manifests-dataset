{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Variable Cage",
  "short_name": "vCage",
  "description": "Replace arbitrary images with pictures of Nicolas Cage. Created by @jacksquaredson, with thanks to @davecowart.",
  "icons": {
    "128": "icon.png"
  },
  "version": "1.1",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "replace_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
