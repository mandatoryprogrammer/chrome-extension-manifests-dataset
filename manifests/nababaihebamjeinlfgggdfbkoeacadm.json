{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BetterSN",
  "short_name": "bettersn",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://tdp.brusselsairlines.com/*"
      ],
      "js": [
        "js/jQuery.js",
        "js/jquery.jeditable.mini.js",
        "js/moment-with-locales.min.js",
        "js/background.js"
      ],
      "css": [
        "css/BetterSN.css"
      ]
    }
