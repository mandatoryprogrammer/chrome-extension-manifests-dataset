  "web_accessible_resources": [
    "i-calendar-legend-low.png",
    "i-calendar-legend-lower.png",
    "spr-prev-next-week.png"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
