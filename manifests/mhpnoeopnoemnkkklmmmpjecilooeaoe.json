{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hue Tab",
  "description": "A customizable HSL color picker on every new tab page",
  "version": "0.0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "offline_enabled": true,
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "incognito": "split",
  "background": {
    "scripts": [
      "newtab.js"
    ],
    "persistent": true
  }
}
