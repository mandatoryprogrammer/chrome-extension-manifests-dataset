{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gmail Quick Copy Email",
  "description": "Scans gmail for emails to copy and add to the clipboard",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "jquery-3.4.1.slim.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
