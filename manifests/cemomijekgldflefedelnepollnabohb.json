{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "USA 2 Georgia Calculator",
  "version": "0.1.2",
  "description": "Calculates shipping cost for USA 2 Georgia",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.amazon.com/*"
      ],
      "js": [
        "u2g_content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "dollar19.png",
      "38": "dollar.png"
    },
    "default_name": "Shipping Weight",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
