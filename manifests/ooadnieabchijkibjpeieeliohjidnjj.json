{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "네이버 동영상 플러그인",
  "version": "1.0.1.8",
  "description": "네이버 동영상 서비스 이용을 위한 확장 프로그램",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.naver.com/*",
        "http://*.nhncorp.com/*",
        "http://*.navercorp.com/*",
        "https://*.naver.com/*",
        "https://*.nhncorp.com/*",
        "https://*.navercorp.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "http://*.naver.com/*",
    "http://*.nhncorp.com/*",
    "http://*.navercorp.com/*",
    "https://*.naver.com/*",
    "https://*.nhncorp.com/*",
    "https://*.navercorp.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://*.naver.com/*",
      "http://*.nhncorp.com/*",
      "http://*.navercorp.com/*",
      "https://*.naver.com/*",
      "https://*.nhncorp.com/*",
      "https://*.navercorp.com/*"
    ]
  }
}
