{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Show a QR-Code for the current URL",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "SK QR-code",
  "offline_enabled": true,
  "permissions": [
    "tabs"
  ],
  "short_name": "SK QR-code",
  "version": "1.0.1"
}
