{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cloud To Other People's Computers",
  "version": "1.0",
  "description": "Replaces the text 'the cloud' with 'other people's computers'.",
  "short_name": "Cloud to Other",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
