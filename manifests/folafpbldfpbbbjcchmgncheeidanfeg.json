{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snippet",
  "version": "1.04",
  "description": "Fulfill your curiosity cravings",
  "author": "Amanda Lee & Dustin Pearson",
  "permissions": [
    "tabs",
    "storage",
    "https://od-api.oxforddictionaries.com/*"
  ],
  "background": {
    "page": "content.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/snippet_16.png",
      "32": "images/snippet_32.png",
      "48": "images/snippet_48.png",
      "128": "images/snippet_128.png"
    }
  },
  "icons": {
    "16": "images/snippet_16.png",
    "32": "images/snippet_32.png",
    "48": "images/snippet_48.png",
    "128": "images/snippet_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "content.html"
  }
}
