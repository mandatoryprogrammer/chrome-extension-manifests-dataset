{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GeekComicViewer",
  "short_name": "GCViewer",
  "version": "2.8.0",
  "description": "Minimalistic viewer for XKCD and CommitStrip comics",
  "manifest_version": 2,
  "author": "cybor97",
  "permissions": [
    "https://cors-anywhere.herokuapp.com/*",
    "https://*.xkcd.com/*",
    "https://*.smbc.com",
    "https://*.commitstrip.com/*",
    "http://*.commitstrip.com/*",
    "http://*.dilbert.com/*",
    "https://*.dilbert.com/*"
  ],
  "icons": {
    "225": "icons/xkcd225.png",
    "128": "icons/xkcd128.png",
    "64": "icons/xkcd64.png",
    "48": "icons/xkcd48.png",
    "32": "icons/xkcd32.png",
    "16": "icons/xkcd16.png"
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": {
      "225": "icons/xkcd225.png",
      "128": "icons/xkcd128.png",
      "64": "icons/xkcd64.png",
      "48": "icons/xkcd48.png",
      "32": "icons/xkcd32.png",
      "16": "icons/xkcd16.png"
    }
  }
}
