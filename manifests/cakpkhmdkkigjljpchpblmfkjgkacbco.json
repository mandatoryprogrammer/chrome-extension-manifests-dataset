{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Back to Top",
  "description": "Scroll to the top of the page. No animation. Minimal memory usage.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icon.png"
    }
  }
}
