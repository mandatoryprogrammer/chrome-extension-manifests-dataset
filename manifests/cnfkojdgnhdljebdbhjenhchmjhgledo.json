{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Birp Downloader",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Download BRIP! tracks or whole playlists",
  "default_locale": "en",
  "icons": {
    "128": "media/birp.fm.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.birp.fm/playlist/*/*"
      ],
      "js": [
        "src/zip/jszip.js",
        "src/save/FileSaver.js",
        "src/inject/download.js"
      ]
    }
  ]
}
