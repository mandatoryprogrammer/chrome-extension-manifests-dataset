{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Potentially Dangerous File Extension Blocker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Block potentially danerous files from being downloaded.",
  "background": {
    "scripts": [
      "/js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "/img/icon128.png"
  },
  "permissions": [
    "downloads"
  ]
}
