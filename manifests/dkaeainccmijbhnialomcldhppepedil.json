{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bert.js",
      "bertview.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "clipboardWrite",
    "<all_urls>",
    "declarativeWebRequest"
  ],
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "description": "Pretty-prints BERT data in the browser.",
  "name": "BERTView",
  "version": "0.3.1",
  "minimum_chrome_version": "30.0",
  "web_accessible_resources": [
    "default.css"
  ]
}
