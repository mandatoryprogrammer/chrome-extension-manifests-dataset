{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GateKeeper",
  "description": "Filter your Facebook front page so you'll only see the things you want to see.",
  "version": "1.0.8",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "js/jquery-2.1.4.min.js",
        "js/bundle.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "hashtag.png"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*.facebook.com/*",
    "http://906a26ef48081c60b08f-58945f604110abde655fc3bf34312b67.r15.cf5.rackcdn.com/trends.json"
  ]
}
