{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "tagatab",
  "description": "Control your tabs",
  "version": "1.0.4",
  "browser_action": {
    "default_icon": "img/tat.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "webNavigation",
    "storage",
    "http://*/*.ico",
    "https://*/*.ico"
  ],
  "omnibox": {
    "keyword": "tat"
  },
  "icons": {
    "16": "img/tat.png",
    "32": "img/tat.png",
    "64": "img/tat_128.png",
    "128": "img/tat_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tat.html"
  },
  "web_accessible_resources": [
    "img/*",
    "fonts/*"
  ]
}
