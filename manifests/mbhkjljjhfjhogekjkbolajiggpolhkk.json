{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "zzzap.it",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "zzzap.it | Send music and pictures to your phone",
  "browser_action": {
    "default_icon": "icon_19x19.png",
    "default_popup": "popup.html",
    "default_title": "zzzap.it"
  },
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
