{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Django Doc Helper",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Django Doc Quixk switch to desired version",
  "content_scripts": [
    {
      "matches": [
        "https://docs.djangoproject.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": "background.html",
  "options_page": "options.html"
}
