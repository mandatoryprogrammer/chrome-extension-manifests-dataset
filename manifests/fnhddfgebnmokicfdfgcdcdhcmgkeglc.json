{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WayFarer+",
  "description": "Enables you to use NianticLab's Wayfarer in the way you want!",
  "version": "1.14.9.1",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "settings/popup.html",
    "default_title": "WayFarer+"
  },
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://wayfarer.nianticlabs.com/*"
      ],
      "js": [
        "js/styler.js",
        "js/mods/nominations.js",
        "js/mods/review.js",
        "js/mods/profile.js",
        "js/mods/header.js",
        "js/mods/all.js",
        "settings/settingsManager.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "pageJs/libs/*.js",
    "pageJs/*.js",
    "assets/*"
  ]
}
