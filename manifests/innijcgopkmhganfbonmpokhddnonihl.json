{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BreakTime!",
  "version": "1.0",
  "description": "Always forgetting to take a break? Use this extension to remind you when you should be taking a break!",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "presistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
