{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.2",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "short_name": "TOI",
  "icons": {
    "16": "icon-16.png",
    "19": "icon-19.png",
    "38": "icon-38.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://twitter.com/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "toi-icon.png"
  ]
}
