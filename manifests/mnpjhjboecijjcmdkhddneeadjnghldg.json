{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Weiboer",
  "version": "0.0.3",
  "description": "围脖儿，微博浏览插件",
  "content_scripts": [
    {
      "matches": [
        "https://weibo.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
