{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MODS SAES",
  "version": "0.70",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "minimum_chrome_version": "18",
  "default_locale": "es",
  "icons": {
    "16": "/css/fu_16.png",
    "48": "/css/fu_48.png",
    "128": "/css/fu_128.png"
  },
  "permissions": [
    "tabs",
    "*://*.ipn.mx/*",
    "*://*.google.com/*",
    "storage",
    "unlimitedStorage"
  ],
  "homepage_url": "https://www.facebook.com/ComplementoSaesChrome",
  "options_page": "opciones.html",
  "background": {
    "scripts": [
      "segundoPlano.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "css/menos.png",
    "css/1.jpg",
    "css/2.jpg",
    "css/conf.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.ipn.mx/PDF/Alumnos/Reinscripciones/*"
      ],
      "js": [
        "sec.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://*.ipn.mx/*"
      ],
      "css": [
        "estilo.css"
      ],
      "js": [
        "jsX.js"
      ],
      "run_at": "document_end"
    }
  ]
}
