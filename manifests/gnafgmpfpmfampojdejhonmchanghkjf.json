{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tweetchan",
  "version": "1.0",
  "description": "Redact names, usernames, like counts, and retweet counts from your Twitter feed",
  "permissions": [
    "tabs",
    "*://*.twitter.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "anonymize.js"
      ],
      "css": [
        "anonymize.css"
      ],
      "matches": [
        "https://twitter.com/"
      ]
    }
  ],
  "manifest_version": 2
}
