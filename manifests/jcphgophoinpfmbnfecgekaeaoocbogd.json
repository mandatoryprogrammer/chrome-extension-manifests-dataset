{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WeBWorK MathView",
  "version": "1.3",
  "description": "Preview properly formatted math equations as you type them into WeBWorK answer boxes!",
  "icons": {
    "16": "icon/icon_16.png",
    "32": "icon/icon_32.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "ext-config.js",
      "event-page.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "katex/*",
    "katex/fonts/*",
    "asciimath-based/*",
    "math-view.js",
    "math-view.css"
  ],
  "permissions": [
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "optional_permissions": [
    "tabs",
    "<all_urls>"
  ]
}
