{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vetary.com Chrome Extension",
  "short_name": "Vetary.com",
  "description": "Access your Vetary Practice Dashboard to refer clients, track financing applications, edit your veterinary practiceâ€™s profile & more",
  "version": "2.0.8",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Launch Vetary.com"
  },
  "background": {
    "scripts": [
      "js/all-vendor.min.js",
      "js/all.min.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.vetary.com/*",
        "https://*.vetary.tech/*"
      ],
      "js": [
        "js/injected/all.min.js"
      ]
    }
  ],
  "icons": {
    "16": "img/vetary-icon-16.png",
    "48": "img/vetary-icon-48.png",
    "128": "img/vetary-icon-128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "background",
    "alarms",
    "cookies",
    "https://*.vetary.com/*"
  ]
}
