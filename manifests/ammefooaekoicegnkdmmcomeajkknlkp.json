{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pwinty Dashboard",
  "description": "Admin page for pwinty merchants",
  "version": "0.0.7",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "permissions": [
    "https://sandbox.pwinty.com/*",
    "https://api.pwinty.com/*"
  ]
}
