{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Steam Offers",
  "description": "List all Steam Offers so you can see it anywhere, and also a search for your favorite game!",
  "version": "1.3.3",
  "manifest_version": 2,
  "icons": {
    "128": "./images/128.png"
