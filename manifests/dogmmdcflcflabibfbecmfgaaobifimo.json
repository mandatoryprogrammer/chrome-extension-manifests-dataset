{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarker",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Bookmarker is a simple Chrome extension to search bookmarks.",
  "omnibox": {
    "keyword": "bb"
  },
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
