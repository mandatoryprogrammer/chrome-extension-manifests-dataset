{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HTML5 media speed controller + subs",
  "description": "Speed controls and subtitles for HTML5 media.",
  "options_ui": {
    "page": "options.html"
  },
  "manifest_version": 2,
  "version": "1.42.7",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "balancetext.min.js",
        "speedcontrol.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "fontSettings",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
