{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Git Fu for Github™",
  "version": "3.2.1.11",
  "manifest_version": 2,
  "description": "A feature set that attempts to aid you in everyday tasks you experience when using Github™ issues",
  "key": "",
  "browser_action": {
    "default_icon": "github-128.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "github-128.png",
    "48": "github-128.png",
    "16": "github-128.png"
  },
  "permissions": [
    "webRequest",
    "identity",
    "https://github.com/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "index.js",
      "ajaxer.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "animate.css",
        "main.css"
      ],
      "js": [
        "jquery.min.js",
        "teacup.js",
        "async.min.js",
        "chart.min.js",
        "colors.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}
