{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Not Now - Close distracting tabs instantly",
  "version": "1.3",
  "description": "A productivity tool that prevents you from accessing distracting pages",
  "icons": {
    "16": "./images/icon16.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ],
  "manifest_version": 2
}
