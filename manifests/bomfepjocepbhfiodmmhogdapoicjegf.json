{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Luke's reddit on Youtubeâ„¢",
  "description": "Adds reddit comments to youtube videos",
  "version": "0.28",
  "permissions": [
    "http://*.youtube.com/*",
    "https://*.youtube.com/*",
    "http://www.reddit.com/*",
    "https://www.reddit.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "css": [
        "reddit.css",
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "images/loading-slide.gif"
  ],
  "manifest_version": 2
}
