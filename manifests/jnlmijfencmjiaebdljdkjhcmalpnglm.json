{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HN - Block NSA content",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Bored of the buzz about NSA and PRISM? Block that news.",
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
