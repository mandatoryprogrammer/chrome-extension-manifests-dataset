{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Apparatus",
  "author": "Omri",
  "description": "Apparatus is an open source bookmark and collaboration tool.",
  "version": "0.2.5",
  "icons": {
    "16": "images/icon.png",
    "48": "images/image.png",
    "128": "images/tile.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Apparatus",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*"
  ]
}
