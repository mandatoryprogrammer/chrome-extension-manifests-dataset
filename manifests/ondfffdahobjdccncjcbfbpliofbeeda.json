{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CryptoDash",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Easiest way to check cryptocurrency prices from anywhere in chrome.",
  "homepage_url": "http://dillionverma.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "CryptoDash",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "chrome://favicon/",
    "storage"
  ]
}
