{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TIMER",
  "short_name": "TMR",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "demonstrative.mp3"
  ],
  "permissions": [
    "<all_urls>"
  ]
}
