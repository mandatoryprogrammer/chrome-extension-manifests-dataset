{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Eric Tang",
  "name": "Save Point",
  "description": "Save Your Current Tabs",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Save Point",
    "default_icon": "images/save-point-icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/save-point-icon.png"
  },
  "manifest_version": 2
}
