{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bloqueador Ley Propiedad Intelectual",
  "short_name": "Bloqueador LPI",
  "version": "2.0",
  "description": "Bloquea las paginas de aquellos medios que apoyan la Ley de Propiedad Intelectual",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "list.js",
      "check_urls.js"
    ]
  },
  "web_accessible_resources": [
    "blocked_url.html"
  ]
}
