{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "chefboard.",
  "description": "Save recipe link and details to your webapp",
  "version": "1.5.5",
  "browser_action": {
    "default_icon": "chefboardicon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "https://chefboard.herokuapp.com/",
    "https://chefboard.herokuapp.com/*",
    "http://chefboard.herokuapp.com/",
    "http://chefboard.herokuapp.com/*",
    "identity"
  ]
}
