{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Feedly",
  "description": "Feedly Cleanup",
  "version": "0.6.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.feedly.com/*",
        "*://feedly.com/*"
      ],
      "js": [
        "feedly.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ]
}
