{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Github Report",
  "description": "View Quick report about any github user in their github profile!",
  "version": "0.0.1",
  "short_name": "gitreport",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*.github.com/*",
    "storage",
    "notifications",
    "https://osrc.dfm.io/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "js": [
        "jquery-2.1.0.min.js",
        "cs.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "resources/logo128x128.png",
    "48": "resources/logo48x48.png"
  },
  "web_accessible_resources": [
    "inject.js",
    "resources/logo.png"
  ],
  "background": {
    "matches": [
      "*://*.github.com/*"
    ],
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
