{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Search Page Enhancer",
  "description": "This extension removes the \"More\" option and adds all entries to the bar.",
  "version": "0.1",
  "content_scripts": [
    {
      "js": [
        "jquery-1.6.4.min.js",
        "enhance.user.js"
      ],
      "matches": [
        "*://www.google.de/*",
        "*://www.google.com/*",
        "*://google.de/*",
        "*://google.com/*"
      ]
    }
  ],
  "permissions": [
    "*://www.google.de/*",
    "*://www.google.com/*",
    "*://google.de/*",
    "*://google.com/*"
  ]
}
