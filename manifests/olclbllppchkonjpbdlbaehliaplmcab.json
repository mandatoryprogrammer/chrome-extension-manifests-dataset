{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PubMed Altmetric",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Replaces journal names with Altmetric scores in PubMed search results",
  "content_scripts": [
    {
      "matches": [
        "*://www.ncbi.nlm.nih.gov/pubmed/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
