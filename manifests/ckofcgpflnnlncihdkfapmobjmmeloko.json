{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "typegator",
  "description": "Navigate web pages, tabs and windows by typing",
  "version": "0.3.1",
  "author": "Tony Findeisen",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Start the type navigation"
    }
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon/16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
