{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Printer",
  "description": "Print currently displayed page on GitHub with beautiful format",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Print GitHub page",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
