{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabs Manager",
  "version": "2.3",
  "description": "Quick overview for your opened tabs in different windows, with the ability to search and check thumbnails for each tab in one view",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/js/lib/lunr.min.js",
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/img/icon_128.png",
    "default_title": "Tabs Manager",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "/img/icon_16.png",
    "48": "/img/icon_48.png",
    "128": "/img/icon_128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Ctrl+Shift+Q"
      },
      "description": "Open Tabs manager on search mode"
    }
  }
}
