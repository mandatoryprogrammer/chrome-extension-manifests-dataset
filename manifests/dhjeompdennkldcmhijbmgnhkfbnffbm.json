{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Avenger Protector",
  "version": "1.0",
  "description": "Obscure all words on a page that could contain references to the avengers end game",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/anarchy16.png",
      "32": "assets/anarchy32.png",
      "48": "assets/anarchy48.png",
      "128": "assets/anarchy128.png"
    }
  },
  "icons": {
    "16": "assets/anarchy16.png",
    "32": "assets/anarchy32.png",
    "48": "assets/anarchy48.png",
    "128": "assets/anarchy128.png"
  },
  "manifest_version": 2
}
