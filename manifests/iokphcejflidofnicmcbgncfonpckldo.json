{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "kds_disabler",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Disable the KDS stupid auth",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://kds.kek.jp/*",
    "https://kds.kek.jp/",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
