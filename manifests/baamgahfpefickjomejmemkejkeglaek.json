{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension Manager",
  "description": "Extension that manages other extensions",
  "version": "0.0.0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_script": "popup.js"
  },
  "permissions": [
    "activeTab",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+L"
      }
    }
  }
}
