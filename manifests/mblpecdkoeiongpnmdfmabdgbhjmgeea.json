{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Radio Wnet Player",
  "description": "Rozszerzenie do słuchania Radia Wnet online. Lista ostatnich poranków z możliwością odsłuchania",
  "short_name": "Radio Wnet Player",
  "version": "4.0.0",
  "author": "Grzegorz Przeździecki",
  "homepage_url": "http://www.radiownet.pl/",
  "options_page": "informacje.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon-32.png",
    "default_popup": "panel.html"
  },
  "permissions": [
    "http://www.radiownet.pl/wnetplayer/MobileAppApi/",
    "https://ssl.google-analytics.com/"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
