{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmarky",
  "version": "0.0.7.1",
  "permissions": [
    "bookmarks",
    "tabs",
    "clipboardWrite"
  ],
  "description": "Lists Bookmarks",
  "icons": {
    "16": "images/b32.png",
    "48": "images/bookmarkie48.png",
    "128": "images/bookmarkie128.png"
  },
  "browser_action": {
    "default_icon": "images/b32.png",
    "default_title": "Bookmarky",
    "default_popup": "popup.html"
  }
}
