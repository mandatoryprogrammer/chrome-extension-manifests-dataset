{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KSL Cars Plus",
  "description": "Adds useful info to KSL Cars listings.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "https://api.edmunds.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.ksl.com/auto/listing*"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ]
}
