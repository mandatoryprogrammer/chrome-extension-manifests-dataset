{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Chrome Event Logger",
  "version": "0.0.77",
  "description": "Log all chrome.* API events.",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "alarms",
    "bookmarks",
    "cookies",
    "contextMenus",
    "debugger",
    "fileBrowserHandler",
    "fontSettings",
    "history",
    "idle",
    "input",
    "management",
    "proxy",
    "storage",
    "ttsEngine",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  }
}
