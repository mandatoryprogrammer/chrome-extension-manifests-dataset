{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jira assign to reporter",
  "description": "Adds 'Assign to reporter' button to Jira tickets",
  "version": "1.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
