{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "XIMGDOWN",
  "description": "Chrome Extension to download all images on any webpage by XSHERU",
  "manifest_version": 2,
  "minimum_chrome_version": "16.0.884",
  "version": "0.0.0.1",
  "permissions": [
    "tabs",
    "downloads",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icon/ximgdown_logo_16.png",
    "32": "images/icon/ximgdown_logo_32.png",
    "64": "images/icon/ximgdown_logo_64.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
