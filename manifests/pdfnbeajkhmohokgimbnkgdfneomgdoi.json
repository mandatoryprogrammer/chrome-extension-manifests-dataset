{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GDL Lower Third",
  "description": "Lower Third Creator for GDL",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.9.8",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "storage",
    {
      "fileSystem": [
        "write"
      ]
    }
  ]
}
