{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shade cloth",
  "description": "Hides other interviewers' feedback on Greenhouse before you enter your own.",
  "version": "0.0.3",
  "web_accessible_resources": [
    "images/*.png"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://app.greenhouse.io/people/*",
        "*://app.greenhouse.io/guides/*/people/*",
        "*://app.greenhouse.io/scorecards/*"
      ],
      "css": [
        "shade-cloth.css"
      ],
      "js": [
        "jquery.js",
        "shade-cloth.js"
      ]
    }
  ]
}
