{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "BetterUserVoice",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Improves UserVoice Admin UI",
  "icons": {
    "128": "Icon-112.png",
    "48": "Icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.uservoice.com/*"
      ],
      "css": [
        "BetterUserVoice.css"
      ],
      "js": [
        "jquery.js",
        "BetterUserVoice.js"
      ]
    }
  ]
}
