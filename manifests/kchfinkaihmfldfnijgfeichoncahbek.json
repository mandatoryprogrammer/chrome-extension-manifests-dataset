{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eyes on Things",
  "description": "Add an eye to any page",
  "version": "1.4",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Add an eye.",
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "draggabilly.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
