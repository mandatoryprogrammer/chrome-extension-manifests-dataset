{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "backgroundSendToServer.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "image/icon_128.png",
    "default_popup": "popup.html",
    "default_title": "Get Login Code AutoCI"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "description": "Công cụ hỗ trợ lấy mã đăng nhập tài khoản AutoCI.",
  "icons": {
    "128": "image/icon_128.png",
    "16": "image/icon.png",
    "48": "image/icon_48.png"
  },
  "manifest_version": 2,
  "name": "Get Login Code AutoCI",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "webRequest",
    "https://*/",
    "webRequestBlocking",
    "webNavigation",
    "notifications",
    "identity",
    "gcm"
  ],
  "version": "1.2"
}
