{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Paper Auto Show TOC",
  "description": "Paper Auto Show TOC",
  "version": "1.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "paper-auto-show-toc",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://paper.dropbox.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
