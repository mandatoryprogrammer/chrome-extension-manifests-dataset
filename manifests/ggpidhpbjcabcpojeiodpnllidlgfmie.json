{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "files/icon-offline.png",
    "default_popup": "popup.html"
  },
  "description": "Powiadamiacz, który pozwoli Ci być na bieżąco ze streamem Emila emsc2.tv",
  "homepage_url": "http://www.emsc2.tv/",
  "icons": {
    "128": "files/icon-128.png",
    "48": "files/icon-48.png"
  },
  "manifest_version": 2,
  "name": "Powiadamiacz EmSc2.Tv",
  "permissions": [
    "http://api.emsc2.tv/*",
    "http://tmi.twitch.tv/*",
    "https://api.twitch.tv/*",
    "https://use.fontawesome.com/releases/v5.3.1/css/all.css",
    "https://client-event-reporter.twitch.tv/v1/stats",
    "notifications",
    "storage",
    "alarms"
  ],
  "version": "2.0.1",
  "web_accessible_resources": [
    "files/avatar.jpg"
  ],
  "content_security_policy": "script-src 'self' https://*.jquery.com; object-src 'self'"
}
