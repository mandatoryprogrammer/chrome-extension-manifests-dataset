{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shiawase Factors",
  "short_name": "ShiawaseFactors",
  "description": "Performs automatic actions for Success Factors",
  "version": "0.3.0",
  "icons": {
    "16": "icons/48.png",
    "48": "icons/48.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/xml2json.min.js",
      "src/utils.js",
      "src/index.js"
    ]
  },
  "options_page": "src/options.html"
}
