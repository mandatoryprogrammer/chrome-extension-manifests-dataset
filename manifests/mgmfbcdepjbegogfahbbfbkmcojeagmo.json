{
  "background": {
    "scripts": [
      "jquery-2.0.0.js",
      "granted_background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "granted.css"
      ],
      "js": [
        "jquery-2.0.0.js",
        "granted.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Reorders reddit comments, so the mirror is on top of the comments",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "EasyMirror",
  "permissions": [
    "tabs",
    "http://*/*",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
