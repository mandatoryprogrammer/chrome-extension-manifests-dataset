{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spotify Web Player Toggler",
  "description": "This extension switches tabs between Spotify's Web Player and the most recently open tab.",
  "version": "0.1.2",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/19.png"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "commands": {
    "toggle-webplayer": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Toggle the Web Player."
    }
  }
}
