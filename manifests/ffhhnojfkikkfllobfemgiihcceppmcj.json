{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Image Inverter",
  "description": "This extension inverts all images on a website. This is meant to be used together with screen inverting that comes with OS X",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "name": "Click to invert all images"
  },
  "icons": {
    "19": "icon19.png",
    "26": "icon26.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
