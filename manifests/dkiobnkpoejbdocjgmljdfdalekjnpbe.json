{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Twitter Follower Counters",
  "description": "Get rid of the followers/following counters on Twitter.",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "css": [
        "hide-counters.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
