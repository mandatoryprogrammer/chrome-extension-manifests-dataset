{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "V2EX Blocker",
  "description": "Block everything you don't like on V2EX",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "http://*.v2ex.com/*",
    "https://*.v2ex.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.v2ex.com/*",
        "https://*.v2ex.com/*"
      ],
      "js": [
        "js/jquery-2.0.3.min.js",
        "js/block.js"
      ],
      "all_frames": false
    }
  ]
}
