{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_exstentionName__",
  "description": "__MSG_exstentionDescription__",
  "version": "2.5",
  "default_locale": "ru",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "vk_inject.js"
      ],
      "css": [
        "vk_styles.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://vk.com/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "Чистка",
    "default_icon": "icons/48x48.png",
    "default_popup": "popup.html"
  }
}
