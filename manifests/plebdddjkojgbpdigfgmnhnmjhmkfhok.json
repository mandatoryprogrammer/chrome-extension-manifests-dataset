{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PikaBooster",
  "version": "0.2.5",
  "description": "Расширение, улучшающее возможности сайта Pikabu.ru",
  "content_scripts": [
    {
      "matches": [
        "http://pikabu.ru/*"
      ],
      "js": [
        "data/js/build/production.min.js"
      ],
      "css": [
        "data/css/main.css",
        "data/css/blackVersion.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "data/images/*"
  ],
  "permissions": [
    "storage",
    "http://job.mslwork.ru/*"
  ],
  "manifest_version": 2
}
