{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Long URL",
  "version": "1.0",
  "description": "Replaces twitters's http://t.co/ with the original url",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
