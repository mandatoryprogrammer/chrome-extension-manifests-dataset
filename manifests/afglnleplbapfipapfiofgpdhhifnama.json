{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "fl_content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "A simple Chrome extension that blocks unwanted websites and and redirects the user to a website of his choice.",
  "name": "Forced Focus",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "version": "1.0",
  "manifest_version": 2
}
