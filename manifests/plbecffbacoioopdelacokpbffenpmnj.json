{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Bernie Filter",
  "version": "0.0.1",
  "description": "Filter Bernie Sanders posts on Reddit!",
  "content_scripts": [
    {
      "matches": [
        "https://*.reddit.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "filter.js"
      ]
    }
  ]
}
