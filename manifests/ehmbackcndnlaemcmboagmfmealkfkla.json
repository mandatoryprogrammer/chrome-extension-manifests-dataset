{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Dashboard",
  "short_name": "New Tab page",
  "description": "Shiny new chrome browser new tab screen.",
  "version": "1.9.3",
  "manifest_version": 2,
  "author": "Cenk SARI (cenk@cenksari.com)",
  "homepage_url": "http://www.cenksari.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "pages/dashboard.html"
  },
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "storage",
    "background",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' http://cors-anywhere.herokuapp.com https://www.metaweather.com/api/location*; object-src 'self'"
}
