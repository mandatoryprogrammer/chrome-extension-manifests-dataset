{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "block KT Spy",
  "description": "block KT spy",
  "version": "0.3.4",
  "background": {
    "scripts": [
      "redirect.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "blockspy.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab"
  ]
}
