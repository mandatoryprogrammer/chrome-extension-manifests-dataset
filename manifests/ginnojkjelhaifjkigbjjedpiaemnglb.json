{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "exclude_globs": [
        "http://*.channel.facebook.com/*",
        "http://static.*.facebook.com/*",
        "http://*.facebook.com/ai.php*",
        "http://*.facebook.com/connect/*",
        "https://*.channel.facebook.com/*",
        "https://static.*.facebook.com/*",
        "https://*.facebook.com/ai.php*",
        "https://*.facebook.com/connect/*"
      ],
      "include_globs": [
        "http://*.facebook.com/*",
        "http://facebook.com/*",
        "https://*.facebook.com/*",
        "https://facebook.com/*",
        "http://linkedin.com/*",
        "http://*.linkedin.com/*",
        "https://linkedin.com/*",
        "https://*.linkedin.com/*"
      ],
      "js": [
        "app.user.js"
      ],
      "matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Notes to friends on facebook",
  "name": "FriendNotes for facebook",
  "manifest_version": 2,
  "author": "lintaba",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://lintabapp.herokuapp.com/*",
    "https://lintabapp.herokuapp.com/*",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*",
    "http://*.linkedin.com/*",
    "https://*.linkedin.com/*"
  ],
  "version": "1.3"
}
