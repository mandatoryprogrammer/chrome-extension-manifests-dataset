{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Poker Tournament Clock",
  "description": "Host Home Poker Tournaments",
  "version": "10.5",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "tts"
  ]
}
