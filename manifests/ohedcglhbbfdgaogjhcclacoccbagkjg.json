{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Адаптер Рутокен Плагин",
  "key": "",
  "version": "1.0.4.0",
  "manifest_version": 2,
  "description": "Позволяет использовать Рутокен Плагин",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "webpage.js"
  ],
  "permissions": [
    "nativeMessaging"
  ]
}
