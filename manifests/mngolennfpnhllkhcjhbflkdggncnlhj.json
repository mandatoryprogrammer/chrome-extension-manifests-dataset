{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Woke Powerschool",
  "description": "The one extension you need if you are serious about your grades",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "*://*.powerschool.westportps.org/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}
