{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Chosen Trump",
  "short_name": "Hail Trump",
  "description": "Replaces the text 'Trump' with Anakin Skywalker.",
  "author": "Henry Shen",
  "version": "1.0",
  "icons": {
    "128": "/icon/trump2.png"
  },
  "browser_action": {
    "default_icon": "/icon/trump2.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
