{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Follow Hackers",
  "version": "1.0.3",
  "description": "Keep track of your favorite users on Hacker News",
  "icons": {
    "128": "yc-company.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "jquery.js",
        "follow_hackers.js"
      ]
    }
  ],
  "permissions": [
    "https://news.ycombinator.com/*",
    "http://follow-hackers.herokuapp.com/*"
  ],
  "manifest_version": 2
}
