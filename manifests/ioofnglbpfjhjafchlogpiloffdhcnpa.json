{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "goto-intial-commits",
  "description": "Simple way to browse initial commits in github repo",
  "version": "0.50",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "sc.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start"
    }
  ]
}
