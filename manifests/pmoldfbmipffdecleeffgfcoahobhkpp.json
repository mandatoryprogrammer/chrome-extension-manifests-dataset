{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Dima PM",
  "manifest_version": 2,
  "name": "Mark Selected Links As Visited",
  "version": "0.3",
  "description": "Marks selected links as visited. If nothing is selected - marks all links on page as visited",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "history",
    "contextMenus"
  ]
}
