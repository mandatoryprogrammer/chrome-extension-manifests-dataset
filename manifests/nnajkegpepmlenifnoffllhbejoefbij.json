{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One True God",
  "version": "0.3",
  "permissions": [
    "http://*/*"
  ],
  "description": "Show your devotion to the Lord",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "replace.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "nick16.png",
    "128": "nick128.png"
  }
}
