{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AtCoder ACer",
  "description": "問題一覧ページにACした人数を表示します",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://atcoder.jp/contests/*/tasks"
      ],
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/main.js"
      ]
    }
  ],
  "permissions": [
    "http://beta.atcoder.jp/"
  ]
}
