{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AuthorIdentify",
  "version": "0.0.0.1",
  "description": "Add AuthorIdentity to money button for B:// file upload!",
  "background": {
    "scripts": [
      "bsv.min.js",
      "bsv-message.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
