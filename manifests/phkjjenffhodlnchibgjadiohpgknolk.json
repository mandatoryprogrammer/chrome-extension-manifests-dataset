{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Nozbe bookmarklet extension"
  },
  "name": "Nozbe bookmarklet extension",
  "description": "Creates a Nozbe task with the web page address you are currently on",
  "homepage_url": "http://productivewizard.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "options.html"
}
