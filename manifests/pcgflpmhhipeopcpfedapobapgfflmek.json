{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TSquare Assignment Calenderifier",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Adds Assignment due dates from TSquare into your Google calendar.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://t-square.gatech.edu/",
    "clipboardWrite",
    "clipboardRead",
    "https://calendar.google.com/calendar/"
  ]
}
