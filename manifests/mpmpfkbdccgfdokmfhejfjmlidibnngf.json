{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sri Sri Quotes",
  "description": "Refresh yourself with Knowledge Quotes!",
  "version": "1.23",
  "browser_action": {
    "default_icon": "images/icon-128.png",
    "default_title": "Refresh yourself with Knowledge Quotes!"
  },
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png"
  },
  "permissions": [
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
