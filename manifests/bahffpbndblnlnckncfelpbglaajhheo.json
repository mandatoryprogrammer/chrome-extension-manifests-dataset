{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ten Letter Word Finder",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Gets 10 letter words",
  "browser_action": {
    "default_icon": "icon_10.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
