{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "icon64.png",
    "48": "icon64.png",
    "128": "icon128.png"
  },
  "name": "Yaad",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "description": "Minimal To-Do list",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
