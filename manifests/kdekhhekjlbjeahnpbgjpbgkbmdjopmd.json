{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "cyberdonos",
  "version": "2.7.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/cyberdonos/cyberdonos-browser-extension",
  "description": "Идентификация и добавление кремлеботов, проплаченных и других способствующих укреплению режима людей во вконтакте, twitter, youtube.",
  "background": {
    "scripts": [
      "js/browser-polyfill.js",
      "js/dayjs.js",
      "js/background-new.js"
    ]
  },
  "content_security_policy": "default-src 'self'; connect-src https://karatel.foss.org.ua http://*.cyberdonos.xyz https://*.cyberdonos.xyz https://vk.com https://blocktogether.org https://raw.githubusercontent.com https://gosvon.net https://pastebin.com https://gist.githubusercontent.com https://gitlab.com;",
  "content_scripts": [
    {
      "matches": [
        "*://*.cyberdonos.xyz/*",
        "https://vk.com/*",
        "https://www.youtube.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "js/browser-polyfill.js",
        "js/content.js"
      ],
      "css": [
        "css/popup.css",
        "css/abuse.css",
        "css/proof-popup.css",
        "css/add-person.popup.css",
        "css/extra.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "assets/*"
  ],
  "icons": {
    "16": "icons/Icon16.png",
    "20": "icons/Icon20.png",
    "32": "icons/Icon32.png",
    "48": "icons/Icon48.png",
    "64": "icons/Icon64.png",
    "128": "icons/Icon128.png"
  },
  "browser_action": {
    "default_title": "Кибердонос",
    "default_icon": {
      "20": "icons/Icon20.png",
      "32": "icons/Icon32.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "*://*.cyberdonos.xyz/*",
    "*://vk.com/*",
    "https://www.youtube.com/*",
    "https://twitter.com/*",
    "https://karatel.foss.org.ua/*",
    "https://blocktogether.org/*",
    "https://raw.githubusercontent.com/*",
    "https://pastebin.com/raw/*",
    "https://gist.githubusercontent.com/*",
    "https://gitlab.com/*",
    "https://gosvon.net/*"
  ]
}
