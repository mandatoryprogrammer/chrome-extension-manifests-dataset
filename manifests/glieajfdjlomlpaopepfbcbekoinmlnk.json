{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quidder",
  "version": "1.0",
  "description": "Adds the current page to the Quidder bookmarking system.",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Quidder",
    "default_icon": "icon.png",
    "default_popup": "QuidderExSignUp.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content-Security-Policy": "script-src 'self' object-src 'self'"
}
