{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VEED.IO - Simple Online Video Editor",
  "version": "1.01",
  "short_name": "Create professional content to grow your audience online",
  "description": "Simple Online Video Editing",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon38.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "Simple Online Video Editing"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
