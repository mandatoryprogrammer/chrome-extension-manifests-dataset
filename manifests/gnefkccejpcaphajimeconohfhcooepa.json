{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Brexit To Breakfast",
  "version": "1.1",
  "description": "Replaces 'Brexit' with 'Breakfast' on all websites.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
