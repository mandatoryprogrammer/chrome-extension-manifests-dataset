{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CleanBox - Easy Unsubscribe",
  "version": "0.2.8",
  "description": "",
  "icons": {
    "48": "icon48x48.png",
    "128": "icon128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/mail/*"
      ],
      "exclude_matches": [
        "*://mail.google.com/mail/help/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "exclude_matches": [
        "*://*.google.com/*"
      ],
      "js": [
        "contentscript_unsubframe.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "src/libs/require.js",
    "src/libs/require-cs.js",
    "src/libs/require/text.js",
    "src/libs/backbone.js",
    "src/libs/underscore.js",
    "src/libs/coffee-script.js",
    "src/libs/cs.js",
    "src/libs/jquery-min.js",
    "src/libs/mustache.js",
    "src/css/style.css",
    "src/main.js",
    "src/unsubframe.js",
    "src/gmailapp.coffee",
    "src/unsubscribe_page.coffee",
    "src/gmailcanvas.coffee",
    "src/linkfinder.coffee",
    "src/views/dialogbox.coffee",
    "src/views/unsubscribe_overlay.coffee",
    "src/views/unsubscribe_button.coffee",
    "src/views/toolbar_area.coffee"
  ]
}
