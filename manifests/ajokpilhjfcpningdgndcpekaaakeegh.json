{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks Share",
  "version": "1.1.0.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "The best way to share Bookmarks with your friends and colleagues!",
  "browser_action": {
    "default_icon": {
      "19": "img/logo-16.png",
      "38": "img/logo-64.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/logo-16.png",
    "128": "img/logo-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.pusher.com; object-src 'self'",
  "permissions": [
    "contextMenus",
    "activeTab",
    "webRequest",
    "storage",
    "https://*.pusher.com/*",
    "*://pusher.com/*",
    "*://*.pusher.com/",
    "http://getcouper.com:8000/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
