{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Codeflower",
  "version": "0.1.6",
  "description": "Visualize repos on github.",
  "icons": {
    "48": "images/icon128.png",
    "128": "images/icon128.png"
  },
  "author": "Jake Mensch",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "html/frame.html"
  ],
  "offline_enabled": false
}
