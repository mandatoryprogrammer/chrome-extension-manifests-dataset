{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "spaceBookMarks",
  "version": "1.12",
  "description": "manage bookMark with namespace",
  "browser_action": {
    "default_icon": "src/png/small__spaceBookMark.png",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "bookmarks",
    "activeTab"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
