{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github 404 Breakdown",
  "description": "Explains why Github returned a 404",
  "version": "2.0.0",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "github-404-breakdown.js"
      ],
      "all_frames": false
    }
  ]
}
