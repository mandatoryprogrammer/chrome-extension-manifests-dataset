{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extensionName__",
  "author": "Terry Yuen",
  "version": "0.2.5",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "icon_16.png",
    "24": "icon_24.png",
    "38": "icon_38.png",
    "48": "icon_48.png",
    "96": "icon_96.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": [
      "punycode.js",
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "16": "icon_16.png",
      "24": "icon_24.png",
      "38": "icon_38.png",
      "48": "icon_48.png"
    },
    "default_title": "__MSG_pageActionTitle__",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "settings.html"
  }
}
