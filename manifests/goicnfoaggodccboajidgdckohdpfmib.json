{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Snake",
  "version": "1.0",
  "description": "Play Snake!",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Snake!",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "s16.png",
    "32": "s32.png",
    "48": "s48.png",
    "64": "s64.png",
    "128": "s128.png"
  }
}
