{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Github Issue Number First",
  "description": "Moves the issue number to the start of the browser title.",
  "version": "1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/issues",
        "https://github.com/*/*/issues/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
