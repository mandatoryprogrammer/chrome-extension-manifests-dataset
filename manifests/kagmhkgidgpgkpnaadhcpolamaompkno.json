{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No Moments",
  "version": "1.1.0",
  "description": "Removes moments from the Twitter menu.",
  "browser_action": {
    "default_icon": "nm.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
