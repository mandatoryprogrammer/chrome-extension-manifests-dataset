{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tilt Head to Scroll (Experimental)",
  "description": "Tilt your head to scroll web pages.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "tilt.css"
      ],
      "js": [
        "js/compatibility.js",
        "js/smoother.js",
        "js/objectdetect.js",
        "js/objectdetect.frontalface.js",
        "js/jquery.js",
        "js/jquery.objectdetect.js",
        "js/tilt.js"
      ]
    }
  ]
}
