{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BananaChain",
  "description": "Replaces all mentions of blockchain with banana. Because it's funnier and an better source of potassium.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "bananachain16.png",
    "128": "bananachain128.png"
  }
}
