{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Procrastinate Later",
  "description": "Procrastinate later by saving up interesting tabs for later viewing.",
  "version": "1.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Procrastinate Later",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "sessions",
    "storage",
    "<all_urls>",
    "http://*/"
  ],
  "background": {
    "page": "background.html"
  }
}
