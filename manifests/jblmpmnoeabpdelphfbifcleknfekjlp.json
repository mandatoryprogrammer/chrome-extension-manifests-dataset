{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nixcraft April Fools",
  "description": "An extension to block you from visiting https://www.cyberciti.biz. It's part from an April Fools' prank.",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webNavigation",
    "webRequestBlocking",
    "https://www.cyberciti.biz/*",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "pranked.html"
  ]
}
