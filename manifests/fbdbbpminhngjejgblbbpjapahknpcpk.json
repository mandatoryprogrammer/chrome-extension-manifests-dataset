{
  "manifest_version": 2,
  "name": "Extension Auto Reloader",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Extension Auto Reloader"
  },
  "description": "Reload all development extensions on each page refresh.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "management",
    "webNavigation"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
