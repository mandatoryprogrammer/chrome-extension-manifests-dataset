{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Circlemarkr 4",
  "version": "4.0",
  "description": "G+のユーザーにマークを付けます",
  "homepage_url": "https://plus.google.com/b/112102646198561777507/",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "config.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "js": [
        "config.js",
        "Circlemarkr.js"
      ],
      "matches": [
        "https://plus.google.com/*"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
