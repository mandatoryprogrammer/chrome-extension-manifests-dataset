{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Expensify Login with Google Apps",
  "description": "Makes Expensify Google Apps SSO work from email ... why they can't get this right, I don't know!",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.expensify.com/signin*"
      ],
      "js": [
        "login.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/favicon.ico",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
