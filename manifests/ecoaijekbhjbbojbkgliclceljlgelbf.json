{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Readiator EPUB Reader",
  "short_name": "Readiator",
  "description": "Open source EPUB reader with full EPUB3 support, works offline",
  "version": "0.4.12",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "assets/icon-16.png",
    "128": "assets/icon-128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ],
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "author": "shenzhuxi"
}
