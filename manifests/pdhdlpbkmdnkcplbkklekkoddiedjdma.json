{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One-click Article Share",
  "description": "This extension demonstrates a browser action with artcile share.",
  "version": "2.0",
  "permissions": [
    "tabs",
    "identity",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
