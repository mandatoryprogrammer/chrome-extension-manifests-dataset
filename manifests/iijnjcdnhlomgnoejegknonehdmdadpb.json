{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Applesauce",
  "description": "Security through obscurity",
  "version": "1.1",
  "background": {
    "persistent": true,
    "scripts": [
      "src/js/http-request/http-request.js",
      "src/js/dictionary/dictionary.js",
      "src/js/main.js"
    ]
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
