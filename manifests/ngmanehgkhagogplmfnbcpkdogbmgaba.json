{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spaffnerds Better Setlist Notes Plugin",
  "version": "2.0",
  "short_name": "Setlist Notes",
  "description": "This extension fixes repetitive setlist notes on Spaffnerds.com.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://spaffnerds.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "https://spaffnerds.com/*"
  ]
}
