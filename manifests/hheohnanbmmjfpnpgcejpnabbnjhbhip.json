{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Control",
  "version": "1.4.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/constants.js",
      "js/TabWrapper.js",
      "js/background.js"
    ]
  },
  "description": "Tab Control stresses over your tabs so you don't have to.",
  "browser_action": {
    "default_title": "Tab Control",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon32.png",
    "48": "img/icon96.png",
    "96": "img/icon192.png",
    "128": "img/icon256.png"
  },
  "author": "Sanketh Hegde"
}
