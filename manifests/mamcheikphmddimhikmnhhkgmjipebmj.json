{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "img/ico-128.png",
    "64": "img/ico-64.png",
    "16": "img/ico-16.png",
    "32": "img/ico-32.png",
    "48": "img/ico-48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.1.3.min.js",
        "js/main.js"
      ],
      "matches": [
        "https://jira.englishtown.com/secure/RapidBoard.jspa*",
        "https://jira.englishtown.com/issues/?filter=*",
        "https://jira.englishtown.com/browse/*?filter=*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "make pull request on Jira dashboard",
  "manifest_version": 2,
  "name": "Jira Pull Request Check",
  "short_name": "PullRequest",
  "omnibox": {
    "keyword": "Jira"
  },
  "permissions": [
    "https://jira.englishtown.com/*"
  ],
  "version": "1.5.1"
}
