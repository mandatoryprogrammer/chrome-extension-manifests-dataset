{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Refecere Stercore: The TL;DR Chrome Extension",
  "description": "Replaces the usage of 'TL;DR' to 'Summary' because reasons.",
  "version": "0.1",
  "icons": {
    "16": "icons/refecere-stercore16.png",
    "48": "icons/refecere-stercore48.png",
    "128": "icons/refecere-stercore128.png",
    "256": "icons/refecere-stercore256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
