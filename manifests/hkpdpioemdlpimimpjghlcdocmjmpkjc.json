{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scroll to Github Top",
  "short_name": "STGT",
  "description": "This extension make you easy to Scroll to Github page Top.",
  "version": "1.8",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "http://*.miaowu.org/*",
        "https://*.miaowu.org/*",
        "http://guides.rubyonrails.org/*",
        "https://guides.rubyonrails.org/*"
      ],
      "css": [
        "button.css",
        "jstree.min.css",
        "tree.css"
      ],
      "js": [
        "button.js",
        "jquery.min.js",
        "catalog.js",
        "jstree.min.js"
      ]
    }
  ]
}
