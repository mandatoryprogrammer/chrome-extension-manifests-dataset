{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA Attachment Download",
  "description": "Select JIRA attachments form a ticket and download them.",
  "author": "Thomas Irgang",
  "version": "1.7",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "18": "icon18.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "<all_urls>",
    "downloads",
    "storage",
    "tabs",
    "activeTab"
  ]
}
