{
  "manifest_version": 2,
  "name": "Trello +",
  "description": "Improve your Trello Boards",
  "permissions": [
    "https://trello.com/*/*"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*/*"
      ],
      "css": [
        "trellios.css"
      ],
      "js": [
        "js/jquery-3.4.0.min.js",
        "js/trellios.min.js"
      ]
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "5.1"
}
