{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Center Logo in Princeton Google Apps",
  "description": "This extension vertically centers the logo on Princeton Google Apps.",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
