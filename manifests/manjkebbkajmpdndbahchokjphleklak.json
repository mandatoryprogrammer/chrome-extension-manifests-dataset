{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove ZergNet",
  "description": "Expunge all sites of ZergNet links.",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [],
      "js": [
        "remove.zergnet.user.js"
      ]
    }
  ],
  "author": "Zac Schellhardt"
}
