{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Instant Dictionary",
  "version": "1.0.5",
  "description": "View definition in both English and Chinese as you browse the web.",
  "author": "Peter Pi",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "Styles.css"
      ],
      "js": [
        "content.js",
        "jquery-3.4.1.min.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
