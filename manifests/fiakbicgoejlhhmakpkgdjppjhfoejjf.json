{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Old Github Layout Plz",
  "author": "Emanuel Quimper <EQuimper>",
  "description": "I want the old layout of github, so I make little change in css. So the feed is on the left side.",
  "version": "1",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "Have a good day"
  },
  "permissions": [
    "activeTab"
  ]
}
