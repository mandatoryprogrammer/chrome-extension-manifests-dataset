{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Touch Keyboard",
  "description": "Keyboard for mobile or any other touch screen devices. It is inspired on Android usability.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "keyboard.html",
    "style.css",
    "images/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "script.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
