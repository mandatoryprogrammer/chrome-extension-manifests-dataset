{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark Commander",
  "short_name": "BC",
  "version": "1.3.3",
  "manifest_version": 2,
  "description": "Commanding Bookmarks, Old Skool",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "bc-16x16.png",
    "48": "bc-48x48.png",
    "128": "bc-128x128.png"
  },
  "browser_action": {
    "default_title": "Manage Bookmarks",
    "default_icon": "bc-32x32.png"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
