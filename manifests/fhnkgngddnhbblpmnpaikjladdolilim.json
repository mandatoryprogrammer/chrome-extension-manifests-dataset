{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoLogin SwissPost /w SwissId for TEST env",
  "description": "auto login functionality for password, sms code and email code",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "version": "1.3.0.0",
  "author": "Stef KÃ¤ser",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://accountint1.post.ch/idp/*",
        "https://accountint2.post.ch/idp/*",
        "https://login.int.swissid.ch/idp/XUI/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
