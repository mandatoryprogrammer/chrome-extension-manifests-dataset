{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gitlab Whitespaces remover",
  "version": "0.0.1",
  "description": "Removes whitespaces in individual lines in a Gitlab merge requests",
  "browser_action": {
    "icon": "icon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
