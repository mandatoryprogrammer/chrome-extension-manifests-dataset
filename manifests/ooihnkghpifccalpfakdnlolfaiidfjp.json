{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "De-Bux",
  "description": "React Redux devtool",
  "version": "1.0.3",
  "permissions": [
    "*://*/*",
    "tabs",
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "icons": {
    "16": "img/DebuxLogo16.png",
    "48": "img/DebuxLogo48.png",
    "128": "img/DebuxLogo128.png",
    "256": "img/DebuxLogo256.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/DebuxLogo16.png",
      "48": "img/DebuxLogo48.png",
      "128": "img/DebuxLogo128.png",
      "256": "img/DebuxLogo256.png"
    }
  },
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    "hook.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
