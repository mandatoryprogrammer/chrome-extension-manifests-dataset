{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Eyuel Tessema",
  "manifest_version": 2,
  "name": "remuncher",
  "version": "0.1",
  "description": "Hourly reminders to make your Munchery order",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png"
  },
  "browser_action": {},
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://munchery.com/checkout/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "minimum_chrome_version": "40"
}
