{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "cTouch external daemon",
  "description": "required for cTouch (external)",
  "version": "0.0.1.2",
  "app": {
    "background": {
      "scripts": [
        "ctouch_chromesocket.js"
      ]
    }
  },
  "permissions": [
    "storage",
    {
      "socket": [
        "tcp-listen"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png"
  }
}
