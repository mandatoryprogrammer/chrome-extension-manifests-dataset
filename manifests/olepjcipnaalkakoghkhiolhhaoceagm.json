{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sessionr",
  "description": "This extension helps you manage your browser sessions",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "./images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "storage",
    "tabs",
    "windows"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "js/lodash.min.js",
      "eventPage.js"
    ],
    "persistent": false
  }
}
