{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BitCoin Ticker",
  "version": "1.31",
  "description": "Show the current bitcoin (or ethereum) price on your toolbar.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "bitcoin.png"
  },
  "icons": {
    "128": "bitcoin.png"
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": [
      "bitticker.js"
    ]
  }
}
