{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mockup Studio : PSD Free Solution",
  "short_name": "MockupStudio",
  "version": "0.6",
  "description": "Create Unique/Free Mockups for Brochures, UI, Stationary and more..",
  "manifest_version": 2,
  "author": "Deepak Ghimire",
  "browser_action": {
    "default_icon": {
      "19": "images/icon_16.png",
      "38": "images/icon_128.png"
    },
    "default_title": "Mockup Studio (Ctrl+M)"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/extension.js"
    ]
  },
  "options_page": "index.html",
  "permissions": [
    "background",
    "contextMenus",
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "notifications",
    "idle",
    "https://*/*",
    "http://*/*",
    "identity",
    "alarms",
    "https://graph.facebook.com/",
    "https://accounts.google.com/o/oauth2/token",
    "https://www.google-analytics.com/",
    "https://www.googleapis.com/*",
    "https://*.googleusercontent.com/*"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://apis.google.com/; object-src 'self'",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+M"
      }
    }
  },
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  }
}
