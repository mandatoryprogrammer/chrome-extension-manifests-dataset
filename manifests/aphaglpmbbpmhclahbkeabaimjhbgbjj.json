{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TirauraResEditAddon",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "チラ裏にレス編集機能を追加する拡張機能",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png"
    },
    "default_title": "Tiraura Res Edit Addon"
  },
  "content_scripts": [
    {
      "matches": [
        "http://tiraura2.sytes.net/rbbs.cgi*",
        "http://tiraura.orz.hm/rbbs.cgi*"
      ],
      "js": [
        "js/jquery-3.0.0.min.js",
        "js/main.js",
        "js/new.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
