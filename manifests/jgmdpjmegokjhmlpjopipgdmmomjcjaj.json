{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Cintel Desktop Sharing Extension",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Desktop sharing for Cintel communicator",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow desktop sharing in Zealcomm PureRTC applications.",
  "externally_connectable": {
    "matches": [
      "*://*.sh.zealcomm.com/*",
      "*://192.168.11.50/*",
      "*://op.cintelcloud.com/*",
      "*://180.153.223.233/*",
      "*://180.153.223.234/*",
      "*://180.153.223.235/*"
    ]
  },
  "icons": {
    "128": "purertc_icon_128.png",
    "16": "purertc_icon_16.png",
    "48": "purertc_icon_48.png"
  },
  "browser_action": {
    "default_icon": "purertc_icon_16.png"
  }
}
