{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "gForm Default Copy Of Response",
  "description": "Makes sure you always get email copy of your responses [me@nishantarora.in]",
  "version": "0.1",
  "permissions": [
    "*://docs.google.com/*/forms/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://docs.google.com/*/forms/*"
      ],
      "js": [
        "form.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
