{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bracket Matcher",
  "version": "1.0",
  "description": "Find that other }, ], or )",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "content_security_policy": "default-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "matcher.css"
      ],
      "js": [
        "thirdParty/jquery-3.3.1.min.js",
        "thirdParty/chrome-extension-async.js",
        "matcher.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/b16.png",
      "32": "images/b32.png",
      "48": "images/b48.png",
      "128": "images/b128.png"
    }
  },
  "icons": {
    "16": "images/b16.png",
    "32": "images/b32.png",
    "48": "images/b48.png",
    "128": "images/b128.png"
  },
  "manifest_version": 2
}
