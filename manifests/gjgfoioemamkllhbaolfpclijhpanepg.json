{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lansira Tab Manager",
  "description": "Easily manage and launch tab sets from the menu bar",
  "version": "0.5",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/lansira_19.png",
      "38": "images/lansira_38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Lansira Tab Manager"
  },
  "icons": {
    "16": "images/lansira_16.png",
    "48": "images/lansira_48.png",
    "128": "images/lansira_128.png"
  },
  "optional_permissions": [
    "bookmarks"
  ],
  "permissions": [
    "notifications",
    "tabs",
    "bookmarks",
    "storage"
  ],
  "web_accessible_resources": [
    "popup.html"
  ],
  "manifest_version": 2
}
