{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "imgur Archiver",
  "description": "Enables creating a clean version of an imgur album without comments for archival purposes",
  "version": "2.3.1",
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "Archive album",
    "default_popup": "archiver.html"
  },
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "author": "Tsuri Kamppuri",
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'sha256-q6ZmvRjJqFQ+wEGR4QGQIZN1BqmNzFDEaX7q0pW0AW0='; object-src 'self'",
  "short_name": "imgurAr"
}
