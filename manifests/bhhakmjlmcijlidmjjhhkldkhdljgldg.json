{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Benedict Cumberbatch",
  "description": "This extension changes Benedict Cumberbatch's name to one of the many other names he's known by.",
  "version": "1.0",
  "icons": {
    "128": "benedict.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
