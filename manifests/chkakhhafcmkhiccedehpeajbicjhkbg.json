{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "name": "Make this page less wide"
  },
  "description": "Shrink your pages.",
  "icons": {
    "128": "icon.png",
    "48": "icon.png"
  },
  "name": "Page Shrinker",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.2.0",
  "manifest_version": 2
}
