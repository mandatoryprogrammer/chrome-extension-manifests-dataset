{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LocomotiveCMS https to http",
  "description": "Replace sign in url from https to http for development",
  "version": "0.3.0",
  "icons": {
    "16": "img/icon16.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/app.js"
      ]
    }
  ]
}
