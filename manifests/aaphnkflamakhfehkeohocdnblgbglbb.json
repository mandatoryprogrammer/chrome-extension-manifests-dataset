{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disable to merge WIP PR on GitHub",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Disable Merge pull request Button when PR is WIP on GitHub.",
  "permissions": [
    "tabs",
    "https://github.com/*/*/pull/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/pull/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ]
}
