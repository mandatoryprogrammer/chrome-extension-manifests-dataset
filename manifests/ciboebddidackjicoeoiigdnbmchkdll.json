{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoCountryRedirect (NCR)",
  "version": "1.7.1711",
  "description": "Makes sure you stay on google.com and blogspot.com by automatically adding 'no country redirect' ('/ncr') to those URLs",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "ncr19x19.png",
    "default_title": "NoCountryRedirect",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "ncr48x48.png",
    "128": "ncr128x128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
