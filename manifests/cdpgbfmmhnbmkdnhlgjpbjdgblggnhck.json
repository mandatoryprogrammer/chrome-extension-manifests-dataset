{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Public IP",
  "description": "This extension shows your public IP",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
