  "version": "2.0",
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "查看公网IP",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://pv.sohu.com/"
  ]
}
