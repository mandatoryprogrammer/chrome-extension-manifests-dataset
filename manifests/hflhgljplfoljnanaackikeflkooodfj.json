{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Not So Weak Passwords",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "Generate password for casual web surfing.",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "NSWP",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "clipboardRead"
  ],
  "options_page": "options.html"
}
