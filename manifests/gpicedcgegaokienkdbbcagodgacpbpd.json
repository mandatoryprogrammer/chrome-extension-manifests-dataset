{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Display Access Keys",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "description": "Displays hint of access keys next ot the elements.",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "display-access-keys.css"
      ]
    }
  ]
}
