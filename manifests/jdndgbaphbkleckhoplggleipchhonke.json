{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "(surf) shameless",
  "version": "0.9.3",
  "manifest_version": 2,
  "description": "This extension saves you from embarrassment",
  "browser_action": {
    "default_icon": "icon.ico",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icon.ico",
    "48": "incognito_smaller.jpg",
    "128": "incognito_small.jpg"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "history",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "js/pbkdf2.js",
      "js/jquery.min.js",
      "js/aes.js",
      "build/background_clean.js"
    ]
  },
  "options_page": "settings.html"
}
