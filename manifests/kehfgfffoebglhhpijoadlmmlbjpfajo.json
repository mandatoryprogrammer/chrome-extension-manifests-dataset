{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BCPS - Content Standards Search",
  "description": "This extension allows the user to search and copy Standards into thier lessons.",
  "version": "1.0",
  "icons": {
    "16": "icon1.png",
    "48": "icon1.png",
    "128": "icon1.png"
  },
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead",
    "notifications"
  ]
}
