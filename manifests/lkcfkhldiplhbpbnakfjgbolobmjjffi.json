{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tabby",
  "version": "0.4.4",
  "manifest_version": 2,
  "description": "Closes unattented tabs.",
  "icons": {
    "16": "files/icon16.png",
    "48": "files/icon48.png",
    "128": "files/icon128.png"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "tabby.js"
    ]
  }
}
