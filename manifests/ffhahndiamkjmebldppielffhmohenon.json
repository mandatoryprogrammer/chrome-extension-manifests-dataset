{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "[in]",
  "version": "1.0.0",
  "description": "probably the most useful extension you'll ever [in]stall",
  "icons": {
    "16": "images/in16.png",
    "48": "images/in48.png",
    "128": "images/in128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
