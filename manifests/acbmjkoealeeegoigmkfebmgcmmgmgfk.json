{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Onegai",
  "version": "1.01",
  "manifest_version": 2,
  "description": "Find videos and share it with your friends",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
