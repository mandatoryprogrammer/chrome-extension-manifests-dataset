{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Mark As Done",
  "description": "This extension allows you to collapse comments on PRs",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "*://github.com/*/pull/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "*://github.com/*/pull/*"
  ]
}
