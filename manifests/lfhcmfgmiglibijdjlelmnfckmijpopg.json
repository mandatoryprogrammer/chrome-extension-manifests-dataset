{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pocket - Article Reading Time",
  "description": "Displays the estimated time it will take to read an article.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.getpocket.com/a/*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ],
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
