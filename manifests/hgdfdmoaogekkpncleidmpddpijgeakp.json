{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Retina Twitter",
  "description": "This extension replaces low resolution twitter avatars with a retina-ready equivalent.",
  "version": "1.1",
  "icons": {
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-2.0.0.min.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://twitter.com/"
  ],
  "web_accessible_resources": [
    "jquery-2.0.0.min.js",
    "jquery.min.map"
  ]
}
