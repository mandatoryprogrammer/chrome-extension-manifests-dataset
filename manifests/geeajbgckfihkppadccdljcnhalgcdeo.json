{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "YT Videos Hider",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "Hide the videos you don't want to see from your Youtubeâ„¢ home page.",
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/hidevideos.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
