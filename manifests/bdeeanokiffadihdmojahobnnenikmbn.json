{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GCache Browser",
  "description": "Easily Browse Google cached webpages",
  "version": "0.4",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://webcache.googleusercontent.com/*"
      ],
      "js": [
        "cache.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
