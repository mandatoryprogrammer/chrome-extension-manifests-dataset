{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Projects",
  "description": "Easily access your current Github projects",
  "version": "1.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Benjamin Reid",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "github-projects.js"
      ],
      "css": [
        "github-projects.css"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
