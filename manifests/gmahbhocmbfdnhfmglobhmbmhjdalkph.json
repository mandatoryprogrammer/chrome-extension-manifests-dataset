{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "backtickback",
  "description": "Quip's backtick shortcut is infuriating. This removes it.",
  "version": "1.0",
  "author": "Pierre Carrier",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "backtickback.js"
      ],
      "matches": [
        "https://*.quip.com/*"
      ]
    }
  ]
}
