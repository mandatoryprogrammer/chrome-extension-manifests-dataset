{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "osmo.mobi - panTo control",
  "author": "Bruno Duy√©",
  "description": "Add a button to osmo.mobi to enable/disable auto map recenter",
  "version": "0.1",
  "icons": {
    "64": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://osmo.mobi/s/*",
        "https://osmo.mobi/s/*"
      ],
      "js": [
        "add_button.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "target-green.png",
    "target-red.png"
  ]
}
