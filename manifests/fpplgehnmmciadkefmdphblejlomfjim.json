{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ClearYoutube",
  "short_name": "CY",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
