{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AltRight-To-NeoNazi",
  "version": "0.3",
  "description": "Replaces the text 'Alt-Right' or 'alt-right' with 'NeoNazi'",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
