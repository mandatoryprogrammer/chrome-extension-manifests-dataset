{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IP FIND",
  "version": "1.1.0",
  "description": "Find your IP.",
  "permissions": [
    "storage",
    "clipboardWrite",
    "clipboardRead"
  ],
  "browser_action": {
    "default_title": "IP FIND",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": []
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://json.geoiplookup.io/; object-src 'self'"
}
