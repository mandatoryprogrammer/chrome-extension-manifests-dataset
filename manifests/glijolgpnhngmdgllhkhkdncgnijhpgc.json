{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TUNet Helper",
  "description": "A webextension for managing Tsinghua campus net devices",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "TUNet助手",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://usereg.tsinghua.edu.cn/*",
    "https://net.tsinghua.edu.cn/*"
  ]
}
