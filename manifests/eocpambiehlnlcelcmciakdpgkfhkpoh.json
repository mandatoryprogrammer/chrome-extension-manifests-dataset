{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Script Click",
  "description": "A lightweight, simple script blocker.",
  "version": "1.6.6",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js",
      "sessionList.js",
      "ruleList.js",
      "blackList.js",
      "whiteList.js",
      "mediaList.js",
      "scriptList.js",
      "elementList.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "activeTab",
    "tabs",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "images/scriptOff.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
