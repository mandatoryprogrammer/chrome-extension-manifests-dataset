{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "timelist",
  "description": "Block all sites except those on whitelists.",
  "version": "0.3.2",
  "icons": {
    "128": "clipboard-list-solid.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "settings.js",
      "background.js"
    ]
  },
  "storage": {
    "managed_schema": "schema.json"
  }
}
