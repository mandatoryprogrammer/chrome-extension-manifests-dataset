{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Booksmark manager",
  "version": "1.2",
  "description": "Manage your bookmarks!. Register on www.bany.pc.pl/bookmarks/",
  "icons": {
    "128": "icon128s.png",
    "48": "icon48s.png",
    "16": "icon16s.png"
  },
  "browser_action": {
    "default_icon": "icon16s.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "bookmarks",
    "activeTab"
  ]
}
