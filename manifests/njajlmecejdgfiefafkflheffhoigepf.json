{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reading Haven",
  "version": "0.4",
  "description": "An extension for reading on some sites.",
  "icons": {
    "128": "ReadingHavenIcon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://royalroadl.com/fiction/chapter/*"
      ],
      "js": [
        "jquery-2.2.4.js",
        "reader.js"
      ],
      "css": [
        "reader.css"
      ]
    },
    {
      "matches": [
        "*://www.wuxiaworld.com/*",
        "*://nightbreezetranslations.wordpress.com/*",
        "*://walkthejianghu.com/*",
        "*://novelstravel.wordpress.com/*"
      ],
      "js": [
        "jquery-2.2.4.js",
        "ISSTHReader.js"
      ],
      "css": [
        "reader.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "ReadingHavenIcon.png"
  }
}
