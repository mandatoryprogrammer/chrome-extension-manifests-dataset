{
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.1.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Navigate through UVic courses much easier. And more..."
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-3.1.1.min.js",
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Navigate through UVic courses much easier. And more...",
  "manifest_version": 2,
  "name": "UVic Course Search Optimizer",
  "permissions": [
    "http://*/",
    "tabs",
    "cookies",
    "storage",
    "https://www.uvic.ca/cas/login",
    "http://schedulecourses.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "web_accessible_resources": [
    "icon.png",
    "ajax-loader.gif",
    "remove.png",
    "jquery-ui.js"
  ]
}
