{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Life Tracker",
  "version": "1.0.1",
  "description": "Keep track of how long you've lived and how long you have left. A reminder to live every day to the fullest!",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon48.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  }
}
