{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TweetPuller by Jane Ullah",
  "version": "0.7",
  "manifest_version": 2,
  "description": "This extension displays the tweets of any public Twitter user with configuration possible via the Options page",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "tweets.html",
    "default_title": "TweetPuller by Jane Ullah"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "http://*.twitter.com/",
    "https://*.twitter.com/",
    "http://*.janeullah.com/",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
