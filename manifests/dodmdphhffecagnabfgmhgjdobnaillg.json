{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Skycust",
  "version": "2.1",
  "manifest_version": 2,
  "description": "A forum avatar-replacer based on a remote canonical image source.",
  "icons": {
    "16": "icon16.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "Skycust Providers",
    "default_popup": "provider_cust.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "bg.js"
    ]
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://skyrates.net/*",
        "http://*.skyrates.net/*"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "gm_compatibility.js",
        "skycust.user.js"
      ],
      "run_at": "document_end"
    }
  ]
}
