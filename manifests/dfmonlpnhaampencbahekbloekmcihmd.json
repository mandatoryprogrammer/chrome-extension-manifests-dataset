{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ASPC Reviews",
  "version": "2.1",
  "description": "This extension will allow you to see reviews for courses and instructors while selecting your classes.",
  "browser_action": {
    "default_icon": "/images/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/logo.png",
    "48": "/images/logo.png"
  },
  "permissions": [
    "https://aspc.pomona.edu/api/*"
  ]
}
