{
  "manifest_version": 2,
  "name": "imgin",
  "version": "0.2.0",
  "description": "A link/thumbnail preview extension like HoverZoom",
  "content_scripts": [
    {
      "js": [
        "imgin.min.js"
      ],
      "css": [
        "imgin.css"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "imgin.js.map",
    "imgin.coffee"
  ]
}
