{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab It Your Way",
  "short_name": "TabItYourWay",
  "description": "Open New tabs as a blank screen or customize with your own HTML/CSS.",
  "version": "0.9.5",
  "author": "M. Pascual",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "custom_new_tab_page.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ]
}
