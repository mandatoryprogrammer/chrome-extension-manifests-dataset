{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Overflow",
  "description": "Attempts to defeat anti-Adblock mechanisms that prevent scrolling and place overlays over content",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "assets/popup.html"
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "libs/jquery-1.12.4.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
