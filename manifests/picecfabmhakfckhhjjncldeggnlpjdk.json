{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Heartbroken",
  "version": "1.1.2",
  "description": "This will change the favorite icon back to a green heart on Imgur",
  "icons": {
    "128": "heart.png"
  },
  "content_scripts": [
    {
      "js": [
        "content-script.min.js"
      ],
      "matches": [
        "http://imgur.com/*",
        "https://imgur.com/*"
      ]
    }
  ]
}
