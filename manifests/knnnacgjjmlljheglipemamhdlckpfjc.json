{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stylify - Unuglify",
  "short_name": "Stylify",
  "version": "1.0.1",
  "description": "Helps with ugly websites. Makes reading experience even more clean.",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "manifest_version": 2
}
