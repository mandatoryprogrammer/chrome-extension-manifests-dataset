{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "redirectr",
  "author": "Noam Ross",
  "description": "Redirects r-bloggers links to their source pages",
  "manifest_version": 2,
  "version": "1.2",
  "permissions": [
    "https://www.r-bloggers.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.r-bloggers.com/*/"
      ],
      "js": [
        "redirectr.js"
      ],
      "run_at": "document_start"
    }
  ]
}
