{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StackOverflow Searcher",
  "description": "This extension will allow you to search StackOverflow Easily",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "stack.html"
  },
  "permissions": [
    "activeTab"
  ]
}
