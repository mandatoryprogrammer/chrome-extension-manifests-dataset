{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome RSS reader.",
  "short_name": "RSS",
  "description": "An app to read RSS/Atom feeds in your browser.",
  "version": "1.1.1",
  "author": "Pawel Psztyc",
  "minimum_chrome_version": "38",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "19": "img/icon_19.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png",
    "256": "img/icon_256.png",
    "512": "img/icon_512.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/workers/structures.js",
        "js/app_db.js",
        "background.js"
      ],
      "persistent": false
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "alarms",
    "syncFileSystem",
    "storage",
    "unlimitedStorage",
    "notifications",
    "webview"
  ],
  "sandbox": {
    "pages": [
      "sandboxed/sandbox.html"
    ]
  }
}
