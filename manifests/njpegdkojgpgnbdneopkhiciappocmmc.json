{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hocus Focus",
  "description": "An extension to keep you on track!",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "extension_images/Wand.png",
    "48": "extension_images/Wand.png",
    "128": "extension_images/Wand.png"
  },
  "browser_action": {
    "default_icon": "extension_images/Wand.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "tabs"
  ]
}
