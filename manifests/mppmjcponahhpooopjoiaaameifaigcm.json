{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "comic.css"
      ],
      "matches": [
        "http://daringfireball.net/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Read Daring Fireball in Comic Sans and Marker Felt",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "name": "Comic Fireball",
  "version": "1.0"
}
