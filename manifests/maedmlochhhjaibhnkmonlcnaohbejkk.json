{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FBNotSeen",
  "description": "Turn off facebook's seen feature",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "content.html"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": [
      "jQuery.js",
      "background.js"
    ],
    "persistent": true
  }
}
