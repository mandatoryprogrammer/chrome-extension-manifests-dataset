{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rockabye",
  "description": "Make your sites rock on a gently rolling sea",
  "version": "0.0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "rockabye.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "singlebrook_boat.png",
    "default_title": "Rock it!"
  },
  "manifest_version": 2
}
