{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dark Settings",
  "author": "Miguel Almeida",
  "version": "0.1.0",
  "description": "Darken chrome settings page",
  "manifest_version": 2,
  "permissions": [
    "chrome://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/settings16.png",
    "32": "images/settings32.png",
    "48": "images/settings48.png",
    "128": "images/settings128.png"
  }
}
