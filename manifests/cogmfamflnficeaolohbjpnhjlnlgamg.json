{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "expand.js",
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "file_handlers": {
    "GNU Zip": {
      "types": [
        "application/x-gzip"
      ],
      "extensions": [
        "gz"
      ],
      "title": "Wakame"
    },
    "UNIX Tape ARchive file": {
      "types": [
        "application/x-tar"
      ],
      "extensions": [
        "tar"
      ],
      "title": "Wakame"
    },
    "UNIX Tape ARchive file Gzipped": {
      "types": [
        "application/x-compressed-tar"
      ],
      "extensions": [
        "tgz",
        "tar.gz"
      ],
      "title": "Wakame"
    }
  },
  "manifest_version": 2,
  "name": "Wakame",
  "description": "Wakame is an utility to decompress GNU Zip files and UNIX TAR files.",
  "version": "0.3.1.0",
  "permissions": [
    {
      "fileSystem": [
        "write",
        "directory"
      ]
    }
  ]
}
