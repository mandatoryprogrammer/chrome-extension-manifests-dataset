{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Search",
  "description": "This extension allows you to easily search reddit by using Google's search engine",
  "version": "1.0",
  "browser_action": {
    "default_popup": "frontend/popup.html"
  },
  "background": {
    "scripts": [
      "frontend/jquery/jquery.min.js",
      "backend/eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
