{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TJournal extension",
  "version": "1.2.1",
  "description": "TJournal extension",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://api.tjournal.ru/"
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://tjournal.ru/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/group.svg"
  ]
}
