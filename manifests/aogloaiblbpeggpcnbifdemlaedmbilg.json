{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AuralDivergence",
  "description": "An extension for finding similar artists to the artist that is highlighted on the screen.",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs"
  ]
}
