{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "rooler",
    "default_popup": "popup.html"
  }
}
