{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "92": "icon.png"
  },
  "name": "Octopus Variable Copier",
  "description": "An unofficial tool for copying variable sets. More Info: https://github.com/navidmatin/OctopusVariableSetChromeExt",
  "version": "0.2.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Octopus Variable Copier!"
  },
  "background": {
    "scripts": [
      "octopusController.js",
      "octopusServerInfo.js",
      "content/sjcl/sjcl.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "http://*/",
    "https://*/"
  ]
}
