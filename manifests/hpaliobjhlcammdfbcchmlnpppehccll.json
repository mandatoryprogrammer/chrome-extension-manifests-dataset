{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "They're problem",
  "description": "This extension will drive the grammar geek in your life crazy. Replaces they're with their.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "replacetext.min.js",
        "contentscript.js"
      ]
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_icon": "bux.png"
  }
}
