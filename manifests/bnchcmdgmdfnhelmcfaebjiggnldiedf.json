{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "newsrdr.us checker",
  "description": "Queries newsrdr.us and displays the number of unread posts in the toolbar.",
  "version": "1.3.6",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "alarms",
    "https://newsrdr.us/"
  ],
  "browser_action": {
    "default_icon": "data/icon.png"
  },
  "icons": {
    "128": "data/icon_128.png"
  },
  "homepage_url": "https://newsrdr.us/"
}
