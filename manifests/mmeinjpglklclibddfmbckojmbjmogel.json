{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UniTranslator",
  "version": "1.1.0.6",
  "manifest_version": 2,
  "description": "Translates a selected text from one language to another.",
  "background": {
    "scripts": [
      "jquery.js",
      "functional.js",
      "array.extensions.js",
      "dictionary.js",
      "string.extensions.js",
      "preferences.js",
      "services.js",
      "unitranslator.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "contextMenus",
    "<all_urls>"
  ],
  "icons": {
    "128": "/media/icons/app-128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ]
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "popup.html",
    "unitranslator.css",
    "/media/icons/close.svg",
    "/media/icons/close-32.png",
    "/media/icons/external.svg"
  ]
}
