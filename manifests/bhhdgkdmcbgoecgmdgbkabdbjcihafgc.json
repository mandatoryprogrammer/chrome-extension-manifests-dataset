{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scroll Back",
  "description": "Change the behaviour of Shift-Wheel to go forward and back in the history, like Firefox.",
  "icons": {
    "128": "icon.png"
  },
  "version": "0.1.1",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/scrollback.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "run_at": "document_start"
    }
  ]
}
