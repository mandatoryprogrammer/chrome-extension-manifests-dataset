{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AppDigger",
  "version": "1.0.3",
  "description": "Collect some information of the app and submit it to the gathering page.",
  "permissions": [
    "tabs",
    "*://*.oa.com/",
    "*://*.tmt.io/*",
    "*://itunes.apple.com/",
    "*://play.google.com/",
    "*://appshare.sinaapp.com/"
  ],
  "browser_action": {
    "default_title": "Digger The App",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
