{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Messenger Clean Up",
  "version": "1.0",
  "description": "Removes Facebook instant game notifications from messenger.com and facebook.com",
  "content_scripts": [
    {
      "matches": [
        "*://www.messenger.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
