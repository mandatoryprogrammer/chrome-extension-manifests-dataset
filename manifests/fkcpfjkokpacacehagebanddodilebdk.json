{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Burning Burning Man",
  "short_name": "Burning",
  "description": "Makes reading about Burning Man more tolerable.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "fireflake_16.png",
    "32": "fireflake_32.png",
    "64": "fireflake_64.png",
    "128": "fireflake_128.png"
  }
}
