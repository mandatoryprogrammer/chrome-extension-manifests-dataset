{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "rupeefy",
  "version": "1.2.2",
  "description": "Convert dollar amounts to rupee",
  "author": "Mayank Bhola",
  "icons": {
    "19": "r19.png",
    "38": "r38.png",
    "128": "r128.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "rupeefy.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "r19.png",
      "38": "r38.png"
    },
    "default_title": "Rupeefy"
  },
  "manifest_version": 2
}
