{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Two-Factor Authenticator",
  "description": "An open-source two-factor Time-based One-Time Password (TOTP) authenticator",
  "author": "InBasic",
  "version": "0.2.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "clipboardWrite"
  ],
  "homepage_url": "https://add0n.com/two-factor-authenticator.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    },
    "default_popup": "data/popup/index.html"
  }
}
