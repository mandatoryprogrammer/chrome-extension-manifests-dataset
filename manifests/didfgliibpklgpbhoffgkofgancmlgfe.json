{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MathAL",
  "short_name": "AL+",
  "version": "1.70",
  "description": "Adds the ability to write LaTeX math equations as answers for Actively Learn questions.",
  "content_scripts": [
    {
      "matches": [
        "https://read.activelylearn.com/*"
      ],
      "css": [
        "css/mathquill.min.css",
        "css/ALMath.css"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/mathquill.min.js",
        "js/ALMath.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage",
    "https://read.activelylearn.com/*"
  ],
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
