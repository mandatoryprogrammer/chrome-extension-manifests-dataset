{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GfyHTML5",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Converts gfycat gif urls to their respective HTML5 urls to significantly speed up viewing",
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
