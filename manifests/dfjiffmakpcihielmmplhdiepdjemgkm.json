{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Click Code Selection",
  "description": "Select the entire contents of all <code> tags with one click.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "selectcode.min.js"
      ]
    }
  ]
}
