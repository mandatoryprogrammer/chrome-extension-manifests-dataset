{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "author": "Jeff Sandberg",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/mmm.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "images/rosens/*",
    "audio/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
