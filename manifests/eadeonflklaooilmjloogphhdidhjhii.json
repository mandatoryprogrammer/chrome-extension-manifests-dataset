{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quink - Your quick bookmark new tab",
  "short_name": "Quink",
  "description": "Quink - Your quick bookmark new tab",
  "version": "2.0",
  "offline_enabled": true,
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "topSites",
    "storage",
    "chrome://favicon/",
    "https://*/*",
    "http://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  }
}
