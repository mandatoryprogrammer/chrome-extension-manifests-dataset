{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shadow Bar",
  "short_name": "Shadow Bar",
  "author": "Arlo Godfrey",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Give your browsing experience a little more depth. Adds a nice shadow to every site you visit",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
