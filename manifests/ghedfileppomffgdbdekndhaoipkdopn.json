{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Archive",
  "version": "0.0.2",
  "description": "Gather all tabs and make Markdown/HTML formed list. to create daily report easily (for me)",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "dist/js/background.js"
    ]
  },
  "commands": {
    "open_list": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Open list of tab URLs"
    },
    "search_tab": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Search tab"
    }
  }
}
