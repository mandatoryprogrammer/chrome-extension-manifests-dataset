{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "key": "",
  "name": "Nag",
  "description": "Nags you when you're slacking off",
  "options_page": "options.html",
  "version": "1.0.7",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon48.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery.timer.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "identity",
    "https://ajax.googleapis.com/"
  ]
}
