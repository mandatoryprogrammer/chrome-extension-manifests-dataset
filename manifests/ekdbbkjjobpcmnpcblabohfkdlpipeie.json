{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "豆瓣首页进化",
  "version": "0.1.12",
  "manifest_version": 2,
  "description": "过滤并记录豆瓣首页你不喜欢的条目信息",
  "content_scripts": [
    {
      "matches": [
        "http://www.douban.com/"
      ],
      "run_at": "document_end",
      "js": [
        "jquery.min.js",
        "douban_dislike.js"
      ]
    },
    {
      "matches": [
        "http://*.douban.com/*"
      ],
      "css": [
        "old-douban.css",
        "douban_dislike.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://douban.com/*",
    "http://*.douban.com/*"
  ],
  "icons": {
    "128": "appicon_128.png"
  }
}
