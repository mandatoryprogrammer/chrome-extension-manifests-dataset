{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/EzURL2QR_16.png",
    "128": "images/EzURL2QR_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/EzURL2QR_128.png",
    "default_title": "EzURL2QR",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "https://api.qrserver.com/v1/create-qr-code/*",
    "http://goqr.me/de/*"
  ]
}
