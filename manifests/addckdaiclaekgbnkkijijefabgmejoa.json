{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Gmail Queue",
  "version": "1.0.8",
  "description": "Access a Youtube Queue from a plugin",
  "background": {
    "scripts": [
      "js/background/ygq.js"
    ]
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "declarativeContent",
    "identity",
    "https://www.googleapis.com/*"
  ],
  "browser_action": {
    "default_title": "Youtube Gmail Queue",
    "default_popup": "queue-popup.html"
  },
  "web_accessible_resources": [
    "js/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "key": "",
  "oauth2": {
    "client_id": "624940088516-4fu3ngnq3jer0f4sdkv57u1l2u0ickrl.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/gmail.modify",
      "https://www.googleapis.com/auth/youtube.readonly"
    ]
  }
}
