{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LocalStorage copier",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "The extension can copy some data from localStorage and paste at other site. Helpful when you are use proxy for testing",
  "browser_action": {
    "default_title": "LocalStorage copier",
    "default_popup": "popup.html"
  },
  "author": "Dmytro Bogdanov",
  "homepage_url": "https://github.com/dimylkas/localstorage-copier",
  "permissions": [
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
