{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "free.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "48": "cyber.png",
    "128": "cyber.png"
  },
  "description": "Allows you to browse the Internet without the cyber-hassle to deal with all the cyber fucking buzzwords.",
  "name": "Cyber-free",
  "version": "1.0.1",
  "page_action": {
    "default_icon": "cyber.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  }
}
