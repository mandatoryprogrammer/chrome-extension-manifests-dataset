{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Things To Do",
  "description": "This is a simple to-do extension for the Google Chrome browser.",
  "background": {
    "scripts": [
      "dscripts/jquery-3.2.1.min.js",
      "dscripts/firebase.js",
      "dscripts/background.js"
    ],
    "persistent": true
  },
  "version": "0.8.4",
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://routbisu-thingstodo.firebaseio.com https://s-usc1c-nss-204.firebaseio.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage"
  ]
}
