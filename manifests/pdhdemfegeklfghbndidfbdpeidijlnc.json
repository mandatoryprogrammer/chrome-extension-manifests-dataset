{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Buzzkill",
  "description": "Down with Buzzfeed! Remove all clickbait style articles",
  "version": "0.2.3",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "vendor/jquery-2.1.4.min.js",
        "vendor/roundslider.min.js",
        "vendor/fa-multi-button.min.js",
        "js/main.js",
        "dict.js",
        "js/popup.js"
      ],
      "css": [
        "vendor/roundslider.min.css",
        "vendor/font-awesome.min.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Buzzkill"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
