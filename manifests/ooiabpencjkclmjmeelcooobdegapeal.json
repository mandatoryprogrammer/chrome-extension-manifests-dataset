{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet Whatever",
  "description": "author: zhujinghui. Let you share contents on your twitter easily",
  "version": "1.1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "64": "icon.png",
    "16": "icon.png",
    "32": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "contextMenus"
  ]
}
