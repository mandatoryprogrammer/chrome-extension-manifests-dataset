{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CookieJar",
  "description": "This extension is what we're using to learn about chrome extensions and interacting with cookies",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "48": "HappyCookieSmall.png",
    "128": "HappyCookie.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "webRequest",
    "history",
    "storage",
    "<all_urls>"
  ]
}
