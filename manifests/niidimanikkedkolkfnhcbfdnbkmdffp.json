{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitHub Notifications Popover",
  "description": "Display the GitHub notifications list in a popover instead of navigating to a new page.",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://www.github.com/*"
      ],
      "js": [
        "run.js"
      ],
      "run_at": "document_end"
    }
  ]
}
