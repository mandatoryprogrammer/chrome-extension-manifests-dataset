{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History Log for Google Chromeâ„¢",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Google Chrome Extension to allow a user to create a log of their activity.",
  "icons": {
    "16": "script-small-16.png",
    "48": "script-small-16.png",
    "128": "script-small-16.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*",
    "notifications",
    "idle"
  ],
  "browser_action": {
    "default_icon": "script-small-16.png",
    "default_title": "Chrome History Log",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-1.6.1.min.js",
      "util.js",
      "Session.js",
      "SessionList.js",
      "LogEntry.js",
      "background.js"
    ]
  }
}
