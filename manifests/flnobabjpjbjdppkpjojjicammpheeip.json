{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitHub Spooky Contribs",
  "version": "0.0.2",
  "description": "Year-round Halloween theme for GitHub contribs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "css": [
        "spooky.css"
      ]
    }
  ],
  "author": "Jaiden Mispy",
  "homepage_url": "http://github.com/mispy/spooky-contribs"
}
