{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qvorak",
  "description": "Access Quora functions from the keyboard",
  "version": "1.8",
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "*://*.quora.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.quora.com/*"
      ],
      "js": [
        "shortcut.js",
        "qvorak.js"
      ],
      "css": [
        "qvorak.css"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
