{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabSnapture",
  "version": "0.9",
  "manifest_version": 2,
  "description": "Life made easier using instant browser screen capture",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon-d.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      }
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage",
    "browsingData",
    "notifications",
    "http://*/*",
    "https://*/*",
    "file://*/*",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon-d.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
