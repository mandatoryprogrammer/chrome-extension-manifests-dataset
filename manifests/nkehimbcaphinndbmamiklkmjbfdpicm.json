{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rice University Official Page",
  "description": "This extension brings back the good old days when the Facebook page was called Rice University Official Page.",
  "version": "2.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "renamer.js"
      ]
    }
  ]
}
