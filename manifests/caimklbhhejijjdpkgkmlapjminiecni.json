{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MadKudu Spectre (Alpha)",
  "description": "Upgrade your Gmail with MadKudu!",
  "version": "1.3.24",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "myapp.js",
        "jquery.min.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "256": "static/madkudu_square_256.png"
  },
  "web_accessible_resources": [
    "sidebar.html",
    "static/madkudu_square_256.png",
    "options.html"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
