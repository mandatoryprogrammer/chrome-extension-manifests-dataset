{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Duolingo Swahili+",
  "version": "1.0.0",
  "description": "This extension provides users with audio during Swahili lessons on Duolingo",
  "short_name": "Swahili ni nzuri!",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.duolingo.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/tanzania-flag-128.png"
  },
  "manifest_version": 2
}
