{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AdBlock DialTrackers",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "punycode.min.js",
      "jquery/jquery.min.js",
      "port.js",
      "functions.js",
      "filtering/domainset.js",
      "filtering/filteroptions.js",
      "filtering/filtertypes.js",
      "filtering/filterset.js",
      "filtering/myfilters.js",
      "filtering/filternormalizer.js",
      "idlehandler.js",
      "dropbox-datastores.js",
      "gab_question.js",
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "d"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_popup": "button/popup.html",
    "default_title": "__MSG_adblock_click_for_details__"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mail.live.com/*",
        "*://*.mastertoons.com/*",
        "*://*.mobilmania.cz/*",
        "*://*.zive.cz/*",
        "*://*.doupe.cz/*",
        "*://*.e15.cz/*",
        "*://*.sportrevue.cz/*",
        "*://*.autorevue.cz/*",
        "*://*.youtube.com/*"
      ],
      "js": [
        "bandaids.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "port.js",
        "functions.js",
        "filtering/filteroptions.js",
        "adblock_start_common.js",
        "adblock_start_chrome.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "uiscripts/blacklisting/rightclick_hook.js",
        "notificationoverlay.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "default_locale": "en",
  "description": "Addblock test Mario",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "options_page": "options/index.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs",
    "idle",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "alarms",
    "storage",
    "notifications"
  ],
  "optional_permissions": [
    "management"
  ],
  "web_accessible_resources": [
    "img/icon24.png",
    "img/icon48.png",
    "jquery/css/images/ui-bg_inset-hard_100_fcfdfd_1x100.png",
    "jquery/css/images/ui-icons_056b93_256x240.png",
    "jquery/css/images/ui-icons_d8e7f3_256x240.png",
    "jquery/css/jquery-ui.custom.css",
    "jquery/css/override-page.css",
    "chrome_oauth_receiver.html"
  ]
}
