{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Anaplan Formula Helper",
  "description": "Writing formulas in Anaplan is fun!",
  "version": "0.2.1",
  "browser_action": {
    "default_title": "Anaplan Formula Helper"
  },
  "manifest_version": 2,
  "icons": {
    "16": "anaplan.png",
    "48": "anaplan.png",
    "128": "anaplan.png"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///Users/pwalls/Desktop/anaplan_formula_helper/formula_bar_test.html",
        "*://*.anaplan.com/*"
      ],
      "css": [
        "app/style.css"
      ],
      "js": [
        "lib/jquery-1.12.3.min.js",
        "lib/arrive.min.js",
        "app/tooltip.js",
        "app/formula.js",
        "lib/lib.js",
        "app/functions.js"
      ],
      "run_at": "document_end"
    }
  ]
}
