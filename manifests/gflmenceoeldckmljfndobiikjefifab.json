{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTime - Youtube video end indicator",
  "version": "1.0",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch?v*"
      ],
      "run_at": "document_end",
      "js": [
        "time.js"
      ]
    }
  ]
}
