{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bellflower",
  "short_name": "Bellflower",
  "version": "1.0.1",
  "description": "Save tabs as each interested groups.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "Open Tab Groups"
  },
  "permissions": [
    "storage",
    "tabs",
    "downloads",
    "chrome://favicon/"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
