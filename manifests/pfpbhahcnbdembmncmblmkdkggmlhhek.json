{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redd Enough",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Prevents you from reading more than the front page of Reddit.",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "permissions": [
    "http://*.reddit.com/",
    "https://*.reddit.com/"
  ],
  "page_action": {
    "default_title": "Redd Enough",
    "default_icon": "icon-19.png"
  }
}
