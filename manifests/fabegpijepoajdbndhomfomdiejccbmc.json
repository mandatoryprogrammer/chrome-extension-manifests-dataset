{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pivotal Linked Github",
  "description": "Links the pivotal-style syntax messages in Github to Pivotal Tracker stories",
  "version": "0.0.7",
  "author": "Ivan Tse",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "pivotal-linked-github.js"
      ]
    }
  ]
}
