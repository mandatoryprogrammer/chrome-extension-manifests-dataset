{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouControl",
  "version": "1.5",
  "description": "Control your YouTube from anywhere in the browser",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "YC"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "manifest_version": 2
}
