{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Desmos Test Mode",
  "description": "Desmos Test Mode Chrome App.",
  "version": "6.0.0",
  "manifest_version": 2,
  "kiosk_enabled": true,
  "offline_enabled": true,
  "storage": {
    "managed_schema": "schema.json"
  },
  "sandbox": {
    "pages": [
      "www/index.html"
    ]
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "power"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
