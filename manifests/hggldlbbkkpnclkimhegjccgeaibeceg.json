{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "chrome-neo",
  "version": "1.0.11",
  "description": "Chrome extension that uses the asterank API",
  "author": "Brad Taniguchi",
  "browser_action": {
    "default_title": "Chrome-NEO",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.asterank.com/api/asterank"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
