{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_title__",
  "version": "1.4",
  "description": "__MSG_description__",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/args.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "manifest_version": 2,
  "default_locale": "zh_CN"
}
