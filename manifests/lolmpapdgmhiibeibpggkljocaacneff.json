{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Shaqtivator",
  "version": "1.11",
  "description": "Shaqtivate everything.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
