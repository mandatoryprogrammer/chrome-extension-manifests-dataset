{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Salesforce formula editor",
  "description": "This extension Improves Saleforce formula editor",
  "version": "0.0.4",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.salesforce.com/**"
      ],
      "css": [],
      "js": [
        "helper.js",
        "setup.js",
        "loader.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
