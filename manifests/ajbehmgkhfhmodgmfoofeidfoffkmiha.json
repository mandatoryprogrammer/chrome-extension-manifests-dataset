{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Donald's quotes",
  "short_name": "Donald's quotes",
  "description": "Random quote generator made out of Donald Trump tweets and statements as new tab page.",
  "version": "1.0.0",
  "icons": {
    "16": "src/icons/16.png",
    "48": "src/icons/48.png",
    "128": "src/icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/html/trump.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {}
}
