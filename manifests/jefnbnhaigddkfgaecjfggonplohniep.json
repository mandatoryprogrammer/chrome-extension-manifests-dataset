{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NGINX Purge Cache",
  "version": "1.0.0",
  "description": "Purge NGINX cache on current tab",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
