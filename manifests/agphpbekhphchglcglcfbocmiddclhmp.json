{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Involve Cloud",
  "description": "A plugin to help you join a Involve Cloud Virtual Meeting Room.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Involve Cloud Plugin",
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
