{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "geojson.io",
  "version": "0.2.0",
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "description": "Edit files on GitHub and elsewhere in geojson.io",
  "homepage_url": "http://geojson.io/",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://gist.github.com/*"
      ],
      "js": [
        "geojsonio.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "geojsonio.js"
  ],
  "manifest_version": 2
}
