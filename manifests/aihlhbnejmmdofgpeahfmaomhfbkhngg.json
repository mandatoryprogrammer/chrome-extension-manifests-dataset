{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "json-formatter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "JSON formatter for Chrome",
  "icons": {
    "128": "json-logo.png"
  },
  "browser_action": {
    "default_title": "JSON Formatter",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
