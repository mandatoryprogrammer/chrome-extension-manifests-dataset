{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WebPage video player",
  "version": "1.2",
  "minimum_chrome_version": "35",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "webview",
    "fullscreen",
    "alwaysOnTopWindows",
    "storage"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ]
  }
}
