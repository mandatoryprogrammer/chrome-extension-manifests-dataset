{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hipster Block",
  "short_name": "HipsterBlock",
  "description": "Avoid mainstream (1M+ views) videos on youtube",
  "version": "0.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "mustache128.png"
  },
  "permissions": [
    "tabs",
    "https://www.youtube.com/*",
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "48": "mustache48.png",
    "128": "mustache128.png"
  }
}
