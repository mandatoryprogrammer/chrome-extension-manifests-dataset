{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simplify Pantip",
  "version": "1.42",
  "description": "Make Pantip.com more compact and easier to read",
  "content_scripts": [
    {
      "matches": [
        "http://*.pantip.com/topic/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
