{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Newtab",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "zepto.min.js",
        "newtab.js"
      ]
    }
  ],
  "description": "Open links in a new tab on demand",
  "browser_action": {
    "default_icon": {
      "19": "assets/off.png"
    }
  },
  "icons": {
    "128": "assets/128.png",
    "64": "assets/64.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "https://github.com/orzogen/newtab",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "background",
    "*://*/*"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+D",
        "mac": "Command+Shift+D",
        "chromeos": "Ctrl+Shift+D",
        "linux": "Ctrl+Shift+D"
      }
    }
  }
}
