{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QMB",
  "manifest_version": 2,
  "version": "0.5",
  "description": "Save the tabs you want. Read it later.",
  "background": {
    "scripts": [
      "myscript.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
    "cns-tab": {
      "suggested_key": {
        "default": "Ctrl+ Q",
        "mac": "Command+ Q"
      },
      "description": "Save and close the current tab"
    },
    "win-recall": {
      "suggested_key": {
        "default": "Ctrl+ M",
        "mac": "Command+ M"
      },
      "description": "Open a new window with all your saved reads."
    },
    "win-dump": {
      "suggested_key": {
        "default": "Ctrl+ B",
        "mac": "Command+ B"
      },
      "description": "Close the magic window and saves all the tabs."
    }
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
