{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Findr",
  "version": "0.2.0",
  "description": "Bookmark fuzzy finder",
  "author": "Jason Younker",
  "homepage_url": "https://chrome.google.com/webstore/detail/findr/blnnnmdjhikmhhjdmjnmllphjkabplee?hl=en",
  "icons": {
    "16": "images/findr-icon-16x16.png",
    "48": "images/findr-icon-48x48.png",
    "128": "images/findr-icon-128x128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Findr",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "history"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Ctrl+B",
        "linux": "Ctrl+B"
      }
    }
  }
}
