{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "directory.ucsf.edu Focus",
  "description": "This extension focuses the search box of the UCSF Directory (directory.ucsf.edu) on page load.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://directory.ucsf.edu/*"
      ],
      "run_at": "document_end",
      "js": [
        "focus.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
