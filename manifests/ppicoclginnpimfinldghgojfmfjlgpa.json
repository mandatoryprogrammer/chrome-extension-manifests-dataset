{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "二维码助手-QRassistant",
  "description": "一键生成URL二维码 支持输入内容转二维码",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/vendor/jquery-1.10.2.min.js",
      "js/jquery.qrcode.js",
      "js/qrcode.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "点击生成二维码",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
