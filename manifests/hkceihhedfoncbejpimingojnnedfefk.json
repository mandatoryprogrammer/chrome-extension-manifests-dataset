{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Garbage Collector",
  "description": "Bookmark and then close tabs after they've been left inactive for too long",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "alarms"
  ],
  "browser_action": {
    "default_icon": "icon-disabled-128.png"
  },
  "icons": {
    "16": "icon-disabled-16.png",
    "48": "icon-disabled-48.png",
    "128": "icon-disabled-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html"
}
