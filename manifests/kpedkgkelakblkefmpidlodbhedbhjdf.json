{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Better Destiny.gg",
  "version": "2.4.1",
  "manifest_version": 2,
  "description": "Make Destiny.gg Great Again Again",
  "icons": {
    "48": "./static/icon/icon.png",
    "128": "./static/icon/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "./static/icon/icon.png",
      "128": "./static/icon/icon128.png"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.destiny.gg/*"
  ],
  "background": {
    "scripts": [
      "./index.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "dgg/*",
    "static/*"
  ]
}
