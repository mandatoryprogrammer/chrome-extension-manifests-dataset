{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SiouXtrack",
  "version": "0.68",
  "description": "Observation du comportement pour un diagnostic ergonomique",
  "homepage_url": "https://sioux.univ-paris8.fr/sioux",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.1.0.min.js",
        "inject.js"
      ],
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "Enregistrer maintenant"
  }
}
