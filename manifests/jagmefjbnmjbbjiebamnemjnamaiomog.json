{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Irene's mood",
  "version": "0.2",
  "description": "An extension to show what mood Irene has at this moment",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_title": "Irene's mood",
    "default_icon": {
      "19": "img/icon19.png",
      "39": "img/icon39.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/popup.js"
      ]
    }
  ]
}
