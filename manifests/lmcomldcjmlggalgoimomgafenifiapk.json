{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.8",
  "name": "Add to Tubulr",
  "short_name": "Tubulr",
  "description": "Add videos to Tubulr - from any web page!",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_title": "Add to Tubulr"
  },
  "author": "Chris Kirkinis",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js https://www.tubulr.tv https://tubulr.herokuapp.com; object-src 'self'",
  "homepage_url": "http://www.tubulr.tv",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
