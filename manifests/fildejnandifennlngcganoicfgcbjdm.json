{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Accidental Likes Preventer for Facebook",
  "version": "1.0.2",
  "description": "__MSG_chrome_extension_description__",
  "default_locale": "en",
  "icons": {
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
