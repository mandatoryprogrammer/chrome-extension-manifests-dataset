{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Radio Station-to-Station Ext",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Free Internet Radio Player",
  "icons": {
    "16": "s2s-icon-16.png",
    "32": "s2s-icon-32.png",
    "48": "s2s-icon-48.png",
    "64": "s2s-icon-64.png",
    "128": "s2s-icon-128.png",
    "256": "s2s-icon-256.png",
    "512": "s2s-icon-512.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "s2s-icon-128.png"
  },
  "homepage_url": "https://radios2s.scriptel.nl",
  "content_security_policy": "script-src 'self' https://ipinfo.io; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
