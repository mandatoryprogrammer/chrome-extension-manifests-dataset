{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SSFP",
  "version": "0.1.0",
  "description": "Stop Shitty Facebook Photos - Facebook Photo Optimization",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "ssfp.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
