{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NSFSquirrels",
  "description": "This extension replaces the thumbnails for NSFW posts on Reddit with pictures of squirrels.",
  "version": "1.1",
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "http://www.reddit.com/*/*"
      ],
      "js": [
        "NSFSquirrels.js"
      ]
    }
  ]
}
