{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK anonymous reader",
  "version": "1.0.0",
  "description": "Read articles on vk.com (e.g. vk.com/@article-name) without logging in.",
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/@*"
      ],
      "js": [
        "block-redirect.js"
      ]
    }
  ],
  "icons": {
    "32": "images/icon.png"
  },
  "manifest_version": 2
}
