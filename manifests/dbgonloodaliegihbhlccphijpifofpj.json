{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "/res/js/ajax.js",
      "/res/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "/res/css/contents/ip.css"
      ],
      "js": [
        "/res/js/contents/ip.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "在网页右下角显示服务器的IP地址及地理位置，使用纯真IP数据库。",
  "icons": {
    "128": "/res/icon/icon.png",
    "64": "/res/icon/icon.png",
    "32": "/res/icon/icon.png",
    "16": "/res/icon/icon.png"
  },
  "manifest_version": 2,
  "name": "Ohrz IP",
  "browser_action": {
    "default_icon": {
      "19": "res/icon/icon.png",
      "38": "res/icon/icon.png"
    },
    "default_title": "点击开启或关闭"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "storage"
  ],
  "version": "1.5"
}
