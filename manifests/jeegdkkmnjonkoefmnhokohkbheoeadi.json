{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BookmarksReminder",
  "author": "pinetree408(Sangyoon Lee)",
  "icons": {
    "128": "static/ext-icon/icon128.png"
  },
  "version": "0.1.0",
  "description": "This extension gives you a reminder alarm for bookmarks.",
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "BookmarksReminder",
    "default_popup": "templates/index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
