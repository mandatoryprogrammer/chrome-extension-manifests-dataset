{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Workflow",
  "version": "2.2.2",
  "description": "An extension that allows you to hide files in pull requests and adds highlighting for notifications.",
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "app/app.js"
      ]
    }
  ]
}
