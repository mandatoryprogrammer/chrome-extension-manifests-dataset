{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nexters.Me",
  "version": "0.1.1",
  "description": "shorten your url",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Shorten Your Url"
  },
  "icons": {
    "16": "./images/logo.png",
    "32": "./images/logo.png",
    "64": "./images/logo.png",
    "128": "./images/logo.png"
  },
  "manifest_version": 2
}
