{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Calculate My GPA",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Select classes to include in your average GPA from the Yale SIS page",
  "short_name": "Caclulate My GPA",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
