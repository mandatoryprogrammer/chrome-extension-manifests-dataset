{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide YouTube Google Apps reminder",
  "version": "0.1",
  "description": "Hides the \"This account is managed by\" warning on YouTube pages. Open source.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "css": [
        "src/inject.css"
      ]
    }
  ],
  "manifest_version": 2
}
