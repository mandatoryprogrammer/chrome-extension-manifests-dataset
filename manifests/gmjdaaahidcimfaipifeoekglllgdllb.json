{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SE Chat Faces",
  "version": "1.0.3",
  "description": "A Chrome Extension that enables the use of your favorite Unicode faces in all open chat windows.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "face.png"
  },
  "web_accessible_resources": [
    "faces.js"
  ],
  "permissions": [
    "storage",
    "tabs",
    "*://chat.stackexchange.com/rooms/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://chat.stackexchange.com/rooms/*"
    ]
  }
}
