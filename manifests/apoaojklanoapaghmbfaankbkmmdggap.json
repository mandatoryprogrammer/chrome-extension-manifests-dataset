{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dailyrush.dk Extensions",
  "description": "Usability extensions for Dailyrush.dk",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.dailyrush.dk/*",
        "http://dailyrush.dk/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "externallinks.js"
      ],
      "run_at": "document_end"
    }
  ]
}
