{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UMass Professor Ratings",
  "version": "1.2",
  "background": {
    "scripts": [
      "jquery-3.2.1.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.spire.umass.edu/*"
      ],
      "js": [
        "jquery-3.2.1.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "http://www.ratemyprofessors.com/*",
    "https://www.ratemyprofessors.com/*",
    "https://www.spire.com.umass.edu/*"
  ]
}
