{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nudge",
  "description": "Raw productivity",
  "version": "1.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "notifications",
    "alarms",
    "storage"
  ]
}
