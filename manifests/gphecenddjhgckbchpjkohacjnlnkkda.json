{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": ".kairos",
  "manifest_version": 2,
  "name": "Mum's Media Launcher",
  "description": "Easily access some of the most helpful websites for mums in the UK.",
  "icons": {
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
