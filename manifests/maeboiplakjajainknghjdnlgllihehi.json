{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDB for Netflix",
  "description": "IMDB rating of movies and series on Netflix",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "imdb.svg"
  ],
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "imdb.png"
  }
}
