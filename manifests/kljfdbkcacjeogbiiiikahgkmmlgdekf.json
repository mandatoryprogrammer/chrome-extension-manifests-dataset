{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Toogles",
  "manifest_version": 2,
  "description": "Redirect YouTube URLs to Toogl.es for fun and profit",
  "version": "1.1.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/watch*"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  }
}
