{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.1",
  "description": "Search bookmarks & Change Tab's background image.",
  "manifest_version": 2,
  "name": "Search Tabs",
  "short_name": "Search Tabs",
  "options_page": "options.html",
  "icons": {
    "16": "image/16.png",
    "32": "image/32.png",
    "48": "image/48.png",
    "128": "image/128.png"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "omnibox": {
    "keyword": "ss"
  },
  "browser_action": {
    "default_title": "Bookmarks & Tabs",
    "default_icon": "image/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
