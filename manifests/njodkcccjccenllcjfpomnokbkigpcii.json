{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "github-old-colors",
  "version": "0.1.1",
  "description": "Makes Github.com colors as they were",
  "icons": {
    "48": "images/github-48.png",
    "96": "images/github-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "css": [
        "css/github-old-colors.css"
      ]
    }
  ]
}
