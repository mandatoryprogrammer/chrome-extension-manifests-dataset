{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "tcp-server.js",
        "main.js"
      ]
    }
  },
  "permissions": [
    {
      "socket": [
        "tcp-listen"
      ]
    },
    "geolocation",
    "clipboardWrite"
  ]
}
