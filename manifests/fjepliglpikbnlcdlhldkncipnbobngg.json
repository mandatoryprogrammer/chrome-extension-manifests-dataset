{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wordcipher",
  "description": "Learn a new word when you open a new tab!",
  "version": "1.0.2",
  "icons": {
    "16": "./assets/icons/logo_16.png",
    "48": "./assets/icons/logo_48.png",
    "128": "./assets/icons/logo_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "background": {
    "scripts": [
      "./build/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
