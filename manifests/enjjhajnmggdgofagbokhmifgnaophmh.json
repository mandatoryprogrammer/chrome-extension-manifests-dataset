{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Resolution Zoom",
  "short_name": "reszoom",
  "description": "Set the zoom level based on your screen resolution.",
  "version": "0.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "system.display"
  ],
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
