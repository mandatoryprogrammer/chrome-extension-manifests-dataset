{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrueAutomation.IO Element Picker",
  "short_name": "TA Element Picker",
  "description": "Write your tests faster and make them more reliable with TrueAutomation.IO smart locators.",
  "version": "1.0.8",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "activeTab",
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon-small.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
