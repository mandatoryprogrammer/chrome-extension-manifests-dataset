{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.2",
  "name": "Dewey launcher",
  "description": "Extension to add a launch icon for Dewey",
  "background": {
    "scripts": [
      "launcher.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon_19.png",
      "38": "icon_38.png"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "management"
  ]
}
