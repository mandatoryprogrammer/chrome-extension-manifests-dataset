{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.4.1",
  "default_locale": "zh_CN",
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "browser_action": {
    "default_icon": "star.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*.imdb.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://movie.douban.com/subject/*"
      ],
      "css": [
        "dbj.css"
      ],
      "js": [
        "dbj.js"
      ]
    }
  ]
}
