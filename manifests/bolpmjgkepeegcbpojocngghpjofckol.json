{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StackOverflow Hub",
  "description": "This Extension allows quick access to StackOverflow",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "StackOverflow"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://stackoverflow.com/*",
    "https://stackexchange.com/*",
    "identity",
    "storage"
  ]
}
