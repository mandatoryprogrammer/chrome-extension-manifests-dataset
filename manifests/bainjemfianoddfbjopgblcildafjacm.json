{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hold To Tab",
  "description": "A one-handed shortcut for cmd/ctrl+click.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "handler.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "manifest_version": 2
}
