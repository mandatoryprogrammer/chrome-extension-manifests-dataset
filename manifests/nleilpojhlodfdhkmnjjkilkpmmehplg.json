{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Problems Tags - Codeforces",
  "version": "1.2.2",
  "description": "Very simple extension to hide/show problems tags",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/*"
      ],
      "js": [
        "problem_tags.js",
        "problemset_tags.js"
      ]
    }
  ],
  "icons": {
    "16": "images/cf16.png",
    "32": "images/cf32.png",
    "48": "images/cf48.png",
    "128": "images/cf128.png"
  },
  "manifest_version": 2
}
