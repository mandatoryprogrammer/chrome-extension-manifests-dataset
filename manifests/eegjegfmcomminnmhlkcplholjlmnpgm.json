{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA support",
  "version": "0.2.1",
  "icons": {
    "16": "/res/favicon-16x16.png",
    "32": "/res/favicon-32x32.png",
    "128": "/res/favicon-128x128.png"
  },
  "browser_action": {
    "default_icon": "/res/jiraSup.png",
    "default_popup": "popup.html",
    "default_title": "JIRA Support"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*/altemista.cloud/*",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "/js/jquery.min.js"
    ],
    "persistent": false
  }
}
