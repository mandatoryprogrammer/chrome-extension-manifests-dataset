{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paymo user checker",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://app.paymoapp.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Paymo user setting"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "Cauldron-icon.png"
  }
}
