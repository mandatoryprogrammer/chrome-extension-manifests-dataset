{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mandarake Item Tracker",
  "version": "1.0.1",
  "author": "Andy Nguyen",
  "manifest_version": 2,
  "description": "Track ekizo.mandarake.co.jp for new item listings.",
  "options_page": "options.html",
  "icons": {
    "128": "img/icon_128.png",
    "64": "img/icon_64.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Mandarake Item Tracker"
  },
  "background": {
    "scripts": [
      "js/jquery-2.0.3.min.js",
      "js/parser.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "http://ekizo.mandarake.co.jp/*"
  ],
  "offline_enabled": false,
  "minimum_chrome_version": "20"
}
