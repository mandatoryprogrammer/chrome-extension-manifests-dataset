{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Баланс абонента Электронного города",
  "short_name": "Баланс ЭГ",
  "version": "0.0.5",
  "description": "Отображение баланса абонента Электронного города",
  "homepage_url": "http://2090000.ru/",
  "minimum_chrome_version": "47",
  "author": "Chunizhekov Sergey <chsergey@chsergey.ru>",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/icon22.png",
    "default_title": "Электронный город",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "storage",
    "https://api.novotelecom.ru/*"
  ]
}
