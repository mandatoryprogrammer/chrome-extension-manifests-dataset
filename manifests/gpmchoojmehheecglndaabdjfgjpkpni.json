{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "src/pass_genny.js"
    ]
  },
  "name": "Password Generator",
  "short_name": "Pass Genny",
  "author": "Lui Crowie",
  "description": "An extension to create hard to crack sudo-random passwords.",
  "version": "0.3",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "browser_action": {
    "default_popup": "src/pass_genny.html",
    "default_icon": "icons/icon32.png",
    "default_title": "Password Generator"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Will open the Pass Genny pop-up page"
    }
  },
  "permissions": [
    "tabs"
  ]
}
