{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Historify Client",
  "description": "Connects to historify to save browser history: http://historify.sveri.de/",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Historify Client"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "src/jquery-3.0.0.min.js",
      "src/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "webNavigation",
    "storage"
  ]
}
