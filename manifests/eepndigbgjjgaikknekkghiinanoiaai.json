{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.1.0",
  "name": "HTML format cleaner",
  "description": "right-click context-menu items to clear HTML formating and copy as plain text",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "*://*/*"
  ],
  "homepage_url": "http://add0n.com/html-format-cleaner.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
