{
  "background": {
    "scripts": [
      "shared.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "https://ads-admin.local.twitter.com/tools/adroles/*"
      ]
    }
  ],
  "description": "Convert base 36 numbers into base 10 numbers",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "B36",
  "permissions": [
    "contextMenus",
    "https://ads-admin.local.twitter.com/tools/adroles/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.4"
}
