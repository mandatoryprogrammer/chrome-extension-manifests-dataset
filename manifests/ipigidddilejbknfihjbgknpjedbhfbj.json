{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PageMerger",
  "options_page": "Tricky.html",
  "description": "Merge websites",
  "version": "1.0",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Extension that merge websites.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "Content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
