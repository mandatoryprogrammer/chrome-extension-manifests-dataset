{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NowOn - Personal News Filter",
  "short_name": "NowOn",
  "description": "A new way to see the news that matters to you.",
  "version": "0.0.7",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "background",
    "storage",
    "geolocation",
    "history",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2,
  "icons": {
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon-128.png"
  }
}
