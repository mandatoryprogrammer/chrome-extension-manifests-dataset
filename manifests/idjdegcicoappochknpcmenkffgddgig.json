{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Screen Sharing",
  "author": "iZND, Inc.",
  "version": "1.4.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for Boardroom demos. It simply captures the content of your screen.",
  "homepage_url": "https://demo.boardroom.work",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "https://demo.boardroom.work/*",
        "https://boardroom2.herokuapp.com/*"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
