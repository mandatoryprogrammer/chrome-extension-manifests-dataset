{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark",
  "version": "2.0.2",
  "manifest_version": 2,
  "description": "search bookmark quickly.",
  "icons": {
    "16": "img/icon_016.png",
    "48": "img/icon_048.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/icon_048.png",
    "default_title": "Bookmark",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "bookmarks",
    "chrome://favicon/"
  ]
}
