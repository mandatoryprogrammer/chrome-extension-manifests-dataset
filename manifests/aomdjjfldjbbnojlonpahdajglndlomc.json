{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Plurk-Smile",
  "version": "3.4.1",
  "description": "__MSG_extdesc__",
  "icons": {
    "128": "128.png",
    "16": "16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.plurk.com/*",
        "https://www.plurk.com/*"
      ],
      "js": [
        "jquery-1.7.min.js",
        "content-script.js"
      ]
    }
  ],
  "default_locale": "en",
  "permissions": [
    "http://www.plurk.com/",
    "https://www.plurk.com/"
  ]
}
