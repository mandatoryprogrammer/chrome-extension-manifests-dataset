{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hard reload",
  "description": "Adds a hard reload button to chrome.",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "hard-reload": {
      "suggested_key": {
        "default": "Alt+R",
        "mac": "Alt+R"
      },
      "description": "Hard reloads"
    }
  },
  "permissions": [
    "activeTab",
    "browsingData"
  ],
  "browser_action": {
    "default_title": "Hard reload this page",
    "default_icon": "hard_reload.png"
  },
  "manifest_version": 2
}
