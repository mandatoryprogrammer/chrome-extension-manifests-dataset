{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Squitter",
  "description": "A simple Chrome extension to re-enable square avatars and UI elements on Twitter.",
  "version": "0.1.1",
  "icons": {
    "128": "squitter-icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "css/squitter.css"
      ]
    }
  ]
}
