{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Download Teams Photos",
  "description": "Downloads Teams Conversation photos that are not saved to SharePoint Document Library.",
  "version": "1.0.2",
  "author": "Alec Maly",
  "content_scripts": [
    {
      "matches": [
        "*://*.teams.microsoft.com/*"
      ],
      "js": [
        "download_teams_photos.js"
      ]
    }
  ],
  "permissions": [
    "*://*.teams.microsoft.com/*",
    "https://us-api.asm.skype.com/*/"
  ]
}
