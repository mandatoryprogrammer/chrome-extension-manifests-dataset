{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dark-Themed Chrome File Explorer",
  "version": "0.5.0",
  "description": "Makes the chrome file explorer dark-themed.",
  "icons": {
    "128": "icon/icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "file://*"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
