{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redmine Monospace Code",
  "short_name": "RMC",
  "description": "Show Redmine's <pre>code</pre> in monospace",
  "manifest_version": 2,
  "version": "1.0.0.1",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*redmine*"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}
