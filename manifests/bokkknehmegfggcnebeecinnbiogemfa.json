{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FifaWorldcup2018",
  "description": "A chrome extention that displays world cup standings when a new tab is opened",
  "version": "1.3",
  "author": "Shubham Zanwar",
  "browser_action": {
    "default_title": "Have a good day",
    "default_icon": "./assets/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
