{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Proxy Switcher",
  "version": "1.4",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "proxy",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon24-grey.png",
    "default_popup": "popup.html",
    "default_title": "Proxy Switcher"
  }
}
