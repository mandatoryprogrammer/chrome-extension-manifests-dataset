{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IBL log",
  "version": "0.1.1",
  "description": "Logs calls to IBL",
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.bbc.co.uk/*",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "",
    "default_icon": "img/logo.png"
  },
  "icons": {
    "48": "img/logo.png"
  }
}
