{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Leadmehome",
  "version": "1.1.3",
  "description": "Find Emails for your prospects",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "https://leadmehome.io/"
  ],
  "background": {
    "scripts": [
      "script/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "app.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://stackpath.bootstrapcdn.com https://cdnjs.cloudflare.com https://maxcdn.bootstrapcdn.com https://leadmehome.io; object-src 'self'",
  "manifest_version": 2
}
