{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "off_icon_24.png",
    "default_title": "Click to activate/deactivate Cool Blue Depaginator for current site"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "name": "Cool Blue Depaginator",
  "icons": {
    "16": "icon_16.png",
    "24": "off_icon_24.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "version": "0.0.1"
}
