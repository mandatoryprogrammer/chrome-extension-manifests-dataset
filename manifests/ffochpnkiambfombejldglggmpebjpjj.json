{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FiX3P",
  "version": "0.7.2",
  "description": "Edit X3P files within chrome",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "chrome.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "favicon.76a72281.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "favicon.76a72281.png"
    }
  },
  "options_page": "settings.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
