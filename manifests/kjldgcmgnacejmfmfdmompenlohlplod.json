{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "platen",
  "description": "Markdown WordPress Editor",
  "version": "1.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardRead",
    "clipboardWrite",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  },
  "sandbox": {
    "pages": [
      "analytics/embedded_ga_host.html"
    ]
  }
}
