{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoXpiher",
  "version": "0.0.1",
  "description": "Blocks any posts from Xpiher on the Darkfall forums.",
  "short_name": "NoXpiher",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "NoXpiher"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://forums.darkfallonline.com/*"
      ],
      "js": [
        "NoXpiher.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}
