{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BIBreaker",
  "version": "0.0.2",
  "description": "Get through the BI paywall in one click.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
