{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Warblers",
  "description": "New way to catch up.",
  "version": "2.0",
  "permissions": [
    "alarms",
    "tabs",
    "bookmarks",
    "activeTab",
    "storage",
    "*://*.youtube.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com.tr/*",
        "*://*.youtube.com/*",
        "*://*.fizy.com/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Narwhals",
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": [
      "*://*.youtube.com/*"
    ]
  },
  "manifest_version": 2
}
