{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "990 Direct",
  "manifest_version": 2,
  "version": "1.4.4",
  "description": "Gets you the direct link to 990.ro tv series/films",
  "browser_action": {
    "name": "990 Direct",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "js": [
        "990Direct.js"
      ],
      "run_at": "document_end",
      "matches": [
        "http://990.ro/*",
        "http://www.990.ro/*",
        "http://superweb.rol.ro/*",
        "http://www.mastervid.com/*",
        "https://openload.co/*",
        "http://thevideo.me/*",
        "http://gorillavid.in/*",
        "http://daclips.in/*",
        "http://allmyvideos.net/*",
        "http://vodlocker.com/*",
        "http://movpod.in/*",
        "http://vidbull.com/*",
        "http://streamin.to/*",
        "http://filehoot.com/*",
        "http://vidzi.tv/*",
        "http://vidspot.net/*",
        "http://vidto.me/*",
        "http://rosharing.net/*"
      ]
    }
  ]
}
