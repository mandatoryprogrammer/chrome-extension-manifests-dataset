{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tube English",
  "description": "Open Tube English by current url",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Send to Tube English"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
