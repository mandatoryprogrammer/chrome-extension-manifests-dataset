{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SO UI - Stack Overflow Special",
  "short_name": "SO UI",
  "description": "Improved user interface of StackOverflow (SO).",
  "version": "0.2.1",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "css.css"
      ],
      "matches": [
        "*://stackoverflow.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
