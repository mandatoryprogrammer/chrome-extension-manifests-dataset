{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "zeroswag",
  "description": "Displays a random item from a set of things.",
  "version": "0.0.0.5",
  "manifest_version": 2,
  "permissions": [
    "management",
    "http://zeroswag.mobil.ninja/"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    },
    "default_title": "zeroswag"
  },
  "background": {
    "scripts": [
      "icon.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
