{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BiliForeign",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "帮助B站海外用户观看版权番",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "header.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_title": "BiliForeign"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.bilibili.com/*",
    "*://interface.bilibili.com/playurl*"
  ]
}
