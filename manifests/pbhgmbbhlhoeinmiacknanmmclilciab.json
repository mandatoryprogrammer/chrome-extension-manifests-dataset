{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TimeM",
  "description": "A guilt-tripping app for students who waste time on social media website!",
  "version": "1.0",
  "background": {
    "scripts": [
      "jquery-2.1.1.js",
      "parse-1.2.19.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.1.js",
        "parse-1.2.19.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "TimeM Time Tracker Beta",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>",
    "management"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://192.185.184.192/~rgbastud/timem.github.io/*"
    ]
  },
  "web_accessible_resources": [
    "news3.png"
  ]
}
