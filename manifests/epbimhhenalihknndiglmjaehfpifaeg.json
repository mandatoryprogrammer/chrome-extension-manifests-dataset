{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ActionScript 3.0 Search",
  "manifest_version": 2,
  "description": "Adds support to the omnibox to search the AS3 reference docs.",
  "background": {
    "scripts": [
      "String.js",
      "index.js",
      "background.js",
      "action/actionHandler.js"
    ]
  },
  "icons": {
    "128": "icons/icon128.png",
    "32": "icons/icon32.png",
    "16": "icons/icon16.png"
  },
  "omnibox": {
    "keyword": "as3"
  },
  "permissions": [
    "tabs",
    "*://help.adobe.com/"
  ],
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "19": "action/flags/19/en_US.jpg",
      "38": "action/flags/38/en_US.jpg"
    },
    "default_title": "AS3 language",
    "default_popup": "action/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://help.adobe.com/*"
      ]
    }
  ],
  "version": "3.1.0"
}
