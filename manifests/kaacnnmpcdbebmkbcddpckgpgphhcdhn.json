{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FAIR-biomed",
  "short_name": "FAIR-biomed",
  "version": "0.1.3",
  "description": "Open data resources in the biomedical domain",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "library/library-min.js",
      "resources/bundle-background.js",
      "js/common.js",
      "js/state.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/fair.css"
      ],
      "js": [
        "resources/bundle.js",
        "library/library-min.js",
        "js/common.js",
        "js/content.js"
      ]
    }
  ],
  "options_page": "html/options.html",
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_action": {
      "32": "resources/images/fair-biomed-32.png",
      "128": "resources/images/fair-biomed-128.png"
    }
  },
  "icons": {
    "32": "resources/images/fair-biomed-32.png",
    "128": "resources/images/fair-biomed-128.png"
  },
  "manifest_version": 2
}
