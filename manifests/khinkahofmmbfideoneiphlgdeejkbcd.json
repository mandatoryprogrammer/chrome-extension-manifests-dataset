{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "npm home",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Redirect npm.org packages directly to the package's actual home page.",
  "content_scripts": [
    {
      "matches": [
        "https://www.npmjs.org/package/*",
        "https://npmjs.org/package/*"
      ],
      "run_at": "document_end",
      "js": [
        "index.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://www.npmjs.org/package/*, https://npmjs.org/package/*"
  ]
}
