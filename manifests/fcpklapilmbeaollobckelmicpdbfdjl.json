{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OPWS Toolkit",
  "version": "0.1.0",
  "icons": {
    "16": "icons/opws-logo-16px.png",
    "48": "icons/opws-logo-48px.png",
    "128": "icons/opws-logo-128px.png"
  },
  "description": "An extension to facilitate profiling websites for OPWS",
  "browser_action": {
    "default_icon": "icons/opws-logo-48px.png",
    "default_title": "OPWS Toolkit",
    "default_popup": "popup.html"
  },
  "author": "Stuart P. Bentley",
  "background": {
    "scripts": [
      "vendor/gistachio.js",
      "scripts/eventpage.js"
    ],
    "persistent": false
  },
  "homepage_url": "https://github.com/opws/opws-surveyor-extension",
  "incognito": "split",
  "permissions": [
    "activeTab",
    "pageCapture"
  ]
}
