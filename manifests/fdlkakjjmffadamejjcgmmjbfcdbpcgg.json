{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "bkmrks",
  "description": "A simple New-Tab-Page for displaying all your bookmarks on one page.",
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "bkmrks.html"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
