{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alla Bolag Chrome",
  "version": "1.0.0",
  "description": "Opens the current selection on www.allabolag.se",
  "author": "Urban Sanden",
  "permissions": [
    "tabs",
    "contextMenus",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/browsericon.png",
      "38": "images/browsericon.png"
    },
    "default_title": "Allabolag"
  },
  "icons": {
    "16": "images/favicon.png",
    "128": "images/favicon.png"
  }
}
