{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Tulio Spuri",
  "background": {
    "persistent": false,
    "scripts": [
      "eventPage.js"
    ]
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "windows": "Alt+Q"
      }
    }
  },
  "description": "In Trello, copy the branch name in a fast way.",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "Get branch",
  "page_action": {
    "default_icon": "icon_19.png",
    "default_title": "Get branch"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "clipboardWrite"
  ],
  "version": "1.4"
}
