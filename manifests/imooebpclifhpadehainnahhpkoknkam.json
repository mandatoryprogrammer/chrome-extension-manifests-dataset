{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Sauce",
  "version": "2.0",
  "description": "Finds the video sauce in the comments of NSFW posts.",
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "css": [
        "sauce.css"
      ],
      "js": [
        "sauce.js"
      ]
    }
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
