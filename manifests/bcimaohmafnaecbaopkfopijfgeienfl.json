{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Primer-BLAST",
  "version": "0.2",
  "description": "Highlight a FASTA sequence and quickly BLAST for primers",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
