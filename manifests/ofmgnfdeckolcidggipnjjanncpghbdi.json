{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Request domain list",
  "version": "1.0.1",
  "description": "Record the domain which be communicated with current tab.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
