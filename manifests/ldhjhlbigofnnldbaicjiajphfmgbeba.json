{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MyHook",
  "description": "MyHook is a Tool which helps you exposing your local services for your clients, team mates and other development tools.",
  "version": "1.0.4",
  "browser_action": {
    "default_icon": "image.png",
    "default_title": "MyHook"
  },
  "icons": {
    "16": "image.png",
    "48": "image.png",
    "128": "image.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/socket.io.js",
      "js/logic.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://*/",
    "http://*/"
  ]
}
