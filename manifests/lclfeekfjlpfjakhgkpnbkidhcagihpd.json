{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Corsane",
  "version": "0.0.32",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/corsane.png",
    "128": "images/corsane.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/corsane.png",
      "38": "images/corsane.png"
    },
    "default_title": "Corsane",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "Corsane"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
