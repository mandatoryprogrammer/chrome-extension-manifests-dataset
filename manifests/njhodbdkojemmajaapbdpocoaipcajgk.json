{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Promote Team Dinner Assistant",
  "version": "1.3",
  "description": "Our mission is to help everyone find their food on GrubHub",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {}
  },
  "permissions": [
    "*://*.grubhub.com/*",
    "tabs",
    "debugger"
  ]
}
