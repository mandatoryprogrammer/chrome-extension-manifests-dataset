{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Crypto Piggy",
  "description": "Crypto Piggy is an open source crypto portfolio extension that allows users to track the performance of their Ethereum addresses",
  "version": "1.1",
  "browser_action": {
    "default_icon": "./style/images/kayla_revised_v2_transparent_background.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./style/images/kayla_revised_v2_transparent_background.png",
    "48": "./style/images/CP_Logo_Gradient_L.png",
    "128": "./style/images/Cryptopiggy_Logo_L.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
