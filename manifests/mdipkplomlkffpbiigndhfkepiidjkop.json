{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Finder",
  "description": "Find a tab with some search conditions",
  "version": "1.2",
  "browser_action": {
    "default_popup": "popup.htm"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
