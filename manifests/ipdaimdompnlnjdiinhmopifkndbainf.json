{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DLsiteWarn",
  "version": "1.0.1",
  "description": "DLサイト閲覧時に要注意サークルを表示する",
  "content_scripts": [
    {
      "matches": [
        "http://www.dlsite.com/*",
        "https://www.dlsite.com/*"
      ],
      "js": [
        "js/jquery-3.1.1.min.js",
        "js/warncircle.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ]
}
