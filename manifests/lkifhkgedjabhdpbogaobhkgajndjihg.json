{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello card counter",
  "description": "Count Trello cards in Lists",
  "version": "1.0.1",
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "author": "Martin Mouterde",
  "content_scripts": [
    {
      "css": [
        "style/style.css"
      ],
      "js": [
        "lib/jquery-1.11.2.js",
        "lib/jquery.waituntilexists.js",
        "scripts/main.js"
      ],
      "matches": [
        "https://trello.com/b*"
      ]
    }
  ],
  "permissions": [
    "https://trello.com/*"
  ],
  "manifest_version": 2
}
