{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Communa",
  "version": "1.0",
  "description": "Plays communist music when certain words are clicked",
  "manifest_version": 2,
  "author": "Nicholas Wang",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "soviet.mp3",
    "china.mp3",
    "note.mp3"
  ]
}
