{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Notifieurs de Live Ashuvidz",
  "version": "1.0.2",
  "description": "Verifie si Ashuvidz est entrain de Streamer et notifie l'utilisateur.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "notifications",
    "https://api.twitch.tv/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "48.png"
  }
}
