{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Leap Tools",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Chrome Leap Tools",
    "default_popup": "menu.html",
    "default_icon": {
      "19": "leaptools19.png",
      "38": "leaptools38.png"
    }
  },
  "manifest_version": 2
}
