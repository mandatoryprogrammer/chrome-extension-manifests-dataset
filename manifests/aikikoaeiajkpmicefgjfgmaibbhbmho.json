{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BitBucket Pull Request Review Helper",
  "description": "This extension allows you to flag individual files in a pull request as reviewed",
  "version": "1.6.0",
  "homepage_url": "https://github.com/reywood/bitbucket-pr-review",
  "author": "Sean Dwyer",
  "key": "",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.org/*/pull-requests/*"
      ],
      "css": [
        "src/style.css"
      ],
      "js": [
        "src/throttle.js",
        "src/dataStore.js",
        "src/fileDiff.js",
        "src/hash.js",
        "src/init.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
