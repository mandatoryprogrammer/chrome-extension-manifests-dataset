{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "G+Bookmark",
  "version": "1.2",
  "description": "G+の投稿をお気に入りに直接保存します",
  "homepage_url": "https://plus.google.com/b/112102646198561777507/",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "permissions": [
    "bookmarks",
    "https://plus.google.com/"
  ],
  "content_scripts": [
    {
      "js": [
        "G+Bookmark.js"
      ],
      "css": [
        "G+Bookmark.css"
      ],
      "matches": [
        "https://plus.google.com/*"
      ]
    }
  ],
  "manifest_version": 2
}
