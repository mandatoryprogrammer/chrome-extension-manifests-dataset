{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ğ—›ğ—®ğ—°ğ—¸ğ—²ğ—¿ ğ—¡ğ—²ğ˜„ğ˜€ ğ—©ğ˜‚ğ—²",
  "short_name": "ğ—›ğ—®ğ—°ğ—¸ğ—²ğ—¿ ğ—¡ğ—²ğ˜„ğ˜€ ğ—©ğ˜‚ğ—²",
  "description": "Make the Hacker News social news site easy on the eyes!",
  "version": "1.1.519",
  "author": "W. \"Mac\" McMeans",
  "homepage_url": "https://HackerNewsVue.com",
  "default_locale": "en",
  "content_security_policy": "default-src 'self' https://hacker-news.firebaseio.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Hacker News Vue"
  },
  "content_scripts": [
    {
      "matches": [
        "*://news.ycombinator.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "*://news.ycombinator.com/*",
        "*://hackernewsvue.com/*",
        "*://hackernewsvue.loc/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "alarms",
    "background",
    "tabs",
    "webNavigation"
  ],
  "externally_connectable": {
    "matches": [
      "*://hackernewsvue.com/*",
      "*://hackernewsvue.loc/*"
    ]
  }
}
