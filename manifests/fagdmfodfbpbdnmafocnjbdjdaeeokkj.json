{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BostonText",
  "version": "0.0.2",
  "description": "Replaces ordinary text with a Boston accent.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "bostontext.js"
      ],
      "run_at": "document_end"
    }
  ]
}
