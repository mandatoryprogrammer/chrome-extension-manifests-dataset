{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "happy query",
  "description": "words query and remembering tool",
  "version": "1.5.3.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png",
    "512": "icon512.png"
  },
  "background": {
    "css": [
      "plugin.css"
    ],
    "scripts": [
      "jquery-1.12.4.js",
      "datatube.js",
      "jquery-ui/jquery-ui.js",
      "storage.js",
      "proper.js",
      "bootstrap4/js/bootstrap.js",
      "datatables/datatables.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "plugin.css",
        "jquery-ui/jquery-ui.css"
      ],
      "js": [
        "jquery-1.12.4.js",
        "jquery.formhelper.js",
        "content.js",
        "datatube.js",
        "storage.js",
        "selection.js",
        "jquery-ui/jquery-ui.js"
      ]
    }
  ],
  "browser_action": {},
  "web_accessible_resources": [
    "template.html",
    "btn_myword_add.gif",
    "btn_myword_del.gif",
    "wordlib.html",
    "cancel.png"
  ],
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "options_page": "options.html",
  "commands": {
    "queryword": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      },
      "description": "query selected word shortcut"
    }
  }
}
