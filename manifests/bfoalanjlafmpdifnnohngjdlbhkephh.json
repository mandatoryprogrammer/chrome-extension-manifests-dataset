{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DQMSL Search Translation",
  "version": "2020.9.16.7",
  "description": "Translate names in DQMSL Search (http://dqmsl-search.net) to Chinese",
  "author": "Ming-Hsien Tsai",
  "homepage_url": "https://github.com/mht208/dqmsl-search-translation",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://dqmsl-search.net/*",
        "http://dqmsl.jpn.org/*",
        "http://dqmsl.gamedbs.jp/*",
        "https://game8.jp/*",
        "https://ドラゴンクエストモンスターズ.gamewith.jp/*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "dqmsl.min.js"
      ]
    }
  ]
}
