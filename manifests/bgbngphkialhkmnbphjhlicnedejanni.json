{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Boring Copy Cat",
  "description": "Boring Copy Cat",
  "version": "1",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Boring Copy Cat",
    "default_icon": "copy_cat.png"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
