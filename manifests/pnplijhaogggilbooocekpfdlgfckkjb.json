{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "汉典字典",
  "description": "汉典字典，汉典词典查词",
  "version": "1.0.2",
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "permissions": [
    "activeTab",
    "http://*.zdic.net/"
  ],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  }
}
