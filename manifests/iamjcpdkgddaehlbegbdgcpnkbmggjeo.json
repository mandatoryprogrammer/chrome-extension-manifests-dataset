{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RaveT",
  "description": "This extension helps you use Medidata products",
  "version": "1.0.1",
  "short_name": "Rave Tool",
  "content_scripts": [
    {
      "matches": [
        "https://*/MedidataRave*",
        "https://www.imedidata.com/*",
        "https://balance.imedidata.com/*",
        "http://coder.imedidata.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "RaveT",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "permissions": [
    "tabs",
    "https://*/MedidataRave*"
  ]
}
