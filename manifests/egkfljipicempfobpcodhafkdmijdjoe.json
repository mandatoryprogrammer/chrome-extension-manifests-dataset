{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hax.To Go",
  "description": "Combine the power of the Hax.to Linking Service, browser add-ons, and keyboard shortcuts!",
  "author": "Hell.sh",
  "version": "2.0.2",
  "icons": {
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon/48.png",
      "128": "icon/128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "activeTab"
  ],
  "commands": {
    "shorten": {
      "description": "Shorten the link of the currently-focused website",
      "suggested_key": {
        "default": "Alt+S"
      },
      "global": false
    }
  },
  "externally_connectable": {
    "matches": [
      "*://*.hax.to/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "/icon.png"
  ]
}
