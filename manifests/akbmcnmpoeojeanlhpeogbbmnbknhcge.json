{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Clear Downloads",
  "version": "1.0.1",
  "description": "Clears the Downloads list every 30 seconds when all downloads are complete.",
  "homepage_url": "https://github.com/milanchheda/auto-clear-downloads",
  "manifest_version": 2,
  "permissions": [
    "browsingData"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
