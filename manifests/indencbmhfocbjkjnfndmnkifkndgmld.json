{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Protect Code from Google Translation",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "Chrome Extension for protecting code from Google Translation",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
