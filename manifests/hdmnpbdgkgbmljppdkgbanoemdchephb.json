{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "GeeksforGeeks meter"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "style.css"
      ],
      "js": [
        "main.js"
      ],
      "matches": [
        "*://www.geeksforgeeks.org/*"
      ]
    }
  ],
  "description": "Ease your experience by crossing done questions",
  "icons": {
    "128": "icon_128x128.png",
    "48": "icon_48x48.png"
  },
  "manifest_version": 2,
  "name": "Geekometer",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "identity"
  ],
  "short_name": "geekometer",
  "version": "1.2.1.1",
  "web_accessible_resources": [
    "funcs.js",
    "jquery.js",
    "jquery-ui.js",
    "listeners.js"
  ]
}
