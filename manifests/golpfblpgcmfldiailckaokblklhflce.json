{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WeatherMe",
  "version": "0.1",
  "description": "A plain stupid text weather information",
  "icons": {
    "19": "icon.png"
  },
