{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BugMeBack",
  "description": "__MSG_description__",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "__MSG_tooltip__",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "console_infos.js"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "manifest_version": 2
}
