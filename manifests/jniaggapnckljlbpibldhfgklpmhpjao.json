{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snellify",
  "description": "Replaces images of key political figures with SNL versions.",
  "version": "0.0.1",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "icons": {
    "48": "snellify48.png",
    "128": "snellify128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "snellify48.png",
      "128": "snellify128.png"
    },
    "default_title": "Snellify",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": [
      "setmenu.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "snellify.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "imgs/*.jpg"
  ]
}
