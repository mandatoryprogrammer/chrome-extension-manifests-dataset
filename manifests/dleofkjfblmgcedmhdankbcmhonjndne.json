{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "App ID Sanity",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Manage your App IDs in the Apple developer provisioning portal",
  "homepage_url": "https://github.com/simonwhitaker/app-id-sanity/",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://developer.apple.com/*"
      ],
      "css": [
        "sanity.css"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "jquery-ui-1.10.0.custom.min.js",
        "sanity.js"
      ]
    }
  ]
}
