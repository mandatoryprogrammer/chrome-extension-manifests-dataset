{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Link to Here",
  "version": "0.2.1",
  "manifest_version": 2,
  "minimum_chrome_version": "26",
  "description": "Adds a context menu to link to the nearest anchor before the selected text.",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "eventPage.js"
    ]
  },
  "offline_enabled": true
}
