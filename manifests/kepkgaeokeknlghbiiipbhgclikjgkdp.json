{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Umbraco Productivity",
  "short_name": "Umbraco",
  "description": "Productivity tool for Umbraco developers",
  "author": "Umbraco HQ",
  "version": "1.0.2",
  "background": {
    "page": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
