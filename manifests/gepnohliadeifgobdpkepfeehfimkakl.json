{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Page Reader",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.3",
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },
  "applications": {
    "gecko": {
      "id": "one-page-reader@devattendant.de"
    }
  },
  "background": {
    "scripts": [
      "js/url-utils.js",
      "js/append.js",
      "js/domains.js",
      "js/one-page-reader.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
