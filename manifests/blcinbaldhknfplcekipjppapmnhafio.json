{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "27 shades",
  "description": "View any web pages in 27 shades of the existing color scheme.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "author": "Shyju Krishnankutty"
}
