{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calculadora Metrovalencia (no oficial)",
  "short_name": "Calculadora Metrovalencia (no oficial)",
  "description": "Calculadora de horarios del metro en Valencia. (no oficial)",
  "version": "0.8.1",
  "permissions": [
    "storage",
    "http://www.metrovalencia.es/planificador.php"
  ],
  "browser_action": {
    "default_title": "Calculadora Metrovalencia (no oficial)",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "options_page": "options.html"
}
