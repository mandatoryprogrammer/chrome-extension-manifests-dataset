{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chatype all the things!!!",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Convert all the text on a web page to Chatype",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "omnibox": {
    "keyword": "chatype"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "Chatype all the things!!!"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "font/chatype-regular.woff"
  ]
}
