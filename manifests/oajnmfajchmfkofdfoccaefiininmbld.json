{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Selection Barcode Generator",
  "description": "Displays barcode for selected text",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
