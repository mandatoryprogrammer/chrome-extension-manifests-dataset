{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fork to Spoon",
  "description": "Changes 'fork' to 'spoon' on GitHub. Why? I'm not sure.",
  "version": "0.1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "*://*.github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "js": [
        "vendor/jquery-3.2.1.min.js",
        "app.js"
      ],
      "run_at": "document_end"
    }
  ]
}
