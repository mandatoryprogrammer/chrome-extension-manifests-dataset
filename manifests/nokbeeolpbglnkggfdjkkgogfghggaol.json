{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PlaidTab",
  "manifest_version": 2,
  "version": "0.0.0.3",
  "description": "A Chrome extension new tab dashboard created for CMU students.",
  "browser_action": {
    "default_icon": "img/icon.jpg"
  },
  "chrome_url_overrides": {
    "newtab": "log_in.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
