{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Notes",
  "description": "A minimal notepad for a new tab page.",
  "version": "0.2.2",
  "permissions": [
    "bookmarks",
    "storage",
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "new_tab_page.html"
  }
}
