{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SearchSavior",
  "version": "2.1.0",
  "description": "A Seamlessly Integrated Browser Extension For Conducting Better Academic Searches",
  "permissions": [
    "activeTab",
    "storage"
