{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit r/random",
  "description": "The new Reddit layout removed the random subreddit button! This will add it back!",
  "manifest_version": 2,
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "random.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
