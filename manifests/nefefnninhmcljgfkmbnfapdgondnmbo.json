{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "중고나라 검색기",
  "description": "중고나라 검색기",
  "version": "1.4.0",
  "browser_action": {
    "default_icon": {
      "19": "icon_16.png",
      "38": "icon_38.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "*://*/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "vendor/bootstrap.min.js",
        "vendor/jquery.min.js",
        "vendor/underscore.js"
      ]
    },
    {
      "all_frames": true,
      "run_at": "document_idle",
      "matches": [
        "*://*/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "vendor/bootstrap.min.js",
        "vendor/jquery.min.js",
        "vendor/underscore.js",
        "view.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://*/",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "icon.png",
    "view.html"
  ]
}
