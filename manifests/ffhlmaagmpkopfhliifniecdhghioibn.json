{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Taringa! shouts toolkit",
  "short_name": "T! shouts",
  "description": "Comparte al instante fotos, links, y texto con tus seguidores!",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "toastr.js",
        "content.js"
      ],
      "css": [
        "toastr.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
