{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disable key input",
  "description": "特定のキー入力（キーボードショートカット）を無効化してページに入力できるようにする",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
