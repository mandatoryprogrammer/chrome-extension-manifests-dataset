{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Commenter",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "*://free-comment.surge.sh/*"
  ],
  "icons": {
    "16": "get_started16.png",
    "48": "get_started48.png",
    "128": "get_started128.png"
  },
  "browser_action": {
    "default_title": "YouTube Commenter",
    "default_icon": {
      "32": "get_started32.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
