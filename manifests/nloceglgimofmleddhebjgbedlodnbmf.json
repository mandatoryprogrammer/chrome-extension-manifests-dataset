{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bubble Blocker",
  "short_name": "Bubble Blocker",
  "version": "1.0",
  "description": "Block those annoying chat bubbles.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://js.intercomcdn.com/*"
  ],
  "background": {
    "scripts": [
      "blocker.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "icons": {
    "128": "bubbleblocker-icon.png"
  }
}
