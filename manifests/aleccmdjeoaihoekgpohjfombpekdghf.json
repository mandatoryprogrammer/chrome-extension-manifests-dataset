{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "firephp-chrome",
  "version": "0.4.7",
  "manifest_version": 2,
  "description": "A firephp log extension for chrome",
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "assets/images/FirePHP_19.png",
    "default_title": ""
  },
  "background": {
    "scripts": [
      "vendor/assets/javascripts/jquery-2.0.2.min.js",
      "assets/javascripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "debugger",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "assets/images/FirePHP_16.png",
    "24": "assets/images/FirePHP_24.png",
    "32": "assets/images/FirePHP_32.png",
    "48": "assets/images/FirePHP_48.png",
    "128": "assets/images/FirePHP_128.png"
  }
}
