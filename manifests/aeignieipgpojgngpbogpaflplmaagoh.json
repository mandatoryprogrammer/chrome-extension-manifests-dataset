{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "papegoja_svt",
  "description": "This extension allows the user to repeat certain part of the videos on the SVT website",
  "version": "1.5",
  "web_accessible_resources": [
    "circle.png",
    "letter_a.png",
    "pause.png",
    "arrow_left_A.png",
    "arrow_left_B.png",
    "arrow_right_A.png",
    "arrow_right_B.png"
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.svtplay.se/video/*"
      ],
      "css": [
        "inject.css"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "circle.png"
  }
}
