{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Phantombuster",
  "version": "1.1.2",
  "description": "Makes Phantombuster easier to use by automatically retrieving your session cookies",
  "icons": {
    "48": "assets/buster-icon-48.png",
    "128": "assets/buster-icon.png"
  },
  "homepage_url": "http://www.phantombuster.com/",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "assets/buster-icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.phantombuster.com/*",
        "*://*.phantombuster.io/*",
        "*://*.zapier.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ],
      "css": [
        "contentscript.css"
      ]
    }
  ],
  "permissions": [
    "notifications",
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://*.phantombuster.com/*",
    "*://*.facebook.com/*",
    "*://*.github.com/*",
    "*://*.instagram.com/*",
    "*://*.intercom.io/*",
    "*://*.linkedin.com/*",
    "*://*.medium.com/*",
    "*://*.producthunt.com/*",
    "*://*.slack.com/*",
    "*://*.twitter.com/*",
    "*://*.uber.com/*",
    "*://*.youtube.com/*",
    "*://*.quora.com/*",
    "*://*.pinterest.com/*"
  ],
  "web_accessible_resources": [
    "assets/*"
  ]
}
