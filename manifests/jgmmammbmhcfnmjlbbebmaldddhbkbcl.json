{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redmine Easy Ticket",
  "description": "Redmine のチケット操作をキーボードだけで楽に更新できるようになります",
  "version": "1.0.6",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon/icon-19.png",
    "default_title": "Redmine Easy Ticket"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "19": "icon/icon-19.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "manifest_version": 2
}
