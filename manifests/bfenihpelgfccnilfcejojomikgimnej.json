{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FireLogger The Chrome Extension",
  "version": "1.1.3",
  "icons": {
    "32": "img/icon32.png",
    "48": "img/icon48.png"
  },
  "minimum_chrome_version": "31.0",
  "description": "Like FireLogger (FireBug extension) for Firefox.",
  "devtools_page": "devtools.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
