{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Follower Amounts on Twitter",
  "short_name": "TwtrFlwBlock",
  "version": "1.2",
  "description": "Hides all Follower counts on Twitter.com",
  "icons": {
    "512": "img/logo-512.jpg"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ]
}
