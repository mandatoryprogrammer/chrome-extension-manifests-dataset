{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email Assistant",
  "description": "Email Assistant Extension sets a Dashboard homepage on chrome-browser with links to various email and many other features.",
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "128": "img/32.png"
    }
  },
  "icons": {
    "128": "img/128.png",
    "64": "img/64.png",
    "32": "img/32.png"
  },
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "history",
    "topSites",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.9"
}
