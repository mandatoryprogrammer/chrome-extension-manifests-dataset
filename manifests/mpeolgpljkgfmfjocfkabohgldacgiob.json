{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Blogger Logo",
  "version": "1.13",
  "description": "To remove blogger logo",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.blogger.com/*"
      ],
      "js": [
        "remove.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
