{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Code Copy",
  "description": "Chrome Extension for copying code blocks to your clipboard on various sites.",
  "version": "0.1",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
