{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Animakai Skip Counter",
  "description": "Pular a contagem de tempo para baixar animes na Animakai.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Skip Animakai Counter"
  },
  "manifest_version": 2
}
