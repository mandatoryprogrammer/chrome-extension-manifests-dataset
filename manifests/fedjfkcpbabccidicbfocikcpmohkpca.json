{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ČD Body - zitranavylet.cz",
  "version": "1.3",
  "description": "Zjistí expiraci ČD bodů (+ mapa a počasí cíle, řazení a další odkazy podle cíle cesty)",
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/train16.png",
      "32": "images/train32.png",
      "48": "images/train48.png",
      "128": "images/train128.png"
    }
  },
  "icons": {
    "16": "images/train16.png",
    "32": "images/train32.png",
    "48": "images/train48.png",
    "128": "images/train128.png"
  },
  "manifest_version": 2
}
