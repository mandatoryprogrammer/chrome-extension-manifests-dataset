{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Omnibar",
  "version": "0.1.2",
  "description": "Omnibar for Github",
  "author": "Ouyang Jichao",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
