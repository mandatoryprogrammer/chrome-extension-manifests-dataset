{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ERPAL Timetracking Statistics",
  "description": "This extension shows an ERPAL Timetracking in details.",
  "version": "1.0.6",
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
