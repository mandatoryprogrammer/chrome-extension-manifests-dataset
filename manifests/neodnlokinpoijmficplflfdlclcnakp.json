{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Viettime Helper",
  "version": "1.1.3",
  "description": "Viettime helper extention",
  "icons": {
    "128": "viettime.png",
    "48": "viettime.png",
    "16": "viettime.png"
  },
  "author": "wallax",
  "content_scripts": [
    {
      "matches": [
        "https://item.taobao.com/*"
      ],
      "js": [
        "zepto.min.js",
        "lib.js",
        "taobao.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://detail.tmall.com/*"
      ],
      "js": [
        "zepto.min.js",
        "lib.js",
        "tmall.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://detail.1688.com/*"
      ],
      "js": [
        "zepto.min.js",
        "lib.js",
        "1688.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "viettime.png",
      "24": "viettime.png",
      "32": "viettime.png"
    },
    "default_title": "Viettime"
  },
  "permissions": [
    "http://viettime.shop/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
