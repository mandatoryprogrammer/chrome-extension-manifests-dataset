{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SpaceMouse in TinkerCAD",
  "version": "0.4.5",
  "description": "Use SpaceMouse/SpaceNavigator/Gamepad in TinkerCAD. This script is unofficial and endorsed neither by Autodesk nor 3DConnexion.",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "optiondata.js",
        "inject.js"
      ],
      "matches": [
        "https://www.tinkercad.com/things/*/edit*"
      ]
    }
  ],
  "web_accessible_resources": [
    "content.js",
    "spacenav.js",
    "toast.js",
    "options.js",
    "optiondata.js"
  ]
}
