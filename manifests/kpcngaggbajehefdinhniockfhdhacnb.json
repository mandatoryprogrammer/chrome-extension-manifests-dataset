{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Coinbag - Track all your coins in one place",
  "description": "Track price of bitcoin, Ethererum, Ripple, IOTA, EOS, Stellar and more than 1000 coins",
  "version": "1.4.1",
  "background": {
    "page": "bg.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "cookies",
    "https://www.coinbag.com/"
  ]
}
