{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "youRoomHelper",
  "short_name": "youRoomHelper",
  "version": "2.2.1.2",
  "description": "add some features to youRoom",
  "background": {
    "persistent": false,
    "scripts": [
      "lib/underscore.min.js",
      "lib/underscore.deferred.min.js",
      "fancy-settings/source/lib/store.js",
      "Extension.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://www.youroom.in/*"
      ],
      "js": [
        "lib/jquery.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'none'; img-src 'self'",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 2,
  "options_page": "fancy-settings/source/index.html",
  "page_action": {
    "default_icon": "images/icon-19.png",
    "default_title": "youRoomHelper is working"
  },
  "permissions": [
    "*://www.youroom.in/*",
    "tabs",
    "notifications"
  ],
  "web_accessible_resources": [
    "css/*",
    "images/*"
  ]
}
