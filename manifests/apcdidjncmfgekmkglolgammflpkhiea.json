{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CBC Music Keyboard Shortcuts",
  "version": "0.2",
  "icons": {
    "128": "icon_128.png"
  },
  "description": "Control the CBC Music player with the keyboard`.",
  "content_scripts": [
    {
      "matches": [
        "http://music.cbc.ca/*"
      ],
      "js": [
        "cbcmusic-kbsc.js"
      ],
      "all_frames": true
    }
  ]
}
