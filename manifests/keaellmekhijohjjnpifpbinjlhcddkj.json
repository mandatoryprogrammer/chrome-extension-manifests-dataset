{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Star",
  "description": "Close all the tabs you no longer need in just one keypress.",
  "version": "1.2.1",
  "icons": {
    "128": "image/icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "commands": {
    "close-unstarred": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      },
      "description": "Close all unstarred tabs"
    },
    "toggle-star": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Star/unstar current tab"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/unstarred.png",
    "default_title": "Star current tab"
  }
}
