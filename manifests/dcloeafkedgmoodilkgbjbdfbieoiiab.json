{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ico16_unactive.png",
    "default_title": "download monster",
    "icons": [
      "ico16_unactive.png"
    ],
    "name": "DownFaster",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "this is a tool for download all resources faster for this webpage.",
  "icons": {
    "16": "ico16.png",
    "48": "ico48.png",
    "128": "ico128.png"
  },
  "name": "DownFaster",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.12.3935.0",
  "manifest_version": 2
}
