{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "Pwcalc",
  "name": "SHA1 Password Calculator",
  "description": "Create strong cryptographic passwords",
  "version": "0.9.4",
  "manifest_version": 2,
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "img/pwcalc_16.png",
    "128": "img/pwcalc_128.png"
  },
  "permissions": [
    "clipboardWrite",
    "storage"
  ]
}
