{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Input Tab in Google Keep",
  "version": "1.0.2",
  "description": "This is an extension for Google Keep. You can input tab(\t) into memos.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts.js"
      ],
      "matches": [
        "*://keep.google.com/*"
      ]
    }
  ]
}
