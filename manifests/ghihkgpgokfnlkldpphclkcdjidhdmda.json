{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "8Tracks Virtuoso",
  "description": "An unobtrusive and powerful player for listening to, managing and exporting 8tracks mixes.",
  "version": "1.25.3",
  "permissions": [
    "storage",
    "tabs",
    "clipboardWrite",
    "notifications",
    "*://*/*",
    "idle",
    "identity"
  ],
  "web_accessible_resources": [
    "oauth/spotify_auth.html"
  ],
  "commands": {
    "next-track": {
      "suggested_key": {
        "default": "Ctrl+T"
      },
      "description": "jump to next track",
      "global": true
    },
    "like-mix": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "like/unlike mix.",
      "global": true
    },
    "fav-track": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "favorite/unfavorite track.",
      "global": true
    },
    "pause-play-track": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      },
      "description": "pause/play track.",
      "global": true
    }
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "options_page": "preferences.html",
  "background": {
    "scripts": [
      "lib/jquery-1.10.2.js",
      "lib/jquery.jplayer.js",
      "lib/md5.js",
      "src/globals.js",
      "oauth/8tracks_auth.js",
      "oauth/spotify_auth.js",
      "src/util.js",
      "src/globals.js",
      "src/spotify.js",
      "src/8tracks.js",
      "src/playlist.js",
      "src/player_model.js",
      "src/player_toast.js",
      "src/player_interface.js",
      "src/chromecast.js"
    ],
    "persistent": true
  },
  "key": "",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
