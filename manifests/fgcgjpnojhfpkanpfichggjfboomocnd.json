{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Last.FM Normalizer",
  "description": "Normalizes Top Artist rankings by time spent listening instead of total plays.",
  "version": "1.0.3",
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*.last.fm/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "arrow_left.png"
  ],
  "manifest_version": 2
}
