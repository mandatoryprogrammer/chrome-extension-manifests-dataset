{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "very simple memo",
  "description": "very simple mamo.",
  "version": "0.2.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "memo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "memo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ]
}
