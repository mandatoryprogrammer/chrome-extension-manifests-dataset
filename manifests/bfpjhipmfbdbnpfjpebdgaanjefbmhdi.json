{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Table Project+",
  "version": "0.1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Making the table project a little more useful",
  "content_scripts": [
    {
      "matches": [
        "http://rockypeak.tableproject.org/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "http://rockypeak.tableproject.org/*"
  ]
}
