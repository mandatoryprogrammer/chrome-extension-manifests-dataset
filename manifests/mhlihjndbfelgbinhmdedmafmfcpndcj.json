{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube15 Chrome Helper",
  "version": "1.0.2",
  "description": "Sends YouTube video info to YouTube15 (Applet for Logitech G15)",
  "permissions": [
    "tabs",
    "*://*.youtube.com/watch*",
    "http://127.0.0.1/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "YouTube15.js"
      ]
    }
  ]
}
