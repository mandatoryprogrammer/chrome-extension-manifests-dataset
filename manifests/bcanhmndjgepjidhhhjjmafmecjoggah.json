{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "images/icon-on.png",
    "32": "images/icon-on-32.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "images/icon-on-32.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "./content.js"
      ],
      "matches": [
        "https://bigquery.cloud.google.com/**"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "",
  "manifest_version": 2,
  "name": "BigQueryâ„¢ Plus",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "unlimitedStorage"
  ],
  "version": "0.0.2"
}
