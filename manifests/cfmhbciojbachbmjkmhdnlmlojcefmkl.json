{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MetaMusic",
  "version": "1.0.2",
  "description": "An extension that inserts the metacritic score of the album you're listening to in Google Music.",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2
}
