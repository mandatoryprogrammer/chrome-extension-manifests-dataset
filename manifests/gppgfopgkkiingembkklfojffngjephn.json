{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Contract Reader",
  "short_name": "CR",
  "version": "0.0.3",
  "description": "Always know what you sign",
  "background": {
    "scripts": [
      "node_modules/jquery/dist/jquery.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "contract-16.png",
    "default_title": "Do you control your content?",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "contract-16.png",
    "48": "contract-48.png",
    "128": "contract-128.png"
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "storage",
    "declarativeContent",
    "tabs",
    "https://contract-reader.herokuapp.com/"
  ],
  "manifest_version": 2
}
