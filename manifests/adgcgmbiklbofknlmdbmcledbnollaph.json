{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "梧桐雨工具箱",
  "version": "1.1",
  "description": "梧桐雨的工具箱,包含了一些常用的浏览器安全模块与常用工具.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "梧桐雨工具箱",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "icon48.png"
  ],
  "options_page": "options.html",
  "permissions": [
    "history",
    "cookies",
    "*://*/*",
    "notifications",
    "browsingData"
  ],
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://api.weibo.com; object-src 'self'"
}
