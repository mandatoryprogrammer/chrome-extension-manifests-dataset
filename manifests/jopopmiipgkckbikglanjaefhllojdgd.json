{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Crunchyroll Filler Labeler",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": [
        "*://www.crunchyroll.com/*",
        "*://www.animefillerlist.com/shows/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}
