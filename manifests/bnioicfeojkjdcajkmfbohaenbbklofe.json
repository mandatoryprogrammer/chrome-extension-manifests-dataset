{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Î¼Loader",
  "version": "2.1.1",
  "manifest_version": 2,
  "author": "Ian Spence",
  "description": "Quickly and easily download all files linked on any site.",
  "icons": {
    "16": "uLoader/assets/img/logo_16.png",
    "48": "uLoader/assets/img/logo_48.png",
    "128": "uLoader/assets/img/logo_128.png",
    "256": "uLoader/assets/img/logo_256.png",
    "512": "uLoader/assets/img/logo_512.png"
  },
  "browser_action": {
    "default_icon": "uLoader/assets/img/logo_256.png",
    "default_popup": "uLoader/main.html"
  },
  "background": {
    "scripts": [
      "uLoader/assets/js/download.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "downloads",
    "http://*/",
    "https://*/"
  ],
  "offline_enabled": true
}
