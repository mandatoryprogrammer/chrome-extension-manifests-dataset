{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WebPlatform Search Companion",
  "version": "1.0",
  "omnibox": {
    "keyword": "wpd"
  },
  "icons": {
    "16": "logo-16.png",
    "128": "logo-128.png"
  },
  "permissions": [
    "http://docs.webplatform.org/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
