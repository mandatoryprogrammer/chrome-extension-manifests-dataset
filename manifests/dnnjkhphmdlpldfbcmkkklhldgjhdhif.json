{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stock Extension",
  "version": "1.0.0",
  "description": "Shows stock information as a browser action",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
