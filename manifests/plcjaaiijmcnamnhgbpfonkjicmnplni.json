{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HNBlocker",
  "description": "This extension lets you block users on Hacker News.",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/item*"
      ],
      "js": [
        "jquery-2.0.0.min.js",
        "contentscript.js"
      ]
    }
  ]
}
