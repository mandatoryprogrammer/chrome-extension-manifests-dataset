{
  "content_scripts": [
    {
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/main.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "Don't just like something, hellllllll yeah it!",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "manifest_version": 2,
  "name": "Hell Yeahify",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.0.0"
}
