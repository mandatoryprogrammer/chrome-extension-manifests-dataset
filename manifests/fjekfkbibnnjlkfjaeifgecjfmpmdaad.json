{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yet another autoplay blocker",
  "version": "0.0.3",
  "description": "Pause videos until you click on them.",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "inject.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
