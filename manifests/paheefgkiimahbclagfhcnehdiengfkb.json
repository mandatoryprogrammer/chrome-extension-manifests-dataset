{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Algorithm to Wizard",
  "version": "1.3",
  "description": "Replaces the text 'algorithm(s)' with 'wizard(s)'; based on Cloud To Butt Plus",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
