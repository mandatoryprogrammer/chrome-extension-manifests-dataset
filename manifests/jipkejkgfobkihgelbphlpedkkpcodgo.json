{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "keyMazony",
  "version": "1.0",
  "description": "Add missing keyboard functionality to Amazon Cloud Player!",
  "background": {
    "scripts": [
      "keymazony.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "listener.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "keymazony-48.png",
    "128": "keymazony-128.png"
  },
  "manifest_version": 2
}
