{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Secure password generator",
  "version": "1.0.3",
  "description": "Simple extension that generates a random password locally, doesn't beam your password over the internet.",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "Secure password generator",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "passwordgen@calebbabin.com",
      "strict_min_version": "42.0"
    }
  }
}
