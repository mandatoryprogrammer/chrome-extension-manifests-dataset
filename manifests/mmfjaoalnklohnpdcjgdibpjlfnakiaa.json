{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "hangitt.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Hangitt"
  },
  "name": "Hangitt",
  "description": "Hang anything you find on the web in your clositt",
  "homepage_url": "http://www.clositt.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.4",
  "manifest_version": 2
}
