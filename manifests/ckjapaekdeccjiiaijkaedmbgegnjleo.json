{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL Saver",
  "version": "0.0.9",
  "description": "Conveniently saves any links you may want to use later",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "manifest_version": 2
}
