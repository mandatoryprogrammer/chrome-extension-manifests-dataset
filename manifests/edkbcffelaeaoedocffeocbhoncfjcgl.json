{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Thena",
  "description": "This extension will record the the wikipedia page you are currently viewing, as well as the page you came from.",
  "key": "",
  "version": "0.3",
  "page_action": {
    "default_icon": {
      "19": "thena_logo_19.png",
      "38": "thena_logo_38.png"
    }
  },
  "icons": {
    "16": "thena_logo_16.png",
    "48": "thena_logo_48.png",
    "128": "thena_logo_128.png"
  },
  "background": {
    "scripts": [
      "thena.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "identity",
    "identity.email",
    "https://*.wikipedia.org/wiki/*",
    "https://www.thena.io/*",
    "http://www.thena.io/*",
    "https://thena.io/*",
    "http://thena.io/*"
  ],
  "oauth2": {
    "client_id": "584973115782-v1ini048en6g4p8pfgrnbtasfkcf570a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
