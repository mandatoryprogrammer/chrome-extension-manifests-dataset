{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Watchdog",
  "description": "An over-engineered timer.",
  "version": "1.0.1",
  "author": "Tony Dinh",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icon@1x.png",
      "24": "assets/icon@1.5x.png",
      "32": "assets/icon@2x.png"
    },
    "default_title": "Watchdog",
    "default_popup": "index.html?popup=true"
  },
  "permissions": [
    "alarms",
    "background",
    "notifications",
    "storage"
  ]
}
