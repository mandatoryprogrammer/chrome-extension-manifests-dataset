{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RemoveIt",
  "version": "1.0.6",
  "description": "Removes the content of the Reddit homepage",
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "js": [
        "jquery.min.js",
        "remove.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/RemoveIt-16.png",
    "48": "images/RemoveIt-48.png",
    "128": "images/RemoveIt-128.png"
  },
  "manifest_version": 2
}
