{
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Colorfox - Mangareader for mangafox",
  "version": "1",
  "icons": {
    "16": "icon16.jpg",
    "40": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "description": "A better and more colorful way to read manga",
  "content_scripts": [
    {
      "matches": [
        "http://mangafox.me/manga/*/v*"
      ],
      "js": [
        "script.js",
        "jquery-1.9.0.js",
        "jquery-ui.js"
      ],
      "css": [
        "user.css",
        "jquery-ui.css"
      ]
    }
  ]
}
