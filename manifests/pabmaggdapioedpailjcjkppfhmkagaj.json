{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Raftaar",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src https://code.jquery.com/ https://api.forismatic.com/api/1.0/ 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Home Page Personalization Tool",
  "browser_action": {
    "default_title": "Raftaar"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "geolocation"
  ]
}
