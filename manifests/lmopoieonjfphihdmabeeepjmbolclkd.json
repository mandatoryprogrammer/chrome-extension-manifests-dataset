{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabHistory",
  "version": "0.1.1",
  "permissions": [
    "tabs",
    "storage"
  ],
  "description": "Keep a History of your Tabs",
  "icons": {
    "16": "img/tabhistory_16.png",
    "48": "img/tabhistory_48.png",
    "128": "img/tabhistory_128.png"
  },
  "browser_action": {
    "default_title": "TabHistory",
    "default_icon": "img/tabhistory_16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "author": "Nick Robinson"
}
