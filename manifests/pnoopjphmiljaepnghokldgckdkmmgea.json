{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TimeToRead",
  "version": "0.0.4",
  "description": "Adds tags describing time needed to read to articles in Pocket",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "web_accessible_resources": [
    "oauth.html",
    "oauth.js"
  ]
}
