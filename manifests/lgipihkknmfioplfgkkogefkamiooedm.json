{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gandhi",
  "version": "1.0",
  "description": "Replaces the text 'Ghandi' with 'Gandhi'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "gandhi.js"
      ],
      "run_at": "document_end"
    }
  ]
}
