{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Uneddit"
  },
  "name": "removeddit",
  "description": "Shows edited or deleted Reddit comments when clicked",
  "homepage_url": "http://removeddit.com",
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*.reddit.com/*",
    "https://*.reddit.com/*"
  ],
  "version": "0.4",
  "manifest_version": 2
}
