{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FUMBBL Gamefinder Highlighting",
  "description": "This extension highlights games on FUMBBL.",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "*://fumbbl.com/*"
      ],
      "css": [
        "classes.css"
      ],
      "js": [
        "jquery.js",
        "highlight.js",
        "fumbbl_highlighter.js"
      ]
    }
  ],
  "permissions": [
    "https://www.fumbbl.com/"
  ]
}
