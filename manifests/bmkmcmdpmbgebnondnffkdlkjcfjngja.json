{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Japanses Dict",
  "version": "1.0.0.2",
  "description": "A tool to check Japanese words on jisho.com within a click",
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Jisho",
    "default_popup": "default.html"
  }
}
