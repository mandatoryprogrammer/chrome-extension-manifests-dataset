{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabBomb",
  "version": "1.0",
  "description": "A time bomb for tabs.",
  "icons": {
    "128": "icons/bomb128.png",
    "32": "icons/bomb32.png",
    "16": "icons/bomb16.png"
  },
  "browser_action": {
    "default_icon": "icons/bomb16.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "commands": {
    "open_tabBomb": {
      "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+B",
        "chromeos": "Ctrl+B",
        "linux": "Ctrl+B"
      },
      "description": "Open new tab and set off TabBomb"
    }
  }
}
