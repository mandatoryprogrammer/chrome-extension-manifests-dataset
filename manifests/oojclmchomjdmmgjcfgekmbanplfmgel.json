{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OERhörnchen",
  "description": "Kleines Helferlein für die Suche nach Inhalten unter freier Lizenz. OER = Open Educational Resources",
  "version": "0.9.1",
  "icons": {
    "48": "assets/icon_squirrel_openclipart_org_241327_cc0_48.png",
    "128": "assets/icon_squirrel_openclipart_org_241327_cc0_128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "data/websitelist.json"
  ]
}
