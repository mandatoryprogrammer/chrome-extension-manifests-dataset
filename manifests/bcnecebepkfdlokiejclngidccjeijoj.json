{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "What's traffic like?",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "a quick google traffic checker",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "name": "Click to open map"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://maps.googleapis.com/*",
    "storage",
    "contextMenus",
    "identity"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "author": "Greg Stupp",
  "content_security_policy": "script-src 'self' https://*.googleapis.com; object-src 'self'"
}
