{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Check <img>",
  "description": "This is tool of checking image size. It turns red if it has defferences between original image size and HTML attribute size.",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "scripts": [
      "contentscript.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "name": "Click to check images",
    "default_icon": "icon/icon16.png"
  }
}
