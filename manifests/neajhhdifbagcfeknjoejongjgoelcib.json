{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "description": "LinkedIn Unfollower - Unfollow People that you not interested on following anymore in bulk",
  "icons": {
    "128": "images/icon.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "LinkedIn Unfollower",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "identity",
    "webRequest",
    "webNavigation",
    "contextMenus",
    "*://*.linkedin.com/*",
    "notifications"
  ],
  "version": "1.1.7",
  "web_accessible_resources": [
    "script.js",
    "images/loading.gif"
  ]
}
