{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Belarusian Dictionary",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Belarusian Dictionary",
  "browser_action": {
    "default_icon": "16x16.png",
    "icons": [
      "128x128.png"
    ]
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "css": [
        "bubble.css"
      ],
      "js": [
        "SelectedTextnotification.js",
        "jquery-1.7.2.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "word.png"
  ],
  "permissions": [
    "http://igeniusdev.com/chrome/belarusian.php/*",
    "https://igeniusdev.com/chrome/belarusian.php/*"
  ]
}
