{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitFileDownloader",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Download individual files from GIthub instead of the entire repository",
  "icons": {
    "256": "logo.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "author": "Pawan Dhananjay",
  "permissions": [
    "contextMenus",
    "downloads",
    "downloads.open"
  ]
}
