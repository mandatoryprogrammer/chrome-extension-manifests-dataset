{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nippori",
  "version": "1.0.2",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "https://api.search.nicovideo.jp/api/*"
  ],
  "browser_action": {
    "default_title": "Nippori",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "data/*.json"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
