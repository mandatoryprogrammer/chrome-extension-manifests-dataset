{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Me!",
  "description": "Calculates how long you've spent on reddit and facebook",
  "version": "1.5",
  "permissions": [
    "tabs",
    "*://*/*",
    "storage"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://facebook.com/*",
        "*://*.facebook.com/*",
        "*://netflix.com/*",
        "*://*.netflix.com/*",
        "*://spotify.com/*",
        "*://*.spotify.com/*",
        "*://reddit.com/*",
        "*://*.reddit.com/*"
      ],
      "js": [
        "time.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "page": "LogMe.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "LogMe.html"
  },
  "manifest_version": 2
}
