{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "facebookPrivacy.js"
      ],
      "matches": [
        "http://facebook.com/*",
        "https://facebook.com/*",
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ]
    }
  ],
  "description": "Clarifies privacy options in Facebook. Read more: http://en.wikipedia.org/wiki/PRISM_(surveillance_program)",
  "name": "Clarify Facebook's privacy options",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.5",
  "manifest_version": 2
}
