{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fullscreen Youtube on Twitter",
  "version": "1.6",
  "description": "Enables fullscreen mode for youtube videos embedded in the twitter feed",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/i/cards/*"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
