{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Play Open Beta Finder",
  "description": "Finds if the Google Play app has open beta testing",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/*"
      ],
      "js": [
        "extension.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "page.js"
  ],
  "permissions": [
    "tabs",
    "https://play.google.com/*"
  ]
}
