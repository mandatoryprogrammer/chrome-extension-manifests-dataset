{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PreGig: More Spotify for your Songkick",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "Concert + Friends + Beer + Songkick + Spotify + 2 hours to spare = PreGig",
  "permissions": [
    "http://www.songkick.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.songkick.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ]
}
