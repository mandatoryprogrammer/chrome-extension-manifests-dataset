{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Colorful List View for Feedly",
  "description": "Feedlyの一覧をFeedごとに色分けします。",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "https://feedly.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
