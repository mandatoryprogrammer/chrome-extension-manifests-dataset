{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trunc8or",
  "description": "This extension truncates tweets to try and fit in 140 chars",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "https://ajax.googleapis.com/"
  ]
}
