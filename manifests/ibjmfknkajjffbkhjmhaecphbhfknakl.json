{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "author": "Akira HIRATA",
  "description": "__MSG_extDescription__",
  "version": "3.0.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "https://*.unsplash.com/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "index.html?options",
  "minimum_chrome_version": "22",
  "homepage_url": "https://psephopaiktes.github.io/tabulasa/",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.google-analytics.com https://*.googletagmanager.com; object-src 'self'"
}
