{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Password Peek",
  "description": "Temporarily enables the user to see the contents of a password field.",
  "homepage_url": "http://www.passwordpeek.com",
  "version": "1.0",
  "icons": {
    "16": "icons/reveal.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "name": "Password Peek",
    "default_title": "Click to reveal passwords",
    "default_icon": "icons/reveal.png"
  },
  "commands": {},
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "4.0",
  "incognito": "split",
  "offline_enabled": true
}
