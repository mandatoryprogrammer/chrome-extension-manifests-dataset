{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "豆瓣歌詞秀 - Douban.fm Lyric Extension",
  "description": "Show the lyric what you listening.",
  "version": "1.11",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery.min.js",
        "js/jquery.plugin.min.js",
        "js/jquery.timer.min.js",
        "douban.js"
      ],
      "matches": [
        "http://douban.fm/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "*://*/*"
  ]
}
