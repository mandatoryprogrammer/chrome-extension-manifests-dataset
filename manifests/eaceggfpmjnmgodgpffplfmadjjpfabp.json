{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linkedin Inbox",
  "short_name": "Linkedin Inbox",
  "description": "Help you check the Linkedin Messages",
  "default_locale": "en",
  "version": "1.1",
  "author": "Ed",
  "browser_action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "./src/popup.html",
    "default_title": "Linkedin Inbox"
  },
  "permissions": [
    "cookies",
    "*://*.linkedin.com/"
  ],
  "icons": {
    "200": "./assets/icon.png"
  }
}
