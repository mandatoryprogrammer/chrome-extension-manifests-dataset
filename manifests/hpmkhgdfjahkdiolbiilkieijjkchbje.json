{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pusula Anket Doldurucu",
  "description": "Finaller öncesi çıkan tüm anketleri hızlıca doldurmayı sağlar.",
  "version": "1.0",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://*.pau.edu.tr/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
