{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.4",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "author": "Yohan Galerneau",
  "default_locale": "en",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
