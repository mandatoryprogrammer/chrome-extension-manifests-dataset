{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Masochist Tabs",
  "description": "Masochist tabs was developed to use a more brute-force way to help teach better tab management.",
  "version": "0.1",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/mt16.png",
    "48": "icons/mt48.png",
    "128": "icons/mt128.png"
  },
  "browser_action": {
    "default_icon": "icons/mt48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ]
}
