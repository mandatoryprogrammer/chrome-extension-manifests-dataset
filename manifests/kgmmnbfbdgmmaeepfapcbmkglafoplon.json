{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Open a new tab where choose what you would like to focus on that day as well as up to three projects you would like to work on.",
  "manifest_version": 2,
  "name": "Be More Mindful",
  "version": "1.2.1",
  "homepage_url": "http://github.com/thehashrocket",
  "icons": {
    "48": "icons/noun_1178590_cc.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/noun_1178590_cc.png"
  },
  "permissions": [
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "54.0a1"
    }
  }
}
