{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "William gay",
  "version": "6.9",
  "description": "william gay",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "declarativeContent",
    "storage",
    "webRequest",
    "<all_urls>",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/williamGay16.png",
      "32": "images/williamGay32.png",
      "48": "images/williamGay48.png",
      "128": "images/williamGay128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "williamGay.js"
      ],
      "css": [
        "williamGay.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/williamGay16.png",
    "32": "images/williamGay32.png",
    "48": "images/williamGay48.png",
    "128": "images/williamGay128.png"
  },
  "web_accessible_resources": [
    "images/williamGayFull.png"
  ],
  "manifest_version": 2
}
