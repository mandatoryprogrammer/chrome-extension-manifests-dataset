{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "name": "Twinkledot Tab Notifier",
  "description": "__MSG_app_desc__",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://*/*"
  ],
  "version": "0.0.2",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
