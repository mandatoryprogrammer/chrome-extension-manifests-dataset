{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkedAdd - Add people automatically",
  "short_name": "LinkedAdd",
  "version": "0.0.2",
  "description": "Automatically adds all results from search to your network",
  "permissions": [
    "activeTab",
    "https://www.linkedin.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
