{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KAF to Wunderlist",
  "version": "0.0.2",
  "description": "Save King Arthur Flour recipes to Wunderlist.",
  "author": "Dana Silver",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.kingarthurflour.com/recipes/*"
      ],
      "js": [
        "vendor/jquery-3.0.0.js",
        "vendor/react.js",
        "vendor/react-dom.js",
        "wunderlist.js",
        "storage.js",
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "identity",
    "storage",
    "https://www.wunderlist.com/*",
    "https://a.wunderlist.com/api/*"
  ],
  "web_accessible_resources": [
    "options.html",
    "img/icon.svg"
  ],
  "options_page": "options.html"
}
