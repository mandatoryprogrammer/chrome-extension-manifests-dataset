{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KeepIt140",
  "version": "0.1",
  "description": "Twitter has upped its limit to 280 characters. This extension lets you #KeepIt140 with the original, brutally terse experience.",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "http://twitter.com/*"
      ],
      "js": [
        "truncate.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
