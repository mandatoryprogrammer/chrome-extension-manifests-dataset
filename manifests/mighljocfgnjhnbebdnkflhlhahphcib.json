{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sparknotes is for Studying!",
  "description": "Removes distracting sidebars from Sparknotes",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.sparknotes.com/lit/*",
        "http://*.sparknotes.com/testprep/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
