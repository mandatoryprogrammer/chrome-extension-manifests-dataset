{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "橘子时光导出插件",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "用于导出橘子时光的日记到本地",
  "browser_action": {
    "fefault_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "点击导出"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "cookies",
    "http://*.juzitime.com/",
    "downloads",
    "tabs"
  ]
}
