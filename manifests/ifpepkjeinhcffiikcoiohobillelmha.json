{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to Downloads",
  "version": "0.1.2",
  "description": "An easy way to save images to the Downloads folder.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "downloads",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
