{
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Сайдекс-сканер — это бесплатное приложение позволяющее онлайн находить наиболее выгодные цены от тысяч интернет-магазинов.",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Сайдекс-сканер цен",
  "permissions": [
    "storage",
    "https://offers.sidex.ru/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.37"
}
