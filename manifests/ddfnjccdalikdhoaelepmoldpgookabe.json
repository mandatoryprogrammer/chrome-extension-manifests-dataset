{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Squirrel",
  "description": "Squirrel away videos for a rainy day. Squirrel is a watch-later website that allows you to save videos from a variety of platforms.",
  "version": "0.0.6",
  "browser_action": {
    "default_icon": "img/icon_acorn_gray144px.png",
    "default_title": "Squirrel Video",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "debugger",
    "storage",
    "cookies",
    "webNavigation",
    "activeTab",
    "background"
  ],
  "icons": {
    "16": "img/icon_acorn16px.png",
    "38": "img/icon_acorn38px.png",
    "48": "img/icon_acorn48px.png",
    "76": "img/icon_acorn76px.png",
    "128": "img/icon_acorn128px.png",
    "144": "img/icon_acorn144px.png",
    "256": "img/icon_acorn256px.png",
    "512": "img/icon_acorn512px.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "author": "Squirrel"
}
