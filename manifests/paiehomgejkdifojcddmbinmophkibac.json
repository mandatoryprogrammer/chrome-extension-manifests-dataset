{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gaana Lyrics App",
  "short_name": "Gaana Lyrics",
  "description": "View lyrics in a always-on-top window for Gaana.com, Saavn, Spotify, Google Music, Rdio, and others. (Requires GLyrics Extension)",
  "author": "Samik",
  "version": "2.6",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "key": "",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "alwaysOnTopWindows",
    "storage"
  ]
}
