{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SFDCSearch Assistant",
  "short_name": "SFDCSearch",
  "description": "This extension allows for quick searching of SFDCSearch.",
  "version": "0.0.2",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
