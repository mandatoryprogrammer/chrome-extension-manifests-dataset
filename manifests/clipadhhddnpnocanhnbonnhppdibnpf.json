{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "duoleavemealone for Duolingo",
  "short_name": "duoleavemealone",
  "version": "0.17",
  "description": "Makes Duolingo more user-friendly",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.duolingo.com/*"
      ],
      "run_at": "document_start",
      "css": [
        "content.css"
      ],
      "js": [
        "constants.js",
        "content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
