{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open link in foreground tab",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Adds a context menu to open a link in a new foreground tab.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
