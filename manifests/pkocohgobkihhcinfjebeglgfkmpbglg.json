{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "雅返利",
  "version": "3.0",
  "manifest_version": 2,
  "homepage_url": "http://www.yarpai.com",
  "description": "通过雅返利您可以在淘宝购物时方便的查询返利。以此推断宝贝是不是值那个价。",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "雅返利"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/global.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.taobao.com/*",
        "*://*.tmall.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "style/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "*://*/*",
    "tabs",
    "contextMenus",
    "cookies"
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
