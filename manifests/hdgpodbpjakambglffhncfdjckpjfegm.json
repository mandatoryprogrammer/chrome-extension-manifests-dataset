{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Repoke",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "All the fun of poking, without the hassle of poking.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/pokes",
        "https://www.facebook.com/pokes/*"
      ],
      "js": [
        "js/jquery/jquery.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
