{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yiddish Klal",
  "version": "0.4",
  "manifest_version": 2,
  "author": "Isaac Bleaman",
  "description": "A phonetic (QWERTY) keyboard layout for Standard Yiddish.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Yiddish Klal",
      "type": "ime",
      "id": "yiddishklal",
      "description": "Yiddish Klal",
      "language": "en-US",
      "layouts": [
        "us::eng"
      ]
    }
  ]
}
