{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pinject",
  "description": "Inject custom css into any webpage",
  "version": "1.0.4",
  "browser_action": {
    "default_icon": "res/icon.png",
    "default_popup": "res/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "res/inject.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ]
}
