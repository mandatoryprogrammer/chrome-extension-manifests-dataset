{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Petal",
  "description": "New customizable Default Tab. Get new inspirational quotes by famous authors, each day.",
  "version": "1.0.0",
  "author": "Parth Pathak",
  "browser_action": {
    "default_title": "Petal",
    "default_popup": "hello.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
