{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello Checklist Hider",
  "version": "0.1",
  "description": "Toggle checklists on trello cards from displaying",
  "manifest_version": 2,
  "browser_action": {
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "*://trello.com/*"
      ]
    }
  ]
}
