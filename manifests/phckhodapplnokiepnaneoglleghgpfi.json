{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker News - Show Links",
  "version": "1.0.1",
  "author": "Joe McDonald",
  "description": "Toggle to show just comments that have links",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "mycontent.js"
      ],
      "css": [
        "mycss.css"
      ]
    }
  ],
  "permissions": [
    "https://news.ycombinator.com/*"
  ]
}
