{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clock Icon",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Shows the time (hours and minutes) next to the omnibox.",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "dd/mm/yyyy hh:mm"
  }
}
