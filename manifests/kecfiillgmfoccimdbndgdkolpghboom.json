{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hyper Edit",
  "short_name": "Hyper Edit",
  "description": "Makes opening the Wordpress edit page blazing fast ðŸ”¥",
  "version": "2.9.2",
  "author": "Daniel Reed",
  "homepage_url": "https://github.com/kwaimind/hyper-edit",
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.hyperisland.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Hyper Edit"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'"
}
