{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UpgradeMixedContent",
  "version": "0.2.0",
  "description": "Resloves some mixedcontent blocking.",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html"
  }
}
