{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Notifications Bot",
  "version": "5.3",
  "options_page": "options.html",
  "description": "Track your github notifications using this bot",
  "permissions": [
    "storage",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/ncsu_logo.png",
      "32": "images/ncsu_logo.png",
      "48": "images/ncsu_logo.png",
      "128": "images/ncsu_logo.png"
    }
  },
  "icons": {
    "16": "images/ncsu_logo.png",
    "32": "images/ncsu_logo.png",
    "48": "images/ncsu_logo.png",
    "128": "images/ncsu_logo.png"
  },
  "manifest_version": 2
}
