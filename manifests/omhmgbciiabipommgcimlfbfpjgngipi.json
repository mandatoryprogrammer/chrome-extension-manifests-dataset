{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChromeGateMJD",
  "description": "Chrome client for MajorDoMo",
  "version": "1.3.4",
  "author": "UseYoVoice",
  "offline_enabled": false,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/lib/require.js",
      "js/init.js",
      "js/main.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    }
  },
  "options_page": "options.html"
}
