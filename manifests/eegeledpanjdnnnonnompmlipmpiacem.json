{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Responsive Dialogs for VK",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "responsive_vk.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "css": [
        "responsive_vk.css"
      ],
      "js": [
        "responsive_vk.js"
      ]
    }
  ]
}
