{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "I&I OpenTok Screen Sharing",
  "author": "Popeye I&I",
  "version": "4.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for http://tokbox.com/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://ii-tokbox-client-vue.herokuapp.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://ii-tokbox-client-vue.herokuapp.com/*",
      "http://localhost:8080/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
