{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sort by Lists for Trello",
  "description": "Sort your cards by lists in Trello",
  "version": "1.2",
  "icons": {
    "128": "icon.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "js/jquery-1.11.3.min.js",
        "js/sortable.min.js",
        "js/script.js",
        "js/options.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
