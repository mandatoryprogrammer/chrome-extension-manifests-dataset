{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stock Watcher -- real time stock quote",
  "description": "Get real time stock prices",
  "version": "0.0.2",
  "permissions": [
    "webRequest",
    "storage",
    "http://finance.google.com/",
    "https://www.google.com/finance/"
  ],
  "browser_action": {
    "default_title": "Real Time Stock Quote",
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "Stock Watcher",
  "manifest_version": 2
}
