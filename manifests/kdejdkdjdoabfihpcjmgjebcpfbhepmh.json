{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Copy Link Address",
  "version": "0.5.5",
  "description": "Copy link address without right-clicking. Just hover the link and hit Ctrl-C / Cmd-C!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "copylinkaddress.js"
      ]
    }
  ],
  "icons": {
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}
