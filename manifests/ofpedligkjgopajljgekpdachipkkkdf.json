{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Holofollowers",
  "author": "Wyatt Arent",
  "version": "1.0.3",
  "description": "Holonis - Get Hollonis Followers.",
  "content_scripts": [
    {
      "matches": [
        "https://www.holonis.com/*",
        "https://holonis.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background_script.js"
    ],
    "persistent": true
  },
  "key": "",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.holonis.com/*"
  ]
}
