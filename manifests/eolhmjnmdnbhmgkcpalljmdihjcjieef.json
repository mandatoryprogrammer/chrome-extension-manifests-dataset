{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Celebrate Pride EVERYWHERE",
  "description": "Rainbow filters over EVERYTHING.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Celebrate Pride EVERYWHERE"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
