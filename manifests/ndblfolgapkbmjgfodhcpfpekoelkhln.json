{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kill Twitter",
  "version": "1.0.0",
  "description": "Gets rid of Twitter news feed.",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "jquery.min.js",
        "kill_twitter.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
