{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clock Badge",
  "version": "1.0.1",
  "description": "Show current time",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "64": "Icon-64.png",
    "128": "Icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "background"
  ],
  "browser_action": {
    "default_icon": "Icon.png",
    "default_title": "Show me the time"
  },
  "manifest_version": 2
}
