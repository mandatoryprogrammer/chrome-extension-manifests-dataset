{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "gemoji-chrome",
  "description": "This extension provides easy access to emoji shortcodes",
  "version": "1.5",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/smiley.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
