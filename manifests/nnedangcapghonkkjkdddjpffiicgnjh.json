{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick QUB",
  "short_name": "QuickQUB",
  "description": "Allowing you to view articles off-campus without going through the library's website.",
  "version": "1.0.1",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "16x16.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
