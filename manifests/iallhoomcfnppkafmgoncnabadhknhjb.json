{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDb TV Series Chart",
  "description": "IMDb TV Series Chart",
  "version": "0.1",
  "icons": {
    "48": "icon_48.png",
    "96": "icon_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.imdb.com/*"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ],
      "css": [
        "css/jquery.modal.min.css",
        "css/style.css"
      ]
    }
  ],
  "permissions": [
    "https://www.imdb.com/*"
  ]
}
