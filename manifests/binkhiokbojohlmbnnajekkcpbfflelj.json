{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hürriyet Gazetesi",
  "description": "Hürriyet Gazetesi Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "Assets/img/hurriyetlogo-16.png",
      "38": "Assets/img/hurriyetlogo-128.png"
