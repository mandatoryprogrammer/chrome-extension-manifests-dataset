{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newsletter",
  "description": "Displays Latest news from recognized sources.",
  "version": "1.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/JQuery.js",
      "js/background.js"
    ]
  },
  "icons": {
    "128": "image/news128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "history",
    "notifications",
    "contextMenus"
  ]
}
