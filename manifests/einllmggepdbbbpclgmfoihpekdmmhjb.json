{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redirection",
  "version": "1.0.2",
  "description": "The End Of Annoying Wait Pages!",
  "background": {
    "scripts": [
      "scripts/jquery/jquery-2.1.3.min.js",
      "scripts/services/urls.js",
      "scripts/services/message.js",
      "scripts/services/redirect.js"
    ]
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery/jquery-2.1.3.min.js",
        "scripts/services/check.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "images/128.png",
    "default_popup": "home.html"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab"
  ]
}
