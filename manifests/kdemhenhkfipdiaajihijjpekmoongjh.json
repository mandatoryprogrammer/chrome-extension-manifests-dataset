{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Artificial Intelligence to Genie in a Bottle",
  "description": "Changes the term \"Artificial Intelligence\" or \"AI\" to \"Genie in a Bottle. Works also in Finnish",
  "version": "0.3.1",
  "icons": {
    "128": "genie.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
