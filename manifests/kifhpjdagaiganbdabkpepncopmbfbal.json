{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gulp Blacklist Marker",
  "version": "0.3.1",
  "description": "it marks blacklisted gulp modules when browsing (NPM and GitHub).",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.npmjs.com/*",
        "https://github.com/*"
      ],
      "js": [
        "base.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
