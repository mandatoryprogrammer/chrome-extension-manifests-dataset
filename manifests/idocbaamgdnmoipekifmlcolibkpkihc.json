{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yet Another Foreign Exchange Extension",
  "short_name": "yafee",
  "version": "0.960",
  "options_page": "htmls/options.html",
  "options_ui": {
    "page": "htmls/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js",
        "jquery-ui.min.js"
      ]
    }
  ],
  "icons": {
    "18": "dollar18x18.png",
    "48": "dollar48x48.png",
    "128": "dollar128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "18": "dollar18x18.png",
      "48": "dollar48x48.png",
      "128": "dollar128x128.png"
    },
    "default_title": "Exchange Rates",
    "default_popup": "htmls/popup.html"
  }
}
