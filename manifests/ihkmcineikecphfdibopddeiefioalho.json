{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "hublr",
  "description": "A Chrome extension for nerds",
  "homepage_url": "https://github.com/ali/hublr",
  "version": "0.0.3",
  "author": [
    {
      "name": "Alisha Ukani"
    },
    {
      "name": "Ali Ukani",
      "email": "ali.ukani@gmail.com"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "https://github.com/",
    "http://github.com/"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/"
      ],
      "js": [
        "vendor/jquery-1.10.2.min.js",
        "scroll.js",
        "hublr.js"
      ],
      "css": [
        "hublr.css"
      ]
    }
  ]
}
