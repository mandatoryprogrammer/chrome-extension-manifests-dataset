{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CSS Helper",
  "version": "1.1.0",
  "description": "CSS Helper is an Extension that displays all relevant CSS Properties of the element currently being hovered over.",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CSS Helper"
  },
  "author": "Rounak Banik",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*, file:///*/*",
        "*://*/*",
        "<all_urls>"
      ],
      "css": [
        "jquery.qtip.css"
      ],
      "js": [
        "jquery.js",
        "jquery.qtip.js",
        "contentscript.js"
      ]
    }
  ]
}
