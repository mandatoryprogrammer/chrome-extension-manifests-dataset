{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WeTube",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Watch YouTube with friends.",
  "permissions": [
    "tabs",
    "contextMenus",
    "declarativeContent"
  ],
  "icons": {
    "16": "icon-large.png",
    "48": "icon-large.png",
    "128": "icon-large.png"
  },
  "web_accessible_resources": [
    "jquery.min.js",
    "script.js"
  ],
  "background": {
    "scripts": [
      "socket.io.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-large.png",
    "default_title": "WeTube",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "input_components": [
    {
      "name": "WeTube",
      "type": "",
      "id": "wetube",
      "description": "Watch YouTube with friends.",
      "language": "en-US",
      "layouts": [
        "us::eng"
      ]
    }
  ]
}
