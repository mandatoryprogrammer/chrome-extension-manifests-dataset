{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Icinga Status",
  "version": "0.1.6",
  "description": "Icinga Status is simple open source extension for Google Chromeâ„¢ that helps you watch over your Icinga or Nagios instance.",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "options.html",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png",
    "160": "img/icon_160.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png",
      "38": "img/icon_38.png"
    },
    "default_title": "Icinga Status",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://api.flattr.com; object-src 'self'",
  "manifest_version": 2
}
