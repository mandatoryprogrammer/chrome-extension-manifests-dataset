{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "creemee.png",
    "default_title": "Creem Me"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This extension changes the words 'soft serve' to 'creemee,' because creemees are so much better than soft serve. #teamcreemee",
  "manifest_version": 2,
  "name": "Creem Me",
  "version": "1.0"
}
