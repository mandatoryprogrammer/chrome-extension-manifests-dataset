{
  "background": {
    "persistent": false,
    "scripts": [
      "eventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "red_icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ]
    }
  ],
  "description": "Show only sponsored posts on your Facebook timeline.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "red_icon.png",
    "64": "icon16.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "ADHUNTER 3.0.2",
  "permissions": [
    "storage",
    "contextMenus",
    "http://productmafia.com/",
    "https://www.productmafia.com/",
    "https://json.geoiplookup.io/",
    "activeTab",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.0.2",
  "web_accessible_resources": [
    "eye-logo.png"
  ]
}
