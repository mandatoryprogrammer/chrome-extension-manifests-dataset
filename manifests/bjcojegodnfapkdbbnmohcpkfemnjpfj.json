{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Castable KissCartoon",
  "description": "Makes KissCartoon usable with Chromecast",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://kisscartoon.es/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "https://kisscartoon.es/*"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  }
}
