{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ChIDE",
  "description": "An IDE for ChromeOS :)",
  "version": "0.8",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
    "storage",
    "unlimitedStorage",
    "clipboardRead",
    "clipboardWrite",
    "notifications"
  ],
  "icons": {
    "256": "chide-256.png",
    "128": "chide-128.png",
    "16": "chide-16.png"
  }
}
