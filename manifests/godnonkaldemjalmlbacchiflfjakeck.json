{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "version": "1.0.0",
  "permissions": [
    "downloads"
  ],
  "background": {
    "scripts": [
      "content/js/background.js"
    ]
  },
  "icons": {
    "16": "content/images/clear-downloads-16x16.png",
    "48": "content/images/clear-downloads-48x48.png",
    "128": "content/images/clear-downloads-128x128.png"
  },
  "browser_action": {
    "default_title": "__MSG_name__"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
