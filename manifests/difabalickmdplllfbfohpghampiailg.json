{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cirão Everywhere",
  "version": "1.2",
  "description": "Trole o seu amiguinho quando ele não estiver perto, mudando todas as imagens dos sites pelo Cirão da Massa. ",
  "icons": {
    "16": "assets/extension/icon16.png",
    "48": "assets/extension/icon48.png",
    "128": "assets/extension/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "ncirao.js"
      ]
    }
  ]
}
