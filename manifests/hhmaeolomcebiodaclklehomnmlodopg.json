{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FOVEA",
  "description": "enhance browsing experience",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "history",
    "http://www.google.com/*",
    "https://www.google.com/*",
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "disclaimer.png",
    "print.svg",
    "required-styles.css",
    "default-styles.css",
    "page.css",
    "fonts/*",
    "icons/*",
    "db.html",
    "db.js"
  ]
}
