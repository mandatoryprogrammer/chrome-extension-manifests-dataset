{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ClearBaidu",
  "description": "block the baidu ads",
  "version": "0.9.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "https://www.baidu.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "short_name": "ClearBaidu"
}
