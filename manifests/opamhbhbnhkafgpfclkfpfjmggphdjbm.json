{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NeuronJS Cookie Manager",
  "description": "Switch on/off debug mode for front-",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon/off.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
