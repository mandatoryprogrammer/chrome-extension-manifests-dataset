{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QR code generator",
  "version": "1.0",
  "description": "Build an Extension!",
  "author": "Yubaraj Poudel",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons.png",
      "32": "images/icons.png",
      "48": "images/icons.png",
      "128": "images/icons.png"
    }
  },
  "manifest_version": 2
}
