{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Virgool Dark",
  "short_name": "VrgDark",
  "version": "1.1",
  "description": "With VirgoolDark extension you can change Virgoolâ„¢ theme to dark.",
  "author": "Alireza Yousefi Azar",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.virgool.io/*",
        "https://virgool.io/*"
      ],
      "js": [
        "js/jquery.js",
        "js/app.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/*",
    "img/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png"
    },
    "default_title": "Virgool Dark",
    "default_popup": "popup/index.html"
  }
}
