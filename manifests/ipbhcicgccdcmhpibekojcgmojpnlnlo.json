{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jar-jar/Gungan Basic",
  "description": "Browse the web as a Gungan!",
  "version": "0.1.1",
  "icons": {
    "128": "images/jarjar.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
