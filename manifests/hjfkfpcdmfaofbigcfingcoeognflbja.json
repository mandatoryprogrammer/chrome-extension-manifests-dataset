{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "迅雷远程下载(非官方)",
  "version": "1.2.2",
  "description": "迅雷远程下载chrome插件，可以在连接上直接右键把下载发送到远程下载器，支持多设备。",
  "icons": {
    "16": "assets/icon16.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://yc.xunlei.com/*"
      ],
      "js": [
        "assets/jquery-1.11.1.min.js",
        "assets/download.js"
      ]
    }
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "alarms",
    "*://yc.xunlei.com/*",
    "*://homecloud.yuancheng.xunlei.com/*"
  ]
}
