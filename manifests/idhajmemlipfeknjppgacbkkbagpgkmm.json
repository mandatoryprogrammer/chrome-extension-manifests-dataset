{
  "name": "Blackboard Grade Calculator",
  "description": "Helps tallying grades on blackboard",
  "version": "1.4",
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://crocodoc.com/view/*"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
