{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "github-tada",
  "version": "1.2.3",
  "description": "Browse Github issues by reaction",
  "content_scripts": [
    {
      "js": [
        "./dist/main.js"
      ],
      "css": [
        "./dist/main.css"
      ],
      "matches": [
        "https://github.com/*/issues/*",
        "https://github.com/*/pull/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/tada-openmoji-16.png",
    "48": "icons/tada-openmoji-48.png",
    "128": "icons/tada-openmoji-128.png"
  },
  "manifest_version": 2
}
