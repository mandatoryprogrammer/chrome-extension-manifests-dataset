{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LockedIn",
  "version": "1.03",
  "description": "All of your passwords in one easy to access, and secure, place.",
  "permissions": [
    "cookies",
    "https://nameless-falls-24533.herokuapp.com/dashboard/*",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/LogoMakr_3VQDvP.png",
      "32": "images/LogoMakr_3VQDvP.png",
      "48": "images/LogoMakr_3VQDvP.png",
      "128": "images/LogoMakr_3VQDvP.png"
    }
  },
  "icons": {
    "16": "images/LogoMakr_3VQDvP.png",
    "32": "images/LogoMakr_3VQDvP.png",
    "48": "images/LogoMakr_3VQDvP.png",
    "128": "images/LogoMakr_3VQDvP.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
