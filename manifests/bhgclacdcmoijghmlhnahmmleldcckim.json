{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KOANタイマーリセット by Takuma.K",
  "short_name": "TK-KOANタイマーリセット",
  "version": "0.0.0.5",
  "description": "KOANでのタイムアウトを無効にします.",
  "icons": {
    "16": "icon/icon64.png",
    "48": "icon/icon128.png",
    "128": "icon/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon64.png"
    },
    "default_title": "KOAN Timer Reset by Takuma.K"
  },
  "content_scripts": [
    {
      "matches": [
        "https://koan.osaka-u.ac.jp/campusweb/*"
      ],
      "js": [
        "jquery.min.js",
        "script/index.js"
      ]
    }
  ]
}
