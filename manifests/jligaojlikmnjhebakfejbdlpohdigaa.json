{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabGroups",
  "description": "This extension allows the user to open a bunch of tabs.",
  "version": "1.0",
  "browser_action": {
    "default_title": "Open a TabGroup",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
