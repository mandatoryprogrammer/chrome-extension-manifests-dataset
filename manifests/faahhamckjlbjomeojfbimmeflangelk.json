{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Retwddit",
  "description": "A button for Twitter to quickly copy in the correct format for Reddit",
  "version": "2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "retwddit.js"
      ],
      "css": [
        "retwddit.css"
      ]
    }
  ]
}
