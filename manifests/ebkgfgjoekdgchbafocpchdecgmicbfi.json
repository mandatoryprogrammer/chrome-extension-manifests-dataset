{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to nvALT",
  "version": "1.0.1",
  "description": "Save pages to nvALT in clean Markdown. No fluff.",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Save to nvALT"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "build.js"
    ]
  },
  "minimum_chrome_version": "14.0"
}
