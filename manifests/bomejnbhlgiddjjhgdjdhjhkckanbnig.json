{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Web FS",
  "description": "This extension provides an in-browser tool for organizing web resources (aka URLs).",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "activeTab",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "contextRegister.js"
    ]
  }
}
