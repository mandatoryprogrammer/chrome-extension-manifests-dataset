{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.2",
  "description": "Gets packet from other Nshare client in your chrome-browser",
  "name": "Nshare",
  "icons": {
    "16": "images/png/ic_launcher16x16.png",
    "48": "images/png/ic_launcher48x48.png",
    "128": "images/png/ic_launcher128x128.png"
  },
  "browser_action": {
    "default_icon": "images/png/ic_launcher128x128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost:12340/"
  ]
}
