{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "屏蔽豆瓣黑名单广播",
  "description": "针对豆瓣web端12.20改版，被友邻转播的黑名单用户的广播将不再出现在你的timeline中",
  "version": "0.1",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.douban.com/*"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "message.js"
      ]
    }
  ]
}
