{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PN",
  "description": "This extension demonstrates a browser action with pn.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.12",
  "permissions": [
    "tabs",
    "cookies",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  }
}
