{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Orange Confort+",
  "description": "Orange Confort+",
  "version": "1.2.0.1",
  "permissions": [
    "tabs",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Orange Confort+",
    "default_icon": {
      "19": "data/icon-19.png",
      "38": "data/icon-38.png"
    }
  },
  "author": "Orange SA",
  "homepage_url": "http://confort-plus.orange.com/",
  "background": {
    "scripts": [
      "js/main.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "css/*.css",
    "images/*.png",
    "help/*.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "conf/hebergement.js",
        "js/ToolbarStrings.js",
        "js/UciUserPref.js",
        "js/UciSimpleStorage.js",
        "language/en.js",
        "language/es.js",
        "language/fr.js",
        "language/pl.js",
        "js/UciAideMotrice.js",
        "js/UciCouleur.js",
        "js/UciApparence.js",
        "js/UciSettings.js",
        "js/UciTypographie.js",
        "js/UciHelp.js",
        "js/UciProfile.js",
        "js/UciValidation.js",
        "js/UciIhm.js",
        "js/toolbar.js",
        "js/mask.js",
        "js/start.extensionChrome.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "data/icon-16.png",
    "48": "data/icon-32.png",
    "128": "data/icon-64.png"
  }
}
