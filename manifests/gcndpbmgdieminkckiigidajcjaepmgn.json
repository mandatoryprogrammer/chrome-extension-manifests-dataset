{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eventFindr",
  "description": "This extension shows a list of events using Eventbrite API",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Find an event now!",
    "default_popup": "index.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://eventbrite.com/*"
  ]
}
