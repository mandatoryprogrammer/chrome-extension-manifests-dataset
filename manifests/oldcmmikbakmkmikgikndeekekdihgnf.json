{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Tab Art",
  "short_name": "New Art Tab",
  "version": "0.4",
  "description": "See a new piece of art every time you open a tab.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": ""
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "getart.js"
      ],
      "all_frames": true
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newpage.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "style.css"
  ]
}
