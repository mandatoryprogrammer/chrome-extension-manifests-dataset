{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Курс доллара, евро, нефти, BTC online + ЦБ",
  "short_name": "Forex Quotes",
  "description": "Курс доллара, евро, нефти, биткоин online и все курсы ЦБ",
  "version": "5.4.1",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "Курс доллара, евро, нефти, биткоин online и все курсы ЦБ",
    "default_icon": {
      "19": "USD19.png",
      "38": "USD38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage",
    "https://query.yahooapis.com/*",
    "https://finance.yahoo.com/*",
    "https://export.yandex.ru/*",
    "https://news.yandex.ru/*",
    "http://www.cbr.ru/*",
    "http://api.bitcoincharts.com/*",
    "https://api.cryptowat.ch/*",
    "https://*.mfcoin.net/*",
    "https://quotes.instaforex.com/*"
  ],
  "background": {
    "scripts": [
      "eventPage.js",
      "popup.js",
      "tv.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "css/*.*",
    "font/*.*"
  ]
}
