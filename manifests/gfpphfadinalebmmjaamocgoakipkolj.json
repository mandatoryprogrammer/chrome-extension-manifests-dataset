{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "checker.js"
      ],
      "css": [
        "checker.css"
      ],
      "matches": [
        "http://*.roblox.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Checks whether or not a user is following you.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "ROBLOX Follow Checker",
  "permissions": [
    "http://*.roblox.com/*"
  ],
  "version": "2"
}
