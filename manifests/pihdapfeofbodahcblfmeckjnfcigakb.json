{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BrowsePass",
  "description": "A read-only, web-enabled version of KeePass.",
  "version": "0.8.1",
  "offline_enabled": true,
  "homepage_url": "https://bitbucket.org/namn/browsepass",
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_title": "BrowsePass"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "key": "",
  "oauth2": {
    "client_id": "595395511986-papjag05ej3ailoqqoj01lfhvfvsf5pe.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.readonly"
    ]
  },
  "permissions": [
    "identity",
    "clipboardWrite",
    "http://*/",
    "https://*/",
    "ftp://*/"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
