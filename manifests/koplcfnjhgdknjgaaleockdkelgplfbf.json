{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "I Love Weiqi",
  "version": "0.5.1",
  "description": "Replace builtin Weiqi viewer with a beautiful and intuitive viewer",
  "background": "background.html",
  "options_page": "options.html",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "iloveweiqi.js"
      ]
    }
  ],
  "permissions": []
}
