{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "bookmarks-from-json",
    "default_icon": "bookmark-64.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "bookmark-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Create a bookmark tree from a json, use it to share bookmarks across teams for free.",
  "manifest_version": 2,
  "name": "bookmarks-from-json",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "version": "0.7.0"
}
