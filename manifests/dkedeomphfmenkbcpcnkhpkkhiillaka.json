{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ants",
  "description": "A life-changing productivity hack",
  "version": "1.2",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "ReadySetGo",
    "default_icon": "./img/bugKiller.png",
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "./src/styles.css"
      ]
    }
  ],
  "manifest_version": 2
}
