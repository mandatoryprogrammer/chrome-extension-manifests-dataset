{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TweetbotByIntent",
  "version": "1.0.0",
  "description": "TweetbotByIntent",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/intent/tweet*"
      ],
      "js": [
        "TweetbotByIntent.js"
      ],
      "run_at": "document_end"
    }
  ]
}
