{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ding Dong Johnson",
  "icons": {
    "16": "baseball.png",
    "32": "baseball.png",
    "64": "pedro.png"
  },
  "version": "1.0",
  "description": "Replaces the text 'home run' with 'ding dong Johnson'.",
  "browser_action": {
    "default_icon": "baseball.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
