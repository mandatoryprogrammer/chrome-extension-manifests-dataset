{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Recent history search",
  "description": " type 'recent' and then search only through recent history",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "recent"
  },
  "permissions": [
    "history"
  ],
  "icons": {
    "16": "icon.png",
    "48": "tie48.png",
    "128": "tie128.png"
  },
  "manifest_version": 2
}
