{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Synology Download Station",
  "description": "__MSG_chromeExtensionDescription__",
  "version": "2.2.13",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "info@download-station-extension.com"
    }
  },
  "minimum_chrome_version": "41",
  "default_locale": "en",
  "homepage_url": "http://www.download-station-extension.com/",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "icons": {
    "16": "Icon-16.png",
    "32": "Icon-32.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "Icon-19-disconnected.png",
      "38": "Icon-38-disconnected.png"
    },
    "default_title": "__MSG_chromeExtensionName__",
    "default_popup": "popover.html"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/lib/jquery.js",
        "js/browser-functions.js",
        "js/content-scripts/content.js"
      ],
      "css": [
        "css/cssreset-context-min.css",
        "css/hud.css"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "http://*/",
    "https://*/",
    "background",
    "notifications",
    "contextMenus",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "Icon-16.png",
    "Icon-19.png",
    "Icon-38.png",
    "Icon-48.png",
    "Icon-128.png",
    "css/img/hud-check.png",
    "css/img/hud-cross.png",
    "css/img/hud-progress.png",
    "css/img/icon-black.png",
    "css/img/icon-black@2x.png",
    "download-dialog.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
