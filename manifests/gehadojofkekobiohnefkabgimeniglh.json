{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "38",
  "version": "0.14",
  "author": "cTn",
  "name": "RapidFlash",
  "short_name": "RapidFlash",
  "description": "Taking ESC flashing to the next level",
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "eventPage.js"
      ],
      "persistent": false
    }
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "https://*.github.com/",
    "http://www.openlrsng.org/",
    "serial",
    "usb",
    "storage",
    "fileSystem",
    "fileSystem.write",
    "notifications"
  ],
  "optional_permissions": [
    {
      "usbDevices": [
        {
          "vendorId": 5824,
          "productId": 1500
        }
      ]
    }
  ],
  "icons": {
    "128": "images/icon_128.png"
  }
}
