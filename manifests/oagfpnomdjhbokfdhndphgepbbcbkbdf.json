{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "author": "Ryosuke Ohta",
  "version": "1.17.0",
  "icons": {
    "128": "image/icon128.png",
    "64": "image/icon64.png",
    "48": "image/icon48.png",
    "16": "image/icon16.png"
  },
  "background": {
    "scripts": [
      "etc/CONST.js",
      "etc/DEFAULT_OPTION_LIST.js",
      "etc/PRESET_OPTION_LIST.js",
      "lib/ponyfill.js",
      "lib/indexeddb.js",
      "lib/share.js",
      "lib/common.js",
      "background/background.js"
    ]
  },
  "options_ui": {
    "page": "option/option.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "etc/CONST.js",
        "lib/ponyfill.js",
        "lib/share.js",
        "model/model.js",
        "model/appModel.js",
        "model/widgetEnableModel.js",
        "model/domainListModel.js",
        "model/requestStatusModel.js",
        "content/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "128": "image/icon128.png",
      "64": "image/icon64.png",
      "48": "image/icon48.png",
      "16": "image/icon16.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "__MSG_extensionName__"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "storage",
    "notifications"
  ],
  "web_accessible_resources": [
    "image/*"
  ]
}
