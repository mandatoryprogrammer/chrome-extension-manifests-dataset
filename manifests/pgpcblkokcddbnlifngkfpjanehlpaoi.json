{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tiny Kitchen",
  "description": "Integration with the Tiny Kitchen web app",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://localhost:8080/"
  ]
}
