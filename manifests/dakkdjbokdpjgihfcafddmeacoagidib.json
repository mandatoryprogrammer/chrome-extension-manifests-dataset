{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Go to Utopia",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "好きな女優のインスタに遷移できる",
  "options_page": "./html/option.html",
  "content_scripts": [
    {
      "js": [
        "./js/jquery-3.3.1.min.js",
        "./js/script.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "./img/icon.png"
  }
}
