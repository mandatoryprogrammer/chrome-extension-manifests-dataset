{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Recess",
  "version": "1.1.1",
  "description": "It's time for Recess.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
