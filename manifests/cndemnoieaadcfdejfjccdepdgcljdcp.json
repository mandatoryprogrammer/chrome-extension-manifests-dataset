{
  "content_scripts": [
    {
      "css": [
        "styles/main.css"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "Browse Facebook like Dave Fontenot.",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "manifest_version": 2,
  "name": "hellyeah",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0"
}
