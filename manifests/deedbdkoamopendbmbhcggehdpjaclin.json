{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fartscroll.js",
  "description": "This extension will load fartscroll.js from The Onion on all pages (http://theonion.github.io/fartscroll.js/).",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery.min.js",
        "fartscroll.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png"
  }
}
