{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CosmosPro Compartilhamento de Tela",
  "author": "CosmosPro",
  "version": "1.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Extens√£o para compartilhamento de tela do Portal CosmosPro.",
  "homepage_url": "https://portal.cosmospro.com.br/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.cosmospro.com.br/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
