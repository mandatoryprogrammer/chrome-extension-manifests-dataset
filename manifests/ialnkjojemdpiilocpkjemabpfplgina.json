{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Offline QR Maker",
  "description": "A tiny offline QR codes generator; no tracking, minimal permissions",
  "version": "0.0.5",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "event_page.js"
    ]
  },
  "browser_action": {},
  "offline_enabled": true,
  "icons": {
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "dialog.html"
  ]
}
