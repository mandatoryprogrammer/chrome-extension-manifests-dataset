{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabShepherd",
  "short_name": "TabShepherd",
  "description": "Press Ctrl + ~",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "pasture.html"
  ],
  "browser_action": {
    "default_icon": "images/tab-shepherd-16.png"
  },
  "icons": {
    "16": "images/tab-shepherd-16.png",
    "32": "images/tab-shepherd-32.png",
    "48": "images/tab-shepherd-48.png",
    "128": "images/tab-shepherd-128.png"
  }
}
