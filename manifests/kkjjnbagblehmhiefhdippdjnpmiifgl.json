{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Addit",
  "short_name": "Addit",
  "author": "https://addit.io",
  "description": "Browser Extension for Addit",
  "version": "0.0.5",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com/ga.js 'unsafe-eval' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Addit"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+A",
        "mac": "Alt+Shift+A",
        "chromeos": "Alt+Shift+A",
        "linux": "Alt+Shift+A"
      }
    }
  },
  "icons": {
    "16": "apple-icon-60x60.png",
    "48": "apple-icon-76x76.png",
    "128": "apple-icon-144x144.png"
  },
  "web_accessible_resources": [],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:8888/",
    "http://localhost:9876/",
    "activeTab",
    "webRequest",
    "notifications"
  ]
}
