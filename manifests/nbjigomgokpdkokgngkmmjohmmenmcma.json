{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to Pinboard",
  "version": "0.5",
  "description": "Save the current URL to Pinboard",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "img/pinboard16.png",
    "48": "img/pinboard48.png",
    "128": "img/pinboard128.png"
  },
  "browser_action": {
    "default_icon": "img/pinboard16.png",
    "default_title": "Save the current page to Pinboard"
  },
  "background": {
    "scripts": [
      "events.js"
    ],
    "persistent": false
  },
  "commands": {
    "save-to-pinboard": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "Save the current page to Pinboard"
    }
  }
}
