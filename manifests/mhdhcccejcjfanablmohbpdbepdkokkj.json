{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VSMonitor Ext",
  "version": "1.0.12",
  "manifest_version": 2,
  "description": "Feita para agregar valores ao GPS, Manobra Única e Manobra Massiva, ainda em estágio alpha (por Valdecir Merli)",
  "author": "Valdecir Merli (G0056638)",
  "options_ui": {
    "page": "Vweb/options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "./Vweb/assets/js/jquery-1.11.0.min.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "desktopCapture",
    "downloads",
    "gcm",
    "geolocation",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "power",
    "sessions",
    "system.cpu",
    "system.memory",
    "tabCapture",
    "tts",
    "ttsEngine",
    "webNavigation",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "tabs",
    "*://acl/",
    "*://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.gvt.net.br/*",
      "*://*.gvt.com.br/*",
      "*://10.40.174.101/*",
      "*://10.40.195.81/*",
      "*://10.200.35.67/*",
      "*://10.200.35.66/*",
      "*://sv2kppag2/*",
      "*://sv2kpwfm3/*",
      "*://gvtapp/*",
      "*://portalefika/*",
      "*://efika/*"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "*://gpsco.gvt.net.br/*",
        "*://gpsco.gvt.com.br/*",
        "*://appsagre.gvt.net.br/Manobraunica*",
        "*://manobramassiva.gvt.net.br/*",
        "*://efika/web/*",
        "*://gvt.etadirect.com/*",
        "*://sv2kppag2/*",
        "*://wisetool.gvt.net.br/*",
        "*://savvioncrm.gvt.com.br/*",
        "*://mbuservices.gvt.net.br/*"
      ],
      "js": [
        "start.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.png",
    "imgs/*",
    "db/shelfs.json",
    "manobraunica/*",
    "manobramassiva/*",
    "wisetool/*",
    "checknote/*",
    "eta/*",
    "gestorbloqueios/*",
    "bpm/*",
    "plcontrol/*",
    "Vweb/vsm-bar.html",
    "Vweb/vsm-bar.js"
  ]
}
