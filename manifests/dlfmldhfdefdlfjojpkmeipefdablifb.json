{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Highlightjs Auto Checker",
  "description": "An extension that allows you to select/deselect all the available language located in the 'Other' section",
  "manifest_version": 2,
  "version": "0.0.1",
  "icons": {
    "128": "icon/hljs-icon.png"
  },
  "permissions": [
    "tabs",
    "https://highlightjs.org/*",
    "activeTab"
  ],
  "background": {
    "page": "html/background.html"
  },
  "page_action": {
    "default_title": "Auto Check/Uncheck the 'Other' Langs",
    "default_icon": "icon/hljs-icon.png"
  }
}
