{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Good Place Cursing",
  "short_name": "Forking Shirtballs",
  "description": "Replaces curse words with the versions used in 'The Good Place'. Modified version of 'Millenials to Snake People' from Eric Bailey",
  "author": "Lizzie Ellis & Eric Bailey",
  "version": "1.1",
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
