{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IPv4 Address Literal Appender",
  "author": {
    "email": "hiro@awa.sfc.keio.ac.jp"
  },
  "manifest_version": 2,
  "description": "Insert .v4 domain to any IPv4 address in the URL. This is an experimental implementation for IPv4 Address Literal in URL.",
  "background": {
    "scripts": [
      "URI.js",
      "wr.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "*://*/*"
  ],
  "version": "2014.7.14.0"
}
