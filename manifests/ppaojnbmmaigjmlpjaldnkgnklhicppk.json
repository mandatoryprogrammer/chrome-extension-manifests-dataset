{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webtime Tracker",
  "description": "Discover your browsing habits! Time tracking at its best.",
  "version": "1.5.2",
  "offline_enabled": true,
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon-19.png",
      "38": "icon/icon-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Webtime Tracker"
  },
  "background": {
    "scripts": [
      "js/config.js",
      "js/fn.js",
      "js/storage-local.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "idle",
    "storage",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ]
}
