{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Reddit Eclipse",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Make Reddit look like the Eclipse Editor",
  "content_scripts": [
    {
      "matches": [
        "http://*.reddit.com/*"
      ],
      "js": [
        "bg.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "left-repeat.png",
    "right-repeat.png",
    "top-repeat.png",
    "top-left.png",
    "top-right.png",
    "project-icon.png",
    "style.css"
  ],
  "permissions": [
    "http://reddit.com/*"
  ]
}
