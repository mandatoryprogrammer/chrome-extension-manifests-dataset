{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Garmin Connect Share Workout",
  "description": "Share workouts between users in Garmin Connect",
  "version": "1.0.0.12",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Garmin Share Workout"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://connect.garmin.com/modern/"
  ]
}
