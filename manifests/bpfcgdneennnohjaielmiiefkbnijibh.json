{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fine Reader",
  "description": "A Chrome extension to have a readable content.",
  "version": "1.0.2",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Enable Fine Reader"
  },
  "web_accessible_resources": [
    "css/letemps.css"
  ]
}
