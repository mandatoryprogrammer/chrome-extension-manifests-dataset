  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      }
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "alarms"
  ],
  "minimum_chrome_version": "58",
  "applications": {
    "gecko": {
      "id": "randombookmark@pikadudeno1.com",
      "strict_min_version": "60.0"
    }
  },
  "manifest_version": 2
