{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Aardvark2",
  "description": "A quick repackaging of Karmatics Aardvark extension",
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "author": "Sudharsan Rangarajan <sudharsan.rangarajan@gmail.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Launch Aardvark Page Editor",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "allinone.js",
    "aardvark.gif",
    "aardvarkhelp.gif",
    "closedbox.gif",
    "aardvarkStrings.js",
    "aardvarkUtils.js",
    "aardvarkDBox.js",
    "aardvarkCommands.js",
    "aardvarkMain.js"
  ],
  "manifest_version": 2
}
