{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mixitch",
  "version": "1.0.1",
  "description": "Format mixer chat to twitch chat",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "https://mixer.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mixer.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "content/*"
  ],
  "icons": {
    "16": "images/16x16.png",
    "68": "images/68x68.png",
    "128": "images/128x128.png"
  }
}
