{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Checkbox marker",
  "description": "Check and uncheck checkboxes easily",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "16": "img/logo_16.png",
    "32": "img/logo_32.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "checkboxes.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Checkbox Marker",
    "default_popup": "popup.html"
  }
}
