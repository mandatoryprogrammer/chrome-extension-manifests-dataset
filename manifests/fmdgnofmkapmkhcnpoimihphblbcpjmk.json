{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Go back to work",
  "manifest_version": 2,
  "version": "0.3.1",
  "description": "Block sites and drive you to work!",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/16.png",
    "20": "icons/20.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "browser_action": {
    "default_title": "Go to work!",
    "default_icon": "icons/24.png"
  },
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "web_accessible_resources": [
    "index.html"
  ]
}
