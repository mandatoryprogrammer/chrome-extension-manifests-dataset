{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Load Lazyload Images",
  "version": "0.1",
  "description": "Load lazyload images automatically.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "load-lazyload-images.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
