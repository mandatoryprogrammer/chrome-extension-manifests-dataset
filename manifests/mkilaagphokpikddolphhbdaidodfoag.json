{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Splunk line-expander",
  "description": "Toggles Lines in Splunk. Depth is set by the user.",
  "version": "1.4",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "shared.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "shared.js",
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
