{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sol",
  "description": "Solidity realtime ethereum compiler and runtime",
  "devtools_page": "devtools.html",
  "version": "1.4",
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/img/sol.png"
  },
  "icons": {
    "48": "assets/img/sol48.png",
    "128": "assets/img/sol128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ]
}
