{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "League of Legends News",
  "short_name": "League News",
  "version": "38",
  "manifest_version": 2,
  "description": "The latest League of Legends News, in your browser!",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "League News",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self'  https://query.yahooapis.com https://ssl.google-analytics.com; object-src 'self'"
}
