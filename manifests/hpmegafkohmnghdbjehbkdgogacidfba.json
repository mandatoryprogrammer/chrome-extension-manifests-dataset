{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "widehub",
  "description": "Make code wider",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "exclude_matches": [
        "https://github.com/*blob*"
      ],
      "js": [
        "jquery-2.1.1.js",
        "widehub_page.js"
      ]
    },
    {
      "matches": [
        "https://github.com/*blob*"
      ],
      "js": [
        "jquery-2.1.1.js",
        "widehub_blob.js"
      ]
    },
    {
      "matches": [
        "https://github.com/*commit*"
      ],
      "js": [
        "jquery-2.1.1.js",
        "widehub_commit.js"
      ]
    }
  ]
}
