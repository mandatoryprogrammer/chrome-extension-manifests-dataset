{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrodic",
  "version": "0.3.2",
  "description": "A mouseover dictionary for Google Chromeâ„¢",
  "permissions": [
    "https://ankiweb.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "translation_box.js",
        "translation_task.js",
        "chrodic.js"
      ],
