{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Deconaby",
  "description": "解説アニメーション付与システム",
  "version": "0.310",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "menu.css"
      ],
      "js": [
        "libs/p5.js",
        "libs/p5.dom.js",
        "libs/jquery-3.1.1.min.js",
        "libs/jquery.ex-scroll.js",
        "menu.js",
        "main.js",
        "connect.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "img/*",
    "html/*"
  ]
}
