{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tag bookmarks with Tabbles ",
  "version": "1.5",
  "short_name": "Tabbles",
  "author": "Tag Forge",
  "homepage_url": "http://tabbles.net",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_title": "Tabbles Chrome extension",
    "default_icon": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "contextMenus"
  ]
}
