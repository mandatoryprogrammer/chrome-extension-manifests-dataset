{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "slack-direct",
  "description": "Love Slack? Hate slack-redir? Use slack-direct!",
  "author": "Jeremy Danyow",
  "icons": {
    "128": "icon128.png"
  },
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "https://*.slack.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
