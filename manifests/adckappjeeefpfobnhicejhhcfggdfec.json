{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Postgres Docs Layout",
  "version": "0.0.1",
  "description": "Improves the readability of Postgres docs by limiting the width of the content.",
  "icons": {
    "128": "postgres-layout-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.postgresql.org/docs/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
