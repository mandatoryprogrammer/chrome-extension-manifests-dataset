{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yelp sort reviews by date",
  "short_name": "YelpDateSort",
  "description": "This extension sorts Yelp reviews by date (newest first)",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "yelp-sort-by-date-script.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "*://*/*"
  ]
}
