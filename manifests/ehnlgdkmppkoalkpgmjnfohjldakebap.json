{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clock New Tab",
  "description": "Simple Css and js Clock",
  "version": "0.5",
  "background": {
    "scripts": [
      "js/index.js",
      "js/jquery.js"
    ]
  },
  "permissions": [
    "storage",
    "http://www.bing.com/HPImageArchive.aspx?format=js&idx=0&n=1"
  ],
  "icons": {
    "362": "img/logo.png",
    "16": "img/icon_16.png",
    "128": "img/icon_120.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
