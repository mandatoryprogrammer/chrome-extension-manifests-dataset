{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Feed Liker",
  "version": "0.5",
  "author": "Vadagon",
  "short_name": "Media Bot",
  "manifest_version": 2,
  "description": "Easy Social Media Automation",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Click to open popup",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "bower_components/*",
    "assets/*",
    "img/*"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "options_page": "options.html"
}
