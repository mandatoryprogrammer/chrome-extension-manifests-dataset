{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Goodbye geocities.jp",
  "short_name": "Goodbye geocities.jp",
  "description": "geocities.jp へのアクセスを archive.org へリダイレクトします",
  "version": "0.0.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.geocities.jp/*",
        "http://*.geocities.co.jp/*",
        "https://*.geocities.jp/*",
        "https://*.geocities.co.jp/*"
      ],
      "js": [
        "jump.js"
      ]
    }
  ]
}
