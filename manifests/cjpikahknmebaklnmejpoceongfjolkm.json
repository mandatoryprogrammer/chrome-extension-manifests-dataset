{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Site Crawler",
  "version": "1.2.3",
  "description": "Crawl a website for a Sitemap, Links and Images.",
  "icons": {
    "480": "icons/icon.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon-128.png",
      "48": "icons/icon-48.png",
      "16": "icons/icon-16.png"
    }
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "downloads"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "short_name": "Site Crawler",
  "manifest_version": 2
}
