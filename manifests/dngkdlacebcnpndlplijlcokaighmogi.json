{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "マリオメーカーのコースIDや、ユーザーIDをリンクに変換",
  "version": "1.1",
  "icons": {
    "16": "icon48.png",
    "48": "icon48.png",
    "128": "icon48.png"
  },
  "description": "work on Miiverse Only",
  "browser_action": {
    "default_icon": "19px.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://miiverse.nintendo.net/*"
      ],
      "match_about_blank": false,
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "https://miiverse.nintendo.net/*"
  ]
}
