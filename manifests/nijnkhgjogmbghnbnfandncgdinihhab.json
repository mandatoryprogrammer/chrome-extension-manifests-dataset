{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FancyTab",
  "description": "Bored of Chrome's clumsy New Tab? FancyTab beautifies your New Tab with beautiful photos hand-selected by Flickr's danielpuglisi.",
  "version": "1.1.2",
  "options_page": "options.html",
  "permissions": [
    "https://api.flickr.com/",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "flickrset.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  }
}
