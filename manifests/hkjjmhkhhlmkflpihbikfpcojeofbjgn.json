{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CRM External Editor App",
  "description": "Allows you to edit your Custom Right-Click Menu script in an editor of your choice.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon-supersmall.png",
    "48": "icons/icon-small.png",
    "128": "icons/icon-large.png"
  },
  "permissions": [
    "app.window",
    "notifications",
    "storage",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  }
}
