{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Au revoir patrie",
  "version": "0.36",
  "manifest_version": 2,
  "description": "Extension d'inversion du genre dans tout les textes en fran√ßais. (Fork of Jailbreak the Patriarchy)",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "data/icon16.png",
    "48": "data/icon48.png",
    "128": "data/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "data/icon19.png",
    "default_title": "Changer le genre"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "lib/ressources.js",
        "lib/myscript.js",
        "chromium/button.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "default-src 'none'; script-src 'self'",
  "options_page": "options.html"
}
