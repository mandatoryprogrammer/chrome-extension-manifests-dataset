{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Corndogify",
  "description": "Turn a page's boring regular images into exciting corndog images!",
  "version": "2.0.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "corndogify.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "corndogs/*"
  ],
  "browser_action": {
    "default_title": "Corndogify!",
    "default_icon": "icon-off.png"
  },
  "manifest_version": 2
}
