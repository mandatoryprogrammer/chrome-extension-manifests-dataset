{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Onemark",
  "shortname": "Onemark",
  "version": "1.0.5",
  "description": "Aggregate browser working session into a single manageable application",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Onemark"
  },
  "icons": {
    "48": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "notifications",
    "nativeMessaging",
    "storage"
  ]
}
