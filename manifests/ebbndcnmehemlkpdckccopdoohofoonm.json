{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "github-multiplay",
  "description": "Google Drive Realtime API on github",
  "manifest_version": 2,
  "version": "0.2",
  "icons": {
    "48": "/img/icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/edit/*"
      ],
      "js": [
        "/js/content_scripts.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "/web_accessible_resources/inner.js"
  ]
}
