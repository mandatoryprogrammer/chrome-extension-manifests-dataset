{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "reddit retitler",
  "description": "A small extension to change the subtitle of the reddit homepage",
  "version": "1.0.2",
  "author": "mammothbane",
  "homepage_url": "https://github.com/mammothbane/reddit-retitler",
  "icons": {
    "16": "icons/reddit-retitler-16.png",
    "48": "icons/reddit-retitler-48.png",
    "128": "icons/reddit-retitler-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/",
        "https://www.reddit.com/#*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": [
    "https://www.reddit.com/*"
  ]
}
