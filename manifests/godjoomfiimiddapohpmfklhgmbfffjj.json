{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vrome",
  "description": "Bringing Vim's elegance to Chrome",
  "version": "2.9.12",
  "content_scripts": [
    {
      "css": [
        "styles/main.css"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "shared/vendor/jquery.js",
        "shared/vendor/jquery.scrollable.js",
        "shared/vendor/jquery.highlight.js",
        "shared/extend.js",
        "shared/message.js",
        "shared/options.js",
        "shared/settings.js",
        "shared/utils.js",
        "frontend/modules/autolink.js",
        "frontend/modules/bookmark.js",
        "frontend/modules/buffer.js",
        "frontend/modules/chinese-pinyin.js",
        "frontend/modules/clipboard.js",
        "frontend/modules/cmd-box.js",
        "frontend/modules/cmd-line.js",
        "frontend/modules/command.js",
        "frontend/modules/custom.js",
        "frontend/modules/debug.js",
        "frontend/modules/dialog.js",
        "frontend/modules/frame.js",
        "frontend/modules/get-key.js",
        "frontend/modules/help.js",
        "frontend/modules/hint.js",
        "frontend/modules/history.js",
        "frontend/modules/insert-mode.js",
        "frontend/modules/key-event.js",
        "frontend/modules/links.js",
        "frontend/modules/marks.js",
        "frontend/modules/page.js",
        "frontend/modules/scroll.js",
        "frontend/modules/search.js",
        "frontend/modules/tab.js",
        "frontend/modules/unfocus.js",
        "frontend/modules/url.js",
        "frontend/modules/utils.js",
        "frontend/modules/window.js",
        "frontend/modules/zoom.js",
        "frontend/cmds.js",
        "frontend/main.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "topSites",
    "storage",
    "unlimitedStorage",
    "bookmarks",
    "history",
    "clipboardRead",
    "clipboardWrite",
    "notifications",
    "proxy",
    "contextMenus",
    "<all_urls>",
    "pageCapture",
    "webNavigation"
  ],
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "browser_action": {
    "default_popup": "background/html/popup.html",
    "default_icon": "images/logo.png",
    "default_title": "Vrome"
  },
  "options_page": "background/html/options.html",
  "manifest_version": 2,
  "minimum_chrome_version": "20",
  "background": {
    "scripts": [
      "shared/vendor/jquery.js",
      "shared/vendor/FileSaver.js",
      "shared/extend.js",
      "shared/message.js",
      "shared/options.js",
      "shared/settings.js",
      "shared/utils.js",
      "background/tab.js",
      "background/vromerc.js",
      "background/modules/background.js",
      "background/modules/bookmark.js",
      "background/modules/buffer.js",
      "background/modules/clipboard.js",
      "background/modules/command.js",
      "background/modules/debug.js",
      "background/modules/develop.js",
      "background/modules/editor.js",
      "background/modules/frame.js",
      "background/modules/history.js",
      "background/modules/main.js",
      "background/modules/shorturl.js",
      "background/modules/vrome.js",
      "background/modules/window.js",
      "oauth/chrome_ex_oauth.js",
      "oauth/chrome_ex_oauthsimple.js",
      "oauth/oauth.js",
      "oauth/onload.js"
    ]
  }
}
