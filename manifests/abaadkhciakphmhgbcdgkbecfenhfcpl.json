{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EtherSafe",
  "description": "Displays a warning over phishing/scam websites targetting Ethereum users.",
  "version": "11.0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "antiphish.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://*/*"
  ],
  "web_accessible_resources": [
    "icon256.png"
  ]
}
