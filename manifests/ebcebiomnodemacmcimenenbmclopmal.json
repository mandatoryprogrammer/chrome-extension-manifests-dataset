{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PropNeut",
  "description": "Propaganda neutralizer",
  "version": "1.0.4",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "config.js",
      "jquery-1.11.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "contentscript.js",
        "pageinspector.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/hug.png",
      "38": "images/blue_marker.png"
    },
    "default_title": "PropNÃ¸yt",
    "default_popup": "popup.html",
    "options_page": "options.html"
  },
  "web_accessible_resources": [
    "insert_files/*.js",
    "insert_files/*.html"
  ]
}
