{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Express Helper",
  "description": "A convenient way to track your package in Chrome Browser",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Print this page",
    "default_popup": "page.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
