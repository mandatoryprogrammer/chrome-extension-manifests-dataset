{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/offline.png"
  },
  "default_locale": "en",
  "description": "Ration your web usage.",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Web Ration Timer",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "version": "0.1.0",
  "web_accessible_resources": [
    "icons/online_full.png",
    "icons/offline_full.png"
  ]
}
