{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stack Don't Kill My Flow",
  "description": "Removes all of the fluff from Stack Overflow to limit distractions",
  "version": "1.0",
  "icons": {
    "128": "male-technologist.png"
  },
  "page_action": {
    "default_icon": "male-technologist.png",
    "default_popup": "popup.html",
    "default_title": "Stack Don't Kill My Flow"
  },
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
