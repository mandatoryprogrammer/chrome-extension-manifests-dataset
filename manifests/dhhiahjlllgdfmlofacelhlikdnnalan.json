{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NPR Volume",
  "manifest_version": 2,
  "version": "0.0.3",
  "content_scripts": [
    {
      "matches": [
        "https://www.npr.org/*"
      ],
      "run_at": "document_end",
      "css": [
        "style.css"
      ],
      "js": [
        "app.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
