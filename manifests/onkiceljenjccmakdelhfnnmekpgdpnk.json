{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Message to Whats",
  "short_name": "MtoWhats",
  "version": "2.1",
  "author": "Rodrigo Antunes",
  "description": "Send message to whatsapp contactless in the phonebook",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
