{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seo Previewer",
  "version": "1.5",
  "description": "Show Seo Tags",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery-1.10.2.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "favicon.png"
  }
}
