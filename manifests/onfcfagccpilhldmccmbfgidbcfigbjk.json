{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stream Pop",
  "version": "0.2",
  "description": "Aggregate the three most popular chat messages on YouTube live in a banner",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "sentimood.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/chrome_icon16.png",
      "32": "images/chrome_icon32.png",
      "48": "images/chrome_icon48.png",
      "128": "images/chrome_icon128.png"
    }
  },
  "icons": {
    "16": "images/chrome_icon16.png",
    "32": "images/chrome_icon32.png",
    "48": "images/chrome_icon48.png",
    "128": "images/chrome_icon128.png"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "manifest_version": 2
}
