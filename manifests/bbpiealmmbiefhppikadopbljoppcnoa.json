{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChromeDash",
  "short_name": "ChromeDash",
  "version": "2.0.5",
  "description": "Enter dashes, accents, Greek letters, or alias in generalâ€”quickly!",
  "browser_action": {
    "default_icon": "browsericon.png",
    "default_popup": "browserpopup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "ChromeDash16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "author": "Ofek Gila",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "general.js",
        "content.js"
      ]
    }
  ]
}
