{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noteblock",
  "version": "1.2.4",
  "description": "Simple text editor",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "editor.html",
    "default_title": "Noteblock"
  },
  "permissions": [
    "storage"
  ]
}
