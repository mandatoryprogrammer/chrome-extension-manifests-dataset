{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Genesis Grade Checker",
  "short_name": "Grade Checker",
  "description": "Easily check whether your grades went up or down.",
  "version": "1.4.7",
  "permissions": [
    "activeTab",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.lrhsd.org/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "genesis16.png",
    "48": "genesis48.png",
    "128": "genesis128.png"
  }
}
