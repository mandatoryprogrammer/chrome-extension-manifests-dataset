{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tabswitcher",
  "version": "1.0.2",
  "browser_action": {
    "default_title": "Show the popup",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "tabswitcher.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "tabswitcher.js",
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
