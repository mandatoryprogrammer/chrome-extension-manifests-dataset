{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fenestrate",
  "version": "1.0",
  "homepage_url": "https://github.com/polypmer/fenestrate",
  "icons": {
    "48": "icons/fenestrate-48.png",
    "96": "icons/fenestrate-96.png"
  },
  "browser_action": {
    "default_icon": {
      "18": "icons/fenestrate-18.png"
    },
    "default_title": "Fenestrate"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content_scripts/solarized-light.css"
      ]
    }
  ],
  "description": "Opens a window into the source."
}
