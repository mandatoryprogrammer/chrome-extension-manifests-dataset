{
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-1.7.1.min.js",
      "asana.js",
      "api_bridge.js",
      "extension_server.js",
      "server_model.js",
      "options.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html",
    "default_title": "Pick-a-Random-Asana-Task"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+A"
      }
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Picks a random task from a selected Asana workspace.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "minimum_chrome_version": "25",
  "name": "Pick-a-Random-Asana-Task",
  "offline_enabled": false,
  "permissions": [
    "activeTab",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://*.asana.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
