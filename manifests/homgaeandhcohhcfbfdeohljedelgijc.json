{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Imgur Gifv as MP4 Downloader",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "0.12",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-48.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://imgur.com/*",
    "https://imgur.com/*",
    "http://*.imgur.com/*",
    "https://*.imgur.com/*",
    "downloads",
    "storage"
  ],
  "author": "Ricky Notaro-Garcia",
  "homepage_url": "https://rickynotaro.com/"
}
