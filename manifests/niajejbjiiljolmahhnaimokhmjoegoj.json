{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BookmarkX2",
  "description": "Bookmarks for your bookmarks.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/bmx48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/bmx16.png",
    "48": "images/bmx48.png",
    "128": "images/bmx128.png"
  },
  "background": {
    "scripts": [
      "chrome-extension-async.js",
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage",
    "notifications"
  ]
}
