{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tinker Chrome",
  "short_name": "Tinker",
  "description": "Get designing. Customize web page text and share with others.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/",
    "storage",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
