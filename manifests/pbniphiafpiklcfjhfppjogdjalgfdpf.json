{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BeFocused (Asana)",
  "description": "Stay focused with Asana, by always seeing which task you are working on.",
  "version": "1.0.5",
  "author": "Bazyli Brz√≥ska",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "app": {
    "background": {
      "persistent": true,
      "scripts": [
        "jquery-1.11.0.min.js",
        "asana.js",
        "api_bridge.js",
        "extension_server.js",
        "server_model.js",
        "options.js",
        "background.js"
      ]
    }
  },
  "permissions": [
    "alwaysOnTopWindows",
    "storage",
    "*://*.asana.com/*"
  ]
}
