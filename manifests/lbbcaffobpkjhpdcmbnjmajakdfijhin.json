{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History Check",
  "version": "1.1",
  "description": "Gives you insight into your history on this site",
  "permissions": [
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "background.html",
    "default_icon": "clock.png"
  },
  "manifest_version": 2
}
