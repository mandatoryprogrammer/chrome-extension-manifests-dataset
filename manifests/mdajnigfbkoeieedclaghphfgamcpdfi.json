{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Disinfotron",
  "version": "1.0",
  "description": "An extension that flags potential disinformation websites based on Princeton University's machine learning classifier.",
  "permissions": [
    "webNavigation",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "classified_sites.txt"
  ],
  "browser_action": {
    "default_icon": "images/citp-128.png"
  },
  "icons": {
    "128": "images/citp-128.png",
    "400": "images/citp-400.png"
  }
}
