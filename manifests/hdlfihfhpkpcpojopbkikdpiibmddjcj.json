{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What the Cuss",
  "short_name": "Cuss",
  "description": "Replaces curse words with the word 'cuss'",
  "author": "Travis Baumann",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script1.js"
      ],
      "run_at": "document_end"
    }
  ]
}
