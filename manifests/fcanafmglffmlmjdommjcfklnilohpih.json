{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "分享到豆瓣东西",
  "description": "分享你喜欢的商品到豆瓣东西",
  "version": "1.1.2",
  "background": {
    "scripts": [
      "common/xregexp-min.js",
      "common/helper.js",
      "app.js"
    ]
  },
  "icons": {
    "48": "icons/icon.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage"
  ],
  "page_action": {
    "default_icon": "icons/cart.png",
    "default_title": "DoubanAutoCommodity"
  }
}
