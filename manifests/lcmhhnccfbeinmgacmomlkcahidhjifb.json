{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scroll Progress",
  "version": "1.0.2",
  "description": "Shows the scroll progress of current page.",
  "icons": {
    "16": "icons/icon@16px.png",
    "48": "icons/icon@48px.png",
    "128": "icons/icon@128px.png"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "./scroll_progress.js"
      ]
    }
  ]
}
