{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bootstrapify",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "http://www.bootstrapcdn.com/"
  ],
  "background": {
    "scripts": [
      "startup.js",
      "bootstrapify.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Import Bootstrap Styles"
  },
  "manifest_version": 2
}
