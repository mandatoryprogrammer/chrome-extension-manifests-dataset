{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Come si pronuncia?",
  "description": "Ma dov'è l'accento? La 'e' è chiusa o aperta? Sì, ora c'è una estensione anche per questo.",
  "version": "0.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "name": "do everything",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
