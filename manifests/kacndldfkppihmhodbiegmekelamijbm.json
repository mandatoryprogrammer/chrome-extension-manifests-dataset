{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Preencher campos",
  "version": "1.3",
  "description": "Preencher os campos dos formulários da página automaticamente.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Preencher campos"
  },
  "icons": {
    "19": "icon_19.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
