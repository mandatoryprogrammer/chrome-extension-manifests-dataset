{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Card Color Titles for Trello",
  "version": "1.0.1",
  "description": "Show the Card label titles on trello.com for easier communication in a team",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "css": [
        "override.css"
      ],
      "all_frames": true
    }
  ]
}
