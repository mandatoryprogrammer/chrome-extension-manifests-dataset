{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github files navigator conversion",
  "description": "Alters github.com html + javascript to allow files browsing in a file browser fashion.",
  "version": "0.2.3",
  "author": "Francois-Michel L'Heureux",
  "browser_action": {
    "default_icon": "images/m128x128.png"
  },
  "icons": {
    "128": "images/m128x128.png",
    "48": "images/m48x48.png",
    "16": "images/m16x16.png"
  },
  "background": {
    "scripts": [
      "jquery-2.1.0.min.js",
      "github-button.js"
    ]
  },
  "permissions": [
    "https://github.com/*",
    "tabs"
  ]
}
