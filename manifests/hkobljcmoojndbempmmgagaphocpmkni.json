{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "*://torrentz.eu/*",
        "*://torrentz.ch/*",
        "*://torrentz.me/*",
        "*://torrentz.in/*",
        "*://torrentz.com/*",
        "*://torrentz2.eu/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Adds magnet links to torrent files on Torrentz2.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Magnet Links for Torrentz2",
  "version": "0.10.5",
  "version_name": "0.9.5-t2",
  "web_accessible_resources": [
    "icon16.png"
  ]
}
