{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "旺中守門員(兼打混守門員）",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "防止旺中，同時防止打混。原始碼下載： github.com/kosinyj/no-wang-wang",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*",
    "*://*.yahoo.com/*",
    "*://*.chinatimes.com/*",
    "*://*.ctitv.com.tw/*",
    "*://*.ctv.com.tw/*"
  ]
}
