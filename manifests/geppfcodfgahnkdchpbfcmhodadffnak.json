{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RT video Helper",
  "description": "This extension helps you keep track of the videos you've seen on RoosterTeeth.com.",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": [
        "http://*.roosterteeth.com/*",
        "http://*.achievementhunter.com/*"
      ],
      "js": [
        "jquery.js",
        "rthelper.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/seen.png",
    "images/check.png"
  ]
}
