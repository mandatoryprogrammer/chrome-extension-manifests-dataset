{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "ChaCha",
  "name": "ChaCha Savings",
  "description": "Save more. Buy less. Prompts you to invest before online spending.",
  "version": "0.0.1",
  "author": "Nick Clear",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "ChaCha",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "tingle/tingle.min.css",
        "modal.css"
      ],
      "js": [
        "tingle/tingle.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "tingle/tingle.min.css",
    "tingle/tingle.js"
  ]
}
