{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twice the Tip",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Multiply your Seamless tip by two or more since it's pretty low as it stands. NOT RESPONSIBLE FOR OVERCHARGES.",
  "homepage_url": "http://github.com/zachfeldman/twice_the_tip",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "tabs",
    "*://*.seamless.com/*"
  ],
  "background": {
    "scripts": [
      "src/background/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.seamless.com/*"
      ],
      "js": [
        "src/inject/jquery-1.11.1.min.js",
        "src/inject/inject.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png"
  }
}
