{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Binance Alerts",
  "description": "Unofficial extension to set price alerts on Binance",
  "version": "1.3",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "logo.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "https://api.binance.com/",
    "notifications"
  ],
  "web_accessible_resources": [
    "logo.png"
  ]
}
