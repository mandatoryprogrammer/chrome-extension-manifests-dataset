{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "lesechos.fr sans limite de lecture",
  "short_name": "les Echos sans limite",
  "description": "Permet de lire lesechos.fr sans limitation du nombre d'article consult√©.",
  "version": "1.4",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "background"
  ],
  "icons": {
    "48": "icon_cookie48.png",
    "128": "icon_cookie128.png"
  },
  "browser_action": {
    "default_icon": "icon_cookie.png"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
