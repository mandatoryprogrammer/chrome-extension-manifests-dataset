{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mail me my Tabs",
  "description": "This extension sends the active window's tabs to you by mail. A mandrill.com account is required.",
  "version": "1.0",
  "options_page": "options.html",
  "icons": {
    "64": "icon64.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
