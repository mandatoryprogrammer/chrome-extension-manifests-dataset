{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabs State Saver",
  "description": "This extension lets you save the tabs state and load it later",
  "version": "3.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "tabState.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
