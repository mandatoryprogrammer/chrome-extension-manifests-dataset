{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TextFiller",
  "description": "Save your basic details and use this extension to fill the forms easily",
  "version": "1.0",
  "background": {
    "scripts": [
      "start.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index2.html"
  },
  "icons": {
    "16": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ]
}
