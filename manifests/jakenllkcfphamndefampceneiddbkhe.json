{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Guilt",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Teens ask their mother to borrow the car; you ask Guilt to borrow the internet.",
  "icons": {
    "128": "guilt_ico.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "js/jquery-1.8.2.min.js",
        "foreground.js"
      ],
      "css": [
        "foreground.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/jquery-1.8.2.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "for_what.png",
    "how_long.png",
    "recent.png",
    "the_job.png"
  ]
}
