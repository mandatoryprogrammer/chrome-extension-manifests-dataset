{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pro-Life plugin",
  "description": "This extension converts anti-choice and anti-abortion language to pro-life",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "filter-script.js"
      ]
    }
  ],
  "browser_action": {},
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
