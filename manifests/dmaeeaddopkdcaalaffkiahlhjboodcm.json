{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "3rd/jquery-3.1.1.min.js",
      "init.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' blob: filesystem: chrome-extension-resource: https://*.oxforddictionaries.com; object-src 'self'",
  "manifest_version": 2,
  "name": "Oxford Dictionaries (Unofficial)",
  "omnibox": {
    "keyword": "o"
  },
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "*://*.oxfordlearnersdictionaries.com/",
    "*://*.oxforddictionaries.com/",
    "storage"
  ],
  "version": "0.7"
}
