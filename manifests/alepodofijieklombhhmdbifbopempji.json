{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Note That",
  "description": "Note That is a Chrome extension that replaces your new tab with a minimalistic editor which you can use to save quick notes.",
  "version": "1.1",
  "author": "Kamal Nayan",
  "browser_action": {
    "default_icon": "img/tab-icon@8x.png",
    "default_title": "Note That"
  },
  "icons": {
    "16": "img/tab-icon.png",
    "32": "img/tab-icon@2x.png",
    "48": "img/tab-icon@3x.png",
    "128": "img/tab-icon@8x.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
