{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Görev Yönetimi",
  "version": "1.8",
  "description": "Essente CRM v1.0 || Yönetim Sistemi",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "16.png",
      "38": "48.png",
      "128": "128.png"
    },
    "default_title": "Essente CRM",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "128.png"
  ]
}
