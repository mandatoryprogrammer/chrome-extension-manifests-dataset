{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Privee",
  "version": "1.2",
  "manifest_version": 2,
  "description": "See what privacy policies say in a convenient label format.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "notifications",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jQuery.js",
        "scraper.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
