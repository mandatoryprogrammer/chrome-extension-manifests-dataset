{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Count",
  "description": "This extension allows the user to see how many tabs they have open.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "dropdown.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "count.js"
    ]
  }
}
