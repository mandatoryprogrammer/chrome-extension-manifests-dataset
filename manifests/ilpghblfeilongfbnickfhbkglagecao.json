{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DF Naver",
  "version": "1.0.0",
  "description": "hide Naver trending searchwords and news topics, which are distracting.",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://*.naver.com/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "manifest_version": 2
}
