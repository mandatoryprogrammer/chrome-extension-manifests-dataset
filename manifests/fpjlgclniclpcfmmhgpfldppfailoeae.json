{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "samsung-expert-academy-extension",
  "short_name": "SEAe",
  "version": "0.1.15",
  "manifest_version": 2,
  "author": "SsangWoo",
  "description": "chrome extension for Samsung expert academy",
  "homepage_url": "https://github.com/tkddn204/samsung-expert-academy-extension",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://*.swexpertacademy.com/*"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "*.png",
    "*.gif"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
