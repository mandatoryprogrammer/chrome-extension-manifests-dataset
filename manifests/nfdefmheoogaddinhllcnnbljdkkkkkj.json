{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PokeNope",
  "description": "Removes Pokemon related stuff from your Facebook News Feed",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "blacklist.js",
        "pokenope.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "pokenope.html",
    "default_title": "PokeNope!"
  }
}
