{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Site Blocker",
  "version": "0.4",
  "short_name": "SiteBlocker",
  "description": "Stop angering your corporate proxy or web filter.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html"
}
