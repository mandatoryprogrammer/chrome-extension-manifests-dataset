{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Random Raptor",
  "author": "Mike Roberts",
  "description": "Dinosaurs - They Could Attack At Any Time! WHAAAAT?!",
  "version": "1.1.1",
  "background": {
    "scripts": [
      "jquery.min.js",
      "runWhenPageLoad.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*",
        "*://jira/*",
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "randomRaptor.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "dinoHead128-1.png"
  },
  "icons": {
    "128": "dinoHead128-1.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "storage",
    "notifications",
    "cookies",
    "alarms"
  ],
  "web_accessible_resources": [
    "raptorRun.html",
    "randomRaptor.js",
    "raptorBikeJPG.png",
    "raptorBikePNG.png",
    "raptorLeg.png",
    "raptorLegRight.png",
    "raptorFullWithRightLeg.png",
    "raptorFullWithRightLeg.png",
    "raptorNoLegs.png",
    "treePeek.png",
    "raptorPeerOut.png",
    "raptorTree1.html",
    "scaryRaptor1.png",
    "raptorScare1.html",
    "tricHead.png",
    "tricNoLegsNoHeadNoTail.png",
    "tricFrontFullLeg.png",
    "tricFrontNotFullLeg.png",
    "tricBackFullLeg.png",
    "tricBackNotFullLeg.png",
    "tricTail.png",
    "dinoScream.mp3",
    "raptorRun.js",
    "spittingDino.html",
    "diloGif.gif",
    "clicking.mp3",
    "seasonal1.html"
  ]
}
