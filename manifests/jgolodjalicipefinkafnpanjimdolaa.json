{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "stacker",
  "description": "Follow people and get their aggregated stackexchange site wide activity feeds",
  "version": "0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Stacker",
    "default_icon": "images/icon.png",
    "default_popup": "feeds.html"
  },
  "minimum_chrome_version": "5.0",
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}
