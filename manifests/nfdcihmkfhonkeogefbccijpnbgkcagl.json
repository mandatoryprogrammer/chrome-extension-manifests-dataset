{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "DSC",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.1",
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "48": "48.png",
    "16": "16.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
