{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UW Myplan RMP auto-checker",
  "version": "0.1",
  "description": "Automatically get the RMP rating of professors shown",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://myplan.uw.edu/course*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
