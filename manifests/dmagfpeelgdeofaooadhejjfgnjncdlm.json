{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Latinize Georgian",
  "version": "0.0.3",
  "description": "Change georgian letters to latin letters",
  "permissions": [],
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ge/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
