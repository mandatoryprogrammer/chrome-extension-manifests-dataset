{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "author": "@ndaidong",
  "version": "1.1.0",
  "default_locale": "en",
  "icons": {
    "16": "icon-16.png",
    "24": "icon-24.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    }
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    "unlimitedStorage"
  ]
}
