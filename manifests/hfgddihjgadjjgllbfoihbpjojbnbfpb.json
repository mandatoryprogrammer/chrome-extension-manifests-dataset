{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save All Tabs to Pocket",
  "description": "This extension saves all open tabs to Pocket",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persisten": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Save all open tabs to Pocket",
    "default_icon": "icon16.png"
  },
  "manifest_version": 2
}
