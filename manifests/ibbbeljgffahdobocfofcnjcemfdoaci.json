{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabsticker",
  "description": "Prevents pinned tabs from being closed",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "tabsticker.js"
    ],
    "persistent": false
  }
}
