{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Grooveshark Germany unlocker",
  "version": "2.4.2",
  "manifest_version": 2,
  "description": "Erlaubt es Grooveshark von Deutschland aus zu verwenden",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "permissions": [
    "http://*.grooveshark.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://grooveshark.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "main.js"
      ]
    }
  ]
}
