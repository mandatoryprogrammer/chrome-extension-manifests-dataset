{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Block Service Workers",
  "description": "Disallow to register Service Workers",
  "version": "1.0.1",
  "icons": {
    "16": "logox16.png",
    "48": "logox48.png",
    "128": "logo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "confirm.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "settings.html",
    "default_icon": "logox32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "index.js"
      ],
      "all_frames": true
    }
  ]
}
