{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gothamist archives",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Retrieving gothamist articles using web.archives.org",
  "homepage_url": "https://github.com/reyrodrigez/nol-archive",
  "icons": {
    "128": "images/icons/icon128.png",
    "48": "images/icons/icon48.png",
    "16": "images/icons/icon16.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": true,
    "matches": [
      "*://*.gothamist.com/*",
      "*://gothamist.com/*"
    ]
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}
