{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Theme Changer",
  "description": "Change the theme of a website - live a little!",
  "version": "1.3",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.0.2.min.js",
        "theme-changer.min.js"
      ]
    }
  ]
}
