{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rabbit Archiver",
  "version": "0.6",
  "version_name": "0.6",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*/inbox/*",
        "https://business.facebook.com/*/inbox/*"
      ],
      "js": [
        "jquery.min.js",
        "bot-speak.js",
        "content.js",
        "rabbit-archiver.js",
        "struct.js",
        "util.js",
        "ignore.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
