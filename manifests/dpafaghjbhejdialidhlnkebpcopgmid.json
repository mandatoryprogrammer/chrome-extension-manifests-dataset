{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save Dat Monie",
  "description": "This extension helps you save money by controlling your spending habits!",
  "version": "1.2",
  "icons": {
    "16": "dollar16.png",
    "48": "dollar48.png",
    "128": "dollar128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "cs.js"
      ]
    }
  ]
}
