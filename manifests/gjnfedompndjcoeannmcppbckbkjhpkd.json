{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "rsstodolist",
  "version": "1.5.1",
  "description": "Send links to rsstodolist - https://rsstodolist.appspot.com/",
  "homepage_url": "https://github.com/paulgreg/rsstodolist-addon",
  "icons": {
    "16": "imgs/rss.png",
    "48": "imgs/rss.png",
    "96": "imgs/rss.png",
    "128": "imgs/rss.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "storage",
    "contextMenus"
  ],
  "applications": {
    "gecko": {
      "id": "jid0-fsmvHJm6vogjlOsg61W1N560CwA@jetpack",
      "strict_min_version": "60.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "imgs/rss.png",
    "default_title": "rsstodolist",
    "default_popup": "index.html"
  }
}
