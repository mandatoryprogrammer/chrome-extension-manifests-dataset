{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mouseless Googling",
  "description": "Google Without Your Mouse. Use the keys, save your time!",
  "version": "0.5.3",
  "manifest_version": 2,
  "icons": {
    "48": "mg-icon64.png"
  },
  "web_accessible_resources": [
    "fonts/*",
    "images/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*",
        "https://www.google.com/*",
        "https://www.google.es/*",
        "http://www.google.es/*"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "myscript.js"
      ]
    }
  ]
}
