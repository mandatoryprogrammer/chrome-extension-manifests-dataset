{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Lion Time",
  "version": "4.1",
  "description": "Shows the time until your next class",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://lion-time.appspot.com/"
  ],
  "browser_action": {
    "default_title": "Lion Time",
    "default_icon": "48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "48.png"
  ]
}
