{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JVideoSpeeder",
  "description": "A HTML5 video speeder",
  "version": "1.0",
  "manifest_version": 2,
  "homepage_url": "https://www.junorz.com",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
