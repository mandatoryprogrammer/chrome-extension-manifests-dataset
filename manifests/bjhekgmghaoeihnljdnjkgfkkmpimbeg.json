{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timeflix",
  "description": "Enables playback rate modification of video elements.",
  "version": "1.1",
  "page_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "declarativeContent"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "style.css"
  ]
}
