{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search with +",
  "short_name": "Search with +",
  "description": "Simple chrome extension that lets you search various popular search engines ",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "extension.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2,
  "icons": {
    "16": "search_plus.png",
    "32": "search_plus.png",
    "48": "search_plus.png"
  }
}
