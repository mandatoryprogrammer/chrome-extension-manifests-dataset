{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Atlas Tabs",
  "description": "Suggest tabs based on personal characteristcs (location & time), as well as people around you",
  "version": "0.6",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "history",
    "geolocation"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "name": "Suggest tabs based on personal characteristcs, location as well as people around you",
    "default_title": "Check what's trending",
    "default_icon": "img/logo128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/logo128.png",
    "48": "img/logo48.png",
    "16": "img/logo16.png"
  },
  "manifest_version": 2
}
