{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "permissions": [
    "http://stackoverflow.com/*",
    "https://stackoverflow.com/*"
  ],
  "offline_enabled": true,
  "content_scripts": [
    {
      "matches": [
        "http://stackoverflow.com/*",
        "https://stackoverflow.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
