{
  "author": "James Tomasino",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "name": "Read",
    "default_icon": "icon128.png",
    "default_title": "Read"
  },
  "content_scripts": [
    {
      "css": [
        "main.css"
      ],
      "js": [
        "jquery.js",
        "jquery.nouislider.js",
        "ReadWord.js",
        "ReadBlock.js",
        "Read.js",
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Read - Speed Reading via RSVP",
  "homepage_url": "http://github.com/jamestomasino/read_plugin",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Read",
  "offline_enabled": true,
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "short_name": "Read",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.0.0",
  "web_accessible_resources": [
    "fonts/DroidSans-webfont.eot",
    "fonts/DroidSans-webfont.svg",
    "fonts/DroidSans-webfont.ttf",
    "fonts/DroidSans-webfont.woff"
  ]
}
