{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coursedy Screenshare Extension",
  "author": "Coursedy",
  "version": "1.0.7",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Chrome Extension granting WebRTC screen capturing capabilities to Coursedy room.",
  "homepage_url": "https://coursedy.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "*://*.coursedy.com/*",
      "*://*.coursedy.tk/*"
    ]
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
