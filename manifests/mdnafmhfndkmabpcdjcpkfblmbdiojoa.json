{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InsideX",
  "short_name": "InsideX",
  "version": "1.1",
  "description": "InsideX Extension!",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "alarms",
    "notifications",
    "activeTab",
    "declarativeContent",
    "storage",
    "https://www.bytes.io/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Insider Moves Monitor",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/iX16.png",
      "32": "images/iX32.png",
      "48": "images/iX48.png",
      "128": "images/iX128.png"
    }
  },
  "icons": {
    "16": "images/iX16.png",
    "32": "images/iX32.png",
    "48": "images/iX48.png",
    "128": "images/iX128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
