{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dezure",
  "description": "Bypasses the annoying step on azure resource deletion",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://portal.azure.com/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
