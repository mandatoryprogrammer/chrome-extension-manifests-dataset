{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SafeClick",
  "version": "0.0.1",
  "description": "Adds rel='noopener' to all anchors with target='_blank' that don't have it.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
