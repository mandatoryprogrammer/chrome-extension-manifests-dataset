{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MadRate",
  "version": "1.02",
  "description": "Build an Extension to get the rate-my-professor score when selecting courses!",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "http://www.ratemyprofessors.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/hunter128.png"
    }
  },
  "icons": {
    "128": "images/hunter128.png"
  },
  "manifest_version": 2
}
