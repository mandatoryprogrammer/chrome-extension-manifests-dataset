{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "inject.js",
    "libs/jquery/jquery-1.9.1.min.js",
    "libs/jquery/jquery-1.9.1.min.js",
    "libs/jquery-harlem-shake/jquery-harlem-shake-1.0.js",
    "libs/jquery-harlem-shake/jquery-harlem-shake-1.0.css",
    "libs/jquery-harlem-shake/harlemshake.ogg"
  ],
  "browser_action": {
    "default_icon": "assets/icon-128.png",
    "default_title": "Do the HarlemShake"
  },
  "name": "HarlemShakeTension",
  "description": "\"HarlemShake\" your favorite websites like you never shaked them before!",
  "homepage_url": "http://github.com/piscis/chrome-harlem-shake-extension",
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.4",
  "manifest_version": 2
}
