{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ask.fm Helper",
  "description": "Ask.fm helper",
  "version": "0.1",
  "icons": {
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "http://ask.fm/*",
    "https://ask.fm/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://ask.fm/*",
        "https://ask.fm/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "askfm-helper.js"
  ]
}
