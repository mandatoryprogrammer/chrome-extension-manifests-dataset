{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Spy-inator",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This will make any hidden inputs on the page visible by changing the type from hidden to text.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Toggle all hidden inputs"
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "debugger"
  ],
  "homepage_url": "http://www.seanmcmahan.info",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
