{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sortabs",
  "description": "Group tabs by their urls",
  "version": "1.0",
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
