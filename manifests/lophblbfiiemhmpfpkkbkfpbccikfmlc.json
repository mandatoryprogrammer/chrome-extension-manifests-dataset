{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Offline Bootstrap 3",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "bootstrap3 offline",
    "default_popup": "index.html",
    "default_icon": {
      "19": "bootstrap.png",
      "128": "bootstrap.png"
    }
  },
  "icons": {
    "16": "bootstrap.png",
    "48": "bootstrap.png",
    "128": "bootstrap.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
