{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calendario de Marketing Estacional 2020",
  "short_name": "Calendario Doppler",
  "version": "3",
  "author": "Doppler LLC",
  "description": "Calendario de Marketing Estacional 2020 - Doppler",
  "icons": {
    "128": "resources/img/extension-icon_128x128.png",
    "48": "resources/img/extension-icon_48x48.png",
    "16": "resources/img/extension-icon_16x16.png"
  },
  "browser_action": {
    "default_icon": "resources/img/extension-icon_48x48.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "configuration.html"
  },
  "background": {
    "scripts": [
      "resources/js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "notifications",
    "tabs",
    "activeTab",
    "browsingData"
  ]
}
