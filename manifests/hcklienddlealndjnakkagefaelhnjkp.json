{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Global Input App",
  "icons": {
    "128": "images/globalInput128.png",
    "48": "images/globalInput48.png",
    "32": "images/globalInput32.png",
    "24": "images/globalInput24.png",
    "16": "images/globalInput16.png"
  },
  "version": "3.0.1",
  "manifest_version": 2,
  "description": "Mobile authentication, mobile input & control, transfer content securely between your mobile and your computer.",
  "homepage_url": "https://globalinput.co.uk/",
  "browser_action": {
    "default_icon": {
      "128": "images/globalInput128.png",
      "48": "images/globalInput48.png",
      "32": "images/globalInput32.png",
      "24": "images/globalInput24.png",
      "16": "images/globalInput16.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
