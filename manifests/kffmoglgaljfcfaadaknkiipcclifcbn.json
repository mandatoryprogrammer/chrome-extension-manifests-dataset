{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Free Rider HD Offline Editor",
  "short_name": "FRHD Editor",
  "description": "Draw and play Free Rider HD tracks offline!",
  "version": "1.6",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
    "fullscreen"
  ],
  "offline_enabled": true,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
