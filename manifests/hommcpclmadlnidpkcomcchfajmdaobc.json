{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Slate",
  "version": "1.0",
  "description": "Blocks distracting websites and helps you manage your time.",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://kit.fontawesome.com https://cdnjs.cloudflare.com; object-src 'self'",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
