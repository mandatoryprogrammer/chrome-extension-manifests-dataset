{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/lib/jquery-min.js",
      "js/lib/underscore-min.js",
      "js/lib/ripple.js",
      "js/lib/ripple.helpers.js",
      "js/lib/ripple.api.js",
      "js/lib/avalon.js",
      "js/common.js",
      "js/bg.js",
      "js/versions.js",
      "js/shortening-url.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/lib/jquery-min.js",
        "js/consumer-helper.js"
      ],
      "matches": [
        "https://fanfou.com/apps.details/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "38": "icons/38.png",
      "19": "icons/19.png"
    },
    "default_popup": "popup.html",
    "default_title": "PREFiX"
  },
  "web_accessible_resources": [
    "manifest.json"
  ],
  "content_security_policy": "default-src 'self'; script-src 'self' 'unsafe-eval'; style-src 'unsafe-inline'; img-src 'self' data: https://s3.meituan.net https://s3-img.meituan.net; connect-src 'self' https://fanfou.com https://api.fanfou.com https://stream.fanfou.com",
  "description": "简洁、易用的饭否客户端.",
  "icons": {
    "256": "icons/256.png",
    "128": "icons/128.png",
    "48": "icons/48.png",
    "40": "icons/40.png",
    "38": "icons/38.png",
    "19": "icons/19.png",
    "16": "icons/16.png"
  },
  "omnibox": {
    "keyword": "f"
  },
  "manifest_version": 2,
  "name": "PREFiX",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "https://fanfou.com/*",
    "https://api.fanfou.com/*",
    "https://stream.fanfou.com/*"
  ],
  "version": "0.9.2",
  "minimum_chrome_version": "47"
}
