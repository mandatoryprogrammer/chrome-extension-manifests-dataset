{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "To Do List",
  "version": "1.8",
  "manifest_version": 2,
  "description": "Simple and best way to organise work and increase productivity. ",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "to-do-list.png",
    "48": "to-do-list.png",
    "128": "to-do-list.png"
  },
  "permissions": [
    "storage"
  ]
}
