{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Audiophiles",
  "version": "1.1",
  "description": "Replaces the text 'audiophile' with 'deluded tool with too much money'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_globs": [
        "*post.php*action=edit*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "author": "Jesse Friedman <jesse.ws>",
  "homepage_url": "https://github.com/okofish/audiophiles"
}
