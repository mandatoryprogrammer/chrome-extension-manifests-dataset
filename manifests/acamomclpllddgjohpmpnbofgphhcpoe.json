{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WordXer",
  "version": "1.0.0",
  "description": "Tools for online crosswords",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
