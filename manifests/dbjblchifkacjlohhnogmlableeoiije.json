{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vee",
  "version": "6.0.0",
  "description": "Vee",
  "content_scripts": [
    {
      "matches": [
        "https://voat.co/*"
      ],
      "js": [
        "jquery.js",
        "vee.js"
      ],
      "css": [
        "vee.css"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://voat.co/*",
    "https://voat.co/*"
  ],
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "32": "icon_32.png",
    "16": "icon_16.png"
  },
  "web_accessible_resources": [
    "tag_10.png"
  ]
}
