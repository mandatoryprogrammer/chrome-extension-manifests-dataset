{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LibrarySearch",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Adds a right-click menu to search your local public library for the selected text.",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "content_script.js",
      "options.js"
    ]
  },
  "icons": {
    "16": "LibrarySearchIcon-16.png",
    "24": "LibrarySearchIcon-24.png",
    "48": "LibrarySearchIcon-48.png"
  },
  "options_page": "options.html"
}
