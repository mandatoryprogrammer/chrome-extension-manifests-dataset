{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL To QR Generator",
  "short_name": "urlTOqr-generator",
  "description": "URL To QR Generator is a free extension for QR Code creation of an active tab.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "32": "icon.png",
    "48": "icon-48.png"
  },
  "browser_action": {
    "default-icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "style.css"
  ]
}
