{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kcilk Exchanger",
  "description": "Exchange module for service Kcilk",
  "version": "1.0.2",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://kcilk.company/*",
    "*://*.timocom.com/*",
    "*://*.timocom.ru/*"
  ]
}
