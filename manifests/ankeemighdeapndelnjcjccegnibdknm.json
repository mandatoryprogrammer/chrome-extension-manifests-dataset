{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "__MSG_shortAppName__",
  "version": "4.5.5",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "identity",
    "tabs",
    "background",
    "notifications",
    "storage",
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "css": [
        "styles/vex.css",
        "styles/vex-theme-default.css",
        "styles/selectize.css",
        "styles/selectize.default.css",
        "styles/main.css"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/chrome-promise.js",
        "scripts/lodash.js",
        "scripts/polyfill.js",
        "scripts/inboxsdk.js",
        "scripts/selectize.js",
        "scripts/vex.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "SecureMail",
    "default_popup": "popup.html"
  },
  "oauth2": {
    "client_id": "4172905106-cjfvnri5090q1a99jd73iqgk0udd8ukl.apps.googleusercontent.com",
    "scopes": [
      "https://www.google.com/m8/feeds"
    ]
  },
  "web_accessible_resources": [
    "styles/main.css",
    "images/icon-48.png"
  ]
}
