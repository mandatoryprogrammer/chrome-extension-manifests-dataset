{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cloud9 button for GitHub",
  "version": "1.1.5",
  "manifest_version": 2,
  "description": "This extension adds a button to GitHub that lets you edit your projects directly in Cloud9 IDE",
  "icons": {
    "16": "c9-16.png",
    "48": "c9-48.png",
    "128": "c9-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "githubc9.user.js"
      ]
    }
  ]
}
