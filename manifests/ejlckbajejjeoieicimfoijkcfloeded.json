{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EasyDict",
  "version": "0.0.3",
  "author": "Yunfeng Wang",
  "description": "A simple interface to check english words",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "clipboardWrite",
    "clipboardRead"
  ],
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
