{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dark Hacker News",
  "version": "0.1.1",
  "description": "Dark Mode For Hacker News",
  "permissions": [
    "https://news.ycombinator.com/*"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/Dh16.png",
      "32": "images/Dh32.png",
      "48": "images/Dh48.png",
      "128": "images/Dh128.png"
    }
  },
  "icons": {
    "16": "images/Dh16.png",
    "32": "images/Dh32.png",
    "48": "images/Dh48.png",
    "128": "images/Dh128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "css": [
        "DarkHacker.css"
      ]
    }
  ],
  "manifest_version": 2
}
