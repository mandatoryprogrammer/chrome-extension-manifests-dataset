{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quote For You",
  "short_name": "Quote For You",
  "description": "Find a beautiful quote in every new tab!",
  "version": "1.0.5",
  "homepage_url": "https://github.com/ptomaszek/quote-for-you",
  "icons": {
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png",
    "96": "icons/icon_96.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage",
    "http://api.forismatic.com/api/1.0/*"
  ],
  "chrome_url_overrides": {
    "newtab": "quote.html"
  }
}
