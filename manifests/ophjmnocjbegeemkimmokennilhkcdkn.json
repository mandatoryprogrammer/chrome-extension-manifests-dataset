{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Foros Del Web Skin",
  "description": "Skin para forosdelweb.com",
  "version": "1.1.4",
  "content_scripts": [
    {
      "matches": [
        "http://www.forosdelweb.com/*"
      ],
      "css": [
        "data/css/global.css",
        "data/css/posts.css",
        "data/css/blackened.css",
        "data/css/threads.css"
      ],
      "js": [
        "data/js/jquery.min.js",
        "data/js/fdwskin.js",
        "data/js/cookies.js",
        "data/js/fdwusability.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "data/images/*"
  ],
  "icons": {
    "128": "data/icons/fdw.png"
  }
}
