{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Social Notifications [Mod]",
  "version": "2.44",
  "description": "Social notifications and quick link",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Social Notifications [Mod]"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "https://github.com/mzino/chrome-messenger-notifications",
  "optional_permissions": [
    "background"
  ],
  "options_page": "options.html",
  "permissions": [
    "https://www.facebook.com/"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ]
}
