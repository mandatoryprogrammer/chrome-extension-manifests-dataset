{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ReadingCloud",
  "description": "Save messages,tabs and links, visit anywhere.",
  "version": "1.5.5",
  "homepage_url": "http://readingcloud.abelcloud.net",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "ReadingCloud"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
