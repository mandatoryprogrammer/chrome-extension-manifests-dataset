{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "note plus",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "note everything",
  "icons": {
    "128": "assets/image/icon_128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "/dist/background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "/assets/image/icon_128.png",
    "default_popup": "/pages/popup.html"
  }
}
