{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "github-firacode",
  "version": "2.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://gist.github.com/*",
        "https://bitbucket.org/*"
      ],
      "css": [
        "fonts.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "FiraCode/*.eot",
    "FiraCode/*.eot",
    "FiraCode/*.woff2",
    "FiraCode/*.woff",
    "FiraCode/*.ttf"
  ],
  "permissions": [
    "activeTab"
  ],
  "description": "A chrome extension that brings FiraCode font to github's code blocks.",
  "icons": {
    "128": "images/icon128.png"
  }
}
