{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0",
  "name": "Slashdot Adwrap Remover",
  "description": "Removes the annoying ad bar that covers half the screen",
  "permissions": [
    "tabs",
    "*://*.slashdot.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.slashdot.org/*"
      ],
      "css": [
        "inject.css"
      ]
    }
  ]
}
