{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "goodbye_news_feed",
  "version": "0.1",
  "description": "Hides your facebook news feed so you can get your life back",
  "icons": {
    "48": "icons/fbkiller48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*",
        "*://www.facebook.com/*"
      ],
      "js": [
        "fbkiller.js"
      ]
    }
  ]
}
