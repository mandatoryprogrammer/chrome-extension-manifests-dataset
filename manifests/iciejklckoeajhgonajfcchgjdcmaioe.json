{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dart Login",
  "short_name": "Dart Login",
  "version": "1.0",
  "author": "John Schlachtenhaufen",
  "description": "Automatically logs you into Dartmouth SSO when accessing Dartmouth pages",
  "permissions": [
    "storage",
    "https://api.sendgrid.com/v3/mail/send"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persisent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://websso.dartmouth.edu/*ogin*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "views/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "views/options.html",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
