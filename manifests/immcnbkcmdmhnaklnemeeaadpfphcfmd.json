{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://movie.douban.com/subject/*"
      ]
    }
  ],
  "description": "豆瓣电影 资源下载, IMDB评分, MPAA 分级",
  "icons": {
    "128": "images/128.png",
    "16": "images/16.png",
    "48": "images/48.png"
  },
  "manifest_version": 2,
  "name": "douban Plus",
  "permissions": [
    "*://www.imdb.com/"
  ],
  "version": "0.0.4"
}
