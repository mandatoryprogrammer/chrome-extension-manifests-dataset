{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SVG2PNG",
  "description": "Save inline <svg> as .png files",
  "version": "1.1",
  "icons": {
    "16": "split-16.png",
    "48": "split-48.png",
    "128": "split-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "activeTab"
  ]
}
