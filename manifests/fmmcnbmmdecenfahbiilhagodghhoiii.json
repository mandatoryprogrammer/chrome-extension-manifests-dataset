{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coinstorm",
  "version": "1.0.15",
  "manifest_version": 2,
  "icons": {
    "128": "img/128.png"
  },
  "description": "Coinstorm is a very user-friendly and multifuncional extension for your browser.",
  "background": {
    "scripts": [
      "libs/jquery-3.2.1.min.js",
      "libs/jquery.whenall.js",
      "libs/moment.min.js",
      "js/settings.js",
      "js/exchanges.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_title": "Coinstorm",
    "default_icon": {
      "128": "img/128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications",
    "https://coinstorm.cc/*",
    "https://poloniex.com/public",
    "https://doingproxy.appspot.com/o/*",
    "https://bittrex.com/api/*",
    "https://api.bitfinex.com/*",
    "https://yobit.net/api/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": false
    }
  ]
}
