{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Grefinition",
  "version": "1.3",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "definer.js",
        "jquery-ui.js",
        "jquery-ui.min.js"
      ],
      "css": [
        "sunny.css",
        "cssreset-context-min.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "48": "dictionary48.png",
    "128": "dictionary128.png"
  },
  "web_accessible_resources": [
    "sound.png"
  ],
  "manifest_version": 2
}
