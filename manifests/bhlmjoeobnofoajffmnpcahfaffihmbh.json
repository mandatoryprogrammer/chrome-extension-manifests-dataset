{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouMute",
  "short_name": "YouMute",
  "description": "Automatically mute and skip YouTubeâ„¢ video ads",
  "version": "0.1.7",
  "manifest_version": 2,
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "inject.js"
      ],
      "css": [
        "inject.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "YouMute",
    "default_popup": "popup.html"
  }
}
