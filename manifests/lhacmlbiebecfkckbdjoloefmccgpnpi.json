{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fly-cat Webpage Awesome-izer!",
  "short_name": " Fly-cat! ",
  "version": "2.0.1",
  "permissions": [
    "activeTab"
  ],
  "description": "Jazz up all of the sites you visit with a mellow type of ambiance that only the Fly-cat can deliver.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "catQuote.js"
      ],
      "css": [
        "styles.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
