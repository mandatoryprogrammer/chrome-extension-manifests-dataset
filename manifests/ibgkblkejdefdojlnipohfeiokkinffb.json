{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Password Uniquifier",
  "description": "Generates unique passwords for any site you visit from a master password",
  "version": "0.1.1",
  "page_action": {
    "default_title": "Generate password for this site",
    "default_icon": {
      "38": "icons/uniquify-38.png",
      "19": "icons/uniquify-19.png"
    },
    "default_popup": "www/index.html"
  },
  "icons": {
    "128": "icons/uniquify-128.png",
    "38": "icons/uniquify-38.png",
    "19": "icons/uniquify-19.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ]
}
