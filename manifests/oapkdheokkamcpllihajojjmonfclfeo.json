{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fry Tally",
  "version": "1.1.0",
  "author": "Sarah Joy Calpo",
  "description": "A new tab that tallies how many tabs you've opened... in french fries.",
  "background": {
    "scripts": [
      "newTab.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
