{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.1",
  "icons": {
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "permissions": [
    "tabs"
  ],
  "options_page": "src/options.html",
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
