{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon_19.png",
    "default_title": "Ekşi Sözlük Göz Yakmayan Tema"
  },
  "content_scripts": [
    {
      "js": [
        "citrumnigrum.js"
      ],
      "matches": [
        "http://eksisozluk.com/*",
        "https://eksisozluk.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Ekşi Sözlük Göz Yakmayan Tema",
  "icons": {
    "128": "images/128.png",
    "19": "images/19.png",
    "256": "images/256.png",
    "48": "images/48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Ekşi Sözlük Göz Yakmayan Tema",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "background",
    "http://eksisozluk.com/*",
    "https://eksisozluk.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "web_accessible_resources": [
    "citrumnigrum.css"
  ]
}
