{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.6",
  "name": "decyber",
  "description": "Rewriting Cybersecurity for Human Beings",
  "homepage_url": "http://www.crypticide.com/cgi-bin/decyber",
  "permissions": [
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "decyber.js"
    ]
  },
  "icons": {
    "16": "decyber-16.png",
    "48": "decyber-48.png",
    "128": "decyber-128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "decyberlib.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "manifest_version": 2,
  "options_page": "options.html"
}
