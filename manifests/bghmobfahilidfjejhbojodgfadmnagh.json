{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Butt To Cloud",
  "version": "1.0.1",
  "description": "Replaces the text 'butt' and 'my butt' with 'cloud' and 'the cloud'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
