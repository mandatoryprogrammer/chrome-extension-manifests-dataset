{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": [
    {
      "email": "ronchalant@gmail.com",
      "name": "Ron Rudy"
    }
  ],
  "name": "Single Click for for Googleâ„¢ Apps",
  "description": "Allow users to select apps from App Launcher that will give single-click access",
  "version": "0.3",
  "icons": {
    "48": "images/sandbar-icon-48.png",
    "128": "images/sandbar-icon-128.png"
  },
  "permissions": [
    "storage",
    "https://mail.google.com/*",
    "https://www.google.com/*"
  ],
  "content_scripts": [
    {
      "css": [],
      "js": [
        "src/jquery.js",
        "app/restore.js"
      ],
      "matches": [
        "http://mail.google.com/*",
        "https://mail.google.com/*",
        "https://www.google.com/calendar/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/jquery.min.map"
  ]
}
