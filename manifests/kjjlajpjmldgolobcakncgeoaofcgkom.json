{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TwitchEmotes",
  "version": "2.11",
  "description": "Displays Twitch, BetterTTV and FrankerFaceZ emotes on all sites.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "js": [
        "jquery-3.3.1.min.js",
        "replacer.js"
      ]
    }
  ],
  "options_page": "settings.html"
}
