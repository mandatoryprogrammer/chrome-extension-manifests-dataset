{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Near Future",
  "description": "Displays the current date and a few subsequent days in globally inambiguous formats",
  "version": "1.1",
  "browser_action": {
    "default_icon": "ic_today_black_24dp_2x.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
