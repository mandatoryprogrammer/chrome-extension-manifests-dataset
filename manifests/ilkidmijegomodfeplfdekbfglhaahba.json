{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.5.0.4",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-128.png",
    "48": "icon-128.png"
  },
  "author": "Jacob „kurtextrem\" Groß",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "minimum_chrome_version": "49",
  "permissions": [
    "*://mail.google.com/*"
  ]
}
