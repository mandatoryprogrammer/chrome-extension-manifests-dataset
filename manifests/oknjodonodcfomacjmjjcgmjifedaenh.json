{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sldshw",
  "version": "0.0.1",
  "description": "An image slideshow that's to the point and that works.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Slideshow",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "events.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "jquery.slides.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
