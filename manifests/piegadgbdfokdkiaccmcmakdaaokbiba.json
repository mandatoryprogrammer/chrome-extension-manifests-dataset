{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tofler It!",
  "description": "Instantly find information on any Indian company, anywhere.",
  "version": "1.0",
  "permissions": [
    "https://www.tofler.in/",
    "contextMenus",
    "tabs"
  ],
  "background": {
