{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter FollowsImages",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "For twitter, add Menu(search images with follower's filter), and Set background",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*.twitter.com/"
  ]
}
