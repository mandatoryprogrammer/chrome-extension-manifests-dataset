{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple New Tab",
  "description": "Replace default new tab with random one of your uploaded pictures",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html"
}
