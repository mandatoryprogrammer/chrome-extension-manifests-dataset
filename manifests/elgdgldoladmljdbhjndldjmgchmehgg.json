{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pin My Screen",
  "version": "2.1",
  "description": "Pin any page you visit into Pinterest to create your bookmark boards.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Pin My Screen",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self';"
}
