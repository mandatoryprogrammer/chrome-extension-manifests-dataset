{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SpaceFox",
  "description": "SpaceFox helps you manage your tabs. You place each topic into its own space/window and SpaceFox keeps track of them for you.",
  "version": "0.1.6",
  "author": "Christian Zangl",
  "homepage_url": "https://github.com/laktak/spacefox",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "sessions"
  ],
  "icons": {
    "96": "icons/spacefox.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "src/spacefox.js",
      "src/names.js",
      "src/main.js"
    ]
  },
  "incognito": "not_allowed",
  "browser_action": {
    "default_icon": "icons/spacefox.png",
    "default_title": "SpaceFox",
    "default_popup": "popup.html",
    "browser_style": true
  }
}
