{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "fpl copy team to clipboard",
  "manifest_version": 2,
  "name": "FPL Team Copy",
  "version": "1.1",
  "icons": {
    "48": "icons/clipboard.png"
  },
  "permissions": [
    "*://*.premierleague.com/*",
    "webRequest",
    "clipboardRead",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icons/clipboard.png",
    "default_title": "Copy your FPL team"
  },
  "background": {
    "scripts": [
      "fpltxt.js"
    ]
  }
}
