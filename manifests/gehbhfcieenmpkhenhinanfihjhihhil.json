{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "RedditAutoTabs",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Open all links on the page in new tabs",
  "page_action": {
    "default_icon": "icon.gif",
    "default_title": "Open all links on this page in new tabs"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "http://*.reddit.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": false
  }
}
