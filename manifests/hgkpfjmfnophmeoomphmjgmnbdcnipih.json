{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FragMrRobot",
  "version": "0.3.5.0",
  "manifest_version": 2,
  "default_locale": "de",
  "description": "Partial(!) AskMrRobot translation",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.1.4.min.js",
        "translations.js",
        "FragMrRobot.js"
      ],
      "matches": [
        "*://www.askmrrobot.com/wow/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "jquery-2.1.4.min.js",
        "EisigeAdern.js"
      ],
      "matches": [
        "http://www.icy-veins.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "*://*.wowhead.com/",
    "storage",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "FragMrRobot",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Alexander Lanin"
}
