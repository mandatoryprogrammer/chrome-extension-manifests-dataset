{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Less is more",
  "description": "\"Less is more\" - get rid of all the unnecessary clutter on facebook",
  "icons": {
    "128": "bower/icon-128x128.png"
  },
  "author": "Maciej Kr√≥l <mail@maciejkrol.pl",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "scripts/script.js"
      ]
    }
  ]
}
