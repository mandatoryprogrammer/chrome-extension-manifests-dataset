{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG__appName__",
  "manifest_version": 2,
  "version": "0.3.2.7",
  "description": "__MSG__appDesc__",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "zh_TW",
  "permissions": [
    "tabs",
    "storage",
    "https://tw.dictionary.yahoo.com/*"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Batch Dict"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://tw.dictionary.search.yahoo.com/*"
      ],
      "css": [
        "myDict.css"
      ],
      "js": [
        "myDict.js"
      ]
    }
  ]
}
