{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "ayinboxpre.js"
      ],
      "matches": [
        "http://inbox.google.com/*",
        "https://inbox.google.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Tweak Google Inbox to see and do more at once.",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "6",
  "name": "Power Google Inbox",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "img/icon19.png",
    "default_title": "Power Google Inbox"
  },
  "permissions": [
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2016.4.28"
}
