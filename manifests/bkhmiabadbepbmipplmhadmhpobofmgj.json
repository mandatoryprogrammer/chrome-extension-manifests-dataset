{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Custom Bookmarks",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "build/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "bookmarks": "bookmarks.html"
  },
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Custom Bookmarks"
  }
}
