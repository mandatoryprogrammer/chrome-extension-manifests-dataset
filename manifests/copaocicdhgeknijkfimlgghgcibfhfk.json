{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "„Realita“ - svk",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Z reforiem spraví „reformy“, z ľavicovej vlády „ľavicovú vládu“, z úspechu „úspech“. Autor pôvodnej českej verzie - Michal Kašpárek",
  "permissions": [
    "http://sme.sk/*",
    "http://*.sme.sk/*",
    "http://aktuality.sk/*",
    "http://*.aktuality.sk/*",
    "http://hnonline.sk/*",
    "http://*.hnonline.sk/*",
    "http://*.etrend.sk/*",
    "http://etrend.sk/*",
    "http://topky.sk/*",
    "http://*.topky.sk/*",
    "http://aktualne.sk/*",
    "http://*.aktualne.sk/*",
    "http://pravda.sk/*",
    "http://*.pravda.sk/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://sme.sk/*",
        "http://*.sme.sk/*",
        "http://aktuality.sk/*",
        "http://*.aktuality.sk/*",
        "http://hnonline.sk/*",
        "http://*.hnonline.sk/*",
        "http://*.etrend.sk/*",
        "http://etrend.sk/*",
        "http://topky.sk/*",
        "http://*.topky.sk/*",
        "http://aktualne.sk/*",
        "http://*.aktualne.sk/*",
        "http://pravda.sk/*",
        "http://*.pravda.sk/*"
      ],
      "js": [
        "lib/xregexp-min.js",
        "lib/unicode-base.js",
        "skript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
