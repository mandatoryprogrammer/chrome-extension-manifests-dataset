{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sun Never Sets on Us",
  "short_name": "Never Sets",
  "description": "Share (but don't overshare) your location",
  "version": "0.1.6",
  "browser_action": {
    "default_icon": "japanese_sunset16.png"
  },
  "icons": {
    "128": "japanese_sunset128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://www.sunneversetson.us/*",
      "*://google.com.kh/*",
      "http://localhost:8000/*",
      "http://localhost:5000/*"
    ]
  },
  "permissions": [
    "geolocation",
    "background",
    "tabs",
    "http://sunneversetson.us/*"
  ],
  "background": {
    "scripts": [
      "jquery-2.1.4.min.js",
      "background.js"
    ]
  }
}
